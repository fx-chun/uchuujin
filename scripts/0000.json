[
    {
        "id": 0,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2144,
            "text_len": 18,
            "text_offset": 2150,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "んっ、んん……。\u0000\n",
        "text_translation": "Nn... nnn...",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2184,
            "text_len": 16,
            "text_offset": 2192,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "Hakase",
        "text": "あっ、起きた。\u0000\n",
        "text_translation": "Oh, (you?) woke up.",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2322,
            "text_len": 46,
            "text_offset": 2328,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "うう……はかせ？\u0000\nもう起きてたんですか……？\u0000\n",
        "text_translation": "Ugh... Hakase? Are you having trouble sleeping again?",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2412,
            "text_len": 12,
            "text_offset": 2418,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "…………。\u0000\n",
        "text_translation": "...",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2466,
            "text_len": 40,
            "text_offset": 2472,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "（はかせの持ってるボタンって……何？）\u0000\n",
        "text_translation": "(What's that button Hakase's holding?)\u0000\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2536,
            "text_len": 18,
            "text_offset": 2544,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "Hakase",
        "text": "おはよ―、なの。\u0000\n",
        "text_translation": "Good morning Nano!\u0000\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2584,
            "text_len": 48,
            "text_offset": 2590,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "お、おはようございます。\u0000\nところではかせ……。\u0000\n",
        "text_translation": "Good morning, Hakase. \u0000\nBy the way...\u0000\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2660,
            "text_len": 10,
            "text_offset": 2668,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "Hakase",
        "text": "なに―？\u0000\n",
        "text_translation": "Hm?\u0000\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2708,
            "text_len": 72,
            "text_offset": 2714,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "起きていきなりでアレですけど、\u0000\nそのボタンで私に何をするつもりですか？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2810,
            "text_len": 70,
            "text_offset": 2818,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "Hakase",
        "text": "なののネジをパワ―アップさせたから\u0000\n新しい機能を試すところだったの。\u0000\n",
        "text_translation": "I was about to try new features,\nso I made the screw on your back power up.",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2914,
            "text_len": 46,
            "text_offset": 2920,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "えぇっ！？\u0000\nもう何かした後だったんですか！？\u0000\n",
        "text_translation": "Eh!? Did you do something to me again!?",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2988,
            "text_len": 42,
            "text_offset": 2996,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "Hakase",
        "text": "何かじゃないよ！\u0000\nパワ―アップさせたの！\u0000\n　；",
        "text_translation": "It's nothing! It's a (power up?)!",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3070,
            "text_len": 58,
            "text_offset": 3076,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "しないでくださいよ〜。\u0000\nただでさえネジで困ってるのに〜。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3156,
            "text_len": 24,
            "text_offset": 3164,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "Hakase",
        "text": "でもパワ―アップだよ？\u0000\n",
        "text_translation": "But it's a power-up?",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3210,
            "text_len": 64,
            "text_offset": 3216,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "一番なくしてほしいネジを\u0000\nパワ―アップされても困るんですよ―。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3312,
            "text_len": 38,
            "text_offset": 3318,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "阪本さんも何とか言ってくださいよ―。\u0000\n",
        "text_translation": "Sakamoto, say something.",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3382,
            "text_len": 12,
            "text_offset": 3390,
            "warnings": 0
        },
        "speaker": "バディ",
        "speaker_translation": "Buddy",
        "text": "…………。\u0000\n",
        "text_translation": "...",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 3436,
            "text_len": 22,
            "text_offset": 3450,
            "warnings": 0
        },
        "speaker": "なの＆はかせ",
        "speaker_translation": "Nano & Hakase",
        "text": "！？　犬だ―――！！\u0000\n",
        "text_translation": "Eh!? It's a dog!!",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3552,
            "text_len": 58,
            "text_offset": 3556,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "くそっ、猫がいるんだから\u0000\n犬でも誤魔化せると思ったのに！\u0000\n",
        "text_translation": "Damn, because I have a cat, I thought that even a dog could be deviated!",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3650,
            "text_len": 30,
            "text_offset": 3654,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "あっ！　アンテナが折られた！\u0000\n",
        "text_translation": "Ah! The antenna was broken!",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3726,
            "text_len": 72,
            "text_offset": 3730,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "……判断を間違えたな。俺よりも\u0000\n地球人たちの方が一枚上手だったらしい。\u0000\n",
        "text_translation": "...looks like I made a mistake. The Earthlings are better than I am.",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3818,
            "text_len": 12,
            "text_offset": 3822,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "ああ……。\u0000\n",
        "text_translation": "Ah...",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3850,
            "text_len": 60,
            "text_offset": 3854,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "でも、なんで犬で行ったんだ？\u0000\n最初からいた猫でよくないか？\u0000\n",
        "text_translation": "But why did you choose a dog? Wasn't the cat from the beginning fine?",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3934,
            "text_len": 6,
            "text_offset": 3938,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "!?",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3960,
            "text_len": 48,
            "text_offset": 3964,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "……俺よりもお前の方が\u0000\n一枚上手だったらしい。\u0000\n",
        "text_translation": "...You seem to have a better player than me.",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4032,
            "text_len": 14,
            "text_offset": 4036,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "えええっ！？\u0000\n",
        "text_translation": "Ehh!?",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4142,
            "text_len": 50,
            "text_offset": 4148,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "改造は許しますから\u0000\nせめてネジを取ってください。\u0000\n",
        "text_translation": "(I'll forgive you for the other modifications), but please remove the key.",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4222,
            "text_len": 46,
            "text_offset": 4230,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "Hakase",
        "text": "あっ、そうだなの―。\u0000\nはかせ、おなかすいた。\u0000\n",
        "text_translation": "Oh, I see... I'm hungry.",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4298,
            "text_len": 50,
            "text_offset": 4304,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "誤魔化さないで、\u0000\n今すぐネジを取ってくださいよ。\u0000\n",
        "text_translation": "Please don't change the topic! Please take it off now!",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4380,
            "text_len": 52,
            "text_offset": 4388,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "Hakase",
        "text": "ごまかしてない！\u0000\nはかせ本当にお腹すいたんだもん！\u0000\n",
        "text_translation": "I'm not lying! I'm really hungry!",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4462,
            "text_len": 22,
            "text_offset": 4468,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "そうじゃなくて……。\u0000\n．",
        "text_translation": "I didn't mean it like that...",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4552,
            "text_len": 44,
            "text_offset": 4558,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "ネジです。ネ・ジ！\u0000\nネジを取ってください！\u0000\n",
        "text_translation": "It's the key. The. Key. Please take off the key!",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4628,
            "text_len": 46,
            "text_offset": 4636,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "Hakase",
        "text": "ちがうもん！\u0000\nネジじゃなくてお腹がすいたの！\u0000\n",
        "text_translation": "(It's different!) It's not about the key, I'm hungry!",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4708,
            "text_len": 58,
            "text_offset": 4714,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "だから、ご飯の前にネジを\u0000\n取ってほしいと言ってるんです！\u0000\n",
        "text_translation": "I'm asking you to take off the key before we eat!",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4798,
            "text_len": 8,
            "text_offset": 4806,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "Hakase",
        "text": "やだ！\u0000\n",
        "text_translation": "No!",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4840,
            "text_len": 62,
            "text_offset": 4846,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "私ははかせがネジを取ってくれるまで\u0000\n朝ご飯作りませんからね！\u0000\n",
        "text_translation": "I'm not going to make breakfast until you take off my key!",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4934,
            "text_len": 52,
            "text_offset": 4942,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "Hakase",
        "text": "いいもん！\u0000\nなぜなら、お菓子がいっぱいあるから―！\u0000\n",
        "text_translation": "Fine! I'll just have snacks for breakfast.",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5020,
            "text_len": 40,
            "text_offset": 5026,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "お菓子を朝ご飯代わりにしちゃダメです！\u0000\n",
        "text_translation": "You shouldn't eat snacks for breakfast!",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5092,
            "text_len": 24,
            "text_offset": 5100,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "Hakase",
        "text": "じゃあ、朝ご飯作って！\u0000\n",
        "text_translation": "Then make breakfast!",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5150,
            "text_len": 16,
            "text_offset": 5156,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "わかりました！\u0000\n",
        "text_translation": "Fine!",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5296,
            "text_len": 24,
            "text_offset": 5302,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "はい！　朝ご飯ですよ！\u0000\n",
        "text_translation": "Yes! Breakfast!",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5348,
            "text_len": 28,
            "text_offset": 5356,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "Hakase",
        "text": "わ―い！\u0000\nいただきま―す！\u0000\n",
        "text_translation": "Nice! Let's eat!",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5412,
            "text_len": 12,
            "text_offset": 5418,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "…………。\u0000\n",
        "text_translation": "...",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5460,
            "text_len": 38,
            "text_offset": 5466,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "（何、普通に朝ご飯作ったの、私！？）\u0000\n　。",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5540,
            "text_len": 70,
            "text_offset": 5546,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "（違う、そうじゃない。ネジを\u0000\n　取ってもらわないと私は学校にも弌弌）\u0000\n",
        "text_translation": "((?) If I don't get the key off by the time I go to school--)",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5642,
            "text_len": 26,
            "text_offset": 5648,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "学校！？\u0000\nい、今何時！？\u0000\n",
        "text_translation": "School!? What time is it now!?",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5696,
            "text_len": 62,
            "text_offset": 5702,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "ああああ――！？　もう遅刻寸前！！\u0000\nはかせ、行ってきま―す！\u0000\n　，",
        "text_translation": "Ahh!? I'm already late!! Hakase, I'm going!",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5878,
            "text_len": 22,
            "text_offset": 5886,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "Hakase",
        "text": "行ってらっしゃ―い！\u0000\n　？￣￣　〆　｝なのネジを取ってください。\u0000\nこれがある限り私は家事をしませんからね。\u0000\n",
        "text_translation": "Goodbye! Nano: Please take off my key. I will not do any chores until it's off.",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6054,
            "text_len": 34,
            "text_offset": 6062,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "Hakase",
        "text": "ならまた別の子作るからいいもん！\u0000\n",
        "text_translation": "Then I will make another robot to do the chores!",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6138,
            "text_len": 6,
            "text_offset": 6144,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "！？\u0000\n　：",
        "text_translation": "!?",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6194,
            "text_len": 12,
            "text_offset": 6200,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "…………。\u0000\n",
        "text_translation": "...",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6234,
            "text_len": 12,
            "text_offset": 6242,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "Hakase",
        "text": "あっ……。\u0000\n",
        "text_translation": "Ah...",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6276,
            "text_len": 44,
            "text_offset": 6284,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "Hakase",
        "text": "嘘だよ。はかせは、\u0000\nなの以外は作らないよ。\u0000\n　？",
        "text_translation": "Just kidding. I would never make a replacement for you.",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6364,
            "text_len": 52,
            "text_offset": 6370,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "……本当ですか？\u0000\n私、いらない子じゃないんですか？\u0000\n",
        "text_translation": "...really? Am I really irreplaceable?",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6444,
            "text_len": 60,
            "text_offset": 6452,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "Hakase",
        "text": "いらなくないよ！\u0000\nはかせ、なのにご飯食べさせて欲しいもん！\u0000\n　！",
        "text_translation": "Of course not! (??)",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6544,
            "text_len": 44,
            "text_offset": 6550,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "はかせ……！\u0000\nはい！　私、朝ご飯作ります！\u0000\n",
        "text_translation": "Hakase...! Okay! I'm going to make breakfast!",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6712,
            "text_len": 18,
            "text_offset": 6720,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "Hakase",
        "text": "ごちそうさま―！\u0000\n",
        "text_translation": "Thank you for the meal!",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6764,
            "text_len": 24,
            "text_offset": 6770,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "はい。お粗末さまです。\u0000\n",
        "text_translation": "???",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6816,
            "text_len": 40,
            "text_offset": 6824,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "Hakase",
        "text": "あっ、なの。\u0000\nそろそろ学校の時間だよ。\u0000\n",
        "text_translation": "Oh, Nano. It's time for school.",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6886,
            "text_len": 50,
            "text_offset": 6892,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "あっ、本当ですね。\u0000\nじゃあはかせ、行ってきます。\u0000\n",
        "text_translation": "Ah, right. Okay then, I'm going to go.",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7046,
            "text_len": 22,
            "text_offset": 7054,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "Hakase",
        "text": "行ってらっしゃ―い。\u0000\n　？，￣　　なのじゃあ、ネジを取ってくれたら\u0000\n朝ご飯作ります。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7210,
            "text_len": 8,
            "text_offset": 7218,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "Hakase",
        "text": "やだ！\u0000\n　。",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7258,
            "text_len": 44,
            "text_offset": 7264,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "何でですか？\u0000\n朝ご飯食べられないんですよ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7330,
            "text_len": 56,
            "text_offset": 7338,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "Hakase",
        "text": "だってはかせ、ネジのために\u0000\n早起きして頑張ったんだよ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7416,
            "text_len": 46,
            "text_offset": 7422,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "こんなことのために\u0000\n早起きしたんですか……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7490,
            "text_len": 50,
            "text_offset": 7498,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "Hakase",
        "text": "せっかく頑張ったのに\u0000\nもう取っちゃうなんてヤダ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7570,
            "text_len": 74,
            "text_offset": 7576,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "……仕方ないですね。それじゃあ、\u0000\n一回だけ新機能を試してもいいですから。\u0000\n　？",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7682,
            "text_len": 24,
            "text_offset": 7690,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "Hakase",
        "text": "はかせお腹減った……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7736,
            "text_len": 52,
            "text_offset": 7744,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "Hakase",
        "text": "疲れてお腹も減ってるのに、\u0000\n朝ご飯食べられないの？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7818,
            "text_len": 12,
            "text_offset": 7824,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "うっ……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7858,
            "text_len": 40,
            "text_offset": 7864,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "Nano",
        "text": "……わかりました。\u0000\n先に朝ご飯ですね。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7926,
            "text_len": 28,
            "text_offset": 7934,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "わ―い！\u0000\nなの大好き――！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8080,
            "text_len": 18,
            "text_offset": 8088,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "ごちそうさま―！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8132,
            "text_len": 34,
            "text_offset": 8138,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "それじゃあはかせ、早くネジを弌弌\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8194,
            "text_len": 46,
            "text_offset": 8202,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "あっ、なの、もう時間だよ？\u0000\n学校行かないと。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8274,
            "text_len": 54,
            "text_offset": 8280,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "わわわっ！？　もうこんな時間！\u0000\nで、でもネジが……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8356,
            "text_len": 36,
            "text_offset": 8362,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "わ―ん！\u0000\nはかせ、行ってきま―す！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8512,
            "text_len": 22,
            "text_offset": 8520,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "行ってらっしゃ―い。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    }
