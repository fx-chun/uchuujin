        "id": 34,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5010,
            "text_len": 10,
            "text_offset": 5016,
            "warnings": 0
        },
        "speaker": "みお",
        "speaker_translation": "",
        "text": "ぃよし！\u0000\n",
        "text_translation": "",
        "type": "dialog"
