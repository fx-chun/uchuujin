        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2236,
            "text_len": 50,
            "text_offset": 2242,
            "warnings": 0
        },
        "speaker": "みお",
