[
    {
        "id": 0,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2138,
            "text_len": 26,
            "text_offset": 2146,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "あれ？　あの後姿は……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2194,
            "text_len": 54,
            "text_offset": 2202,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "笹原じゃない。\u0000\nあいつ、こんなところで何してんのよ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2278,
            "text_len": 46,
            "text_offset": 2286,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "まさか、\u0000\nまたはた迷惑なことしてるんじゃ弌弌\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2436,
            "text_len": 40,
            "text_offset": 2444,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "外で食事！！\u0000\nしかもあれは……カニ！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2506,
            "text_len": 66,
            "text_offset": 2512,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "バリッ！　ベキッ！　ホジホジ……\u0000\nパクリ……モグモグ……ゴクン。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2690,
            "text_len": 14,
            "text_offset": 2696,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "うむ、美味。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2790,
            "text_len": 52,
            "text_offset": 2798,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "笹原！　あんた、\u0000\nこんなところで何やってんのよ！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2884,
            "text_len": 44,
            "text_offset": 2890,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "？　私は一人静かに食事をしていただけだが。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2956,
            "text_len": 58,
            "text_offset": 2964,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "なにが静かによ！\u0000\nカニ解体する音がまわりに響いてるわよ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3044,
            "text_len": 50,
            "text_offset": 3050,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "……そうであったか。\u0000\nそれはすまないことをした。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3126,
            "text_len": 62,
            "text_offset": 3132,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "（カニ三昧フルコ―スが\u0000\n　もれなく焼きガニになってしまった）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3234,
            "text_len": 62,
            "text_offset": 3244,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "お、カニか！　美味そうだな。\u0000\n一度食ってみたかったんだよな！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3326,
            "text_len": 10,
            "text_offset": 3334,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "……え？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3360,
            "text_len": 8,
            "text_offset": 3366,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "むっ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3404,
            "text_len": 46,
            "text_offset": 3414,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "（し、しまった！\u0000\n　つい匂いにつられて……）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3486,
            "text_len": 14,
            "text_offset": 3492,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "こやつ……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3528,
            "text_len": 14,
            "text_offset": 3538,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "に、にゃ〜。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3576,
            "text_len": 26,
            "text_offset": 3584,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "ちょ、ちょっと笹原……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3632,
            "text_len": 40,
            "text_offset": 3638,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "猫の分際で大儀である。\u0000\n一杯進ぜよう。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3706,
            "text_len": 62,
            "text_offset": 3716,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "（え、喋ったことはスル―！？\u0000\n　……ま、いいや、ラッキ―！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3802,
            "text_len": 42,
            "text_offset": 3810,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "やめなさいよ！\u0000\nあんた、何考えてんのよ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3874,
            "text_len": 62,
            "text_offset": 3882,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "猫にカニは良くないのよ！\u0000\nビタミンＡ１分解酵素がどうとかで！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3972,
            "text_len": 24,
            "text_offset": 3982,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "（え、そうなのか！？）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4034,
            "text_len": 76,
            "text_offset": 4042,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "だから、ごめんね。ところで笹原弌弌\u0000\nあ、死にたくなかったらあっち行ってね。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4150,
            "text_len": 68,
            "text_offset": 4160,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "（ぬおおおおお〜〜〜〜っ！？\u0000\n　警告してすぐ撃つんじゃねえ〜〜！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4304,
            "text_len": 52,
            "text_offset": 4308,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "ふう、危ないなぁ。\u0000\nぎりぎり避けられてよかったぜ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4388,
            "text_len": 12,
            "text_offset": 4392,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "……あっ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4428,
            "text_len": 18,
            "text_offset": 4432,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "ん？　どうした？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4492,
            "text_len": 28,
            "text_offset": 4496,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "アンテナに……命中してた。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4616,
            "text_len": 56,
            "text_offset": 4622,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "ところで立花みさとよ。\u0000\n１つ聞きたいことがあるのだが。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4700,
            "text_len": 24,
            "text_offset": 4708,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "え？　な、なによ……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4754,
            "text_len": 54,
            "text_offset": 4760,
            "warnings": 2
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "俗世にはカニカマなる庶民の食品が\u0000\nあるらしいが。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4836,
            "text_len": 48,
            "text_offset": 4842,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "あれは何なのだ？\u0000\nカニなのか、カマボコなのか。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4912,
            "text_len": 74,
            "text_offset": 4918,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "一体どんな味がするのものなのか……\u0000\nまったく想像できんのだ。教えてくれ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5014,
            "text_len": 62,
            "text_offset": 5022,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "ど、どんなって言われても……\u0000\nカニっぽい味のするカマボコよ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5106,
            "text_len": 58,
            "text_offset": 5112,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "カニっぽい味のカマボコと言われても……\u0000\nどうも解せんな。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5194,
            "text_len": 58,
            "text_offset": 5200,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "カニの味を楽しみたいなら\u0000\nカニを食べれば良いではないか。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5280,
            "text_len": 56,
            "text_offset": 5286,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "カマボコを食べたいなら\u0000\nカマボコを食べれば良いはずだ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5374,
            "text_len": 80,
            "text_offset": 5380,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "あえてカニカマを食すのは、カニカマ独自の\u0000\n味わいを求めているからではないのか。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5482,
            "text_len": 66,
            "text_offset": 5488,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "私はそれが知りたい。\u0000\nカニカマとは、いかなる味がするものなのか。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5616,
            "text_len": 52,
            "text_offset": 5624,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "ってか、気になるなら\u0000\n実際食べてみればいいでしょ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5698,
            "text_len": 86,
            "text_offset": 5704,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "何を馬鹿な。笹原家の長男ともあろう者が、\u0000\nあのような庶民の食品を買うなど考えられん。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5862,
            "text_len": 12,
            "text_offset": 5868,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5988,
            "text_len": 52,
            "text_offset": 5996,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "何が庶民の食品よ！\u0000\nカニカマ、馬鹿にしないでよね！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6068,
            "text_len": 50,
            "text_offset": 6076,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "っていうか、\u0000\nあんたん家だってただの農家でしょ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6142,
            "text_len": 68,
            "text_offset": 6148,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "農家だからこそだ。漁師ならば\u0000\nカニカマも食す機会があるのだろうが。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6236,
            "text_len": 60,
            "text_offset": 6244,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "関係ないでしょ！\u0000\n漁師が水揚げしてるわけじゃないんだから！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6330,
            "text_len": 24,
            "text_offset": 6336,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "漁師が……水揚げ……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6474,
            "text_len": 36,
            "text_offset": 6480,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "……ぷっ、はははは。これは愉快だ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6538,
            "text_len": 76,
            "text_offset": 6544,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "立花みさとにも可愛いところがあるのだな。\u0000\nカニカマが海で泳いでいる、とは！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6656,
            "text_len": 42,
            "text_offset": 6664,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "なっ！？\u0000\nな、な、な、何言ってるのよ！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6728,
            "text_len": 32,
            "text_offset": 6734,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "なに、恥ずかしがることはない。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6788,
            "text_len": 62,
            "text_offset": 6794,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "女人であれば、メルヘンチックな空想も\u0000\n許されようというもの。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6878,
            "text_len": 68,
            "text_offset": 6884,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "しかし……立花みさとがな……\u0000\nそうか……人には意外な一面がある弌弌\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7108,
            "text_len": 12,
            "text_offset": 7114,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7242,
            "text_len": 60,
            "text_offset": 7250,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "なに馬鹿なこと言ってるのよ！\u0000\nいい加減なこと言わないで！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7332,
            "text_len": 64,
            "text_offset": 7340,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "私が可愛いだなんて、\u0000\nそんな、そんな、そんな〜〜〜〜〜〜っ！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7536,
            "text_len": 12,
            "text_offset": 7542,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7612,
            "text_len": 58,
            "text_offset": 7620,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "公共の公園で下らないこと考えないでよ！\u0000\n近所迷惑なのよ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7700,
            "text_len": 68,
            "text_offset": 7706,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "いやしかし、疑問は解決するに如かず。\u0000\n気になるものは気になるのだ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7796,
            "text_len": 60,
            "text_offset": 7804,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "あ―もうめんどくさいわね！\u0000\nちょっとそこで待ってなさいよ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7912,
            "text_len": 4,
            "text_offset": 7918,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8058,
            "text_len": 48,
            "text_offset": 8066,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "ほら、買ってきてあげたわ。\u0000\nこれがカニカマよ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8136,
            "text_len": 66,
            "text_offset": 8142,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "そうか、これが夢にまで見た……\u0000\n立花みさとよ、わざわざすまぬな。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8230,
            "text_len": 40,
            "text_offset": 8238,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "い、いいから！\u0000\nさっさと食べなさいよ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8300,
            "text_len": 36,
            "text_offset": 8306,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "うむ、ではさっそく。馳走になろう。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8374,
            "text_len": 32,
            "text_offset": 8382,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "（あ、あれ！？　しまった！！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8440,
            "text_len": 54,
            "text_offset": 8448,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "（間違えて笹かま\u0000\n　買って来ちゃった〜〜〜〜っ！！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8540,
            "text_len": 66,
            "text_offset": 8546,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "ふむ、これがカニカマか。\u0000\nしかし、これの一体どこがカニなのやら。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8634,
            "text_len": 64,
            "text_offset": 8642,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "ち、違うの！　そ、その……\u0000\nそれはカニカマじゃなくて笹かまよ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8734,
            "text_len": 58,
            "text_offset": 8742,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "笹原のこと考えてたら……\u0000\n笹ってのが頭にあったから……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8822,
            "text_len": 28,
            "text_offset": 8828,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "なるほど、これは笹かまか。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8878,
            "text_len": 80,
            "text_offset": 8884,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "カニカマがカニのようなカマボコなら、\u0000\n笹かまは私のようなカマボコなのだろうな。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8986,
            "text_len": 6,
            "text_offset": 8994,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "は？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9022,
            "text_len": 58,
            "text_offset": 9028,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "気に入った！\u0000\n今日からこの笹かまの名は笹原カマジロ―だ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9112,
            "text_len": 12,
            "text_offset": 9120,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9154,
            "text_len": 70,
            "text_offset": 9160,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "うむ。と言うわけで、今日の記念にカマジロ―を\u0000\n立花みさとに進ぜよう。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9262,
            "text_len": 26,
            "text_offset": 9270,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "え？　わ、私にくれるの？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9318,
            "text_len": 54,
            "text_offset": 9324,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "うむ。せいぜい可愛がってやってくれ。\u0000\n……では失礼。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9406,
            "text_len": 24,
            "text_offset": 9414,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "笹原の……笹かま……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9542,
            "text_len": 10,
            "text_offset": 9550,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "ぱくっ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9620,
            "text_len": 12,
            "text_offset": 9626,
            "warnings": 0
        },
        "speaker": "執事",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9678,
            "text_len": 56,
            "text_offset": 9686,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "（あ、カニカマ……\u0000\n　あるならさっさと出しなさいよね）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9764,
            "text_len": 66,
            "text_offset": 9770,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "ふむ、これがカニカマというものか。\u0000\nではさっそく食してみるかな。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9858,
            "text_len": 50,
            "text_offset": 9864,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "赤い部分が甲羅か。\u0000\nなるほど、良くできているな。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 85,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9956,
            "text_len": 6,
            "text_offset": 9964,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 86,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10014,
            "text_len": 6,
            "text_offset": 10020,
            "warnings": 0
        },
        "speaker": "執事",
        "speaker_translation": "",
        "text": "！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 87,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 10056,
            "text_len": 64,
            "text_offset": 10064,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "（あ、赤いところを避けて食べてる……\u0000\n　甲羅だと思って……？）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 88,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10158,
            "text_len": 36,
            "text_offset": 10164,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "ふむ、これは……なんというか……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 89,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 10226,
            "text_len": 12,
            "text_offset": 10234,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 90,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10350,
            "text_len": 26,
            "text_offset": 10356,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "……なんとも、言えんな。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    }
]