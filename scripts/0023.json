[
    {
        "id": 0,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2144,
            "text_len": 38,
            "text_offset": 2150,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "いらっしゃいませ〜\u0000\nご注文をどうぞ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2210,
            "text_len": 24,
            "text_offset": 2218,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "う〜ん……そうね……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2268,
            "text_len": 66,
            "text_offset": 2276,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "カフェ・ラテのグランデを。\u0000\nあ、それにア―モンドシロップ追加で。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2364,
            "text_len": 20,
            "text_offset": 2370,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "かしこまりました。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2412,
            "text_len": 32,
            "text_offset": 2420,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "さて、どこに座ろうかなっと弌弌\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2482,
            "text_len": 6,
            "text_offset": 2490,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2526,
            "text_len": 52,
            "text_offset": 2532,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "む、奇遇だな。\u0000\n立花みさともコ―ヒ―ブレイクとは。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2652,
            "text_len": 26,
            "text_offset": 2660,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "何がコ―ヒ―ブレイクよ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2712,
            "text_len": 62,
            "text_offset": 2720,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "そんなもの被って！\u0000\nちょっとはお店の迷惑考えたらどうなの！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2844,
            "text_len": 66,
            "text_offset": 2850,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "ふむ、やはりそうか。\u0000\nでは今後カフェでの役作りは控えるとしよう。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2948,
            "text_len": 30,
            "text_offset": 2956,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "え！？　あ、あれ？　え―！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3016,
            "text_len": 38,
            "text_offset": 3024,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "うう……な、なぜ僕がこんな目に……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3102,
            "text_len": 22,
            "text_offset": 3110,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "え！？　べ、別人！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3158,
            "text_len": 42,
            "text_offset": 3166,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "なに？　なんなのこれ？　どういうこと！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3230,
            "text_len": 64,
            "text_offset": 3236,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "自分の役を客観的に見るということは\u0000\n演技において非常に重要だ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3322,
            "text_len": 20,
            "text_offset": 3330,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "……だから、何よ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3372,
            "text_len": 62,
            "text_offset": 3378,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "だから、通りがかりの下級生にあれを被せて\u0000\n観察していたのだ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3466,
            "text_len": 12,
            "text_offset": 3474,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3518,
            "text_len": 56,
            "text_offset": 3528,
            "warnings": 0
        },
        "speaker": "校長先生",
        "speaker_translation": "",
        "text": "通りすがりの下級生……\u0000\nということは、彼も私の生徒！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3606,
            "text_len": 54,
            "text_offset": 3616,
            "warnings": 0
        },
        "speaker": "校長先生",
        "speaker_translation": "",
        "text": "……おや？\u0000\nこれは一体、私はなぜこんなところに……？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3692,
            "text_len": 78,
            "text_offset": 3702,
            "warnings": 1
        },
        "speaker": "校長先生",
        "speaker_translation": "",
        "text": "いえ、そんなことより！　君は……\u0000\n１Ｐの中之条くんだね！　しっかりなさい！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3888,
            "text_len": 12,
            "text_offset": 3896,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3928,
            "text_len": 36,
            "text_offset": 3938,
            "warnings": 0
        },
        "speaker": "校長先生",
        "speaker_translation": "",
        "text": "む、これは！\u0000\n呼吸をしていない！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3990,
            "text_len": 32,
            "text_offset": 4000,
            "warnings": 0
        },
        "speaker": "校長先生",
        "speaker_translation": "",
        "text": "では、さっそく人工呼吸を……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4074,
            "text_len": 60,
            "text_offset": 4084,
            "warnings": 0
        },
        "speaker": "校長先生",
        "speaker_translation": "",
        "text": "……悪化した！？\u0000\nならば、急いで病院へ連れて行きましょう！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4170,
            "text_len": 52,
            "text_offset": 4180,
            "warnings": 0
        },
        "speaker": "校長先生",
        "speaker_translation": "",
        "text": "死なせはしません！\u0000\n私たちの生徒は、誰一人として！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4314,
            "text_len": 66,
            "text_offset": 4318,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "ダ、ダメだ、本人の意思が強すぎて、\u0000\nまったく操作を受け付けない！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4410,
            "text_len": 54,
            "text_offset": 4414,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "まずい！\u0000\nこのままだと操作電波の圏外に出てしまうぞ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4490,
            "text_len": 60,
            "text_offset": 4494,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "なんだと、圏外に！？\u0000\n……で、圏外になったらどうなるんだ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4576,
            "text_len": 16,
            "text_offset": 4580,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "そりゃあ……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4618,
            "text_len": 16,
            "text_offset": 4622,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "やっぱり……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4680,
            "text_len": 20,
            "text_offset": 4684,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "……なるほどね―。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4804,
            "text_len": 46,
            "text_offset": 4810,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "あの……すみません。\u0000\n次の方、ご注文を……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4878,
            "text_len": 34,
            "text_offset": 4884,
            "warnings": 0
        },
        "speaker": "みお",
        "speaker_translation": "",
        "text": "さ、笹原先輩！　先輩の番ですよ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4944,
            "text_len": 14,
            "text_offset": 4950,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "む、そうか。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4998,
            "text_len": 58,
            "text_offset": 5006,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（どうなのよ……\u0000\n　笹原先輩は普通に注文できるの……？）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5090,
            "text_len": 80,
            "text_offset": 5098,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（いやいや、そんなはずない。笹原先輩だよ？\u0000\n　きっと盛大にボケてくれるはず！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5200,
            "text_len": 22,
            "text_offset": 5206,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "ふむ、そうだな……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5248,
            "text_len": 26,
            "text_offset": 5256,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（ドキドキドキドキ……）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5312,
            "text_len": 18,
            "text_offset": 5318,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "よきにはからえ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5368,
            "text_len": 36,
            "text_offset": 5374,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "…………？\u0000\nか、かしこまりました。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5448,
            "text_len": 6,
            "text_offset": 5456,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5488,
            "text_len": 70,
            "text_offset": 5496,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "ちょっと笹原！　店員さん困ってるじゃない！\u0000\nちゃんと注文しなさいよ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5680,
            "text_len": 12,
            "text_offset": 5688,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5810,
            "text_len": 38,
            "text_offset": 5818,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "あ、あれ！？\u0000\nもう、なんでよ―っ！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5882,
            "text_len": 46,
            "text_offset": 5888,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "立花みさとよ、\u0000\nなにやら困っているようだな。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5962,
            "text_len": 60,
            "text_offset": 5970,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "な、なによ！　あんたには関係ないでしょ！\u0000\n放っといてよ！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6052,
            "text_len": 36,
            "text_offset": 6058,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "いやいや、そう言うわけにはいかん。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6116,
            "text_len": 64,
            "text_offset": 6122,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "私と立花みさとの仲ではないか。\u0000\n何事も遠慮なく相談するがいい。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6212,
            "text_len": 6,
            "text_offset": 6220,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6294,
            "text_len": 14,
            "text_offset": 6302,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "た…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6338,
            "text_len": 6,
            "text_offset": 6344,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "た？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6372,
            "text_len": 22,
            "text_offset": 6380,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "……弾が、出ないの。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6424,
            "text_len": 42,
            "text_offset": 6430,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "そのようだな。\u0000\nよし、私が見て進ぜよう。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6508,
            "text_len": 62,
            "text_offset": 6516,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "いやいやいやいや！　それってどうなの？\u0000\n撃たれかけたのに！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6608,
            "text_len": 12,
            "text_offset": 6614,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6648,
            "text_len": 22,
            "text_offset": 6656,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "どう？　何かわかる？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6700,
            "text_len": 38,
            "text_offset": 6706,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "立花みさとよ、弾が出ないのは当然だ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6766,
            "text_len": 60,
            "text_offset": 6772,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "この兵器の装填弾数は一発。\u0000\n先程、すでに一度撃っただろう。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6854,
            "text_len": 46,
            "text_offset": 6860,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "ならば弾を込めてやらねば\u0000\n撃てるわけがない。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6944,
            "text_len": 34,
            "text_offset": 6952,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "あ、そっか。\u0000\nうっかりしてたわ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7008,
            "text_len": 50,
            "text_offset": 7014,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "こうして弾をこめれば……よし、\u0000\nこれで問題ない。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7094,
            "text_len": 34,
            "text_offset": 7100,
            "warnings": 0
        },
        "speaker": "みお",
        "speaker_translation": "",
        "text": "すご―い！　さすが笹原先輩です！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7204,
            "text_len": 36,
            "text_offset": 7212,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "お、お礼なんて言わないんだからね！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7274,
            "text_len": 54,
            "text_offset": 7282,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "さ、笹原になんか、\u0000\n絶対、感謝なんかしないんだから！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7450,
            "text_len": 12,
            "text_offset": 7456,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7526,
            "text_len": 30,
            "text_offset": 7534,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "わ、私と笹原の……仲……！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7600,
            "text_len": 6,
            "text_offset": 7606,
            "warnings": 0
        },
        "speaker": "みお",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7648,
            "text_len": 70,
            "text_offset": 7654,
            "warnings": 0
        },
        "speaker": "みお",
        "speaker_translation": "",
        "text": "さ、笹原先輩……あ、あ、あの……\u0000\nお、お二人は一体……どういう……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7746,
            "text_len": 46,
            "text_offset": 7754,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "ど、ど、ど、どういう関係って言われても……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7826,
            "text_len": 52,
            "text_offset": 7834,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "ちょっと笹原！\u0000\n誤解されるようなこと言わないでよ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7912,
            "text_len": 24,
            "text_offset": 7918,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "？　誤解などではない。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7964,
            "text_len": 66,
            "text_offset": 7970,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "私と立花みさとは切っても切れぬ縁。\u0000\n今さら何を隠すことがあろう。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8086,
            "text_len": 28,
            "text_offset": 8092,
            "warnings": 9
        },
        "speaker": "みお",
        "speaker_translation": "",
        "text": "９〜！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8146,
            "text_len": 34,
            "text_offset": 8154,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "わ―！？　みおちゃんしっかり！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8210,
            "text_len": 30,
            "text_offset": 8218,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "〜〜〜〜〜〜〜〜〜〜〜〜！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8362,
            "text_len": 12,
            "text_offset": 8370,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8492,
            "text_len": 76,
            "text_offset": 8498,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "お待たせ致しました。カフェ・ラテのグランデ、\u0000\nア―モンドシロップ入りです。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8600,
            "text_len": 24,
            "text_offset": 8608,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "え、あ、ど、どうも弌弌\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8656,
            "text_len": 38,
            "text_offset": 8662,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "ふむ、ご苦労。\u0000\n……うむ、美味なり。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8736,
            "text_len": 62,
            "text_offset": 8744,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "あ……さ、笹原！　それ、私のよ！！\u0000\nなに勝手に飲んでるのよ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8828,
            "text_len": 38,
            "text_offset": 8834,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "む、それは失敬。\u0000\nでは返すとしよう。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8894,
            "text_len": 18,
            "text_offset": 8902,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "え……あの……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8942,
            "text_len": 26,
            "text_offset": 8948,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "どうした、早く飲まぬか。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8996,
            "text_len": 26,
            "text_offset": 9004,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "あ、うん……ごくり……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 85,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9052,
            "text_len": 24,
            "text_offset": 9058,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "どうだ、立花みさとよ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 86,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9104,
            "text_len": 58,
            "text_offset": 9112,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "うん、おいし……はっ！？\u0000\nそ、そんなわけないでしょっ！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 87,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9196,
            "text_len": 64,
            "text_offset": 9204,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "これ、もういらないわよ！\u0000\n笹原、責任とって全部飲みなさいよね！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 88,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9290,
            "text_len": 54,
            "text_offset": 9296,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "そうか、では遠慮なく頂くとしよう。\u0000\n……うむ、美味。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 89,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9372,
            "text_len": 16,
            "text_offset": 9380,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "か、間接……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 90,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9514,
            "text_len": 38,
            "text_offset": 9520,
            "warnings": 0
        },
        "speaker": "みお",
        "speaker_translation": "",
        "text": "きえ〜〜〜〜〜〜〜〜〜〜〜〜っ！！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 91,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9614,
            "text_len": 44,
            "text_offset": 9622,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "い、意外〜。\u0000\n笹原先輩が良い人っぽいよ〜。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 92,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9696,
            "text_len": 28,
            "text_offset": 9702,
            "warnings": 0
        },
        "speaker": "みお",
        "speaker_translation": "",
        "text": "笹原先輩は良い人だってば！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 93,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9768,
            "text_len": 12,
            "text_offset": 9776,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 94,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9832,
            "text_len": 30,
            "text_offset": 9840,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "あ、あれ？　行っちゃったよ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 95,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9886,
            "text_len": 16,
            "text_offset": 9892,
            "warnings": 0
        },
        "speaker": "みお",
        "speaker_translation": "",
        "text": "なんなんだろ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 96,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9930,
            "text_len": 52,
            "text_offset": 9936,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "手助けはいらぬということか。\u0000\nさすがは立花みさと。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 97,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10018,
            "text_len": 24,
            "text_offset": 10024,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "お待たせ致しました〜。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 98,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10080,
            "text_len": 34,
            "text_offset": 10086,
            "warnings": 0
        },
        "speaker": "みお",
        "speaker_translation": "",
        "text": "あ、笹原先輩、注文がきましたよ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 99,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10144,
            "text_len": 38,
            "text_offset": 10150,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "うむ、では外のベンチで頂くとしよう。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 100,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10320,
            "text_len": 78,
            "text_offset": 10326,
            "warnings": 0
        },
        "speaker": "みお",
        "speaker_translation": "",
        "text": "（し、信じられない！　学校の外で\u0000\n　笹原先輩とこ〜ひ〜タイムできるなんて！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 101,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 10432,
            "text_len": 24,
            "text_offset": 10440,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（そんなことより……）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 102,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 10582,
            "text_len": 58,
            "text_offset": 10590,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（コ―ヒ―飲んでる場合じゃないんじゃない？\u0000\n　私ら弌弌）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 103,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10672,
            "text_len": 72,
            "text_offset": 10678,
            "warnings": 0
        },
        "speaker": "みお",
        "speaker_translation": "",
        "text": "（ああ……笹原先輩と……！\u0000\n　わ、わたし、もう死んでも良い〜〜っ！！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 104,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 10908,
            "text_len": 12,
            "text_offset": 10916,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "……あれ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 105,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10948,
            "text_len": 24,
            "text_offset": 10954,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "な、なぜ、私だけ……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    }
]