            "text_len": 12,
            "text_offset": 7108,
            "warnings": 0
        },
        "speaker": "中村先生",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
