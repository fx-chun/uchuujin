            "speaker_offset": 7892,
            "text_len": 6,
            "text_offset": 7902,
            "warnings": 0
        },
        "speaker": "中村先生",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7938,
            "text_len": 12,
            "text_offset": 7946,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
