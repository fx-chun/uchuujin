        "id": 34,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5052,
            "text_len": 20,
            "text_offset": 5060,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "も、もう一回……！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 6,
