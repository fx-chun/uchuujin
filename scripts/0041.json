            "speaker_offset": 5048,
            "text_len": 26,
            "text_offset": 5056,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "（もうバテてる――！！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
