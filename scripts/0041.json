        "id": 45,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5556,
            "text_len": 58,
            "text_offset": 5564,
            "warnings": 0
        },
        "speaker": "イタコ",
        "speaker_translation": "",
        "text": "金を払うのじゃ……払わなければ……\u0000\n打ち首獄門の刑じゃ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5644,
            "text_len": 6,
            "text_offset": 5652,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
