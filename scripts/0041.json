        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2754,
            "text_len": 6,
            "text_offset": 2762,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
