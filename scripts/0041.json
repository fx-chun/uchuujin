        "id": 39,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4942,
            "text_len": 68,
            "text_offset": 4950,
            "warnings": 0
        },
        "speaker": "イタコ",
        "speaker_translation": "",
        "text": "だ、だから、どこまで逃げても、\u0000\n無駄……だから、早く止まって……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
