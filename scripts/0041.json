            "speaker_offset": 4006,
            "text_len": 14,
            "text_offset": 4014,
            "warnings": 0
        },
        "speaker": "イタコ",
        "speaker_translation": "",
        "text": "むむむ……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
