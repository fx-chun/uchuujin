        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2696,
            "text_len": 20,
            "text_offset": 2704,
            "warnings": 0
        },
        "speaker": "イタコ",
        "speaker_translation": "",
        "text": "見〜つ〜け〜た〜。\u0000\n",
