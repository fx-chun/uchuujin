            "speaker_offset": 4714,
            "text_len": 6,
            "text_offset": 4722,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
