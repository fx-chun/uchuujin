        "id": 37,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4744,
            "text_len": 36,
            "text_offset": 4752,
            "warnings": 0
        },
        "speaker": "イタコ",
        "speaker_translation": "",
        "text": "今度はスプリンタ―の霊を降ろした。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 6,
