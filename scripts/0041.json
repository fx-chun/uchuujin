        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5696,
            "text_len": 40,
            "text_offset": 5704,
            "warnings": 0
        },
        "speaker": "イタコ",
        "speaker_translation": "",
        "text": "払わぬなら、呪ってやろう、モヒカンを。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
