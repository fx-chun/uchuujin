[
    {
        "id": 0,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2138,
            "text_len": 14,
            "text_offset": 2146,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ただいま―！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2282,
            "text_len": 62,
            "text_offset": 2292,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "おっ、帰ってきた弌弌ん？\u0000\nおい、なんだその口元のクリ―ムは。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2380,
            "text_len": 70,
            "text_offset": 2388,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あっ、な、なのには内緒だよ？\u0000\nおねえさんに、ごちそうしてもらったの。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2480,
            "text_len": 70,
            "text_offset": 2490,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "お姉さん……？　まあ、誰かは知らんが\u0000\nその人に聞きに行ってたんだな？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2586,
            "text_len": 8,
            "text_offset": 2594,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "えっ？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2634,
            "text_len": 8,
            "text_offset": 2644,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "えっ？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2698,
            "text_len": 12,
            "text_offset": 2706,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "…………。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2740,
            "text_len": 42,
            "text_offset": 2750,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "ネジのことを\u0000\n聞いてきたんじゃないのか？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2814,
            "text_len": 16,
            "text_offset": 2822,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "……忘れてた。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2864,
            "text_len": 42,
            "text_offset": 2874,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "えええっ！？\u0000\nじゃあ今まで何してたんだ？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2938,
            "text_len": 20,
            "text_offset": 2946,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ご飯食べてました。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2992,
            "text_len": 18,
            "text_offset": 3002,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "照れるとこか！？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3074,
            "text_len": 12,
            "text_offset": 3082,
            "warnings": 0
        },
        "speaker": "バディ",
        "text": "…………。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3110,
            "text_len": 56,
            "text_offset": 3120,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "犬まで拾ってきたのか。\u0000\n娘が飼うのを許すかどうか……。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3212,
            "text_len": 16,
            "text_offset": 3220,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "！？　犬だ！？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3266,
            "text_len": 26,
            "text_offset": 3276,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "拾ったんじゃないのか！？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3366,
            "text_len": 34,
            "text_offset": 3374,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "わ―――！？　こっち来るな――！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3442,
            "text_len": 26,
            "text_offset": 3452,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "こら！　物を投げるな！！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3558,
            "text_len": 6,
            "text_offset": 3562,
            "warnings": 1
        },
        "speaker": "",
        "text": "！？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3610,
            "text_len": 56,
            "text_offset": 3614,
            "warnings": 1
        },
        "speaker": "",
        "text": "ティッシュの箱、アンテナに直撃！\u0000\nアンテナ壊れました！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3686,
            "text_len": 38,
            "text_offset": 3690,
            "warnings": 1
        },
        "speaker": "",
        "text": "なんてこった！\u0000\n動物虐待じゃないか！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3744,
            "text_len": 62,
            "text_offset": 3748,
            "warnings": 1
        },
        "speaker": "",
        "text": "でもさ、俺たち、この猫に\u0000\nアンテナ刺して利用してるんだよな？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3826,
            "text_len": 42,
            "text_offset": 3830,
            "warnings": 1
        },
        "speaker": "",
        "text": "なんてこった！？\u0000\n動物虐待じゃないか！？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3964,
            "text_len": 54,
            "text_offset": 3972,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "でも、おねえさんは\u0000\nネジが可愛いって言ってくれたよ？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4048,
            "text_len": 14,
            "text_offset": 4058,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "マジか……。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4094,
            "text_len": 38,
            "text_offset": 4102,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ふふふ―、はかせが正しかったでしょ？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4162,
            "text_len": 24,
            "text_offset": 4172,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "む……じゃあ他の人は？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4218,
            "text_len": 8,
            "text_offset": 4226,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "えっ？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4256,
            "text_len": 26,
            "text_offset": 4266,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "そいつ以外の人の意見は？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4314,
            "text_len": 16,
            "text_offset": 4322,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "わかりません。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4370,
            "text_len": 26,
            "text_offset": 4380,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "だからなんで照れるんだ？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4438,
            "text_len": 78,
            "text_offset": 4448,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "それに、わからないってどういうことだ？\u0000\n色んなやつに意見聞きに行ったんだろ？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4548,
            "text_len": 76,
            "text_offset": 4556,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "だっておねえさんにしか聞いてないもん。\u0000\nでもきっとみんな可愛いって言うよ？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4654,
            "text_len": 34,
            "text_offset": 4664,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "それじゃあ聞きに行った意味が弌弌\u0000\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4720,
            "text_len": 28,
            "text_offset": 4730,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "……もういい。俺は寝るぞ。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4788,
            "text_len": 12,
            "text_offset": 4796,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "…………！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4834,
            "text_len": 78,
            "text_offset": 4842,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "（ぷぷぷ！　阪本ったら自分が間違ってるって\u0000\n　わかったから豫ねちゃってる！）\u0000\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4972,
            "text_len": 62,
            "text_offset": 4980,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "大丈夫だよ！　阪本のセンスが悪くても\u0000\n猫だから仕方ないもん。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5066,
            "text_len": 26,
            "text_offset": 5076,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "悪いのはガキのセンスだ。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5124,
            "text_len": 62,
            "text_offset": 5132,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "そんなことないもんね―。\u0000\nおねえさんも可愛いって言ったもん。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5216,
            "text_len": 36,
            "text_offset": 5226,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "ならその人のセンスもおかしいんだ。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5284,
            "text_len": 52,
            "text_offset": 5292,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ぷぷぷ。何を言っても２対１で\u0000\n阪本の負けだもんね。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5366,
            "text_len": 60,
            "text_offset": 5376,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "娘は嫌がってるんだから\u0000\n少なくとも２対２で負けではないな。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5468,
            "text_len": 6,
            "text_offset": 5476,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "！？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5504,
            "text_len": 66,
            "text_offset": 5514,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "むしろ背中につけられる娘が\u0000\n嫌がってるんだからこっちの勝ちだな。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5602,
            "text_len": 6,
            "text_offset": 5610,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "！？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5638,
            "text_len": 54,
            "text_offset": 5646,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "じゃあ作るはかせはいっぱい疲れるから\u0000\nはかせの勝ち！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5726,
            "text_len": 6,
            "text_offset": 5736,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "！？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5856,
            "text_len": 60,
            "text_offset": 5864,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あ、そうだ。作るのはかせなんだから\u0000\n好きに作ればいいんだ。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5944,
            "text_len": 6,
            "text_offset": 5954,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "！？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6018,
            "text_len": 76,
            "text_offset": 6026,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "阪本の気持ちもわかるよ。でもはかせと\u0000\nおねえさんが正しいのは認めないとね。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6132,
            "text_len": 52,
            "text_offset": 6142,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "意見が少なすぎて認めたくても\u0000\n認められないんだよ。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6216,
            "text_len": 46,
            "text_offset": 6226,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "大体、そのお姉さんって誰だ？\u0000\n親戚か何かか？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6294,
            "text_len": 42,
            "text_offset": 6302,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ううん。はかせも今日初めて会った人だよ。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6366,
            "text_len": 68,
            "text_offset": 6376,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "……おい。もしかして、知らない人に\u0000\n声かけて回るつもりだったのか？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6466,
            "text_len": 8,
            "text_offset": 6474,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "うん。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6504,
            "text_len": 60,
            "text_offset": 6514,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "危ないだろ……。\u0000\nお前はまだガキなんだから何があるか……。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6596,
            "text_len": 62,
            "text_offset": 6604,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "大丈夫大丈夫。だってあのおねえさん、\u0000\nなのと知り合いだもん。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6692,
            "text_len": 12,
            "text_offset": 6702,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "…………。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6818,
            "text_len": 18,
            "text_offset": 6828,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "……ならいいか。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6862,
            "text_len": 8,
            "text_offset": 6870,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "うん。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6942,
            "text_len": 66,
            "text_offset": 6950,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "残念だけど、これが一般の意見だからね。\u0000\n猫には難しかったかな―。\u0000\n",
