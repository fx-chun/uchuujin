[
    {
        "id": 0,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2144,
            "text_len": 30,
            "text_offset": 2150,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "さて、今度こそ帰るとしよう。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2202,
            "text_len": 22,
            "text_offset": 2208,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "だが、その前に……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2254,
            "text_len": 26,
            "text_offset": 2260,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "行くんだな？　コジロウ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2324,
            "text_len": 12,
            "text_offset": 2334,
            "warnings": 0
        },
        "speaker": "コジロウ",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2372,
            "text_len": 54,
            "text_offset": 2378,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "わかっているさ。\u0000\nあの鹿との決着はまだついていない。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2454,
            "text_len": 46,
            "text_offset": 2460,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "よし行こう……。\u0000\nもう一度、あの鹿の場所へ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2624,
            "text_len": 26,
            "text_offset": 2630,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "ハイヨ―！　コジ―ロウ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2798,
            "text_len": 12,
            "text_offset": 2808,
            "warnings": 0
        },
        "speaker": "校長先生",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2844,
            "text_len": 6,
            "text_offset": 2850,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2884,
            "text_len": 30,
            "text_offset": 2890,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "いかんっ、止まれコジロウ！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2996,
            "text_len": 6,
            "text_offset": 3006,
            "warnings": 0
        },
        "speaker": "校長先生",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3040,
            "text_len": 42,
            "text_offset": 3046,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "しまった……\u0000\n校長をはねてしまったか！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3166,
            "text_len": 68,
            "text_offset": 3170,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "こんな場所で偶然、動物にはねられるなんて、\u0000\n不運なオッサンだなぁ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3262,
            "text_len": 74,
            "text_offset": 3266,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "ほんと、こんな偶然でアンテナが\u0000\n壊れたりしたらと思うと、ヒヤヒヤするよ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3362,
            "text_len": 30,
            "text_offset": 3366,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "ははっ、もう心配いらないぜ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3418,
            "text_len": 22,
            "text_offset": 3422,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "お、どういう事だい？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3466,
            "text_len": 24,
            "text_offset": 3470,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "もう壊れているからさ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3536,
            "text_len": 30,
            "text_offset": 3540,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "なるほど！　ははははははは！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3586,
            "text_len": 24,
            "text_offset": 3590,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "あっはっはっはっは！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3634,
            "text_len": 18,
            "text_offset": 3642,
            "warnings": 2
        },
        "speaker": "＆",
        "speaker_translation": "",
        "text": "いや――――っ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3856,
            "text_len": 12,
            "text_offset": 3862,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "これは！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3982,
            "text_len": 30,
            "text_offset": 3988,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "校長……なんと痛ましい事だ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4122,
            "text_len": 40,
            "text_offset": 4128,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "しかし、解せん。\u0000\n何故、鹿が人を襲う？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4184,
            "text_len": 62,
            "text_offset": 4190,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "いや、校長は例外として、\u0000\nどうして執豫なまでに私を襲うのだ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4268,
            "text_len": 20,
            "text_offset": 4276,
            "warnings": 0
        },
        "speaker": "イタコ",
        "speaker_translation": "",
        "text": "弌弌霊の仕業だね。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4422,
            "text_len": 48,
            "text_offset": 4428,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "む、貴様は？\u0000\nそんな事よりも霊の仕業とは一体？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4498,
            "text_len": 52,
            "text_offset": 4506,
            "warnings": 0
        },
        "speaker": "イタコ",
        "speaker_translation": "",
        "text": "そのままの意味ですよ。\u0000\n霊が悪さをしておるんです。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4672,
            "text_len": 54,
            "text_offset": 4680,
            "warnings": 0
        },
        "speaker": "イタコ",
        "speaker_translation": "",
        "text": "あのインコには鹿の父親の霊が\u0000\n乗り移っておりまして。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4750,
            "text_len": 16,
            "text_offset": 4756,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "父親の霊……？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4788,
            "text_len": 46,
            "text_offset": 4796,
            "warnings": 0
        },
        "speaker": "イタコ",
        "speaker_translation": "",
        "text": "そうです。\u0000\nですが、インコでは鹿と話せない。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4868,
            "text_len": 16,
            "text_offset": 4880,
            "warnings": 0
        },
        "speaker": "謎のインコ",
        "speaker_translation": "",
        "text": "………………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4912,
            "text_len": 66,
            "text_offset": 4920,
            "warnings": 0
        },
        "speaker": "イタコ",
        "speaker_translation": "",
        "text": "だから父親の霊は新たな肉体として\u0000\nそのヤギを狙っておるのです！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5004,
            "text_len": 16,
            "text_offset": 5010,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "な、なんと！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5140,
            "text_len": 60,
            "text_offset": 5146,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "（そうか、これまでの戦いに\u0000\n　そんな意味があったのか……）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5244,
            "text_len": 12,
            "text_offset": 5254,
            "warnings": 0
        },
        "speaker": "コジロウ",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5288,
            "text_len": 44,
            "text_offset": 5294,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "降りていろだと？\u0000\nコジロウ、行く気か……？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5402,
            "text_len": 38,
            "text_offset": 5408,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "よし、コジロウ！　決着をつけてこい。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5470,
            "text_len": 30,
            "text_offset": 5478,
            "warnings": 0
        },
        "speaker": "イタコ",
        "speaker_translation": "",
        "text": "いや、まだ話は終わって……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5530,
            "text_len": 60,
            "text_offset": 5536,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "老婆よ、男には……\u0000\n否、雄には戦わねばならん時があるのだ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5622,
            "text_len": 20,
            "text_offset": 5628,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "いけ！　コジロウ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5690,
            "text_len": 8,
            "text_offset": 5700,
            "warnings": 0
        },
        "speaker": "コジロウ",
        "speaker_translation": "",
        "text": "！！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 5746,
            "text_len": 12,
            "text_offset": 5750,
            "warnings": 0
        },
        "speaker": "鹿",
        "speaker_translation": "",
        "text": "！！！！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5796,
            "text_len": 34,
            "text_offset": 5802,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "最後だ！　いけっ、コジロウ！！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5866,
            "text_len": 6,
            "text_offset": 5876,
            "warnings": 0
        },
        "speaker": "コジロウ",
        "speaker_translation": "",
        "text": "！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 5970,
            "text_len": 12,
            "text_offset": 5974,
            "warnings": 0
        },
        "speaker": "鹿",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6150,
            "text_len": 14,
            "text_offset": 6156,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "……勝った。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6210,
            "text_len": 26,
            "text_offset": 6218,
            "warnings": 0
        },
        "speaker": "イタコ",
        "speaker_translation": "",
        "text": "いや、あの……インコは？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6270,
            "text_len": 50,
            "text_offset": 6276,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "そんな事はどうでもいい。\u0000\n私たちは勝利したのだ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6348,
            "text_len": 24,
            "text_offset": 6356,
            "warnings": 0
        },
        "speaker": "イタコ",
        "speaker_translation": "",
        "text": "あっ、インコが逃げた！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6402,
            "text_len": 34,
            "text_offset": 6408,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "捨て置け。\u0000\nどうせ何も出来んわ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6464,
            "text_len": 52,
            "text_offset": 6470,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "しかし……父の為に戦うか。\u0000\n敵ながら天晴れなヤツ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6544,
            "text_len": 72,
            "text_offset": 6550,
            "warnings": 2
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "これぞ戦友と書いてともというヤツか。\u0000\n戦とは、無常なものだな……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6650,
            "text_len": 12,
            "text_offset": 6660,
            "warnings": 0
        },
        "speaker": "コジロウ",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6694,
            "text_len": 26,
            "text_offset": 6700,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "ああ、行くか、コジロウ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6742,
            "text_len": 18,
            "text_offset": 6750,
            "warnings": 0
        },
        "speaker": "イタコ",
        "speaker_translation": "",
        "text": "いや、あの……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6876,
            "text_len": 24,
            "text_offset": 6882,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "ハイヨ―！　コジロウ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6936,
            "text_len": 12,
            "text_offset": 6944,
            "warnings": 0
        },
        "speaker": "イタコ",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7070,
            "text_len": 20,
            "text_offset": 7080,
            "warnings": 0
        },
        "speaker": "校長先生",
        "speaker_translation": "",
        "text": "だ、だ、誰か……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7158,
            "text_len": 58,
            "text_offset": 7164,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "少し待て、コジロウ。\u0000\nまだこの老婆の話は終わっていない。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7248,
            "text_len": 46,
            "text_offset": 7256,
            "warnings": 0
        },
        "speaker": "イタコ",
        "speaker_translation": "",
        "text": "その通りでございます。\u0000\n情報提供料として弌弌\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7328,
            "text_len": 34,
            "text_offset": 7334,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "はっはっは、なんと謙虚な老婆か！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7410,
            "text_len": 38,
            "text_offset": 7416,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "本当に言いたい事はそうではなかろう？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7476,
            "text_len": 10,
            "text_offset": 7484,
            "warnings": 0
        },
        "speaker": "イタコ",
        "speaker_translation": "",
        "text": "え……？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7516,
            "text_len": 76,
            "text_offset": 7522,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "見たところ霊媒師の類と見受ける。\u0000\n以前学校で聞いたイタコではないだろうか？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7620,
            "text_len": 22,
            "text_offset": 7628,
            "warnings": 0
        },
        "speaker": "イタコ",
        "speaker_translation": "",
        "text": "その通りですが……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7672,
            "text_len": 64,
            "text_offset": 7678,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "ならば、その父親の霊とやらを降ろして\u0000\n鹿と語らってやればいい。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7778,
            "text_len": 10,
            "text_offset": 7786,
            "warnings": 0
        },
        "speaker": "イタコ",
        "speaker_translation": "",
        "text": "なっ！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7818,
            "text_len": 54,
            "text_offset": 7824,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "イタコならば簡単であろう？\u0000\nそら、やってみるといい。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7906,
            "text_len": 20,
            "text_offset": 7914,
            "warnings": 0
        },
        "speaker": "イタコ",
        "speaker_translation": "",
        "text": "うっ……ぐっ……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7956,
            "text_len": 8,
            "text_offset": 7962,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "さあ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8002,
            "text_len": 44,
            "text_offset": 8010,
            "warnings": 0
        },
        "speaker": "イタコ",
        "speaker_translation": "",
        "text": "……シ、シカッ！　シカッ！　シカシカッ！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 8088,
            "text_len": 6,
            "text_offset": 8092,
            "warnings": 0
        },
        "speaker": "鹿",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8142,
            "text_len": 12,
            "text_offset": 8148,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8264,
            "text_len": 50,
            "text_offset": 8270,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "うむ、上出来ではないか。\u0000\nこれにて一件落着だな！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8340,
            "text_len": 6,
            "text_offset": 8348,
            "warnings": 0
        },
        "speaker": "イタコ",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8412,
            "text_len": 60,
            "text_offset": 8418,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "まあ待てコジロウ。\u0000\n貴様が行く前に、まだ打つべき手がある。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8500,
            "text_len": 22,
            "text_offset": 8506,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "そう、そこの老婆だ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8550,
            "text_len": 80,
            "text_offset": 8556,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "見たところ、霊媒師の類と見受ける。ならば\u0000\nきっと、良い解決策があるのであろう？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8662,
            "text_len": 10,
            "text_offset": 8670,
            "warnings": 0
        },
        "speaker": "イタコ",
        "speaker_translation": "",
        "text": "え……？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8706,
            "text_len": 20,
            "text_offset": 8714,
            "warnings": 0
        },
        "speaker": "イタコ",
        "speaker_translation": "",
        "text": "いや、無いですよ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8762,
            "text_len": 20,
            "text_offset": 8768,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "……そう来たか！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8820,
            "text_len": 36,
            "text_offset": 8826,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "仕方が無い、ならば行くかコジロウ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8884,
            "text_len": 12,
            "text_offset": 8892,
            "warnings": 0
        },
        "speaker": "イタコ",
        "speaker_translation": "",
        "text": "あの……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8926,
            "text_len": 8,
            "text_offset": 8932,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "何だ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8966,
            "text_len": 30,
            "text_offset": 8974,
            "warnings": 0
        },
        "speaker": "イタコ",
        "speaker_translation": "",
        "text": "情報提供料５千円になります。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 85,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9036,
            "text_len": 12,
            "text_offset": 9042,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 86,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9172,
            "text_len": 22,
            "text_offset": 9178,
            "warnings": 0
        },
        "speaker": "笹原",
        "speaker_translation": "",
        "text": "行くぞ、コジロウ！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 87,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9226,
            "text_len": 10,
            "text_offset": 9234,
            "warnings": 0
        },
        "speaker": "イタコ",
        "speaker_translation": "",
        "text": "あっ！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 88,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 9270,
            "text_len": 6,
            "text_offset": 9274,
            "warnings": 0
        },
        "speaker": "鹿",
        "speaker_translation": "",
        "text": "！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    }
