[
    {
        "id": 0,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2144,
            "text_len": 60,
            "text_offset": 2152,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "あと少しで体育祭だね―。\u0000\nなんかテンション上がってくるよ。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2234,
            "text_len": 52,
            "text_offset": 2240,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "そうですね。\u0000\n私は体育祭は初めてなので楽しみです。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2318,
            "text_len": 68,
            "text_offset": 2324,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "じゃあ少し練習した方がいいかもね。\u0000\nほら、リレ―のバトン渡しとか。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2416,
            "text_len": 48,
            "text_offset": 2424,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "お、そうだね。\u0000\nなのちゃんしたことないでしょ？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2494,
            "text_len": 8,
            "text_offset": 2500,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "はい。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2530,
            "text_len": 52,
            "text_offset": 2538,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "じゃあ少しだけ練習する？\u0000\nバトンは携帯でいいよね。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2612,
            "text_len": 22,
            "text_offset": 2618,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "はい。お願いします。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2762,
            "text_len": 18,
            "text_offset": 2768,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "じゃあ行くよ―。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2808,
            "text_len": 22,
            "text_offset": 2814,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "はい、お願いします！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2864,
            "text_len": 16,
            "text_offset": 2870,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "よっ、と……。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2932,
            "text_len": 12,
            "text_offset": 2938,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "…………！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2972,
            "text_len": 74,
            "text_offset": 2980,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "なのちゃん、みおちゃんが近くに来たら\u0000\n走りながらバトンを受け取るんだよ。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3076,
            "text_len": 12,
            "text_offset": 3082,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "は、はい！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3122,
            "text_len": 28,
            "text_offset": 3128,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "なのちゃん、バトン渡すよ。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3178,
            "text_len": 12,
            "text_offset": 3184,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "はいっ！！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3320,
            "text_len": 24,
            "text_offset": 3328,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "おっ、ナイスキャッチ！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3374,
            "text_len": 50,
            "text_offset": 3380,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "そのまま走って、\u0000\n今度はゆっこにバトンを渡して！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3452,
            "text_len": 10,
            "text_offset": 3458,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "はい！！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3514,
            "text_len": 10,
            "text_offset": 3520,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "わっ！？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3638,
            "text_len": 56,
            "text_offset": 3644,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "いたたた……\u0000\nどうしてこんなところにワンちゃんが……。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3722,
            "text_len": 48,
            "text_offset": 3728,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "あっ！！　ご、ごめんなさい！\u0000\nすぐに退くから！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3854,
            "text_len": 48,
            "text_offset": 3858,
            "warnings": 1
        },
        "speaker": "",
        "text": "ふぅ、間一髪だぜ。\u0000\nあいつが助かってよかった。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3930,
            "text_len": 62,
            "text_offset": 3934,
            "warnings": 1
        },
        "speaker": "",
        "text": "とっさにバリアを展開したのか。\u0000\nお前の犬好きには恐れ入るよ。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4018,
            "text_len": 62,
            "text_offset": 4022,
            "warnings": 1
        },
        "speaker": "",
        "text": "へへっ。そういうお前も、\u0000\n反重力場を発生させてたじゃないか。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4106,
            "text_len": 60,
            "text_offset": 4110,
            "warnings": 1
        },
        "speaker": "",
        "text": "ま、俺も犬は嫌いじゃないしな。\u0000\n辛い目には遭わせたくない。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4192,
            "text_len": 48,
            "text_offset": 4196,
            "warnings": 1
        },
        "speaker": "",
        "text": "……その結果、\u0000\n俺たちが辛い目に遭いそうだな。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4266,
            "text_len": 66,
            "text_offset": 4270,
            "warnings": 1
        },
        "speaker": "",
        "text": "所詮は試作機能だったからな。\u0000\n負荷がかかりすぎちまったみたいだ。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4390,
            "text_len": 12,
            "text_offset": 4398,
            "warnings": 2
        },
        "speaker": "＆",
        "text": "…………。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4502,
            "text_len": 56,
            "text_offset": 4510,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "さあ急いでなのちゃん！\u0000\nちんたらしてるとビリになるよ！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4588,
            "text_len": 38,
            "text_offset": 4594,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "はい！！\u0000\n急いでバトンを渡します！！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4658,
            "text_len": 58,
            "text_offset": 4664,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（これは練習だけど、練習だと思って\u0000\n　手を抜いたら駄目）\u0000\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4748,
            "text_len": 72,
            "text_offset": 4754,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（これは本番だと思って全力で走る！\u0000\n　全力で相生さんにバトンを渡す！）\u0000\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4856,
            "text_len": 18,
            "text_offset": 4862,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "うおおおおお！！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4998,
            "text_len": 22,
            "text_offset": 5004,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "おおおおおおお！？？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5056,
            "text_len": 6,
            "text_offset": 5062,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "！？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5088,
            "text_len": 6,
            "text_offset": 5096,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "！？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5220,
            "text_len": 26,
            "text_offset": 5226,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（と、飛んでった―！？）\u0000\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5284,
            "text_len": 32,
            "text_offset": 5290,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "相生さん！　避けてください！！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5348,
            "text_len": 20,
            "text_offset": 5356,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "はああああああ！！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5430,
            "text_len": 20,
            "text_offset": 5436,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "受け止めた――！？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5492,
            "text_len": 12,
            "text_offset": 5500,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5534,
            "text_len": 8,
            "text_offset": 5542,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "！！！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5606,
            "text_len": 12,
            "text_offset": 5612,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "…………。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5664,
            "text_len": 24,
            "text_offset": 5670,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（あっ！？　手！！！）\u0000\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5766,
            "text_len": 70,
            "text_offset": 5772,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（早くあの手をはめ直さないと\u0000\n　片手が無い姿を多くの人に見られる！）\u0000\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5864,
            "text_len": 32,
            "text_offset": 5870,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "相生さん！　止まってください！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5924,
            "text_len": 14,
            "text_offset": 5932,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………！！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5990,
            "text_len": 78,
            "text_offset": 5996,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（ダメだ、止まらない……追いかけるにしても\u0000\n　こんなに距離を離されたら……）\u0000\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6106,
            "text_len": 54,
            "text_offset": 6112,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（いや！？　諦めるな！！\u0000\n　まだ手は残っている！！）\u0000\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6198,
            "text_len": 38,
            "text_offset": 6204,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（文字通り、片手だけは残っている！）\u0000\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6288,
            "text_len": 24,
            "text_offset": 6294,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "いっけぇえええええ！！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6426,
            "text_len": 6,
            "text_offset": 6434,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "！？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6460,
            "text_len": 20,
            "text_offset": 6468,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "はああああああ！！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6632,
            "text_len": 24,
            "text_offset": 6638,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "また受け止められた！？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6698,
            "text_len": 12,
            "text_offset": 6706,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6744,
            "text_len": 12,
            "text_offset": 6750,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "…………。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6866,
            "text_len": 8,
            "text_offset": 6874,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "！！！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6908,
            "text_len": 10,
            "text_offset": 6914,
            "warnings": 1
        },
        "speaker": "なの",
        "text": "！！！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6996,
            "text_len": 70,
            "text_offset": 7002,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（手を相生さんに持っていかれた！\u0000\n　……いや、それよりも問題は……）\u0000\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7102,
            "text_len": 12,
            "text_offset": 7108,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7146,
            "text_len": 50,
            "text_offset": 7152,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（手が飛ぶところを\u0000\n　長野原さんに見られた……）\u0000\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7244,
            "text_len": 80,
            "text_offset": 7250,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（これで私がロボだということが広まったら\u0000\n　もう学校に通うことはできない……）\u0000\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7356,
            "text_len": 12,
            "text_offset": 7362,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7396,
            "text_len": 18,
            "text_offset": 7402,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "長野原さん……。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7446,
            "text_len": 22,
            "text_offset": 7452,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "えっ！？　な、なに？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7496,
            "text_len": 18,
            "text_offset": 7502,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "……見ましたか？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7546,
            "text_len": 12,
            "text_offset": 7552,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7590,
            "text_len": 12,
            "text_offset": 7596,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "…………。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7630,
            "text_len": 20,
            "text_offset": 7636,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "……な、何のこと？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7678,
            "text_len": 20,
            "text_offset": 7684,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "それはもちろん弌弌\u0000\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7730,
            "text_len": 72,
            "text_offset": 7736,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "ああ、なのちゃんのバトンを受け取る\u0000\nタイミングは見たよ。１００点満点。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7836,
            "text_len": 14,
            "text_offset": 7842,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "…………！！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7886,
            "text_len": 46,
            "text_offset": 7892,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（良かった！\u0000\n　手が飛ぶ所を見られてない！）\u0000\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7960,
            "text_len": 60,
            "text_offset": 7966,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "あ、ありがとうございます！\u0000\nじゃあもう一度練習しましょう！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8048,
            "text_len": 36,
            "text_offset": 8054,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "あ、うん、ゆっこ呼んでこないとね。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8112,
            "text_len": 46,
            "text_offset": 8118,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "私行ってきます！\u0000\nちょっと待っててください。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8186,
            "text_len": 20,
            "text_offset": 8192,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "う、うん、お願い。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8238,
            "text_len": 56,
            "text_offset": 8244,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（私はまた学校に通えるんだ……\u0000\n　本当に嘘みたい……）\u0000\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8404,
            "text_len": 22,
            "text_offset": 8410,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "……飛ぶんだ、あれ。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8458,
            "text_len": 6,
            "text_offset": 8464,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "！？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8524,
            "text_len": 34,
            "text_offset": 8530,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "な、なのちゃん……今、手が……。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8590,
            "text_len": 52,
            "text_offset": 8596,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "え？　手が、ど、どうしたんですか？\u0000\n痛いんですか？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8680,
            "text_len": 36,
            "text_offset": 8686,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "えっ？\u0000\nいや、なのちゃんの手が弌弌\u0000\n",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8744,
            "text_len": 64,
            "text_offset": 8750,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "相生さん、どこまで行ったんでしょうね。\u0000\n全然帰ってきませんよ？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8840,
            "text_len": 6,
            "text_offset": 8846,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "！？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 85,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8898,
            "text_len": 24,
            "text_offset": 8906,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "はっ、はっ、はっ……。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 86,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8956,
            "text_len": 58,
            "text_offset": 8962,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "あっ、お帰りなさい相生さん。\u0000\nどこまで行ってたんですか？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 87,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9048,
            "text_len": 60,
            "text_offset": 9056,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "あ、ちょっとそこまで……\u0000\nところでなのちゃん……この手弌弌\u0000\n",
        "type": "dialog"
    },
    {
        "id": 88,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9138,
            "text_len": 28,
            "text_offset": 9144,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "あっ、ちょっとすみません。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 89,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9220,
            "text_len": 8,
            "text_offset": 9226,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "！！！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 90,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 9304,
            "text_len": 6,
            "text_offset": 9318,
            "warnings": 0
        },
        "speaker": "ゆっこ＆みお",
        "text": "！？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 91,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9360,
            "text_len": 38,
            "text_offset": 9366,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "弌弌えっと、なんでしょうか相生さん。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 92,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9430,
            "text_len": 12,
            "text_offset": 9438,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 93,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9478,
            "text_len": 12,
            "text_offset": 9484,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………。\u0000\n",
        "type": "dialog"
    },
