[
    {
        "id": 0,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 2138,
            "text_len": 22,
            "text_offset": 2152,
            "warnings": 0
        },
        "speaker": "店員（玉村）",
        "speaker_translation": "",
        "text": "いらっしゃいませ―。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2200,
            "text_len": 12,
            "text_offset": 2208,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2246,
            "text_len": 26,
            "text_offset": 2254,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（また来てしまった……）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2306,
            "text_len": 84,
            "text_offset": 2314,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（ちょっと喉が渇いただけだから自販機か\u0000\n　それとも少し我慢して家で何か飲めばいい）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2424,
            "text_len": 34,
            "text_offset": 2432,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（それなのに入ってしまった……）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2492,
            "text_len": 60,
            "text_offset": 2500,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（……わかってる。\u0000\n　本当は理由なんかとっくにわかってる）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2586,
            "text_len": 62,
            "text_offset": 2594,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（悔しかったんだ。だからリベンジしたくて\u0000\n　やってきたんだ）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2692,
            "text_len": 66,
            "text_offset": 2700,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（注文もろくに出来なかったことが\u0000\n　今も私の胸の中に残っている）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2792,
            "text_len": 46,
            "text_offset": 2800,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（今こそリベンジを果たす時！\u0000\n　頑張れ私！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2882,
            "text_len": 32,
            "text_offset": 2888,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "いらっしゃいませ。ご注文は弌弌\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2946,
            "text_len": 12,
            "text_offset": 2952,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "あっ……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3000,
            "text_len": 36,
            "text_offset": 3008,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（うっ！？　あの時の店員さん！？）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3074,
            "text_len": 74,
            "text_offset": 3082,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（だ、大丈夫。気圧されちゃ駄目。\u0000\n　あの時の私と違うところを見せないと）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3188,
            "text_len": 20,
            "text_offset": 3194,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "あの、お客様……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3236,
            "text_len": 42,
            "text_offset": 3244,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "あっ、は、はい！\u0000\nえ、えっと今日は……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3308,
            "text_len": 70,
            "text_offset": 3314,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "あの、当店はペットを連れての入店は\u0000\nお断りさせて頂いていまして……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3406,
            "text_len": 18,
            "text_offset": 3414,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "へっ？　ペット？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3460,
            "text_len": 12,
            "text_offset": 3468,
            "warnings": 0
        },
        "speaker": "バディ",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3502,
            "text_len": 62,
            "text_offset": 3510,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "あっ！？　あの、この犬は違うんです！\u0000\nほらほら、あっち行け。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3648,
            "text_len": 68,
            "text_offset": 3652,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "やっぱり駄目か。ペットを装えば\u0000\n正面からでも行けると思ったんだが。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3742,
            "text_len": 74,
            "text_offset": 3746,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "動物のときは店に入られると困るよな。\u0000\nこっそり入っても気付かれやすいし。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3842,
            "text_len": 42,
            "text_offset": 3846,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "仕方ない。\u0000\n出てくるまで店の前で待つか。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3910,
            "text_len": 50,
            "text_offset": 3914,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "だな。\u0000\n……おっ？　なんだか人が集まってきたぞ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3986,
            "text_len": 60,
            "text_offset": 3990,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "おお、撫でられてる。\u0000\n店のマスコットとでも思われてるのか？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4072,
            "text_len": 58,
            "text_offset": 4076,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "ちょっ！　も、もっと優しく！\u0000\nそんな乱暴に撫でたら弌弌！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4174,
            "text_len": 22,
            "text_offset": 4182,
            "warnings": 2
        },
        "speaker": "＆",
        "speaker_translation": "",
        "text": "あっ……折れた……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4254,
            "text_len": 24,
            "text_offset": 4262,
            "warnings": 2
        },
        "speaker": "＆",
        "speaker_translation": "",
        "text": "うおおおお――――！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4378,
            "text_len": 16,
            "text_offset": 4384,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "ご注文は……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4424,
            "text_len": 18,
            "text_offset": 4432,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "あ、えっと……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4482,
            "text_len": 54,
            "text_offset": 4490,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（うっ……いざメニュ―を前にすると\u0000\n　緊張する……）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4570,
            "text_len": 60,
            "text_offset": 4578,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（謎の文字の羅列と店員さんの\u0000\n　無言のプレッシャ―……！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4664,
            "text_len": 50,
            "text_offset": 4672,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（頑張れ私！\u0000\n　この雰囲気に呑まれちゃ駄目よ！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4790,
            "text_len": 24,
            "text_offset": 4798,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "う―ん、え―っと……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4848,
            "text_len": 12,
            "text_offset": 4854,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4892,
            "text_len": 72,
            "text_offset": 4900,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（あ、悩んだらもっと駄目だ！\u0000\n　時間をかけたら、その分焦りが生じる！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4994,
            "text_len": 22,
            "text_offset": 5000,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "あの、ご注文は……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5048,
            "text_len": 58,
            "text_offset": 5056,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（もう何でもいい！\u0000\n　パッと目についたものを適当に……）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5152,
            "text_len": 28,
            "text_offset": 5160,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "こ、これ！　一番大きいの！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5214,
            "text_len": 12,
            "text_offset": 5220,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5262,
            "text_len": 78,
            "text_offset": 5270,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（し、しまった！　これじゃメニュ―を\u0000\n　読めないと言っているのと同じだ！！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5370,
            "text_len": 52,
            "text_offset": 5376,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "え―……エスプレッソフラペチ―ノの\u0000\n一番大きな弌弌\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5450,
            "text_len": 46,
            "text_offset": 5458,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "ド、ドッピオ！\u0000\nドド、ド、ドッピオください！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5540,
            "text_len": 12,
            "text_offset": 5546,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5580,
            "text_len": 12,
            "text_offset": 5588,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "…………！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5628,
            "text_len": 58,
            "text_offset": 5634,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "あの……一番大きなサイズだと\u0000\nベンティになりますが……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5732,
            "text_len": 34,
            "text_offset": 5740,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（ベベベヴェ！！　ベンティ！？）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5804,
            "text_len": 38,
            "text_offset": 5812,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（べべべべべべべべべべベンティ！？）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5872,
            "text_len": 48,
            "text_offset": 5878,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "あの、サイズはベンティで\u0000\nよろしいでしょうか？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5952,
            "text_len": 14,
            "text_offset": 5960,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "…………！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6004,
            "text_len": 16,
            "text_offset": 6012,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "もう知らん！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6080,
            "text_len": 28,
            "text_offset": 6086,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "えええ―！？　お客様―！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6222,
            "text_len": 66,
            "text_offset": 6230,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（ベンティって、ベンティって……\u0000\n　なんなんだ―――――！！！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6356,
            "text_len": 12,
            "text_offset": 6364,
            "warnings": 0
        },
        "speaker": "きよし",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6398,
            "text_len": 58,
            "text_offset": 6406,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "あっ……キ、キミ、先にどうぞ。\u0000\n私まだ決まってないから。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6486,
            "text_len": 40,
            "text_offset": 6494,
            "warnings": 0
        },
        "speaker": "きよし",
        "speaker_translation": "",
        "text": "あっ、は―い。\u0000\nありがとうございます。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6560,
            "text_len": 72,
            "text_offset": 6568,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（よし、これでインタ―バルを置いて\u0000\n　仕切り直しよ。焦るな、私……！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6670,
            "text_len": 16,
            "text_offset": 6678,
            "warnings": 0
        },
        "speaker": "きよし",
        "speaker_translation": "",
        "text": "え―っと……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6732,
            "text_len": 66,
            "text_offset": 6740,
            "warnings": 0
        },
        "speaker": "きよし",
        "speaker_translation": "",
        "text": "ト―ルのオレンジクリ―ムフラペチ―ノに\u0000\nエスプレットショットを。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6856,
            "text_len": 6,
            "text_offset": 6864,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6900,
            "text_len": 70,
            "text_offset": 6906,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "はい。ト―ルのオレンジクリ―ム\u0000\nフラペチ―ノにエスプレットショット。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7014,
            "text_len": 28,
            "text_offset": 7022,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（ノ―ミスだとおおお！？）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7076,
            "text_len": 48,
            "text_offset": 7084,
            "warnings": 0
        },
        "speaker": "きよし",
        "speaker_translation": "",
        "text": "それと、キノコとベ―コンの\u0000\nパンデサルを２つ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7154,
            "text_len": 46,
            "text_offset": 7160,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "はい。キノコとベ―コンの\u0000\nパンデサルを２つ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7244,
            "text_len": 32,
            "text_offset": 7252,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（またもやノ―ミッスィ―ズ！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7310,
            "text_len": 48,
            "text_offset": 7318,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（何この子……！\u0000\n　この歳でパンデサル……！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7392,
            "text_len": 24,
            "text_offset": 7398,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "以上でよろしいですか？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7444,
            "text_len": 26,
            "text_offset": 7452,
            "warnings": 0
        },
        "speaker": "きよし",
        "speaker_translation": "",
        "text": "あっ、えっと最後に……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7500,
            "text_len": 76,
            "text_offset": 7508,
            "warnings": 0
        },
        "speaker": "きよし",
        "speaker_translation": "",
        "text": "ヘ―ゼルナッツキャラメルカフェミスト\u0000\nキャラメルマキア―トのグランデ１つ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7624,
            "text_len": 6,
            "text_offset": 7632,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7664,
            "text_len": 34,
            "text_offset": 7670,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "はい。以上１７５０円になります。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7726,
            "text_len": 10,
            "text_offset": 7734,
            "warnings": 0
        },
        "speaker": "きよし",
        "speaker_translation": "",
        "text": "は―い。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7778,
            "text_len": 12,
            "text_offset": 7786,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7820,
            "text_len": 16,
            "text_offset": 7826,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "次の方どうぞ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7868,
            "text_len": 44,
            "text_offset": 7876,
            "warnings": 0
        },
        "speaker": "きよし",
        "speaker_translation": "",
        "text": "あっ、お姉ちゃん、ありがとう。\u0000\n次どうぞ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7950,
            "text_len": 10,
            "text_offset": 7958,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "死ね！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 8106,
            "text_len": 14,
            "text_offset": 8120,
            "warnings": 0
        },
        "speaker": "きよし＆玉村",
        "speaker_translation": "",
        "text": "えええっ！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8192,
            "text_len": 46,
            "text_offset": 8200,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（そ、そうだ！\u0000\n　前にみおちゃんは確か……）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8278,
            "text_len": 56,
            "text_offset": 8286,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "ホ、ホワイトチョコレ―ト\u0000\nモカフラペチ―ノ、ト―ルで！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8368,
            "text_len": 8,
            "text_offset": 8374,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "！！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8404,
            "text_len": 64,
            "text_offset": 8410,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "はい！　ホワイトチョコレ―ト\u0000\nモカフラペチ―ノのト―ルですね。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8514,
            "text_len": 16,
            "text_offset": 8522,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（ＸＤＲ！！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8564,
            "text_len": 60,
            "text_offset": 8572,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（やった、やったよゆっこ！\u0000\n　ちゃんと注文できたじゃん！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8658,
            "text_len": 78,
            "text_offset": 8666,
            "warnings": 1
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（もう私は前までの私じゃないみたいね。\u0000\n　ふふふ、レベルアップ―って感じ）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8766,
            "text_len": 34,
            "text_offset": 8772,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "注文は以上でよろしいでしょうか？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8828,
            "text_len": 18,
            "text_offset": 8836,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "はい、それで弌弌\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 85,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8880,
            "text_len": 70,
            "text_offset": 8888,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（待って、待つのよゆっこ！\u0000\n　これは、みおちゃんの真似をしただけ！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 86,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8984,
            "text_len": 44,
            "text_offset": 8992,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（注文ができたとは言えないんじゃないの？）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 87,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9058,
            "text_len": 16,
            "text_offset": 9064,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "あ、あの……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 88,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9106,
            "text_len": 78,
            "text_offset": 9114,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（どうすれば……どうすれば……ん？\u0000\n　追加の……カスタマイズメニュ―…！？）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 89,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9226,
            "text_len": 36,
            "text_offset": 9234,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（レベルアップ、もらった――っ！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 90,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9292,
            "text_len": 20,
            "text_offset": 9298,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "あの、ご注文は弌弌\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 91,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9340,
            "text_len": 84,
            "text_offset": 9348,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "じゃあ追加で！　エスプレッソのショ―ト。\u0000\nエスプレッソのショ―トをしょ―っとだけ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 92,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9464,
            "text_len": 76,
            "text_offset": 9472,
            "warnings": 1
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "ショ―トでも、サイズはト―ルですよ？\u0000\nちゃんと聞いと―る？　なんちゃって\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 93,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9584,
            "text_len": 84,
            "text_offset": 9590,
            "warnings": 2
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "ホワイトチョコレ―トモカフラペチ―ノの\u0000\nト―ルに…エスプレットショットですね？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 94,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9700,
            "text_len": 44,
            "text_offset": 9708,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "えっ？\u0000\nあ、あの……もう一回お願いします。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 95,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9774,
            "text_len": 88,
            "text_offset": 9780,
            "warnings": 2
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "ホ、ホワイトチョコレ―トモカフラペチ―ノの\u0000\nト―ルにエスプレットショットですよね？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 96,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9890,
            "text_len": 32,
            "text_offset": 9898,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "エ、エスプレット……ショット？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 97,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9952,
            "text_len": 16,
            "text_offset": 9958,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "……ショット。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 98,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 10086,
            "text_len": 20,
            "text_offset": 10094,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "（レベルダウン！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    }
]