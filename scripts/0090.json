        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7012,
            "text_len": 66,
            "text_offset": 7018,
            "warnings": 0
        },
        "speaker": "みお",
        "speaker_translation": "",
        "text": "あっ、やった―！　先輩、出ました！\u0000\nオレンジジュ―ス出ましたよ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 61,
