[
    {
        "id": 0,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2150,
            "text_len": 34,
            "text_offset": 2158,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "ウェボシ―！　笹原見なかった！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2216,
            "text_len": 36,
            "text_offset": 2228,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "speaker_translation": "",
        "text": "え、いや〜、ちょっとわかんないな。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2290,
            "text_len": 28,
            "text_offset": 2298,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "そう。見かけたら教えてね！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2480,
            "text_len": 72,
            "text_offset": 2488,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "あいつ、一体どこ行ったのよ！？\u0000\n今日という今日は絶対許さないんだから！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2596,
            "text_len": 6,
            "text_offset": 2606,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2640,
            "text_len": 34,
            "text_offset": 2650,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "ちょっ……あの、た、立花さんっ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2708,
            "text_len": 24,
            "text_offset": 2716,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "は、はい。なんですか？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2762,
            "text_len": 48,
            "text_offset": 2772,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "あ、あの……その、\u0000\n廊下は走っちゃダメですよ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2848,
            "text_len": 38,
            "text_offset": 2856,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "あ、そ、そうですね。すみません……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2916,
            "text_len": 18,
            "text_offset": 2926,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "そ、それと……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2972,
            "text_len": 20,
            "text_offset": 2980,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "え、まだ何か……？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3022,
            "text_len": 76,
            "text_offset": 3032,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "それ！　と言うか、え―っとですね。\u0000\nとても言いにくいことなんですけど……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3130,
            "text_len": 24,
            "text_offset": 3140,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "その立花さんの手の弌弌\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3186,
            "text_len": 12,
            "text_offset": 3194,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "……手の？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3228,
            "text_len": 36,
            "text_offset": 3238,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "弌弌手の爪、ちょっと長いですよね！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3306,
            "text_len": 48,
            "text_offset": 3314,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "えっ！？\u0000\nあ、そう言えば切ってくるの忘れてた。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3384,
            "text_len": 72,
            "text_offset": 3392,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "先生、教えてくれてありがとうございます。\u0000\n部活までには切っときますね。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3486,
            "text_len": 44,
            "text_offset": 3496,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "そ、そうですね。\u0000\n割れたら危ないですから。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3562,
            "text_len": 32,
            "text_offset": 3570,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "じゃあ先生、私、急いでるんで！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3662,
            "text_len": 14,
            "text_offset": 3672,
            "warnings": 0
        },
        "speaker": "校長先生",
        "speaker_translation": "",
        "text": "……おっと。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3718,
            "text_len": 44,
            "text_offset": 3726,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "あっ……校長先生……\u0000\nす、すみません……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3800,
            "text_len": 68,
            "text_offset": 3810,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "ほら、立花さんったら。\u0000\n廊下は走っちゃダメって言ったばかりなのに。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3910,
            "text_len": 18,
            "text_offset": 3918,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "す、すみません。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3960,
            "text_len": 48,
            "text_offset": 3970,
            "warnings": 0
        },
        "speaker": "校長先生",
        "speaker_translation": "",
        "text": "はっはっは。\u0000\nこれからは気をつけてくださいね。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4040,
            "text_len": 10,
            "text_offset": 4048,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "は―い。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4136,
            "text_len": 12,
            "text_offset": 4140,
            "warnings": 0
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "なあ……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4176,
            "text_len": 6,
            "text_offset": 4180,
            "warnings": 0
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "ん？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4208,
            "text_len": 26,
            "text_offset": 4212,
            "warnings": 0
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "なにか変だと思わないか？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4260,
            "text_len": 12,
            "text_offset": 4264,
            "warnings": 0
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "何がだよ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4298,
            "text_len": 58,
            "text_offset": 4302,
            "warnings": 0
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "いや……何がって言われると困るが……\u0000\nとにかく変だよな。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4382,
            "text_len": 64,
            "text_offset": 4386,
            "warnings": 0
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "そうか？　オレにはさっぱりわからんな。\u0000\nいや、わかりたくない！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4480,
            "text_len": 44,
            "text_offset": 4484,
            "warnings": 0
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "……なあ二人とも、そろそろ現実をみようぜ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4574,
            "text_len": 34,
            "text_offset": 4582,
            "warnings": 0
        },
        "speaker": "＆",
        "speaker_translation": "",
        "text": "現実なんて見たくな――――いっ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4708,
            "text_len": 40,
            "text_offset": 4718,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "あ、あの！　も、もうひとついいですか？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4780,
            "text_len": 18,
            "text_offset": 4788,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "あ、はい。何か？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4834,
            "text_len": 66,
            "text_offset": 4844,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "その手の……て、手の銃です！\u0000\n学校への銃の持ち込みは禁止ですっ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4960,
            "text_len": 66,
            "text_offset": 4970,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "これは先生が預かっておきますから、\u0000\n放課後取りに来てくださいね。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5060,
            "text_len": 24,
            "text_offset": 5068,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "……はい、すみません。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5132,
            "text_len": 6,
            "text_offset": 5140,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "ん？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5176,
            "text_len": 20,
            "text_offset": 5184,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "笹原見つけたっ！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5260,
            "text_len": 60,
            "text_offset": 5268,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "待ちなさい笹原！\u0000\n今日という今日は絶対許さないんだからね！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5354,
            "text_len": 16,
            "text_offset": 5364,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "まだ持ってた！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5402,
            "text_len": 42,
            "text_offset": 5410,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "あっ！\u0000\nす、すみません、銃は禁止でした。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5474,
            "text_len": 40,
            "text_offset": 5484,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "いえ、わかってもらえればいいんですよ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5546,
            "text_len": 46,
            "text_offset": 5554,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "あ、笹原が行っちゃう！\u0000\nこら、待ちなさ―い！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5642,
            "text_len": 74,
            "text_offset": 5652,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "よかった、銃はあぶないもんね。\u0000\n……私、少しは生徒指導らしくできたかな？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5748,
            "text_len": 68,
            "text_offset": 5758,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "それにしても立花さん、あんなに一生懸命に。\u0000\n若いって良いですね―。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5848,
            "text_len": 56,
            "text_offset": 5858,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "……あら？\u0000\nでも立花さん、なんで笹原くんとは逆方向へ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5992,
            "text_len": 10,
            "text_offset": 6002,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "……！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6042,
            "text_len": 14,
            "text_offset": 6050,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "笹原―っ！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6172,
            "text_len": 18,
            "text_offset": 6182,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "なんてこった！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6248,
            "text_len": 32,
            "text_offset": 6256,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "ちょっと待ちなさい、笹原―っ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6310,
            "text_len": 60,
            "text_offset": 6320,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "あ……立花さん、廊下は走っちゃ……\u0000\nあぁ……行っちゃった。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6402,
            "text_len": 68,
            "text_offset": 6412,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "仕方ないですね……とりあえず、これを\u0000\n職員室へ持っていきましょう。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6506,
            "text_len": 10,
            "text_offset": 6516,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "あっ！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6788,
            "text_len": 6,
            "text_offset": 6794,
            "warnings": 0
        },
        "speaker": "みお",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6822,
            "text_len": 6,
            "text_offset": 6830,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6860,
            "text_len": 56,
            "text_offset": 6866,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "……な、なんでしょう？\u0000\nなんだか、みんなの視線が……？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6960,
            "text_len": 64,
            "text_offset": 6968,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "はぁ……笹原を見失っちゃった。\u0000\n武器もないし、どうしよう……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7054,
            "text_len": 6,
            "text_offset": 7062,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7090,
            "text_len": 46,
            "text_offset": 7098,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "そこの１年生！　悪いけど武器になってくれる？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7174,
            "text_len": 56,
            "text_offset": 7180,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "へっ！？　え？　あれ？\u0000\nわ、私、なんでこんな姿に―！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7350,
            "text_len": 34,
            "text_offset": 7358,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "さあ、頑張って笹原を倒すわよ！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7408,
            "text_len": 48,
            "text_offset": 7414,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "え―っ！？\u0000\nなんで、どうしてこんなことに―！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7516,
            "text_len": 46,
            "text_offset": 7524,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "先生、それ返してください！\u0000\n今必要なんです！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7592,
            "text_len": 36,
            "text_offset": 7602,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "ダ、ダメですよ！\u0000\nこれは没収です！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7660,
            "text_len": 52,
            "text_offset": 7668,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "そんな―。\u0000\nちょっとだけ、ちょっとだけですから―！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7742,
            "text_len": 18,
            "text_offset": 7752,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "ダメなんです―！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7842,
            "text_len": 8,
            "text_offset": 7850,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "あ！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7874,
            "text_len": 10,
            "text_offset": 7884,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "銃が！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8016,
            "text_len": 66,
            "text_offset": 8026,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "speaker_translation": "",
        "text": "ふふふ……今日のわら人形は亟身の一作。\u0000\n我ながら会心の出来だな。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8120,
            "text_len": 62,
            "text_offset": 8130,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "speaker_translation": "",
        "text": "さて、今日はこれを燃やして、\u0000\n一段上の呪いを試してみようか。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8214,
            "text_len": 58,
            "text_offset": 8224,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "speaker_translation": "",
        "text": "今までのやり方では効果が薄い……\u0000\n校長の髪が薄いだけに。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8304,
            "text_len": 20,
            "text_offset": 8314,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "speaker_translation": "",
        "text": "……面白くないな。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8356,
            "text_len": 78,
            "text_offset": 8366,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "speaker_translation": "",
        "text": "それはさておき、ここまで見事な出来だと、\u0000\n燃やしてしまうのは惜しい気も……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8508,
            "text_len": 68,
            "text_offset": 8518,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "speaker_translation": "",
        "text": "おや？　何か、落ちてきたぞ。\u0000\n焼却炉に入ってしまったようだが……？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8608,
            "text_len": 36,
            "text_offset": 8618,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "立花さん、走っちゃダメですよ―っ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8674,
            "text_len": 22,
            "text_offset": 8682,
            "warnings": 0
        },
        "speaker": "みさと",
        "speaker_translation": "",
        "text": "ああ、火の中にっ！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8726,
            "text_len": 36,
            "text_offset": 8736,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "speaker_translation": "",
        "text": "それに何か騒がしい。\u0000\nいったい弌弌\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8930,
            "text_len": 28,
            "text_offset": 8940,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "speaker_translation": "",
        "text": "弌弌何が――――――っ！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    }
]