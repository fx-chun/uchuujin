[
    {
        "id": 0,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2144,
            "text_len": 40,
            "text_offset": 2156,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "大福―、大福―。\u0000\n大福はいらんかね―。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2222,
            "text_len": 20,
            "text_offset": 2230,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "父さん。ただいま。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2272,
            "text_len": 56,
            "text_offset": 2284,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "おお、帰ってきたか。\u0000\n悪いが店の手伝いをしてくれんか。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2362,
            "text_len": 54,
            "text_offset": 2370,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "うん。じゃあ、荷物置いてくるから\u0000\nちょっと待ってて。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2446,
            "text_len": 8,
            "text_offset": 2458,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "ああ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2588,
            "text_len": 42,
            "text_offset": 2596,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "よ―し、がんばって手伝うぞ。\u0000\nお父さ弌弌\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2664,
            "text_len": 38,
            "text_offset": 2676,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "はぁ……大福の売り上げが悪いな……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2746,
            "text_len": 6,
            "text_offset": 2754,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2796,
            "text_len": 50,
            "text_offset": 2808,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "一体どうすれば\u0000\n売れるようになるんだろうか……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2884,
            "text_len": 16,
            "text_offset": 2892,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "（父さん……）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2934,
            "text_len": 32,
            "text_offset": 2942,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "……父さん。\u0000\n手伝い始めるよ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2996,
            "text_len": 54,
            "text_offset": 3008,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "おっ、来たか。では早速、\u0000\n大福くんで接客を頼んだぞ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3084,
            "text_len": 28,
            "text_offset": 3092,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "わ、わかった。がんばるよ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3146,
            "text_len": 56,
            "text_offset": 3154,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "（本当は嫌だけど……\u0000\n　あんなこと聞いたら断れないや）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3246,
            "text_len": 12,
            "text_offset": 3254,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3388,
            "text_len": 36,
            "text_offset": 3396,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "大福、売り上げが落ちてるのか……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3454,
            "text_len": 80,
            "text_offset": 3462,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "いつもはそんなこと感じさせないのに……\u0000\nそれだけ経営が危ないのかもしれないな。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3564,
            "text_len": 54,
            "text_offset": 3572,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "どうしたらいいんだろう。\u0000\n今の僕にできることは……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3658,
            "text_len": 44,
            "text_offset": 3666,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "……とりあえず今は\u0000\n大福くんになることか。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3736,
            "text_len": 52,
            "text_offset": 3744,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "よし。恥ずかしいけど、\u0000\n顔が見えないのが救いだな。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3822,
            "text_len": 12,
            "text_offset": 3832,
            "warnings": 0
        },
        "speaker": "校長先生",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3870,
            "text_len": 30,
            "text_offset": 3878,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "あっ、校長先生。こんにちは。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3962,
            "text_len": 12,
            "text_offset": 3972,
            "warnings": 0
        },
        "speaker": "校長先生",
        "speaker_translation": "",
        "text": "ぐあっ！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4006,
            "text_len": 56,
            "text_offset": 4014,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "あっ！？　す、すみません！\u0000\n被り物が大きくて距離感が！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4150,
            "text_len": 46,
            "text_offset": 4154,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "うわああ！\u0000\n今の頭突きでアンテナが折れたぁ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4242,
            "text_len": 68,
            "text_offset": 4246,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "くそぉ！　かわいそうだから\u0000\n大福買ってあげようと思っただけなのに！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4330,
            "text_len": 28,
            "text_offset": 4334,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "返せ！　俺の優しさを返せ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4378,
            "text_len": 42,
            "text_offset": 4382,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "返って来てるじゃないか！\u0000\n仇となってな！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4520,
            "text_len": 24,
            "text_offset": 4528,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "私に良い考えがあるよ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4578,
            "text_len": 24,
            "text_offset": 4586,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "わっ！　あ、相生さん！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4636,
            "text_len": 60,
            "text_offset": 4644,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "ごめんね。今の話、聞いちゃった。\u0000\nなんだか大変みたいだね。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4726,
            "text_len": 36,
            "text_offset": 4734,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "う、うん。僕も今知ったんだけどね。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4792,
            "text_len": 42,
            "text_offset": 4800,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "ところで、さっき良い考えがあるって……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4864,
            "text_len": 40,
            "text_offset": 4872,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "フフフ、知りたい？\u0000\n知りたいでしょ―？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4934,
            "text_len": 16,
            "text_offset": 4942,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "う、うん……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4980,
            "text_len": 62,
            "text_offset": 4988,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "いいでしょういいでしょう。\u0000\nじゃ、ちょっとその被り物貸して。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5090,
            "text_len": 16,
            "text_offset": 5098,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "えっ？　これ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5136,
            "text_len": 70,
            "text_offset": 5144,
            "warnings": 1
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "そうそう。せっかくのマスコットキャラが\u0000\nこれじゃあ台無しだからね〜\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5242,
            "text_len": 62,
            "text_offset": 5250,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "ここをこうやって……\u0000\nうんうん、良い感じ。勢いよくガ―ッと！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5334,
            "text_len": 6,
            "text_offset": 5342,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5386,
            "text_len": 56,
            "text_offset": 5394,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "ジャジャ―ン！\u0000\nこれを被れば客寄せ効果もバッチリ―！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5478,
            "text_len": 14,
            "text_offset": 5486,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "…………！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5572,
            "text_len": 18,
            "text_offset": 5580,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "な、何コレ……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5620,
            "text_len": 32,
            "text_offset": 5628,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "大福マン！　カッコイイでしょ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5688,
            "text_len": 10,
            "text_offset": 5696,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "か……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5736,
            "text_len": 38,
            "text_offset": 5744,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "（カッコよくない！　なさすぎる！！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5808,
            "text_len": 40,
            "text_offset": 5816,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "（いや違う、カッコイイ悪いの前に……）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5882,
            "text_len": 24,
            "text_offset": 5890,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "（絵が下手すぎる！！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5940,
            "text_len": 86,
            "text_offset": 5948,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "（どうしてこれで客が増えると思えるんだ？\u0000\n　小さな子供が見たら泣いてしまうのでは？）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6060,
            "text_len": 70,
            "text_offset": 6068,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "（いや、でも相生さんはうちのことを思って\u0000\n　真剣に考えてくれたんだ）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6164,
            "text_len": 70,
            "text_offset": 6172,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "（礼も言わず、ただ下手だと言うのは\u0000\n　あまりにも失礼じゃないのか？）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6264,
            "text_len": 52,
            "text_offset": 6272,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "ね―ね―、どう？\u0000\n大福マン、カッコイイと思わない？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6356,
            "text_len": 12,
            "text_offset": 6364,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6398,
            "text_len": 22,
            "text_offset": 6406,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "……あ―、そうだね。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6460,
            "text_len": 12,
            "text_offset": 6468,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "でしょ―！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6506,
            "text_len": 12,
            "text_offset": 6514,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6552,
            "text_len": 68,
            "text_offset": 6560,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "（ここは相生さんを立てるんだ。\u0000\n　それが僕にできるせめてもの礼だ）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6650,
            "text_len": 30,
            "text_offset": 6658,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "ね―ね―、早く被ってみてよ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6714,
            "text_len": 6,
            "text_offset": 6722,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6764,
            "text_len": 80,
            "text_offset": 6772,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "（コ、コレを被って客引きをしろと！？\u0000\n　そんなことすれば客足は更に遠のく……）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6878,
            "text_len": 58,
            "text_offset": 6886,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "（だが、くっ……今の僕は\u0000\n　相生さんに従うしか……！！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6984,
            "text_len": 56,
            "text_offset": 6992,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "おお―！　いいねいいね！\u0000\nそれで店の前に立ってみてよ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7070,
            "text_len": 12,
            "text_offset": 7078,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "…………！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7112,
            "text_len": 50,
            "text_offset": 7120,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "……い、いらっしゃ―い。\u0000\n美味しい大福あるよ―。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7202,
            "text_len": 20,
            "text_offset": 7210,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "あはははは……あ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7252,
            "text_len": 16,
            "text_offset": 7260,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "？　相生さん？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7296,
            "text_len": 12,
            "text_offset": 7302,
            "warnings": 0
        },
        "speaker": "警察",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7422,
            "text_len": 6,
            "text_offset": 7430,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7510,
            "text_len": 46,
            "text_offset": 7518,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "な、何これ……！！\u0000\nただの落書きじゃないか！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7586,
            "text_len": 32,
            "text_offset": 7594,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "落書きじゃないって。大福マン。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7648,
            "text_len": 16,
            "text_offset": 7656,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "だ、大福マン？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7694,
            "text_len": 32,
            "text_offset": 7702,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "うん、大福マン。私が考えたの。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7760,
            "text_len": 6,
            "text_offset": 7768,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7806,
            "text_len": 60,
            "text_offset": 7814,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "ささ、それ被ってみてって。\u0000\nきっといっぱいお客さんくるよ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7896,
            "text_len": 62,
            "text_offset": 7904,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "ダメだって。こんなんじゃ更に\u0000\nお客さんが減るに決まってるよ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7988,
            "text_len": 32,
            "text_offset": 7996,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "いいから試しに被ってみなって。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8050,
            "text_len": 38,
            "text_offset": 8058,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "う―ん……ダメだと思うけどなぁ……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8136,
            "text_len": 34,
            "text_offset": 8144,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "おっ、いいね―！　似合ってるよ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8200,
            "text_len": 20,
            "text_offset": 8208,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "そ、そうかな……？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8250,
            "text_len": 40,
            "text_offset": 8258,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "うん。早速お客さんを呼び込んでみてよ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8320,
            "text_len": 48,
            "text_offset": 8328,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "あ、うん。\u0000\n……大福、大福―。いかがですか―。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 8396,
            "text_len": 12,
            "text_offset": 8408,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "この……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8448,
            "text_len": 6,
            "text_offset": 8456,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "ん？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 8602,
            "text_len": 20,
            "text_offset": 8614,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "バカ息子がぁぁ！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8654,
            "text_len": 6,
            "text_offset": 8662,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 85,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8688,
            "text_len": 6,
            "text_offset": 8696,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 86,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8766,
            "text_len": 14,
            "text_offset": 8774,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "な、何コレ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 87,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8810,
            "text_len": 30,
            "text_offset": 8818,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "大福マン！\u0000\n私の自信作だよ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 88,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8870,
            "text_len": 28,
            "text_offset": 8878,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "自信作……？　これが……？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 89,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8928,
            "text_len": 32,
            "text_offset": 8936,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "むっ……なにさ、文句があるの？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 90,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8990,
            "text_len": 56,
            "text_offset": 8998,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "だって、自信ありげに描いたわりには\u0000\n絵が下手だし……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 91,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9086,
            "text_len": 6,
            "text_offset": 9094,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 92,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9132,
            "text_len": 56,
            "text_offset": 9140,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "まあ絵が下手なのはいいとして、\u0000\nどうしてアッカンベ―？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 93,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9228,
            "text_len": 6,
            "text_offset": 9236,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 94,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9264,
            "text_len": 50,
            "text_offset": 9272,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "これじゃ、\u0000\nお客さんにケンカ売ってるみたいだよ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 95,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9436,
            "text_len": 10,
            "text_offset": 9444,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "死ねっ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 96,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9478,
            "text_len": 22,
            "text_offset": 9486,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "（えええ―――！？）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    }
