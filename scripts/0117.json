[
    {
        "id": 0,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2144,
            "text_len": 30,
            "text_offset": 2152,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "１３６、１３７、１３８……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2204,
            "text_len": 54,
            "text_offset": 2212,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "何とか落とさず教室まで行けそう……っと。\u0000\n……あれ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2288,
            "text_len": 74,
            "text_offset": 2296,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "向こうから来るのって関口さん？\u0000\n本読んでるから顔は見えないけど……多分。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2392,
            "text_len": 28,
            "text_offset": 2400,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "すみませ―ん、関口さ―ん。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2460,
            "text_len": 28,
            "text_offset": 2466,
            "warnings": 0
        },
        "speaker": "関口",
        "speaker_translation": "",
        "text": "……あ、桜井くん。どうも。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2516,
            "text_len": 66,
            "text_offset": 2524,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "こんにちは。歩きながら\u0000\n本読んでる人が居るからびっくりしました。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2612,
            "text_len": 36,
            "text_offset": 2620,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "ほんとにいつも本読んでるんですね。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2678,
            "text_len": 62,
            "text_offset": 2684,
            "warnings": 0
        },
        "speaker": "関口",
        "speaker_translation": "",
        "text": "私もリフティングしながら\u0000\n歩いてくる人がいるから驚きました。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2768,
            "text_len": 40,
            "text_offset": 2774,
            "warnings": 0
        },
        "speaker": "関口",
        "speaker_translation": "",
        "text": "ほんとにいつもボ―ル蹴ってるんですね。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2836,
            "text_len": 50,
            "text_offset": 2844,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "もちろんですよ。\u0000\n囲碁サッカ―、大好きですから。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2916,
            "text_len": 12,
            "text_offset": 2922,
            "warnings": 0
        },
        "speaker": "関口",
        "speaker_translation": "",
        "text": "なるほど。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2970,
            "text_len": 40,
            "text_offset": 2976,
            "warnings": 0
        },
        "speaker": "関口",
        "speaker_translation": "",
        "text": "でも、少しは前を見たほうがいいですよ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3038,
            "text_len": 6,
            "text_offset": 3046,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "え？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3076,
            "text_len": 14,
            "text_offset": 3082,
            "warnings": 0
        },
        "speaker": "関口",
        "speaker_translation": "",
        "text": "ほら、足元。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3126,
            "text_len": 6,
            "text_offset": 3134,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3172,
            "text_len": 70,
            "text_offset": 3180,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "まさかオレ、踏みかけてた！？\u0000\n……ってこの猫、何で逃げないんだろう？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3328,
            "text_len": 48,
            "text_offset": 3332,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "くそっ、こいつ！\u0000\nアンテナが折れちまっただろ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3404,
            "text_len": 82,
            "text_offset": 3408,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "バリアで猫は平気だが、万一のことがあったら\u0000\nどうするんだよ！　とっちめてやる！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3514,
            "text_len": 30,
            "text_offset": 3518,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "……これから？　どうやって？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3570,
            "text_len": 68,
            "text_offset": 3574,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "どうやってて、そりゃお前、\u0000\n今、アンテナが壊れたんだから弌弌あれ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3688,
            "text_len": 12,
            "text_offset": 3692,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3720,
            "text_len": 18,
            "text_offset": 3724,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "やるせねぇ……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3838,
            "text_len": 12,
            "text_offset": 3846,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3886,
            "text_len": 12,
            "text_offset": 3892,
            "warnings": 0
        },
        "speaker": "関口",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3938,
            "text_len": 32,
            "text_offset": 3946,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "（あれ……？　会話終わった？）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4018,
            "text_len": 58,
            "text_offset": 4026,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "（そういえば、\u0000\n　いつもは関口さんと何を話してたっけ？）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4110,
            "text_len": 50,
            "text_offset": 4118,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "（……そうか！\u0000\n　いつもは間に部長が居るんだ！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4194,
            "text_len": 80,
            "text_offset": 4202,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "（で、原因がわかった所でどうしよう。\u0000\n　挨拶しておいて何も話さないのも変だし）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4344,
            "text_len": 78,
            "text_offset": 4352,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "（ここは一番得意なジャンルで勝負！\u0000\n　とりあえずさっきの話を膨らませて……）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4456,
            "text_len": 84,
            "text_offset": 4464,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "そ、そう！　囲碁サッカ―が大好きだから\u0000\nいつだって技を出す準備も出来てるんですよ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4570,
            "text_len": 8,
            "text_offset": 4576,
            "warnings": 0
        },
        "speaker": "関口",
        "speaker_translation": "",
        "text": "はぁ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4606,
            "text_len": 44,
            "text_offset": 4614,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "例えばこうやってボ―ルを高く浮かせて……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4680,
            "text_len": 14,
            "text_offset": 4688,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "ここで碁石！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4724,
            "text_len": 6,
            "text_offset": 4730,
            "warnings": 0
        },
        "speaker": "関口",
        "speaker_translation": "",
        "text": "え？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4758,
            "text_len": 30,
            "text_offset": 4766,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "そして碁石と高さをあわせて！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4818,
            "text_len": 68,
            "text_offset": 4824,
            "warnings": 0
        },
        "speaker": "関口",
        "speaker_translation": "",
        "text": "あの、まさかとは思いますけど\u0000\nこんな所でシュ―トなんてしたら……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4924,
            "text_len": 20,
            "text_offset": 4932,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "シュ――――ト！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4980,
            "text_len": 6,
            "text_offset": 4986,
            "warnings": 0
        },
        "speaker": "関口",
        "speaker_translation": "",
        "text": "あ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5024,
            "text_len": 74,
            "text_offset": 5032,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "碁石も蹴ったように見せかけて\u0000\n実はボ―ルしか蹴らない！　これが必殺の弌弌\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5164,
            "text_len": 10,
            "text_offset": 5172,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "あ……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5214,
            "text_len": 30,
            "text_offset": 5224,
            "warnings": 0
        },
        "speaker": "富岡先生",
        "speaker_translation": "",
        "text": "あ、赤城先生大丈夫ですか！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5274,
            "text_len": 28,
            "text_offset": 5284,
            "warnings": 0
        },
        "speaker": "富岡先生",
        "speaker_translation": "",
        "text": "ダメだ、白目剥いてる……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5336,
            "text_len": 78,
            "text_offset": 5346,
            "warnings": 0
        },
        "speaker": "富岡先生",
        "speaker_translation": "",
        "text": "誰だ！　こんな所でボ―ルを蹴ったのは！！\u0000\nさては、わざと赤城先生を狙ったな！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5446,
            "text_len": 82,
            "text_offset": 5454,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "ど、どうしよう、わざとじゃないのに……\u0000\nあの関口さん、一緒に誤解を弌弌居ない！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5654,
            "text_len": 46,
            "text_offset": 5664,
            "warnings": 0
        },
        "speaker": "富岡先生",
        "speaker_translation": "",
        "text": "あっ、犯人はそこのお前か！？\u0000\nちょっと来い！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5726,
            "text_len": 12,
            "text_offset": 5734,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "……はい。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5802,
            "text_len": 18,
            "text_offset": 5810,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "（あ、そうだ！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5860,
            "text_len": 18,
            "text_offset": 5868,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "あの、関口さん。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5908,
            "text_len": 14,
            "text_offset": 5914,
            "warnings": 0
        },
        "speaker": "関口",
        "speaker_translation": "",
        "text": "なんですか？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5950,
            "text_len": 58,
            "text_offset": 5958,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "良かったらその本、\u0000\nちょっと読ませて貰ってもいいですか？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6038,
            "text_len": 14,
            "text_offset": 6044,
            "warnings": 0
        },
        "speaker": "関口",
        "speaker_translation": "",
        "text": "いいですよ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6080,
            "text_len": 48,
            "text_offset": 6088,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "ありがとう。\u0000\nそれじゃ、代わりにボ―ルどうぞ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6158,
            "text_len": 6,
            "text_offset": 6164,
            "warnings": 0
        },
        "speaker": "関口",
        "speaker_translation": "",
        "text": "え？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6192,
            "text_len": 34,
            "text_offset": 6200,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "オレだけ借りてたら不公平でしょ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6256,
            "text_len": 22,
            "text_offset": 6262,
            "warnings": 0
        },
        "speaker": "関口",
        "speaker_translation": "",
        "text": "いえ、私は別に……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6306,
            "text_len": 74,
            "text_offset": 6314,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "使いすぎてボロボロですけど、\u0000\n初めて買ったボ―ルなんで許してくださいね。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6414,
            "text_len": 6,
            "text_offset": 6420,
            "warnings": 0
        },
        "speaker": "関口",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6464,
            "text_len": 54,
            "text_offset": 6470,
            "warnings": 0
        },
        "speaker": "関口",
        "speaker_translation": "",
        "text": "そんな大事なもの、私が使うわけには。\u0000\nそもそも……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6546,
            "text_len": 52,
            "text_offset": 6554,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "いえ、使ってください！\u0000\nむしろ使って欲しいんです。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6632,
            "text_len": 52,
            "text_offset": 6640,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "関口さんは大切な囲碁サッカ―部の\u0000\n仲間ですから！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6718,
            "text_len": 80,
            "text_offset": 6726,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "（そうだ、話しにくいだなんて失礼な。\u0000\n　仲間なんだからもっと親しくしないと！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6832,
            "text_len": 6,
            "text_offset": 6838,
            "warnings": 0
        },
        "speaker": "関口",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6870,
            "text_len": 12,
            "text_offset": 6876,
            "warnings": 0
        },
        "speaker": "関口",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6992,
            "text_len": 24,
            "text_offset": 6998,
            "warnings": 0
        },
        "speaker": "関口",
        "speaker_translation": "",
        "text": "ありがとうございます。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7074,
            "text_len": 74,
            "text_offset": 7082,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "（そうだ、囲碁サッカ―に興味を持ってもらう\u0000\n　いいチャンスじゃないか！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7192,
            "text_len": 38,
            "text_offset": 7200,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "（よ―し、そうと決まればその路線で）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7260,
            "text_len": 84,
            "text_offset": 7268,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "いや―、思わず廊下でリフティングするぐらい\u0000\n囲碁サッカ―って本当に面白いんですよ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7374,
            "text_len": 88,
            "text_offset": 7382,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "学校に囲碁サッカ―部があったのが嬉しくて、\u0000\n最近は特にテンションあがってるんですよね！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7492,
            "text_len": 12,
            "text_offset": 7498,
            "warnings": 0
        },
        "speaker": "関口",
        "speaker_translation": "",
        "text": "はぁ……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7532,
            "text_len": 84,
            "text_offset": 7540,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "そ、それにプロリ―グも盛り上がってて、\u0000\nオリンピック競技になるかもしれないですし！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7646,
            "text_len": 18,
            "text_offset": 7652,
            "warnings": 0
        },
        "speaker": "関口",
        "speaker_translation": "",
        "text": "そうなんですか。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7700,
            "text_len": 12,
            "text_offset": 7708,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7756,
            "text_len": 52,
            "text_offset": 7764,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "（あ、あれ〜？\u0000\n　何かリアクション薄いんだけど？）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7852,
            "text_len": 36,
            "text_offset": 7860,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "（よし、こうなったら単刀直入に！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7918,
            "text_len": 74,
            "text_offset": 7926,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "あの……もしかして関口さんって、\u0000\n囲碁サッカ―あんまり興味ないんですか？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8022,
            "text_len": 14,
            "text_offset": 8028,
            "warnings": 0
        },
        "speaker": "関口",
        "speaker_translation": "",
        "text": "無いですね。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8078,
            "text_len": 12,
            "text_offset": 8086,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8134,
            "text_len": 46,
            "text_offset": 8140,
            "warnings": 0
        },
        "speaker": "関口",
        "speaker_translation": "",
        "text": "そろそろ授業なので行きますね。\u0000\nまた部室で。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8212,
            "text_len": 12,
            "text_offset": 8220,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8336,
            "text_len": 24,
            "text_offset": 8344,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "speaker_translation": "",
        "text": "うちの部……ダメかも。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    }
]