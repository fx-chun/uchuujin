[
    {
        "id": 0,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2152,
            "text_len": 60,
            "text_offset": 2158,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "も―、私ったら理科実験室に忘れ物なんて\u0000\nうっかりしすぎっ。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2240,
            "text_len": 72,
            "text_offset": 2246,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "早くしないと次の授業が……\u0000\n数学の赤城先生、厳しいから怒られちゃうよ！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2346,
            "text_len": 8,
            "text_offset": 2352,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "あっ！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2386,
            "text_len": 12,
            "text_offset": 2394,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "うわっ！！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2512,
            "text_len": 12,
            "text_offset": 2518,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "きゃっ！！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2552,
            "text_len": 24,
            "text_offset": 2560,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "だ、大丈夫、安中さん？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2616,
            "text_len": 76,
            "text_offset": 2622,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "あ、うん！　ありがとう中之条くん。\u0000\n急いでいたら、思わず走っちゃって……。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2720,
            "text_len": 72,
            "text_offset": 2728,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "気持ちはわかるけど、走って変に転んだら、\u0000\n大ケガして大変だと思うんだ。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2822,
            "text_len": 48,
            "text_offset": 2828,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "うん、ありがとう。\u0000\nぶつかっちゃってごめんね。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2908,
            "text_len": 30,
            "text_offset": 2914,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "それじゃ、私急いでるからっ。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2978,
            "text_len": 42,
            "text_offset": 2986,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "あ、安中さん！\u0000\nそんなに走ったらまた弌弌\u0000\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3088,
            "text_len": 12,
            "text_offset": 3098,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "…………。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3134,
            "text_len": 14,
            "text_offset": 3140,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "きゃあっ！！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3180,
            "text_len": 6,
            "text_offset": 3190,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "！？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3248,
            "text_len": 44,
            "text_offset": 3256,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "弌弌ぶつかるよって……\u0000\n大丈夫、安中さん？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3324,
            "text_len": 14,
            "text_offset": 3330,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "えへへ……。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3420,
            "text_len": 36,
            "text_offset": 3424,
            "warnings": 1
        },
        "speaker": "",
        "text": "いや―、この子はほほえましいなぁ。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3492,
            "text_len": 20,
            "text_offset": 3496,
            "warnings": 1
        },
        "speaker": "",
        "text": "すっごく和むよな。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3538,
            "text_len": 66,
            "text_offset": 3542,
            "warnings": 1
        },
        "speaker": "",
        "text": "ああ、今のでアンテナが折れたことなんて\u0000\n大したことじゃないよな！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3630,
            "text_len": 26,
            "text_offset": 3634,
            "warnings": 1
        },
        "speaker": "",
        "text": "そうさ！　はははははっ！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3682,
            "text_len": 18,
            "text_offset": 3686,
            "warnings": 1
        },
        "speaker": "",
        "text": "ははははははっ！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3758,
            "text_len": 26,
            "text_offset": 3766,
            "warnings": 2
        },
        "speaker": "＆",
        "text": "大したことだ―――っ！！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3884,
            "text_len": 50,
            "text_offset": 3890,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "でも、今ので\u0000\n中之条くんまで倒れなくて良かった。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3962,
            "text_len": 74,
            "text_offset": 3968,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "いつも第一発見者なのに、今度は\u0000\n犯人になっちゃうとこだったよ―。なんて。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4074,
            "text_len": 66,
            "text_offset": 4082,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "……あ、そういえば、\u0000\n何度も高崎先生を呼んできてくれたんだよね。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4170,
            "text_len": 36,
            "text_offset": 4178,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "えっと……\u0000\nありがとう、安中さん。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4238,
            "text_len": 62,
            "text_offset": 4244,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "どういたしまして。\u0000\nでも、何でいつもあんなことになってるの？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4328,
            "text_len": 24,
            "text_offset": 4336,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "う―ん、それは多分弌弌\u0000\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4496,
            "text_len": 12,
            "text_offset": 4504,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "ぐげっ！！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4536,
            "text_len": 10,
            "text_offset": 4542,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "！！！？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4674,
            "text_len": 12,
            "text_offset": 4682,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "…………。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4720,
            "text_len": 52,
            "text_offset": 4726,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "な、中之条くんっ、しっかりして！！\u0000\n目を覚まして！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4836,
            "text_len": 60,
            "text_offset": 4842,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "ど、どうしよう！\u0000\n高崎先生を呼びに行く時間もないのに……。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4922,
            "text_len": 12,
            "text_offset": 4930,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "…………。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4964,
            "text_len": 24,
            "text_offset": 4970,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "……こ、こうなったら。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5052,
            "text_len": 10,
            "text_offset": 5058,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "えいっ！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5094,
            "text_len": 14,
            "text_offset": 5102,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "ぐほぁっ！？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5148,
            "text_len": 20,
            "text_offset": 5154,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "よし、これで……。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5194,
            "text_len": 12,
            "text_offset": 5202,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "…………。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5242,
            "text_len": 22,
            "text_offset": 5248,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "あ、あれ、起きない？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5292,
            "text_len": 16,
            "text_offset": 5298,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "も、もう一回！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5362,
            "text_len": 12,
            "text_offset": 5370,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "げふっ！！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5412,
            "text_len": 12,
            "text_offset": 5420,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "…………。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5454,
            "text_len": 20,
            "text_offset": 5460,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "弌弌起きない……。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5506,
            "text_len": 12,
            "text_offset": 5512,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "…………。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5646,
            "text_len": 18,
            "text_offset": 5652,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "高崎せんせ―！！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5738,
            "text_len": 26,
            "text_offset": 5744,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "な、中之条くんしっかり！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5786,
            "text_len": 14,
            "text_offset": 5794,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "う―ん……。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5830,
            "text_len": 30,
            "text_offset": 5836,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "大丈夫？　ねえ中之条くん！！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5890,
            "text_len": 18,
            "text_offset": 5898,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "う……いたたた。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5940,
            "text_len": 36,
            "text_offset": 5946,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "あっ、良かった！\u0000\n大丈夫っぽいよ！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6014,
            "text_len": 16,
            "text_offset": 6022,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "今、何が……？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6060,
            "text_len": 36,
            "text_offset": 6066,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "手が飛んできて頭に当たったんだよ。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6124,
            "text_len": 28,
            "text_offset": 6132,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "手……？　手が飛んできた？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6182,
            "text_len": 12,
            "text_offset": 6188,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "そうだよ。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6264,
            "text_len": 12,
            "text_offset": 6272,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "ぐへっ！！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6338,
            "text_len": 28,
            "text_offset": 6344,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "ほら、ちょうどこんな風に。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6404,
            "text_len": 26,
            "text_offset": 6410,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "って、ええ――――っ！？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6458,
            "text_len": 42,
            "text_offset": 6464,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "中之条くん、しっかり！\u0000\nしっかりして！！\u0000\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6526,
            "text_len": 12,
            "text_offset": 6534,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "…………。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6650,
            "text_len": 20,
            "text_offset": 6656,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "……ダメだこりゃ。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6732,
            "text_len": 12,
            "text_offset": 6740,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "…………。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6774,
            "text_len": 18,
            "text_offset": 6780,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "気絶してる……。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6820,
            "text_len": 70,
            "text_offset": 6826,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "……どうしよう、早く忘れ物取りに行かないと\u0000\n授業はじまっちゃうのに。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6918,
            "text_len": 34,
            "text_offset": 6924,
            "warnings": 0
        },
        "speaker": "安中",
