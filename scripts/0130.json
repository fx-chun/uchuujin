        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7592,
            "text_len": 16,
            "text_offset": 7600,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "は――――い！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7652,
            "text_len": 32,
            "text_offset": 7658,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "（……ふう。\u0000\n　さて、と……）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
