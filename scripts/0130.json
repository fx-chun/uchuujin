        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8374,
            "text_len": 42,
            "text_offset": 8380,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "（バジブルッドの弌弌バジルブレッド！？）\u0000\n",
        "text_translation": "",
