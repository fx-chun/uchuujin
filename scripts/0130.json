            "speaker_offset": 8328,
            "text_len": 6,
            "text_offset": 8334,
            "warnings": 0
        },
        "speaker": "玉村",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
