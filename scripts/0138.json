    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7756,
            "text_len": 38,
            "text_offset": 7764,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "んじゃ、いただきま―す。ごくっ……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7824,
            "text_len": 62,
            "text_offset": 7832,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "マズ―！！　なにこれ！？\u0000\nこんな甘いのコ―ヒ―じゃないよ！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7930,
            "text_len": 12,
            "text_offset": 7936,
            "warnings": 0
        },
        "speaker": "みお",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
