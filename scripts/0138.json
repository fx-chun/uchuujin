    {
        "id": 74,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7688,
            "text_len": 36,
            "text_offset": 7694,
            "warnings": 0
        },
        "speaker": "みお",
        "speaker_translation": "",
        "text": "（うぎゃ！？　なんじゃこりゃ！？）\u0000\n",
        "text_translation": "",
