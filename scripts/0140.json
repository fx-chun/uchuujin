        "id": 49,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5710,
            "text_len": 28,
            "text_offset": 5716,
            "warnings": 0
        },
        "speaker": "みお",
        "speaker_translation": "",
        "text": "えっ？　どうしたんですか？\u0000\n",
