            "speaker_offset": 5884,
            "text_len": 6,
            "text_offset": 5890,
            "warnings": 0
        },
        "speaker": "みお",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5922,
            "text_len": 12,
            "text_offset": 5932,
            "warnings": 0
        },
        "speaker": "中村先生",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6058,
            "text_len": 14,
            "text_offset": 6068,
            "warnings": 0
        },
        "speaker": "中村先生",
        "speaker_translation": "",
        "text": "逃げろっ！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6134,
            "text_len": 12,
            "text_offset": 6140,
            "warnings": 0
        },
        "speaker": "みお",
        "speaker_translation": "",
