        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5428,
            "text_len": 12,
            "text_offset": 5436,
            "warnings": 0
        },
        "speaker": "イタコ",
        "speaker_translation": "",
