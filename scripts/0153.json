            "speaker_len": 6,
            "speaker_offset": 7092,
            "text_len": 14,
            "text_offset": 7100,
            "warnings": 0
        },
        "speaker": "イタコ",
        "speaker_translation": "",
        "text": "失礼します。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
