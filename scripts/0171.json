[
    {
        "id": 0,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2144,
            "text_len": 64,
            "text_offset": 2156,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "さあいらっしゃい、白いよ―、丸いよ―。\u0000\nその上黒くて甘いよ―。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2242,
            "text_len": 18,
            "text_offset": 2250,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "あっ、やたいだ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2294,
            "text_len": 54,
            "text_offset": 2306,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "ははは、お嬢ちゃん。\u0000\n屋台じゃなくて大福屋さんだよ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2382,
            "text_len": 44,
            "text_offset": 2390,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "あっ、ホントだ！\u0000\n大福がいっぱい売ってる！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2456,
            "text_len": 28,
            "text_offset": 2468,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "ああ、大福屋さんだからね。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2522,
            "text_len": 70,
            "text_offset": 2530,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "すごいすごい！　大福屋さんだって！\u0000\nこれはなのに教えてあげなきゃ！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2622,
            "text_len": 44,
            "text_offset": 2634,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "お友達かな？\u0000\nどんどん教えてあげておくれ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2700,
            "text_len": 30,
            "text_offset": 2708,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "うん、帰ったら教えなくちゃ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2760,
            "text_len": 28,
            "text_offset": 2772,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "お嬢ちゃんは大福好きかい？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2826,
            "text_len": 36,
            "text_offset": 2834,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "うん。でもケ―キの方がすきだけど。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2902,
            "text_len": 6,
            "text_offset": 2914,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3048,
            "text_len": 20,
            "text_offset": 3060,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "大福をなめるな！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3100,
            "text_len": 6,
            "text_offset": 3108,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3134,
            "text_len": 30,
            "text_offset": 3142,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "阪本！　大福仞めちゃダメ！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3192,
            "text_len": 18,
            "text_offset": 3202,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "えええ―――！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3240,
            "text_len": 40,
            "text_offset": 3252,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "何！？\u0000\n大福を仞めたのか、この猫は！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3312,
            "text_len": 18,
            "text_offset": 3322,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "えええ―――！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3420,
            "text_len": 30,
            "text_offset": 3424,
            "warnings": 0
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "とばっちりでアンテナがっ！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3500,
            "text_len": 24,
            "text_offset": 3504,
            "warnings": 0
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "勘弁してくれ――っ！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3628,
            "text_len": 44,
            "text_offset": 3640,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "（くっ、やっぱり子供はケ―キが好きなのか）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3710,
            "text_len": 74,
            "text_offset": 3722,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "（やはりシックで落ち着いた外見の魅力は\u0000\n　大人にしかわからないのか……）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3826,
            "text_len": 26,
            "text_offset": 3838,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "（いや、違う、違うぞ！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3890,
            "text_len": 60,
            "text_offset": 3902,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "（これは子供を大福に目覚めさせる\u0000\n　チャンスじゃないか！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3998,
            "text_len": 62,
            "text_offset": 4010,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "（そうだ、ここで諦めていては\u0000\n　大福ブ―ムなど夢のまた夢！）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4094,
            "text_len": 64,
            "text_offset": 4106,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "お嬢ちゃん、大福もおいしいんだよ。\u0000\nほら、１つ食べてみなさい。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4194,
            "text_len": 38,
            "text_offset": 4202,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "あっ、くれるの！？　いただきま―す！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4262,
            "text_len": 16,
            "text_offset": 4270,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "あむあむ……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4312,
            "text_len": 44,
            "text_offset": 4320,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "おいしい！！\u0000\nすごい、大福おいしいです！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4396,
            "text_len": 24,
            "text_offset": 4408,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "そうだろうそうだろう！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4454,
            "text_len": 66,
            "text_offset": 4466,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "さ、気に入ったならどんどん食べなさい！\u0000\n今お茶も亞れてあげよう！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4584,
            "text_len": 34,
            "text_offset": 4596,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "ほらほら！　どんどん食べなさい！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4652,
            "text_len": 40,
            "text_offset": 4660,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "わわっ、凄い凄い！\u0000\n甘い！　おいしい！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4722,
            "text_len": 40,
            "text_offset": 4734,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "まだまだおかわりならたくさんあるぞ―！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4796,
            "text_len": 20,
            "text_offset": 4804,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "すごい！　すごい！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4846,
            "text_len": 70,
            "text_offset": 4858,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "はははっ、本当にたくさんあるからな！\u0000\nほら、もっともっと食べなさい！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5052,
            "text_len": 56,
            "text_offset": 5064,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "おや、手が止まってるじゃないか。\u0000\nまだたくさんあるぞ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5148,
            "text_len": 58,
            "text_offset": 5156,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "え、えっと……ちょっと\u0000\nお腹がいっぱいになってきたかも。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5236,
            "text_len": 62,
            "text_offset": 5248,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "ははは、甘いものは別腹と言うじゃないか。\u0000\nまだ沢山あるぞ―！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5434,
            "text_len": 70,
            "text_offset": 5446,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "ほら、また手が止まっているじゃないか！\u0000\nさあもっと大福を食べるんだ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5550,
            "text_len": 44,
            "text_offset": 5558,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "も、もうムリ―！\u0000\nうわ―ん、大福怖い―！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5634,
            "text_len": 6,
            "text_offset": 5646,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5702,
            "text_len": 18,
            "text_offset": 5710,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "た、助けて―！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5832,
            "text_len": 30,
            "text_offset": 5844,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "わ、私は何か間違いを……！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5932,
            "text_len": 30,
            "text_offset": 5940,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "うん。ありがとう、おじさん！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5992,
            "text_len": 58,
            "text_offset": 6000,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "あむあむ……んくっ、んくっ……ぷは―！\u0000\nお茶もおいしい！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 6080,
            "text_len": 38,
            "text_offset": 6092,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "そうだろう？\u0000\nさ、もっと食べなさい。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6152,
            "text_len": 52,
            "text_offset": 6160,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "は―い！\u0000\nそうだ、おじさんも一緒に食べればいいよ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 6234,
            "text_len": 38,
            "text_offset": 6246,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "そうか、それじゃ私も食べるとしよう。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6412,
            "text_len": 28,
            "text_offset": 6420,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "ふぅ、ごちそうさまでした！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 6472,
            "text_len": 20,
            "text_offset": 6484,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "おいしかったかい？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6526,
            "text_len": 52,
            "text_offset": 6534,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "うん、すっごくおいしかった！\u0000\nケ―キより好きかも！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 6608,
            "text_len": 34,
            "text_offset": 6620,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "そうかそうか！\u0000\nそれは良かった！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6676,
            "text_len": 50,
            "text_offset": 6684,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "あっ、そろそろ行かなきゃ！\u0000\nごちそうさまでした！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 6766,
            "text_len": 66,
            "text_offset": 6778,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "……自信を失いかけていた所を\u0000\nあの子に助けられたかもしれないな。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 6866,
            "text_len": 32,
            "text_offset": 6878,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "よし、はりきって大福を売るぞ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 6946,
            "text_len": 12,
            "text_offset": 6958,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 7002,
            "text_len": 22,
            "text_offset": 7014,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "いっ、一個も無い！？\u0000\n",
