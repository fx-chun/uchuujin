        "id": 57,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 7154,
            "text_len": 28,
            "text_offset": 7166,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "speaker_translation": "",
        "text": "（全部食べてしまった……）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 6,
