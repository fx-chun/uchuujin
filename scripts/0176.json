[
    {
        "id": 0,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 2138,
            "text_len": 12,
            "text_offset": 2154,
            "warnings": 0
        },
        "speaker": "ジェントルマン",
        "speaker_translation": "",
        "text": "ふむ……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 2192,
            "text_len": 16,
            "text_offset": 2208,
            "warnings": 0
        },
        "speaker": "ジェントルマン",
        "speaker_translation": "",
        "text": "………………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 2246,
            "text_len": 36,
            "text_offset": 2262,
            "warnings": 0
        },
        "speaker": "ジェントルマン",
        "speaker_translation": "",
        "text": "屋上から眺める時定市、絶景ですな。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 2324,
            "text_len": 16,
            "text_offset": 2340,
            "warnings": 0
        },
        "speaker": "ジェントルマン",
        "speaker_translation": "",
        "text": "………………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 2378,
            "text_len": 32,
            "text_offset": 2394,
            "warnings": 0
        },
        "speaker": "ジェントルマン",
        "speaker_translation": "",
        "text": "……しかし、なぜわしはここに？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 2458,
            "text_len": 78,
            "text_offset": 2474,
            "warnings": 0
        },
        "speaker": "ジェントルマン",
        "speaker_translation": "",
        "text": "この学校に迷い込んで、はや数時間。\u0000\n一体、いつになったら出られるのやら……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 2576,
            "text_len": 30,
            "text_offset": 2590,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "speaker_translation": "",
        "text": "……だよ。やっぱりアレだよ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2636,
            "text_len": 30,
            "text_offset": 2648,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "speaker_translation": "",
        "text": "うん。紳士だ。絶対に紳士だ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 2700,
            "text_len": 20,
            "text_offset": 2716,
            "warnings": 0
        },
        "speaker": "ジェントルマン",
        "speaker_translation": "",
        "text": "……ん、この声は。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 2762,
            "text_len": 62,
            "text_offset": 2778,
            "warnings": 0
        },
        "speaker": "ジェントルマン",
        "speaker_translation": "",
        "text": "……おや、お嬢さんが二人。\u0000\nごきげんよう。良いお日柄ですな。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 2862,
            "text_len": 50,
            "text_offset": 2876,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "speaker_translation": "",
        "text": "すご―い！　ごきげんようだって！\u0000\nお日柄だって！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2948,
            "text_len": 52,
            "text_offset": 2960,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "speaker_translation": "",
        "text": "さすが紳士！　やるわね。\u0000\nけど、こっちだって……！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3046,
            "text_len": 22,
            "text_offset": 3058,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "speaker_translation": "",
        "text": "ボ……ボンボヤ―ジ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 28,
            "speaker_offset": 3140,
            "text_len": 16,
            "text_offset": 3170,
            "warnings": 0
        },
        "speaker": "ジェントルマン＆フェっちゃん",
        "speaker_translation": "",
        "text": "………………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 3232,
            "text_len": 22,
            "text_offset": 3246,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "speaker_translation": "",
        "text": "ウェボシ―、何それ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3290,
            "text_len": 56,
            "text_offset": 3302,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "speaker_translation": "",
        "text": "挨拶に決まってるじゃない。\u0000\nどう？　ジェントルでしょ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 3380,
            "text_len": 54,
            "text_offset": 3394,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "speaker_translation": "",
        "text": "ん―、違うんじゃない？\u0000\nていうかボンボヤ―ジって何？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3470,
            "text_len": 58,
            "text_offset": 3482,
            "warnings": 2
        },
        "speaker": "ウェボシ―",
        "speaker_translation": "",
        "text": "だから挨拶だって。\u0000\nイタリア語でごきんげようだっけ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 3570,
            "text_len": 56,
            "text_offset": 3586,
            "warnings": 2
        },
        "speaker": "ジェントルマン",
        "speaker_translation": "",
        "text": "ボンボヤ―ジは良い旅を！という\u0000\nフランス語ですぞ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 3684,
            "text_len": 38,
            "text_offset": 3698,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "speaker_translation": "",
        "text": "だってさ。\u0000\nウェボシ―かっこ悪〜い。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3798,
            "text_len": 12,
            "text_offset": 3808,
            "warnings": 0
        },
        "speaker": "校長先生",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3840,
            "text_len": 64,
            "text_offset": 3852,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "speaker_translation": "",
        "text": "あはは……ん？　あれ？\u0000\nフェっちゃん、あそこに校長先生が……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 3948,
            "text_len": 46,
            "text_offset": 3962,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "speaker_translation": "",
        "text": "ホントだ。\u0000\n何やってんだろ。あんな隅っこで。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4036,
            "text_len": 26,
            "text_offset": 4048,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "speaker_translation": "",
        "text": "校長―！　ボンボヤ―ジ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 4098,
            "text_len": 58,
            "text_offset": 4112,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "speaker_translation": "",
        "text": "だから、それは良い旅をだから。\u0000\nこんにちはじゃないって。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4204,
            "text_len": 16,
            "text_offset": 4214,
            "warnings": 0
        },
        "speaker": "校長先生",
        "speaker_translation": "",
        "text": "ボンボヤ―ジ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 24,
            "speaker_offset": 4264,
            "text_len": 6,
            "text_offset": 4290,
            "warnings": 0
        },
        "speaker": "ウェボシ―＆フェっちゃん",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 4330,
            "text_len": 48,
            "text_offset": 4346,
            "warnings": 0
        },
        "speaker": "ジェントルマン",
        "speaker_translation": "",
        "text": "い、いかん……\u0000\nあの者は飛び降りる気じゃ……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 24,
            "speaker_offset": 4424,
            "text_len": 52,
            "text_offset": 4450,
            "warnings": 0
        },
        "speaker": "ウェボシ―＆フェっちゃん",
        "speaker_translation": "",
        "text": "！？　こ、校長先生―！\u0000\n待って、早まらないで―！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4578,
            "text_len": 54,
            "text_offset": 4582,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "おおおお！？\u0000\nなんだ、急に女の子たちがこっちに……！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4668,
            "text_len": 78,
            "text_offset": 4672,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "抱きつかれた！？　ま、まさか、これが噂の\u0000\n桃源郷！？　ハ―レムってやつじゃ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4772,
            "text_len": 88,
            "text_offset": 4776,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "そうか！　さっきのボンボヤ―ジって言葉には\u0000\nそういう意味があったんだな！　地球すげえ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4886,
            "text_len": 56,
            "text_offset": 4890,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "まさか、ボンボヤ―ジが、\u0000\nそんな夢の暗号だったとは弌弌\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4992,
            "text_len": 20,
            "text_offset": 5004,
            "warnings": 3
        },
        "speaker": "＆＆",
        "speaker_translation": "",
        "text": "……………………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5044,
            "text_len": 52,
            "text_offset": 5056,
            "warnings": 3
        },
        "speaker": "＆＆",
        "speaker_translation": "",
        "text": "そうか……\u0000\n桃源郷って……こうやって行くんだ……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5200,
            "text_len": 36,
            "text_offset": 5212,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "speaker_translation": "",
        "text": "あはは……流石は紳士。詳しいね―。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 5282,
            "text_len": 44,
            "text_offset": 5296,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "speaker_translation": "",
        "text": "ところで紳士は\u0000\nこんなところで何してたの？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 5362,
            "text_len": 32,
            "text_offset": 5378,
            "warnings": 0
        },
        "speaker": "ジェントルマン",
        "speaker_translation": "",
        "text": "それが、いつの間にか迷って弌弌\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5434,
            "text_len": 50,
            "text_offset": 5446,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "speaker_translation": "",
        "text": "あっ、わかった！\u0000\n子供の様子を見に来たんでしょ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 5554,
            "text_len": 50,
            "text_offset": 5568,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "speaker_translation": "",
        "text": "紳士だったら、\u0000\n子供って言うよりは孫じゃないの？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5658,
            "text_len": 48,
            "text_offset": 5670,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "speaker_translation": "",
        "text": "まあそうかな。\u0000\nねえねえ、紳士の孫の名前は何？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 5742,
            "text_len": 26,
            "text_offset": 5758,
            "warnings": 0
        },
        "speaker": "ジェントルマン",
        "speaker_translation": "",
        "text": "孫の名前？　ただしじゃ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 5808,
            "text_len": 36,
            "text_offset": 5822,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "speaker_translation": "",
        "text": "ただし？　うちのクラスにいたかな。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 5882,
            "text_len": 46,
            "text_offset": 5898,
            "warnings": 0
        },
        "speaker": "ジェントルマン",
        "speaker_translation": "",
        "text": "たかしは小学生じゃから\u0000\nいないと思うがのぉ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5972,
            "text_len": 38,
            "text_offset": 5984,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "speaker_translation": "",
        "text": "たかし！？　増えた！　更に孫がいた！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 6056,
            "text_len": 34,
            "text_offset": 6072,
            "warnings": 0
        },
        "speaker": "ジェントルマン",
        "speaker_translation": "",
        "text": "いやいや、孫はさとし一人じゃぞ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 6134,
            "text_len": 52,
            "text_offset": 6148,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "speaker_translation": "",
        "text": "また増えた―！\u0000\nしかも一人しか孫と認めてな―い！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 6234,
            "text_len": 58,
            "text_offset": 6246,
            "warnings": 1
        },
        "speaker": "ウェボシ―",
        "speaker_translation": "",
        "text": "し、紳士って実は見えないところで\u0000\n色大変なんだね……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 6334,
            "text_len": 52,
            "text_offset": 6350,
            "warnings": 0
        },
        "speaker": "ジェントルマン",
        "speaker_translation": "",
        "text": "ふむ……？\u0000\nそういえばつよしは今何をしとるのかの？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 24,
            "speaker_offset": 6436,
            "text_len": 22,
            "text_offset": 6462,
            "warnings": 0
        },
        "speaker": "ウェボシ―＆フェっちゃん",
        "speaker_translation": "",
        "text": "また増えた―――！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6514,
            "text_len": 38,
            "text_offset": 6522,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "……えっ？　今、僕のこと呼びました？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 24,
            "speaker_offset": 6594,
            "text_len": 20,
            "text_offset": 6620,
            "warnings": 0
        },
        "speaker": "ウェボシ―＆フェっちゃん",
        "speaker_translation": "",
        "text": "居た―――――！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 24,
            "speaker_offset": 6696,
            "text_len": 34,
            "text_offset": 6722,
            "warnings": 0
        },
        "speaker": "ウェボシ―＆フェっちゃん",
        "speaker_translation": "",
        "text": "（ってお前か―――――っ！！？）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 6800,
            "text_len": 14,
            "text_offset": 6816,
            "warnings": 0
        },
        "speaker": "ジェントルマン",
        "speaker_translation": "",
        "text": "……誰かの？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 24,
            "speaker_offset": 6864,
            "text_len": 26,
            "text_offset": 6890,
            "warnings": 0
        },
        "speaker": "ウェボシ―＆フェっちゃん",
        "speaker_translation": "",
        "text": "認められてな――――い！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6958,
            "text_len": 16,
            "text_offset": 6966,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "……誰ですか？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 24,
            "speaker_offset": 7016,
            "text_len": 28,
            "text_offset": 7042,
            "warnings": 0
        },
        "speaker": "ウェボシ―＆フェっちゃん",
        "speaker_translation": "",
        "text": "こっちも認めてな――い！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 7112,
            "text_len": 68,
            "text_offset": 7128,
            "warnings": 0
        },
        "speaker": "ジェントルマン",
        "speaker_translation": "",
        "text": "すまんがこの学校の外に\u0000\n連れて行ってくれんかの？　迷ってしまって。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7218,
            "text_len": 38,
            "text_offset": 7226,
            "warnings": 0
        },
        "speaker": "中之条",
        "speaker_translation": "",
        "text": "あ、はい。\u0000\n良いですよ、おじいさん。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 24,
            "speaker_offset": 7298,
            "text_len": 22,
            "text_offset": 7324,
            "warnings": 0
        },
        "speaker": "ウェボシ―＆フェっちゃん",
        "speaker_translation": "",
        "text": "他人行儀だ―――！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 24,
            "speaker_offset": 7440,
            "text_len": 20,
            "text_offset": 7466,
            "warnings": 0
        },
        "speaker": "ウェボシ―＆フェっちゃん",
        "speaker_translation": "",
        "text": "……………………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 7602,
            "text_len": 34,
            "text_offset": 7614,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "speaker_translation": "",
        "text": "……あの二人、赤の他人じゃない？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 7664,
            "text_len": 10,
            "text_offset": 7678,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "speaker_translation": "",
        "text": "だよね！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 7738,
            "text_len": 74,
            "text_offset": 7754,
            "warnings": 0
        },
        "speaker": "ジェントルマン",
        "speaker_translation": "",
        "text": "いや、実は迷ってしまいましてな。\u0000\n気付いたらこんなところにいたんじゃよ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 7852,
            "text_len": 60,
            "text_offset": 7864,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "speaker_translation": "",
        "text": "え―？　紳士、迷ってたの？\u0000\n紳士でも迷うことがあるんだ―。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 7948,
            "text_len": 50,
            "text_offset": 7962,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "speaker_translation": "",
        "text": "じゃあ本当はどこに行きたかったの？\u0000\n職員室かな？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 8036,
            "text_len": 50,
            "text_offset": 8052,
            "warnings": 0
        },
        "speaker": "ジェントルマン",
        "speaker_translation": "",
        "text": "いや……\u0000\nわしはバディを捜していただけなんじゃ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 8126,
            "text_len": 10,
            "text_offset": 8138,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "speaker_translation": "",
        "text": "バディ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 8178,
            "text_len": 58,
            "text_offset": 8192,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "speaker_translation": "",
        "text": "あ、知ってる、確かバディって\u0000\n友達とか仲間って意味だよ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 8278,
            "text_len": 64,
            "text_offset": 8290,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "speaker_translation": "",
        "text": "おお！　紳士、仲間探ししてんの！？\u0000\nこれから旅にでも出るの！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 8388,
            "text_len": 74,
            "text_offset": 8404,
            "warnings": 0
        },
        "speaker": "ジェントルマン",
        "speaker_translation": "",
        "text": "？？　旅に出るわけじゃありませんぞ。\u0000\nただ、バディを捜しているだけです。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 8500,
            "text_len": 56,
            "text_offset": 8512,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "speaker_translation": "",
        "text": "く〜〜〜！　ただ仲間を求めるなんて、\u0000\n紳士カッコイイ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 8602,
            "text_len": 66,
            "text_offset": 8616,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "speaker_translation": "",
        "text": "じゃあさ、じゃあさ、\u0000\nどんなバディを捜してるの？　希望の職業は？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 8706,
            "text_len": 72,
            "text_offset": 8718,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "speaker_translation": "",
        "text": "おっ、重要だよ、そこ。条件が合えば\u0000\nあたしも仲間にしてもらおっかな―。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 8814,
            "text_len": 68,
            "text_offset": 8830,
            "warnings": 0
        },
        "speaker": "ジェントルマン",
        "speaker_translation": "",
        "text": "条件？　職業？　う―む……\u0000\nわしの探しているバディは犬なんじゃが。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 24,
            "speaker_offset": 8942,
            "text_len": 6,
            "text_offset": 8968,
            "warnings": 0
        },
        "speaker": "ウェボシ―＆フェっちゃん",
        "speaker_translation": "",
        "text": "！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 24,
            "speaker_offset": 9034,
            "text_len": 16,
            "text_offset": 9060,
            "warnings": 0
        },
        "speaker": "ウェボシ―＆フェっちゃん",
        "speaker_translation": "",
        "text": "（……犬！？）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 9124,
            "text_len": 38,
            "text_offset": 9138,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "speaker_translation": "",
        "text": "（もしかして……バディって飼い犬？）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 9222,
            "text_len": 86,
            "text_offset": 9234,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "speaker_translation": "",
        "text": "（犬扱い……そこまで自分を捨てさせないと\u0000\n　仲間にしないとは……ヘヴィな紳士め……）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 9438,
            "text_len": 40,
            "text_offset": 9454,
            "warnings": 0
        },
        "speaker": "ジェントルマン",
        "speaker_translation": "",
        "text": "（バディは一体どこにいるのかのう……）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 9548,
            "text_len": 72,
            "text_offset": 9562,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "speaker_translation": "",
        "text": "子供って……紳士の年齢を考えたら\u0000\n私たちと同じくらいなわけないじゃん。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 9658,
            "text_len": 54,
            "text_offset": 9670,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "speaker_translation": "",
        "text": "いや、生徒じゃなくて\u0000\n先生って可能性もアリでしょ！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 9758,
            "text_len": 28,
            "text_offset": 9772,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "speaker_translation": "",
        "text": "あっ！　そっちがあった！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 9830,
            "text_len": 32,
            "text_offset": 9846,
            "warnings": 0
        },
        "speaker": "ジェントルマン",
        "speaker_translation": "",
        "text": "いや……ただ、迷っただけで弌弌\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 9924,
            "text_len": 50,
            "text_offset": 9936,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "speaker_translation": "",
        "text": "誰かな誰かな？\u0000\n高崎先生？　それとも桜井先生！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 85,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 10008,
            "text_len": 62,
            "text_offset": 10022,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "speaker_translation": "",
        "text": "高崎先生は無いよね。この紳士っぽさを\u0000\n全く受け継いでないし。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 86,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 10118,
            "text_len": 20,
            "text_offset": 10134,
            "warnings": 0
        },
        "speaker": "ジェントルマン",
        "speaker_translation": "",
        "text": "……………………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 87,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 10186,
            "text_len": 48,
            "text_offset": 10198,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "speaker_translation": "",
        "text": "じゃあ桜井先生か、\u0000\nもしかしたら中村先生とか？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 88,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 10268,
            "text_len": 34,
            "text_offset": 10282,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "speaker_translation": "",
        "text": "あっ、そっか、中村先生がいたね！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 89,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 10344,
            "text_len": 26,
            "text_offset": 10360,
            "warnings": 0
        },
        "speaker": "ジェントルマン",
        "speaker_translation": "",
        "text": "（中村……誰じゃろう？）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 90,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 10406,
            "text_len": 38,
            "text_offset": 10422,
            "warnings": 0
        },
        "speaker": "ジェントルマン",
        "speaker_translation": "",
        "text": "（というか……ここはどこじゃろう？）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 91,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 10568,
            "text_len": 20,
            "text_offset": 10584,
            "warnings": 0
        },
        "speaker": "ジェントルマン",
        "speaker_translation": "",
        "text": "……………………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 92,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 10620,
            "text_len": 22,
            "text_offset": 10636,
            "warnings": 0
        },
        "speaker": "ジェントルマン",
        "speaker_translation": "",
        "text": "……良い天気じゃの。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    }
]