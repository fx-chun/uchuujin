[
    {
        "id": 0,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2090,
            "text_len": 18,
            "text_offset": 2098,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "ただいま〜……。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2138,
            "text_len": 52,
            "text_offset": 2146,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "はぁ……疲れた……\u0000\nバイトってこんなに大変なんだ。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2220,
            "text_len": 62,
            "text_offset": 2228,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "もう動けないよ……\u0000\nおじいちゃんたちを捜しにも行けない……。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 2316,
            "text_len": 18,
            "text_offset": 2332,
            "warnings": 0
        },
        "speaker": "ジェントルマン",
        "text": "おお、おかえり。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2372,
            "text_len": 36,
            "text_offset": 2380,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "うん、おじいちゃん。\u0000\nただいま―。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2442,
            "text_len": 12,
            "text_offset": 2450,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "…………。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2488,
            "text_len": 28,
            "text_offset": 2496,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "あれ！？　おじいちゃん！？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2554,
            "text_len": 12,
            "text_offset": 2562,
            "warnings": 0
        },
        "speaker": "バディ",
        "text": "…………。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2600,
            "text_len": 12,
            "text_offset": 2610,
            "warnings": 0
        },
        "speaker": "コバディ",
        "text": "…………。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2642,
            "text_len": 54,
            "text_offset": 2650,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "バディ！　コバディ！　お前たちも！\u0000\n帰ってたのか！？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 2730,
            "text_len": 26,
            "text_offset": 2746,
            "warnings": 0
        },
        "speaker": "ジェントルマン",
        "text": "夕方には帰っておったよ。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2794,
            "text_len": 14,
            "text_offset": 2802,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "そんな……。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2838,
            "text_len": 70,
            "text_offset": 2846,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "僕、おじいちゃんたちを捜しに行って、\u0000\nこんなに疲れるまで働いて……。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 2938,
            "text_len": 12,
            "text_offset": 2954,
            "warnings": 0
        },
        "speaker": "ジェントルマン",
        "text": "…………？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2992,
            "text_len": 50,
            "text_offset": 3000,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "（あっ、バイトのことを言ったら\u0000\n　怒られるかも）\u0000\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3072,
            "text_len": 60,
            "text_offset": 3080,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "えっと、なんでもないよ。\u0000\nそれよりみんなが無事で良かった。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3166,
            "text_len": 80,
            "text_offset": 3174,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "（いっぱい心配して、街を走り回って、\u0000\n　バイトでくたくたになるまで働いて……）\u0000\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3280,
            "text_len": 72,
            "text_offset": 3288,
            "warnings": 1
        },
        "speaker": "きよし",
        "text": "（色あったけど、でも、みんな無事だから\u0000\n　結果的にこれで良かっ弌弌）\u0000\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3386,
            "text_len": 12,
            "text_offset": 3394,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "…………。\u0000\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3432,
            "text_len": 40,
            "text_offset": 3440,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "（良かったんだ。良かったんだけど……）\u0000\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3506,
            "text_len": 86,
            "text_offset": 3514,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "（バディたちの心配をして、おじいちゃんの\u0000\n　心配もして、バイトすることになって……）\u0000\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3626,
            "text_len": 60,
            "text_offset": 3634,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "（何だか、今日一番損しているのは\u0000\n　僕だった気がする……）\u0000\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3720,
            "text_len": 74,
            "text_offset": 3728,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "（バディたちはおじいちゃんに任せて\u0000\n　遊びに行けばよかったんじゃ……！）\u0000\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 3906,
            "text_len": 32,
            "text_offset": 3922,
            "warnings": 0
        },
        "speaker": "ジェントルマン",
        "text": "……どうしたんじゃ？　かずし？\u0000\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3974,
            "text_len": 16,
            "text_offset": 3982,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "きよしだよ！！\u0000\n",
        "type": "dialog"
    }
]