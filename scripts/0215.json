[
    {
        "id": 0,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 2090,
            "text_len": 20,
            "text_offset": 2104,
            "warnings": 0
        },
        "speaker": "なの＆はかせ",
        "speaker_translation": "",
        "text": "ご馳走様でした―。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2236,
            "text_len": 50,
            "text_offset": 2242,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "お洗濯はもう終わってるから……\u0000\n洗い物しよっと。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2318,
            "text_len": 24,
            "text_offset": 2326,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "はかせは研究しよっと。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2372,
            "text_len": 62,
            "text_offset": 2378,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "待ってください。\u0000\n後ろに隠したお菓子を置いていってください。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2466,
            "text_len": 24,
            "text_offset": 2474,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "そ、そんなのないもん。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2526,
            "text_len": 24,
            "text_offset": 2532,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "あります。\u0000\nさ、早く。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2578,
            "text_len": 30,
            "text_offset": 2586,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "うう……なのの意地悪―……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2652,
            "text_len": 62,
            "text_offset": 2662,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "朝からあいつらは何やってるんだか。\u0000\n……ま、いつもと同じか。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2860,
            "text_len": 32,
            "text_offset": 2870,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "ふあぁ―。\u0000\nそろそろお昼か弌弌\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2934,
            "text_len": 18,
            "text_offset": 2944,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "弌弌ん、なんだ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3066,
            "text_len": 12,
            "text_offset": 3076,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "雨か……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3202,
            "text_len": 56,
            "text_offset": 3212,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "……って、おい、娘！\u0000\n洗濯物を干したんじゃないのか！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3296,
            "text_len": 16,
            "text_offset": 3306,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "………………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3346,
            "text_len": 26,
            "text_offset": 3356,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "お―い、娘！　どこだ―！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3398,
            "text_len": 16,
            "text_offset": 3404,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "台所ですよ―。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3446,
            "text_len": 44,
            "text_offset": 3456,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "雨が降ってるぞ―！\u0000\n洗濯物、濡れちまうぞ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3516,
            "text_len": 60,
            "text_offset": 3522,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "でも、今は手が泡だらけで……\u0000\nはかせ―！　お願いしま―す！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3606,
            "text_len": 58,
            "text_offset": 3614,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "だめ―！　今、研究中―！　手が離せない！\u0000\n阪本やって―！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3700,
            "text_len": 16,
            "text_offset": 3710,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "猫にできるか！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3830,
            "text_len": 54,
            "text_offset": 3840,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "どっちでもいい！\u0000\n早くしないと洗濯物が濡れちまうぞ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3910,
            "text_len": 12,
            "text_offset": 3916,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "はかせ―！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3944,
            "text_len": 10,
            "text_offset": 3952,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "阪本―！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3982,
            "text_len": 16,
            "text_offset": 3992,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "できるか―！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 18,
            "speaker_offset": 4124,
            "text_len": 18,
            "text_offset": 4144,
            "warnings": 0
        },
        "speaker": "ナレ―タ―お姉さん",
        "speaker_translation": "",
        "text": "そして結局……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4298,
            "text_len": 20,
            "text_offset": 4308,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "通り雨だったが弌弌\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4348,
            "text_len": 68,
            "text_offset": 4354,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "……洗濯物……全滅してました……\u0000\n一緒に干しておいたお布団も……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4438,
            "text_len": 30,
            "text_offset": 4448,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "弌弌結構な降りだったからな。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4594,
            "text_len": 32,
            "text_offset": 4602,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "うそ！？　どうやって寝るの！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4658,
            "text_len": 40,
            "text_offset": 4664,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "毛布に包まって寝るしかないですね……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4730,
            "text_len": 32,
            "text_offset": 4738,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "なんで洗濯物入れなかったの！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4796,
            "text_len": 60,
            "text_offset": 4802,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "だって洗い物してたんですよ！？\u0000\n手が泡だらけだったんです！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4888,
            "text_len": 50,
            "text_offset": 4896,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "はかせだってそうだもん！\u0000\n研究中だったんだもん！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4968,
            "text_len": 28,
            "text_offset": 4974,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "……ポテチの研究、ですか？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5028,
            "text_len": 8,
            "text_offset": 5036,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "ドキ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5066,
            "text_len": 68,
            "text_offset": 5072,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "口の周りにポテチのカスがついてますよ？\u0000\n本当に研究してたんですか？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5166,
            "text_len": 48,
            "text_offset": 5174,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "きゅ、休憩中だったんだもん！\u0000\n仕方ないんだよ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5248,
            "text_len": 52,
            "text_offset": 5254,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "仕方なくないです！\u0000\n休憩中だったら入れてください！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5332,
            "text_len": 38,
            "text_offset": 5340,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "休憩中はお菓子食べるのが仕事だもの！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5408,
            "text_len": 18,
            "text_offset": 5418,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "二人共落ち着け。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5468,
            "text_len": 22,
            "text_offset": 5474,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "でも、はかせが……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5520,
            "text_len": 20,
            "text_offset": 5528,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "でも、なのが……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5578,
            "text_len": 66,
            "text_offset": 5588,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "ケンカして洗濯物が乾くのか？\u0000\n違うだろ？　それに責任転嫁するな。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 5688,
            "text_len": 14,
            "text_offset": 5702,
            "warnings": 0
        },
        "speaker": "なの＆はかせ",
        "speaker_translation": "",
        "text": "は―い……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5750,
            "text_len": 46,
            "text_offset": 5760,
            "warnings": 2
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "返事ははいだろ！\u0000\n気抜けた声を出すな！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 5848,
            "text_len": 10,
            "text_offset": 5862,
            "warnings": 0
        },
        "speaker": "なの＆はかせ",
        "speaker_translation": "",
        "text": "はいっ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5902,
            "text_len": 54,
            "text_offset": 5912,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "それでよし。\u0000\nやればできるのに、なんでやんないんだ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5988,
            "text_len": 32,
            "text_offset": 5998,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "さっきだってそうだ。\u0000\n弌弌娘。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6062,
            "text_len": 12,
            "text_offset": 6068,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "は、はい！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6102,
            "text_len": 68,
            "text_offset": 6112,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "手が泡だらけでも、洗えばいいはずだ。\u0000\n……１０秒あればできるだろ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6202,
            "text_len": 24,
            "text_offset": 6208,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "はい……そうです……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6262,
            "text_len": 12,
            "text_offset": 6272,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "次、ガキ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6316,
            "text_len": 12,
            "text_offset": 6324,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "は、はい！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6358,
            "text_len": 42,
            "text_offset": 6368,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "他人に甘えんな。\u0000\nおまえの悪いくせだぞ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6432,
            "text_len": 18,
            "text_offset": 6440,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "は、は―い……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6480,
            "text_len": 70,
            "text_offset": 6490,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "それから、大人になりたいなら、\u0000\n自分勝手な解釈はやめたほうがいいぞ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6582,
            "text_len": 20,
            "text_offset": 6590,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "……ごめんなさい。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6632,
            "text_len": 72,
            "text_offset": 6642,
            "warnings": 1
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "あと、この際だから言わせてもらうが、\u0000\nおまえには色と悪いところが弌弌\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6740,
            "text_len": 16,
            "text_offset": 6748,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "う……う……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6796,
            "text_len": 22,
            "text_offset": 6802,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "やめてくださ―い！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6850,
            "text_len": 70,
            "text_offset": 6856,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "はかせは悪いとこばかりじゃないんです。\u0000\n良いとこだってあるんですよ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6942,
            "text_len": 64,
            "text_offset": 6952,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "は？　いや、そんなことはわかってるが……\u0000\nこれは説教だから弌弌\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7038,
            "text_len": 12,
            "text_offset": 7046,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "……なの？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7080,
            "text_len": 84,
            "text_offset": 7086,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "はかせ、今日は私の毛布も使ってください。\u0000\n敷き布団みたいにすれば十分温かいですよ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7192,
            "text_len": 20,
            "text_offset": 7200,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "でも、なのが……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7242,
            "text_len": 62,
            "text_offset": 7248,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "私は大丈夫です。\u0000\nロボットだから、寒さなんてへっちゃらです！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7332,
            "text_len": 12,
            "text_offset": 7340,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "なの……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7374,
            "text_len": 56,
            "text_offset": 7382,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "ごめんなさい……\u0000\nはかせが悪かったの……ごめんなさい！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7460,
            "text_len": 54,
            "text_offset": 7466,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "はかせだけじゃありません！\u0000\n私だって悪かったんです！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7542,
            "text_len": 12,
            "text_offset": 7550,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "な―の―！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7584,
            "text_len": 12,
            "text_offset": 7590,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "はかせ―！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 7632,
            "text_len": 12,
            "text_offset": 7646,
            "warnings": 0
        },
        "speaker": "なの＆はかせ",
        "speaker_translation": "",
        "text": "うあ―ん！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7688,
            "text_len": 38,
            "text_offset": 7698,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "弌弌ふう。\u0000\nま、わかったならいいか。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7840,
            "text_len": 36,
            "text_offset": 7846,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "あ、はかせ、お昼は何がいいですか？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7898,
            "text_len": 34,
            "text_offset": 7906,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "オムライス―！\u0000\nあとハンバ―グ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7956,
            "text_len": 58,
            "text_offset": 7962,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "それじゃ今日は特別。\u0000\nレストランに行っちゃいましょうか！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8036,
            "text_len": 34,
            "text_offset": 8044,
            "warnings": 1
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "やった―！　だからなの大好き―\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8094,
            "text_len": 34,
            "text_offset": 8100,
            "warnings": 1
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "もう、はかせったら……えへへへ\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8150,
            "text_len": 32,
            "text_offset": 8158,
            "warnings": 0
        },
        "speaker": "はかせ",
        "speaker_translation": "",
        "text": "それじゃ阪本、出かけてくるね！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8206,
            "text_len": 26,
            "text_offset": 8212,
            "warnings": 0
        },
        "speaker": "なの",
        "speaker_translation": "",
        "text": "お留守番、お願いします。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8254,
            "text_len": 30,
            "text_offset": 8264,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "おう！\u0000\n仲良く行ってこいよ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8398,
            "text_len": 18,
            "text_offset": 8408,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "………………ん？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8446,
            "text_len": 28,
            "text_offset": 8456,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "speaker_translation": "",
        "text": "なんで俺がメシ抜きなんだ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    }
]