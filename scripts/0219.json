[
    {
        "id": 0,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2106,
            "text_len": 34,
            "text_offset": 2118,
            "warnings": 0
        },
        "speaker": "スタ―ラ姫",
        "speaker_translation": "",
        "text": "はあ……どいつもこいつも……か。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2174,
            "text_len": 22,
            "text_offset": 2184,
            "warnings": 0
        },
        "speaker": "おつき３",
        "speaker_translation": "",
        "text": "ひ、姫様……結果は？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2232,
            "text_len": 12,
            "text_offset": 2244,
            "warnings": 0
        },
        "speaker": "スタ―ラ姫",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2278,
            "text_len": 10,
            "text_offset": 2290,
            "warnings": 0
        },
        "speaker": "スタ―ラ姫",
        "speaker_translation": "",
        "text": "終わり。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2326,
            "text_len": 42,
            "text_offset": 2336,
            "warnings": 0
        },
        "speaker": "おつき３",
        "speaker_translation": "",
        "text": "お、終わり？　ということは不合格ですか？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2400,
            "text_len": 44,
            "text_offset": 2412,
            "warnings": 0
        },
        "speaker": "スタ―ラ姫",
        "speaker_translation": "",
        "text": "違う。終わりは終わりだ。\u0000\n……部屋に戻る。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 2518,
            "text_len": 40,
            "text_offset": 2522,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "どういうことだ？　本当に終わりなのか？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2586,
            "text_len": 54,
            "text_offset": 2596,
            "warnings": 0
        },
        "speaker": "おつき３",
        "speaker_translation": "",
        "text": "姫がそう言ったんだから……\u0000\n終わりなんだよ、きっと。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 2672,
            "text_len": 56,
            "text_offset": 2676,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "まさかドッキリとか？\u0000\nここでのリアクションで……また？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 2758,
            "text_len": 66,
            "text_offset": 2762,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "マジかよ！？　もう勘弁してくれよ！\u0000\nまだ死にたくないんだよ―っ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2852,
            "text_len": 68,
            "text_offset": 2862,
            "warnings": 0
        },
        "speaker": "おつき３",
        "speaker_translation": "",
        "text": "落ち着けって！　ドッキリじゃない！\u0000\n本当に終わりだ！　……たぶん。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 2954,
            "text_len": 18,
            "text_offset": 2958,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "信じていいのか？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2998,
            "text_len": 54,
            "text_offset": 3008,
            "warnings": 0
        },
        "speaker": "おつき３",
        "speaker_translation": "",
        "text": "ぶっちゃけ、これがドッキリなら\u0000\n俺も知らされてない。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3084,
            "text_len": 30,
            "text_offset": 3088,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "わかった。なら……解散だな。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3146,
            "text_len": 50,
            "text_offset": 3150,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "はああああぁぁぁぁぁぁ……\u0000\nた、助かったぁ……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3224,
            "text_len": 26,
            "text_offset": 3228,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "けど、姫、どうしたんだ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3278,
            "text_len": 50,
            "text_offset": 3282,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "飽きたんだろ？\u0000\n……ったく、もう二度とごめんだ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3356,
            "text_len": 60,
            "text_offset": 3366,
            "warnings": 0
        },
        "speaker": "おつき３",
        "speaker_translation": "",
        "text": "ぶっちゃけ俺もな。いつ落とされるか\u0000\nヒヤヒヤしてたんだぜ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3450,
            "text_len": 34,
            "text_offset": 3462,
            "warnings": 3
        },
        "speaker": "＆＆",
        "speaker_translation": "",
        "text": "……姫、いなくなればいいのにな。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3622,
            "text_len": 32,
            "text_offset": 3632,
            "warnings": 0
        },
        "speaker": "おつき３",
        "speaker_translation": "",
        "text": "おい、大変だ！　姫がいないぞ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3686,
            "text_len": 28,
            "text_offset": 3690,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "は？　トイレじゃないのか？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3748,
            "text_len": 60,
            "text_offset": 3758,
            "warnings": 0
        },
        "speaker": "おつき３",
        "speaker_translation": "",
        "text": "違う！　本当にいないんだ！\u0000\n艦内のどこにもいないんだよ！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3840,
            "text_len": 44,
            "text_offset": 3844,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "そ、そう言われても……\u0000\n母艦はどうなんだ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3910,
            "text_len": 38,
            "text_offset": 3920,
            "warnings": 0
        },
        "speaker": "おつき３",
        "speaker_translation": "",
        "text": "もう連絡した。……戻ってないそうだ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3980,
            "text_len": 26,
            "text_offset": 3984,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "だとしたら……神隠しか？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4034,
            "text_len": 52,
            "text_offset": 4038,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "それとも今度はかくれんぼとか？\u0000\n見つけたら金一封？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4114,
            "text_len": 36,
            "text_offset": 4118,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "見つけられなかったら……また、か？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4184,
            "text_len": 44,
            "text_offset": 4194,
            "warnings": 0
        },
        "speaker": "おつき３",
        "speaker_translation": "",
        "text": "バカなこと言ってる場合か！\u0000\n早く探すんだ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4264,
            "text_len": 42,
            "text_offset": 4268,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "どこを！？　艦内はチェックしたんだろ！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4332,
            "text_len": 32,
            "text_offset": 4342,
            "warnings": 0
        },
        "speaker": "おつき３",
        "speaker_translation": "",
        "text": "だとしたら……あの惑星、とか？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4396,
            "text_len": 26,
            "text_offset": 4400,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "なんで？　辺估な所だぞ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 16,
            "speaker_offset": 4456,
            "text_len": 22,
            "text_offset": 4474,
            "warnings": 0
        },
        "speaker": "？？？（７０番）",
        "speaker_translation": "",
        "text": "……いや、ありえる。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4522,
            "text_len": 36,
            "text_offset": 4532,
            "warnings": 0
        },
        "speaker": "おつき３",
        "speaker_translation": "",
        "text": "おまえは確か……離発着担当の……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4590,
            "text_len": 30,
            "text_offset": 4598,
            "warnings": 0
        },
        "speaker": "７０番",
        "speaker_translation": "",
        "text": "ああ、７０番。またの名を弌弌\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4652,
            "text_len": 40,
            "text_offset": 4662,
            "warnings": 0
        },
        "speaker": "おつき３",
        "speaker_translation": "",
        "text": "今はいい。それより何か知っているのか？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4724,
            "text_len": 86,
            "text_offset": 4732,
            "warnings": 0
        },
        "speaker": "７０番",
        "speaker_translation": "",
        "text": "昨日の深夜、格納庫が開いた形跡があった。\u0000\nそして大気圏突入用の宇宙船が１隻……ない。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4842,
            "text_len": 48,
            "text_offset": 4846,
            "warnings": 1
        },
        "speaker": "",
        "speaker_translation": "",
        "text": "マジかよ！？\u0000\nそれじゃあ……姫はやっぱり……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4922,
            "text_len": 58,
            "text_offset": 4932,
            "warnings": 0
        },
        "speaker": "おつき３",
        "speaker_translation": "",
        "text": "どうするんだ、７０番！！\u0000\nおまえの管理不行き届きだぞ！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5014,
            "text_len": 68,
            "text_offset": 5022,
            "warnings": 0
        },
        "speaker": "７０番",
        "speaker_translation": "",
        "text": "ちょ、ちょっと待てよ！\u0000\n昨日は疲れてたんだ。そ、それにまさか……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5112,
            "text_len": 32,
            "text_offset": 5122,
            "warnings": 0
        },
        "speaker": "おつき３",
        "speaker_translation": "",
        "text": "言い訳はいい！　早く見つけろ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5180,
            "text_len": 28,
            "text_offset": 5188,
            "warnings": 0
        },
        "speaker": "７０番",
        "speaker_translation": "",
        "text": "じゃ、じゃないと俺は……？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5238,
            "text_len": 54,
            "text_offset": 5248,
            "warnings": 0
        },
        "speaker": "おつき３",
        "speaker_translation": "",
        "text": "処罰されるだろうな。\u0000\n良くて停職、悪くて……ま、な。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5328,
            "text_len": 56,
            "text_offset": 5336,
            "warnings": 4
        },
        "speaker": "７０番",
        "speaker_translation": "",
        "text": "ま、なって何がま、なだよ！？\u0000\n教えてくれよ！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5414,
            "text_len": 54,
            "text_offset": 5424,
            "warnings": 0
        },
        "speaker": "おつき３",
        "speaker_translation": "",
        "text": "安心しろ。\u0000\n……落とし穴だけは免れるようにしてやる。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5504,
            "text_len": 66,
            "text_offset": 5512,
            "warnings": 0
        },
        "speaker": "７０番",
        "speaker_translation": "",
        "text": "うそおおおおおおおおおっ！？\u0000\n俺死刑！？　俺マジ死刑なんのっ！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5604,
            "text_len": 38,
            "text_offset": 5612,
            "warnings": 0
        },
        "speaker": "７０番",
        "speaker_translation": "",
        "text": "探さなきゃ！　早くあの星行かなきゃ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5676,
            "text_len": 36,
            "text_offset": 5684,
            "warnings": 0
        },
        "speaker": "７０番",
        "speaker_translation": "",
        "text": "うおおおおおおおおおおおおっ！！！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5766,
            "text_len": 68,
            "text_offset": 5776,
            "warnings": 0
        },
        "speaker": "おつき３",
        "speaker_translation": "",
        "text": "ちょっ！？　おまえ装備無しで何するんだ！？\u0000\nあわてんぼうすぎるぞ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5964,
            "text_len": 52,
            "text_offset": 5972,
            "warnings": 0
        },
        "speaker": "７０番",
        "speaker_translation": "",
        "text": "そう！　俺のあだ名は！！\u0000\nあわてんぼうの７０番だ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6040,
            "text_len": 46,
            "text_offset": 6048,
            "warnings": 0
        },
        "speaker": "７０番",
        "speaker_translation": "",
        "text": "我ながらあわてんぼうにも程があったぜえええ！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6328,
            "text_len": 12,
            "text_offset": 6336,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "のわっ！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6378,
            "text_len": 12,
            "text_offset": 6388,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6434,
            "text_len": 12,
            "text_offset": 6442,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "…………。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6558,
            "text_len": 20,
            "text_offset": 6566,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "廊下に立ってます。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6682,
            "text_len": 38,
            "text_offset": 6692,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "弌弌あ。相生さん、どうしたんですか？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6756,
            "text_len": 46,
            "text_offset": 6764,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "……え〜とぉ……廊下に立つことになりまして。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6832,
            "text_len": 28,
            "text_offset": 6842,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "また宿題を忘れたんですか？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6894,
            "text_len": 48,
            "text_offset": 6904,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "speaker_translation": "",
        "text": "居眠りですよ。\u0000\nあと、夢見が悪かったんだよな？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6976,
            "text_len": 42,
            "text_offset": 6984,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "ま、まあ……アンテナがこう、ブス―ッと。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7048,
            "text_len": 58,
            "text_offset": 7058,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "相生さん、悩みでもありますか？\u0000\n先生、相談にのりますよ？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7140,
            "text_len": 40,
            "text_offset": 7150,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "speaker_translation": "",
        "text": "それよりどうしました？　何か用事でも？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7212,
            "text_len": 50,
            "text_offset": 7222,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "あ……ご、ごめんなさい！\u0000\nあの……転校生、です。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7294,
            "text_len": 18,
            "text_offset": 7304,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "speaker_translation": "",
        "text": "授業中にですか？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7344,
            "text_len": 58,
            "text_offset": 7354,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "はい、都合で遅れたそうで……\u0000\nフェイさん、お入りなさい。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 20,
            "speaker_offset": 7440,
            "text_len": 16,
            "text_offset": 7462,
            "warnings": 0
        },
        "speaker": "？？？（スタ―ラ姫）",
        "speaker_translation": "",
        "text": "……失礼する。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7586,
            "text_len": 12,
            "text_offset": 7594,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "あ―っ！？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7622,
            "text_len": 38,
            "text_offset": 7632,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "どうしました？　フェイさんとお友達？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7686,
            "text_len": 74,
            "text_offset": 7694,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "じゃなくて……何て説明したらいいか……\u0000\nけど……あれ、夢のはずだし……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7784,
            "text_len": 50,
            "text_offset": 7794,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "speaker_translation": "",
        "text": "相生、本当にどうしたんだ？\u0000\nまだ寝惚けてるのか？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7860,
            "text_len": 40,
            "text_offset": 7870,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "あのぉ……フェイさんの自己紹介を……。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7930,
            "text_len": 38,
            "text_offset": 7940,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "speaker_translation": "",
        "text": "あああああああ！？　す、すみません！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7994,
            "text_len": 24,
            "text_offset": 8004,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "それじゃ、フェイさん。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 22,
            "speaker_offset": 8044,
            "text_len": 42,
            "text_offset": 8068,
            "warnings": 0
        },
        "speaker": "スタ―ラ（スタ―ラ姫）",
        "speaker_translation": "",
        "text": "フェイ・スタ―ラだ。\u0000\n以後よろしく頼む。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8126,
            "text_len": 64,
            "text_offset": 8136,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "speaker_translation": "",
        "text": "席は……あ、相生さんの隣が空いてるわね。\u0000\nそこにお願いします。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 22,
            "speaker_offset": 8216,
            "text_len": 38,
            "text_offset": 8240,
            "warnings": 0
        },
        "speaker": "スタ―ラ（スタ―ラ姫）",
        "speaker_translation": "",
        "text": "わかった。相生とやら、世話になるぞ。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8386,
            "text_len": 30,
            "text_offset": 8394,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "う、うん……あの……お姫様？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 22,
            "speaker_offset": 8440,
            "text_len": 10,
            "text_offset": 8464,
            "warnings": 0
        },
        "speaker": "スタ―ラ（スタ―ラ姫）",
        "speaker_translation": "",
        "text": "お姫様？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8490,
            "text_len": 38,
            "text_offset": 8498,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "ううん、何でもない！　気にしないで！\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8654,
            "text_len": 64,
            "text_offset": 8664,
            "warnings": 0
        },
        "speaker": "スタ―ラ",
        "speaker_translation": "",
        "text": "（ふう……何とか潜り込めたな。\u0000\n　しかし……やはり辺估な所だ）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8748,
            "text_len": 70,
            "text_offset": 8758,
            "warnings": 0
        },
        "speaker": "スタ―ラ",
        "speaker_translation": "",
        "text": "（ここにどんな面白いことがあるのだろう？\u0000\n　……ワクワクしてきたぞ）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8848,
            "text_len": 66,
            "text_offset": 8858,
            "warnings": 0
        },
        "speaker": "スタ―ラ",
        "speaker_translation": "",
        "text": "（兵隊たちの番組だけではつまらん。\u0000\n　やはり自分で体験せんとな）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8944,
            "text_len": 74,
            "text_offset": 8954,
            "warnings": 0
        },
        "speaker": "スタ―ラ",
        "speaker_translation": "",
        "text": "（だが、私一人で全ての楽しみを\u0000\n　見つけるのは不可能だ……手駒がいるな）\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9044,
            "text_len": 34,
            "text_offset": 9054,
            "warnings": 0
        },
        "speaker": "スタ―ラ",
        "speaker_translation": "",
        "text": "ふむ……だとしたら……アレ、か。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9198,
            "text_len": 28,
            "text_offset": 9206,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "どうしたの？　何か言った？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9250,
            "text_len": 14,
            "text_offset": 9260,
            "warnings": 0
        },
        "speaker": "スタ―ラ",
        "speaker_translation": "",
        "text": "いや、何も。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 85,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9290,
            "text_len": 50,
            "text_offset": 9300,
            "warnings": 0
        },
        "speaker": "スタ―ラ",
        "speaker_translation": "",
        "text": "それより相生と言ったか。\u0000\n１つ頼みがあるのだが。\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 86,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9372,
            "text_len": 10,
            "text_offset": 9380,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "speaker_translation": "",
        "text": "なぁに？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    },
    {
        "id": 87,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9494,
            "text_len": 30,
            "text_offset": 9504,
            "warnings": 0
        },
        "speaker": "スタ―ラ",
        "speaker_translation": "",
        "text": "これを頭に付けてもらえんか？\u0000\n",
        "text_translation": "",
        "type": "dialog"
    }
]