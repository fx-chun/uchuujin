#  speaker 
#  warnings: 0 
#: sc/0003:0 
msgid "なの"
msgstr "Nano"

#  text 
#  warnings: 0 
#: sc/0003:0 
msgid "ふぅ、ここまで来たら安心かな……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:1 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:1 
msgid "まったく。はかせのせいで\n今日はひどい１日になっちゃった。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:2 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:2 
msgid "上手く誤魔化せたから私がロボットだと\n気付かれていないけど……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:3 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:3 
msgid "でも、こんなネジは\n早く取ってもらわないと私の心臓に悪いよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:4 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:4 
msgid "いつネジが回り始めるかわからないし、\n回り始めたら何が起こるかもわからない……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:5 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:5 
msgid "そんな生活、考えただけで……うぅ、\nお腹痛くなってきた。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:6 
msgid "笹原"
msgstr "Sasahara"

#  text 
#  warnings: 0 
#: sc/0003:6 
msgid "そこの娘。落ち込んでいるようだが、\n何かあったのか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:7 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:7 
msgid "えっ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:8 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:8 
msgid "気分が悪いのであれば良い病院を\n紹介してやろう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:9 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:9 
msgid "あっ、ありがとうございます。\nでも大丈夫です。家はすぐそこですから。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:10 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:10 
msgid "そうか。ならば良いのだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:11 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0003:11 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0003:12 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:12 
msgid "む？　校長？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:13 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:13 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:14 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:14 
msgid "えっ、えっ？　なんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:15 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:15 
msgid "……ふむ。そうであったか。\nお主の顔が暗いのは校長が原因だな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:16 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:16 
msgid "えっ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:17 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:17 
msgid "みなまで言わずとも良い。\n笹原家長男として、校長を退治してくれよう！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:18 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:18 
msgid "！？\n"
msgstr "!?\n"

#  text 
#  warnings: 1 
#: sc/0003:19 
msgid "アンテナ折られました！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0003:20 
msgid "おい、ちょっと待て！　もしかしてこの男、\n俺たちの存在に気付いている！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0003:21 
msgid "それに今の言葉、タ―ゲットも俺たちに\n気付いているってことじゃないのか！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0003:22 
msgid "なんてこった！　俺たちは\n地球人たちに踊らされていたのか！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0003:23 
msgid "……いや、ただの偶然が重なっただけだろ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:24 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:24 
msgid "あっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:25 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:25 
msgid "むっ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:26 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:26 
msgid "（またネジが回り始めた！？\n　今度は一体何が弌弌）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:27 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:27 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:28 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:28 
msgid "（腕が飛んでいった！？\n　意外と普通だ！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:29 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:29 
msgid "（あっ、違う！\n　今のはコ―クスクリュ―仕様だ！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:30 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:30 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:31 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:31 
msgid "あっ、しまった……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:32 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:32 
msgid "（家まであと少しなのに\n　最後の最後でまた見られちゃった……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:33 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:33 
msgid "い、今のは……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:34 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:34 
msgid "い、今のは、マジックです！\n腕を飛ばすマジック！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:35 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:35 
msgid "なんと、娘。お主は奇術師であったか！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:36 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:36 
msgid "はい！　今のは１日に２回しか使えない\n手首飛ばしのマジックです！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:37 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:37 
msgid "素晴らしい出来だ。まるで本当に\nその腕を飛ばしたかのように錯覚したぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:38 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:38 
msgid "ありがとうございます！\nではサ―ビスでもう一回行きます！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:39 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:39 
msgid "うむ！　見事だ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:40 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:40 
msgid "この笹原を个らせるとは恐れ入った。\n将来は良い奇術師になれるであろう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:41 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:41 
msgid "ありがとうございます！\nそれでは私はこれで！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:42 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:42 
msgid "よし！　何とか誤魔化せた！\nこのまま早く家に弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:43 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:43 
msgid "ああああ！？　両手が無い！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:44 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:44 
msgid "え、えっと……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:45 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:45 
msgid "何と言う素晴らしい回転だ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:46 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:46 
msgid "……はい？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:47 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:47 
msgid "稀に見る美しいコ―クスクリュ―だ。\nなかなかやりおるな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:48 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:48 
msgid "は、はあ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:49 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:49 
msgid "（何だかよくわからないけど\n　手が飛んだことを変に思ってない？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:50 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:50 
msgid "天晴れ天晴れ。娘、良い物を見せてくれた、\n礼を言おう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:51 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:51 
msgid "あ、いえ。\nそれじゃ、私はこれで……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:52 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:52 
msgid "（飛んでいった手を回収しないと）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:53 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:53 
msgid "……しかし、手が飛ぶとは、\n面妖な娘であった。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:54 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:54 
msgid "えっ？　今、何かありました？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:55 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:55 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:56 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:56 
msgid "そういえば何か音がした気がしますけど。\n何かあったんでしょうか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:57 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:57 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:58 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:58 
msgid "プッ。ワハハハハハハ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:59 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:59 
msgid "まあ良い、詮索はせずにおこう。それより娘。\nその手のままでは、不便ではないか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:60 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:60 
msgid "えっ？　何ですか？\n私はちゃんと手がありますよ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:61 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:61 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:62 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:62 
msgid "あっ、もしかしてこっちの手ですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:63 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:63 
msgid "これは男の人には見えないように\nなってるんですよ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:64 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:64 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:65 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:65 
msgid "ワハハハハハ！\nそうか、そうであったか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:66 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:66 
msgid "いらぬ心配をしてしまったようだ。\nそれでは、私は失礼しよう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:67 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:67 
msgid "はい。さようなら。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0003:68 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0003:68 
msgid "フフッ、ぬかしおるよ、あの娘。\n"
msgstr ""
