#  speaker 
#  warnings: 0 
#: sc/0007:0 
msgid "ゆっこ"
msgstr "Yuuko"

#  text 
#  warnings: 0 
#: sc/0007:0 
msgid "（はぁ……朝からブル―だ……）\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0007:1 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0007:1 
msgid "（あんなに清しい気持ちだったのが\n　今ではこの変わり様……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:2 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:2 
msgid "（今日は良い日になる気がしたんだけど、\n　あの犬のせいで悪い予感しかしない）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:3 
msgid "高崎先生"
msgstr "Takasaki-sensei"

#  text 
#  warnings: 0 
#: sc/0007:3 
msgid "……であるからして、\nこの漢文はこのように訳せるわけだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:4 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:4 
msgid "（気分が乗らないと授業の話も\n　右から左に抜けるだけだ……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:5 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:5 
msgid "（……ま、普段から\n　右から左に抜けてるけどね。ふふっ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:6 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:6 
msgid "（……おっ？　今ちょっと面白かった？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:7 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:7 
msgid "（……そうか。私はあの犬に怯えて\n　いつもの力が出せなかったんだ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:8 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:8 
msgid "（ということは、\n　今ボケたら笑いが取れたりして？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:9 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:9 
msgid "よしっ。\nねえ、みおちゃん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:10 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:10 
msgid "ん？\n"
msgstr "Hmm?\n"

#  speaker 
#  warnings: 0 
#: sc/0007:11 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0007:11 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0007:12 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:12 
msgid "あっ、校長！\nどうされました？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:13 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:13 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:14 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:14 
msgid "校長？　……あ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:15 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:15 
msgid "校長、頭に虫が。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:16 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:16 
msgid "！？\n"
msgstr "!?\n"

#  text 
#  warnings: 1 
#: sc/0007:17 
msgid "な、なにぃ！？\n校長の頭を叩いただと！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0007:18 
msgid "どういうことだ？\n校長先生って一番偉いんじゃないのか？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0007:19 
msgid "わからん。\nただ１つだけわかることは……。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0007:20 
msgid "今のでアンテナが折れたってことだな！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0007:21 
msgid "その通り！\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0007:22 
msgid "＆"
msgstr "＆"

#  text 
#  warnings: 2 
#: sc/0007:22 
msgid "うわあああああああ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:23 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:23 
msgid "相生。私語をするとは余裕だな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:24 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:24 
msgid "えっ？　あ、その……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:25 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:25 
msgid "授業の邪魔をするなら廊下に立ってろ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:26 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:26 
msgid "うっ……。\n"
msgstr "Uh\n"

#  speaker 
#  warnings: 0 
#: sc/0007:27 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:27 
msgid "（確かに授業を真面目に受けない私も悪い）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:28 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:28 
msgid "（でも、今はボケたい！\n　ボケて、みおちゃんを笑わせたい！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:29 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:29 
msgid "（……あれ？　でもこれで失敗したら\n　あの犬の予言は本物ということに……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:30 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:30 
msgid "（ええい、恐怖に負けるもんか！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:31 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:31 
msgid "先生！　お題をください！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:32 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:32 
msgid "はっ？　お、お題？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:33 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:33 
msgid "はい！　何でもいいから\n思い浮かんだお題を言ってください！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:34 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:34 
msgid "ぬっ、そ、それじゃあ……キノコ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:35 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:35 
msgid "キノコ！　頂きました！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:36 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:36 
msgid "みんな注目！　行くよ―！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:37 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:37 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:38 
msgid "みお"
msgstr "Mio"

#  text 
#  warnings: 0 
#: sc/0007:38 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:39 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:39 
msgid "はぁぁぁああ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:40 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:40 
msgid "キノコが、生き残った―――！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:41 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:41 
msgid "ゆっこ……！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:42 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:42 
msgid "相生……！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:43 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:43 
msgid "みんな！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:44 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:44 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:45 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:45 
msgid "廊下に立ってろうか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:46 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:46 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:47 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:47 
msgid "（……ダメだ。ここでスベったら\n　私は自信をなくしてしまう……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:48 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:48 
msgid "……廊下に立ってきます。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:49 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:49 
msgid "……どうした相生。元気が無いが、\nもしかして体調が悪いのか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:50 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:50 
msgid "（おっ？　高崎先生が心配するなんて、\n　そんなに元気が無いのかな？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:51 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:51 
msgid "（みんなを心配させてもいけないし、\n　ここはさっきのネタで笑いを狙ってみるか）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:52 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:52 
msgid "すみません。だから今日の授業内容も\n右から左に抜けて行っちゃって。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:53 
msgid "田中"
msgstr "Tanaka"

#  text 
#  warnings: 0 
#: sc/0007:53 
msgid "あははは、相生の場合は\nいつも右から左に抜けてるよな―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:54 
msgid "生徒たち"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:54 
msgid "わははははははは！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:55 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:55 
msgid "田中のヤロ――！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:56 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:56 
msgid "（くっ！　ここは耐えるのよゆっこ！\n　笑いを取りにいくのは諦めるのよ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:57 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:57 
msgid "あ、いや、私語じゃないですよ。\n消しゴム忘れたから借りようとしただけです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:58 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:58 
msgid "ん、そうだったのか。\nすまんな、怒鳴ったりして。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:59 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:59 
msgid "いえいえ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:60 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:60 
msgid "じゃあゆっこ、はい消しゴム。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:61 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:61 
msgid "うん、ありがと―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:62 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:62 
msgid "よし、では授業を続ける。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:63 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:63 
#, fuzzy
msgid "は―い。\n"
msgstr "Alright.\n"

#  speaker 
#  warnings: 0 
#: sc/0007:64 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:64 
msgid "……で、この文の訳し方は\n前回の授業で教えたように……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:65 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:65 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:66 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:66 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:67 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:67 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0007:68 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0007:68 
msgid "（つまんね―……）\n"
msgstr ""
