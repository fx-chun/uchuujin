#  speaker 
#  warnings: 0 
#: sc/0009:0 
msgid "ゆっこ"
msgstr "Yuuko"

#  text 
#  warnings: 0 
#: sc/0009:0 
msgid "（今日は厄日なのかな……\n　何をしても辛い結果にしかならない）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:1 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:1 
msgid "（特に……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:2 
msgid "バディ"
msgstr "Buddy."

#  text 
#  warnings: 0 
#: sc/0009:2 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0009:3 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:3 
msgid "（あの犬がいる時はよく辛い目に遭う。\n　やっぱり元凶はあの犬なんじゃ……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:4 
msgid "イタコ"
msgstr "Itako"

#  text 
#  warnings: 0 
#: sc/0009:4 
msgid "ん？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:5 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:5 
msgid "えっ？\n"
msgstr "Huh?\n"

#  speaker 
#  warnings: 0 
#: sc/0009:6 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:6 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:7 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:7 
msgid "…………？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:8 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:8 
msgid "……何だろう？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:9 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:9 
msgid "あの人、私を見てたけど……何？\nもしかして私じゃなくて、私の後ろ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:10 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0009:10 
msgid "！？\n"
msgstr "!?\n"

#  speaker 
#  warnings: 0 
#: sc/0009:11 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:11 
msgid "……え？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:12 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:12 
msgid "（何で校長がここに？\n　もしかしてずっと私の後ろをついてきた？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:13 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:13 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:14 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:14 
msgid "……うわあああ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:15 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:15 
msgid "！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0009:16 
msgid "ひどい！\nいきなり殴ることないじゃないか！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0009:17 
msgid "いや、それは仕方ないだろ。\n女の子には怖いことだからさ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0009:18 
msgid "だって、こうでもしないと\n放送できないし！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0009:19 
msgid "そうだけど、仕方ないさ。\nこれはこっちが悪かったんだ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0009:20 
msgid "じゃあ、この後のことも\n仕方ないで納得するのか？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0009:21 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0009:22 
msgid "＆"
msgstr "&"

#  text 
#  warnings: 2 
#: sc/0009:22 
msgid "うわああああ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:23 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:23 
msgid "あっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:24 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:24 
msgid "……そ、そうか！　そうだったんだ！\n今、全てが繋がった！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:25 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:25 
msgid "今日の不調は犬のせいだと思ってたけど……\n私に悪霊がついてたんだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:26 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:26 
msgid "さっきの人、霊媒師みたいな感じだったから、\nきっと私についてた悪霊が見えたんだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:27 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:27 
msgid "すみませ―ん！\nちょっと待ってくださ―い！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:28 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:28 
msgid "はっ？　なんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:29 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:29 
msgid "あの、もしかしてですけど、私、\n悪霊に取り亅かれてるんですよね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:30 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:30 
msgid "……はあ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:31 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:31 
msgid "そうですよね？\nだから今日は不運続きなんですよね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:32 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:32 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:33 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:33 
msgid "……あっ。そうです。\nあなたは悪霊に取り亅かれています。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:34 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:34 
msgid "やっぱり！　お願いです！\nすぐに除霊してください！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:35 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:35 
msgid "わかりました。この霊にとっておきの物が\nこちらのお守り。５千円になります。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:36 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:36 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:37 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:37 
msgid "お、お金を取るの！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:38 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:38 
msgid "こちらも商売ですから。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:39 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:39 
msgid "くっ、でも、学生にとって\n５千円は大金だから……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:40 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:40 
msgid "そう思って安くしてるんですよ。\n本当なら１万円頂くところです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:41 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:41 
msgid "えっ！？　じゃあ半額！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:42 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:42 
msgid "はい。あなたのためを思って。\nこれでもこちらは赤字なんですよ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:43 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:43 
msgid "買います！　はい、樋口！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:44 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:44 
msgid "まいどあり―。\nそれではどうぞ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:45 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:45 
msgid "（よし、これでいつもの調子を\n　取り戻せる！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:46 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:46 
msgid "じゃあ、おばあさん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:47 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:47 
msgid "はい？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:48 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:48 
msgid "コ―ラをこぼしておこ―られた！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:49 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:49 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:50 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:50 
msgid "…………！！\n"
msgstr "...... !!\n"

#  speaker 
#  warnings: 0 
#: sc/0009:51 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:51 
msgid "はっはっはっは……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:52 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:52 
msgid "やった――！！\n私、完全復活――！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:53 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:53 
msgid "……はぁ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:54 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:54 
msgid "ご、５千円って……またまた。\n冗談が上手いなぁ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:55 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:55 
msgid "本気ですよ。この不況の時代、\nタダじゃ商売上がったりですからね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:56 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:56 
msgid "（くっ、不況の話は学生には分が悪い。\n　それを言われると何も言い返せない）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:57 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:57 
msgid "悪霊にひどい目に遭わされたんでしょう？\nそれがなくなると思えば安い物でしょう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:58 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:58 
msgid "た、確かに……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:59 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:59 
msgid "（今日みたいなことがずっと続くなんて……\n　考えただけで恐ろしい！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:60 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:60 
msgid "くっ……しかし、５千円は……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:61 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:61 
msgid "それでは大サ―ビスで３千円に。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:62 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:62 
msgid "買った！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:63 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:63 
msgid "まいどあり―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:64 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:64 
msgid "（３千円でも高い。だけど除霊が３千円なら\n　きっと相場的にも安いはず……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:65 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:65 
msgid "よ―し！　早速みおちゃんたちに\nいつもの私を見せてあげよう！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:66 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:66 
msgid "おばあさんありがとう！\nまたね―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:67 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:67 
msgid "は―い。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:68 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:68 
msgid "……フフ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:69 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:69 
msgid "いや、いくら私が学生でも\nそれはぼったくりってわかるよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:70 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:70 
msgid "妥当な値段です。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:71 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:71 
msgid "ただのお守りじゃん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:72 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:72 
msgid "失礼な。これは恐山で一番霊力の強い場所で\n三日三晩清め続けられたお守りですよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:73 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:73 
msgid "お、恐山？　あの有名な？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:74 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:74 
msgid "はい。私は日頃そこにいますから。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:75 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:75 
msgid "（そう言われると一気に信亅性が\n　高くなってきた……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:76 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:76 
msgid "まあ、いらないというのであれば\n私は別に構いませんが。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:77 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:77 
msgid "うっ……。\n"
msgstr "Uh....\n"

#  speaker 
#  warnings: 0 
#: sc/0009:78 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:78 
msgid "それでは、私はこれで。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:79 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:79 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:80 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:80 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0009:81 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0009:81 
msgid "……待って！　やっぱり買う！\n買わせてくださ―い！\n"
msgstr ""
