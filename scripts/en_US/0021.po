msgstr ""

#  text 
#  warnings: 0 
#: sc/0021:70 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0021:71 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0021:71 
msgid "はて、どうしたのだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0021:72 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0021:72 
msgid "まあ良い。では帰るとするか。\n"
msgstr ""

