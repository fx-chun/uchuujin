msgstr "Butler"

#  text 
#  warnings: 0 
#: sc/0021:74 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0021:75 
msgid "みさと"
msgstr "Misato"

#  text 
#  warnings: 0 
#: sc/0021:75 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0021:76 
msgid "通行人男性"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0021:76 
msgid "うわっ、何だあれ！？　ほら見ろよ。\n鹿と執事と……大砲？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0021:77 
msgid "通行人女性"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0021:77 
msgid "しっ、やめなさいよ。\n"
msgstr ""

