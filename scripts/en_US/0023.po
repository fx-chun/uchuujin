#  speaker 
#  warnings: 0 
#: sc/0023:0 
msgid "玉村"
msgstr "Tamamura"

#  text 
#  warnings: 0 
#: sc/0023:0 
msgid "いらっしゃいませ〜\nご注文をどうぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:1 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:1 
msgid "う〜ん……そうね……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:2 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:2 
msgid "カフェ・ラテのグランデを。\nあ、それにア―モンドシロップ追加で。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:3 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:3 
msgid "かしこまりました。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:4 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:4 
msgid "さて、どこに座ろうかなっと弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:5 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:5 
msgid "！？\n"
msgstr "!?\n"

#  speaker 
#  warnings: 0 
#: sc/0023:6 
msgid "笹原"
msgstr "Sasahara"

#  text 
#  warnings: 0 
#: sc/0023:6 
msgid "む、奇遇だな。\n立花みさともコ―ヒ―ブレイクとは。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:7 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:7 
msgid "何がコ―ヒ―ブレイクよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:8 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:8 
msgid "そんなもの被って！\nちょっとはお店の迷惑考えたらどうなの！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:9 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:9 
msgid "ふむ、やはりそうか。\nでは今後カフェでの役作りは控えるとしよう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:10 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:10 
msgid "え！？　あ、あれ？　え―！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:11 
msgid "中之条"
msgstr "Nakanojo"

#  text 
#  warnings: 0 
#: sc/0023:11 
msgid "うう……な、なぜ僕がこんな目に……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:12 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:12 
msgid "え！？　べ、別人！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:13 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:13 
msgid "なに？　なんなのこれ？　どういうこと！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:14 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:14 
msgid "自分の役を客観的に見るということは\n演技において非常に重要だ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:15 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:15 
msgid "……だから、何よ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:16 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:16 
msgid "だから、通りがかりの下級生にあれを被せて\n観察していたのだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:17 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:17 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0023:18 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0023:18 
msgid "通りすがりの下級生……\nということは、彼も私の生徒！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:19 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:19 
msgid "……おや？\nこれは一体、私はなぜこんなところに……？\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0023:20 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0023:20 
msgid "いえ、そんなことより！　君は……\n１Ｐの中之条くんだね！　しっかりなさい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:21 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:21 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:22 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:22 
msgid "む、これは！\n呼吸をしていない！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:23 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:23 
msgid "では、さっそく人工呼吸を……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:24 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:24 
msgid "……悪化した！？\nならば、急いで病院へ連れて行きましょう！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:25 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:25 
msgid "死なせはしません！\n私たちの生徒は、誰一人として！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0023:26 
msgid "ダ、ダメだ、本人の意思が強すぎて、\nまったく操作を受け付けない！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0023:27 
msgid "まずい！\nこのままだと操作電波の圏外に出てしまうぞ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0023:28 
msgid "なんだと、圏外に！？\n……で、圏外になったらどうなるんだ？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0023:29 
msgid "そりゃあ……。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0023:30 
msgid "やっぱり……。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0023:31 
msgid "……なるほどね―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:32 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:32 
msgid "あの……すみません。\n次の方、ご注文を……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:33 
msgid "みお"
msgstr "Mio"

#  text 
#  warnings: 0 
#: sc/0023:33 
msgid "さ、笹原先輩！　先輩の番ですよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:34 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:34 
msgid "む、そうか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:35 
msgid "ゆっこ"
msgstr "Yuuko"

#  text 
#  warnings: 0 
#: sc/0023:35 
msgid "（どうなのよ……\n　笹原先輩は普通に注文できるの……？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:36 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:36 
msgid "（いやいや、そんなはずない。笹原先輩だよ？\n　きっと盛大にボケてくれるはず！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:37 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:37 
msgid "ふむ、そうだな……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:38 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:38 
msgid "（ドキドキドキドキ……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:39 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:39 
msgid "よきにはからえ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:40 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:40 
msgid "…………？\nか、かしこまりました。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:41 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:41 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:42 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:42 
msgid "ちょっと笹原！　店員さん困ってるじゃない！\nちゃんと注文しなさいよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:43 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:43 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:44 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:44 
msgid "あ、あれ！？\nもう、なんでよ―っ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:45 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:45 
msgid "立花みさとよ、\nなにやら困っているようだな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:46 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:46 
msgid "な、なによ！　あんたには関係ないでしょ！\n放っといてよ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:47 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:47 
msgid "いやいや、そう言うわけにはいかん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:48 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:48 
msgid "私と立花みさとの仲ではないか。\n何事も遠慮なく相談するがいい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:49 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:49 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:50 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:50 
msgid "た…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:51 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:51 
msgid "た？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:52 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:52 
msgid "……弾が、出ないの。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:53 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:53 
msgid "そのようだな。\nよし、私が見て進ぜよう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:54 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:54 
msgid "いやいやいやいや！　それってどうなの？\n撃たれかけたのに！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:55 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:55 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:56 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:56 
msgid "どう？　何かわかる？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:57 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:57 
msgid "立花みさとよ、弾が出ないのは当然だ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:58 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:58 
msgid "この兵器の装填弾数は一発。\n先程、すでに一度撃っただろう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:59 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:59 
msgid "ならば弾を込めてやらねば\n撃てるわけがない。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:60 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:60 
msgid "あ、そっか。\nうっかりしてたわ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:61 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:61 
msgid "こうして弾をこめれば……よし、\nこれで問題ない。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:62 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:62 
msgid "すご―い！　さすが笹原先輩です！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:63 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:63 
msgid "お、お礼なんて言わないんだからね！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:64 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:64 
msgid "さ、笹原になんか、\n絶対、感謝なんかしないんだから！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:65 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:65 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:66 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:66 
msgid "わ、私と笹原の……仲……！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:67 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:67 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:68 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:68 
msgid "さ、笹原先輩……あ、あ、あの……\nお、お二人は一体……どういう……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:69 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:69 
msgid "ど、ど、ど、どういう関係って言われても……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:70 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:70 
msgid "ちょっと笹原！\n誤解されるようなこと言わないでよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:71 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:71 
msgid "？　誤解などではない。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:72 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:72 
msgid "私と立花みさとは切っても切れぬ縁。\n今さら何を隠すことがあろう。\n"
msgstr ""

#  speaker 
#  warnings: 9 
#: sc/0023:73 
msgid "みお"
msgstr ""

#  text 
#  warnings: 9 
#: sc/0023:73 
msgid "９〜！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:74 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:74 
msgid "わ―！？　みおちゃんしっかり！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:75 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:75 
msgid "〜〜〜〜〜〜〜〜〜〜〜〜！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:76 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:76 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:77 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:77 
msgid "お待たせ致しました。カフェ・ラテのグランデ、\nア―モンドシロップ入りです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:78 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:78 
msgid "え、あ、ど、どうも弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:79 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:79 
msgid "ふむ、ご苦労。\n……うむ、美味なり。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:80 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:80 
msgid "あ……さ、笹原！　それ、私のよ！！\nなに勝手に飲んでるのよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:81 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:81 
msgid "む、それは失敬。\nでは返すとしよう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:82 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:82 
msgid "え……あの……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:83 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:83 
msgid "どうした、早く飲まぬか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:84 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:84 
msgid "あ、うん……ごくり……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:85 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:85 
msgid "どうだ、立花みさとよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:86 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:86 
msgid "うん、おいし……はっ！？\nそ、そんなわけないでしょっ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:87 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:87 
msgid "これ、もういらないわよ！\n笹原、責任とって全部飲みなさいよね！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:88 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:88 
msgid "そうか、では遠慮なく頂くとしよう。\n……うむ、美味。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:89 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:89 
msgid "か、間接……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:90 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:90 
msgid "きえ〜〜〜〜〜〜〜〜〜〜〜〜っ！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:91 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:91 
msgid "い、意外〜。\n笹原先輩が良い人っぽいよ〜。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:92 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:92 
msgid "笹原先輩は良い人だってば！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:93 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:93 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:94 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:94 
msgid "あ、あれ？　行っちゃったよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0023:95 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0023:95 
msgid "なんなんだろ？\n"
msgstr ""

