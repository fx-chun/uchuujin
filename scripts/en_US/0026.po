msgstr ""
"Capture operation didn't commence,\n"
"but at last the dawn's early light.\n"

#  speaker 
#  warnings: 0 
#: sc/0026:2 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0026:2 
