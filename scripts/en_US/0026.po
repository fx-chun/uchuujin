msgstr ""

#  text 
#  warnings: 0 
#: sc/0026:57 
msgid "こ、怖くなんかない。これが普通だ。\n"
msgstr "D-don’t be scared. This is normal.\n"

#  speaker 
#  warnings: 0 
#: sc/0026:58 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0026:58 
