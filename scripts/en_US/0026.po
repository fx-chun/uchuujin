msgstr ""
"Ah—……\n"
"Um, I don't think the face would be good either…….\n"

#  speaker 
#  warnings: 0 
#: sc/0026:41 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0026:41 
