#  speaker 
#  warnings: 0 
#: sc/0031:0 
msgid "大工"
msgstr "Daiku"

#  text 
#  warnings: 0 
#: sc/0031:0 
msgid "失礼しま―す。\n"
msgstr "Excuse me--.\n"

#  speaker 
#  warnings: 0 
#: sc/0031:1 
msgid "桜井先生"
msgstr "Sakurai Sensei"

#  text 
#  warnings: 0 
#: sc/0031:1 
msgid "あ、大工君。こっちです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:2 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:2 
msgid "どうしたんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:3 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:3 
msgid "もしかして囲碁サッカ―部の顧問に！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:4 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:4 
msgid "あっ、えっと、スイマセン。\nそうじゃなくて、さっき電話がかかってきて。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:5 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:5 
msgid "えっと……うちの囲碁サッカ―部と\n練習試合がしたいそうなんですよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:6 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:6 
msgid "れ、練習試合―――っ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:7 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:7 
msgid "はい、なので部長さんに連絡しようと思って。\nどうしますか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:8 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:8 
msgid "もちろん受けます！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:9 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:9 
msgid "はい、先方には私から連絡しておきますね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:10 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:10 
msgid "ありがとうございます。\nそれじゃ、早速部員に伝えてきます！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:11 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:11 
msgid "あっ、大工君、前っ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:12 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:12 
msgid "あ、あぶない――っ！！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0031:13 
msgid "ね、猫が踏まれる――っ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0031:14 
msgid "回避―っ！\nダメだ、間に合わない、うわぁぁぁぁっ！！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0031:15 
msgid "いや待て！！\nタ、タ―ゲットが踏みとどまったぞ！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0031:16 
msgid "ほ、本当だ！\nギリギリの所で足を止めている！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0031:17 
msgid "その代わり足がつったみたいだが……\nすごい反応だったな、この少年。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0031:18 
msgid "まったくだ。アンテナは折れたが、彼の頑張りの\n前には大した問題じゃないさ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0031:19 
msgid "そうだな！　はははははっ！\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0031:20 
msgid "＆"
msgstr "＆"

#  text 
#  warnings: 2 
#: sc/0031:20 
msgid "あ―っはっはっはっはっは！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:21 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:21 
msgid "と、いうわけなんだ！\n練習試合だぞ、練習試合！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:22 
msgid "桜井誠"
msgstr "Makoto"

#  text 
#  warnings: 0 
#: sc/0031:22 
msgid "ええっ、本当ですか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:23 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:23 
msgid "こんな事で嘘つくわけないだろ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:24 
msgid "関口"
msgstr "Sekiguchi"

#  text 
#  warnings: 0 
#: sc/0031:24 
msgid "……本気ですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:25 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:25 
msgid "当たり前だろ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:26 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:26 
msgid "……高校に上がってからの初試合だ。\nどんな強敵が……待ってるんだろう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:27 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:27 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0031:28 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:28 
msgid "なんだよ関口、初の練習試合なんだぞ？\nお前ももっと喜べよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:29 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:29 
msgid "部長。\n"
msgstr "Chief.\n"

#  speaker 
#  warnings: 0 
#: sc/0031:30 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:30 
msgid "どうした関口。\n"
msgstr "What is it Sekiguchi?\n"

#  speaker 
#  warnings: 0 
#: sc/0031:31 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:31 
msgid "部長、試合できるんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:32 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:32 
msgid "関口……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:33 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:33 
msgid "はい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:34 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:34 
msgid "ムリに決まってるだろ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:35 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:35 
msgid "！！！？\n"
msgstr "!!!?\n"

#  speaker 
#  warnings: 0 
#: sc/0031:36 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:36 
msgid "ええ―――っ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:37 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:37 
msgid "桜井くん！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:38 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:38 
msgid "は、はい？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:39 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:39 
msgid "任せたぜ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:40 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:40 
msgid "ぶ、部長！\nせめて練習くらいは……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:41 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:41 
msgid "そうですよ、とりあえず練習して、\n基本くらいは覚えましょうよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:42 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:42 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:43 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:43 
msgid "関口、桜井くん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:44 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:44 
msgid "めんどい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:45 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:45 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:46 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:46 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:47 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:47 
msgid "あ、いや……ってのは冗談だ！\nわかってるさ、ちゃんと練習くらいするって！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:48 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:48 
msgid "そ、それじゃあ放課後は部室に集合な！\nそろそろ授業あるから戻るぞ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:49 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:49 
msgid "……部長、今本気でしたよね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:50 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:50 
msgid "多分ムリだな！　まあ桜井くんが居るんだから、\nどうとでもなるさ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:51 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:51 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:52 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:52 
msgid "関口さん、大丈夫。\n部長には僕が教えるからさ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:53 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:53 
msgid "本当は顧問の先生が居たらいいんだけど、\n高崎先生はまだ渋ってるし。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:54 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:54 
msgid "おっと、そういえばうちには\n未だに顧問の先生がいなかったな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:55 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:55 
msgid "それって大問題じゃ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:56 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:56 
msgid "よし、今日の授業が終わったら\nその辺について話し合いでもしよう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:57 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:57 
msgid "そうですね！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:58 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:58 
msgid "関口も来てくれよ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:59 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:59 
msgid "私、試合に出ませんよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:60 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:60 
msgid "何言ってんだ、\n関口も大切な仲間じゃないか！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:61 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:61 
msgid "……わかりました。\n"
msgstr ".....Understood.\n"

#  speaker 
#  warnings: 0 
#: sc/0031:62 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:62 
msgid "よし、じゃあまた放課後に！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:63 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:63 
msgid "はい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:64 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:64 
msgid "……そもそも、二人で団体戦って出来るの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:65 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:65 
msgid "関口。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:66 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:66 
msgid "はい。\n"
msgstr "Yes.\n"

#  speaker 
#  warnings: 0 
#: sc/0031:67 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:67 
msgid "安心しろ、うちの部は強い。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:68 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:68 
msgid "なんてったって中学ＬＵＯの\n桜井くんが居るんだぞ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:69 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:69 
msgid "高校でどこまでやれるか不安ですけどね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:70 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:70 
msgid "大丈夫さ、なんてったって中学ＬＵＯだろ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:71 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:71 
msgid "はい、中学ＬＵＯは伊達じゃないってとこ、\n試合ではしっかりお見せしますよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:72 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:72 
msgid "期待してるぜ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:73 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:73 
msgid "頑張ります。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:74 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:74 
msgid "それで、部長は？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:75 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:75 
msgid "それに、うちは県大会出場経験だってある。\nこの界隈じゃ強豪校のはずさ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:76 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:76 
msgid "そう、間違いなくうちは強い。\nだから練習試合で負けるわけがないだろ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:77 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:77 
msgid "でも、あの成績って……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:78 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:78 
msgid "よし、今日の放課後は絶対出席な。\n頼むぞ、二人とも。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:79 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:79 
msgid "はい！\n"
msgstr "Yes!\n"

#  speaker 
#  warnings: 0 
#: sc/0031:80 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:80 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0031:81 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0031:81 
msgid "それにしても練習試合か……\n面白くなってきやがったぜ。\n"
msgstr ""
