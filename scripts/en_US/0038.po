msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0038:0 
msgid "（の、乗り切った……！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0038:1 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0038:1 
msgid "（モヒカンが幽霊になっていることは\n　誰にも気付かれていない）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0038:2 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0038:2 
msgid "（とは言え油断はできない。\n　安心するのはうちに帰ってからだ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0038:3 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0038:3 
msgid "（このままダラダラとするより、\n　早いとこうちに帰ろう）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0038:4 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0038:4 
msgid "あっ、お―い中之条。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0038:5 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0038:5 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0038:6 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0038:6 
msgid "……な、何？\n僕、急いでるんだけど……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0038:7 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0038:7 
msgid "今日の夜、みんなで心霊スポットに\n行くことになったんだ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0038:8 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0038:8 
msgid "心霊スポット？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0038:9 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0038:9 
msgid "おう。だからお前も来ないか―？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0038:10 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0038:10 
msgid "（何かと思ったらそんなこと……\n　まあ気付かれるよりはいいか）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0038:11 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0038:11 
msgid "保護者同伴だから\n親にも怒られないぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0038:12 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0038:12 
msgid "保護者？\n田中君のお父さん？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0038:13 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0038:13 
msgid "い―や。校長。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0038:14 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0038:14 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0038:15 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0038:15 
msgid "！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0038:16 
msgid "お、おい、いいのかよ、\nそんな安請け合いしちまって……。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0038:17 
msgid "いいって。\nどうせ霊なんか出ないし。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0038:18 
msgid "そ、そうかな……\nま、まあ、わかってるけどさ……。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0038:19 
msgid "…………。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0038:20 
msgid "ふ―ん？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0038:21 
msgid "な、なんだよ？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0038:22 
msgid "別に？　……あ、そうだ！\nこれも中継してやろうぜ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0038:23 
msgid "えっ！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0038:24 
msgid "怪談とか苦手なやつも、強制参加！\n面白そうじゃね？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0038:25 
msgid "あ、ああ……。\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0038:26 
msgid "＆"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0038:26 
