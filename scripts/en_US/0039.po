#  speaker 
#  warnings: 0 
#: sc/0039:0 
msgid "中之条"
msgstr "Nakanojo"

#  text 
#  warnings: 0 
#: sc/0039:0 
msgid "いつになったら、この髪が元に戻るんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:1 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:1 
msgid "今はまだ誰にもバレていないけど……\nこのまま治らなければ、いつかきっと……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:2 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:2 
msgid "こんな時こそ科学の出番だが……\n勉強不足な僕には手も足も出ない症状だ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:3 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:3 
msgid "……ん？　なんだここ？\n東雲研究所？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:4 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:4 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0039:5 
msgid "はかせ"
msgstr "Hakase"

#  text 
#  warnings: 0 
#: sc/0039:5 
msgid "あっ！　モヒカンだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:6 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:6 
msgid "えっ！？\n"
msgstr "Ehh!?\n"

#  speaker 
#  warnings: 0 
#: sc/0039:7 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:7 
msgid "あははは、モヒカンだ！\n初めて見た！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:8 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:8 
msgid "（なんだこの子……\n　もしかしてこの研究所の子？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:9 
msgid "バディ"
msgstr "Buddy"

#  text 
#  warnings: 0 
#: sc/0039:9 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:10 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:10 
msgid "わっ！　犬だ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:11 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:11 
msgid "助けて！　噛まれちゃう！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:12 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:12 
msgid "えっ？　ああ、大丈夫だよ。\n犬はこうやって頭を撫でれば弌弌\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0039:13 
msgid "うわあっ！　アンテナがピンチだ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0039:14 
msgid "耐えてくれ……耐えてくれ―っ！！\nええい、もっと頑丈に作っておけよっ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0039:15 
msgid "し、仕方ないだろ、予算が足りなかったんだ！\n文句があるなら姫にでも言ってくれ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0039:16 
msgid "くそ―っ！\n姫のばか――――っ！！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0039:17 
msgid "ええっ！？\nまだ、壊れていなかったはずだぞ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0039:18 
msgid "……あ、姫のとこに回線つながってた。\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0039:19 
msgid "＆"
msgstr "＆"

#  text 
#  warnings: 2 
#: sc/0039:19 
msgid "姫のばか――――っ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:20 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:20 
msgid "ねえお嬢ちゃん、ここ、研究所だよね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:21 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:21 
msgid "うん。はかせの研究所だよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:22 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:22 
msgid "その博士は今どこにいるの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:23 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:23 
msgid "はかせがはかせだよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:24 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:24 
msgid "えっ？　お嬢ちゃんが博士！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:25 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:25 
msgid "うん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:26 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:26 
msgid "（この歳で博士……？\n　信じられない……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:27 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:27 
msgid "（いや、だが、天才に年齢は関係ない。\n　小さくても天才なんだ、この子は）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:28 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:28 
msgid "じゃあ博士、お願いなんですけど……\nこの髪の毛、見てくれますか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:29 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:29 
msgid "モヒカン！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:30 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:30 
msgid "そう。だけどモヒカンの部分だけ\n幽霊になってるんです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:31 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:31 
msgid "！？\n"
msgstr "!?\n"

#  speaker 
#  warnings: 0 
#: sc/0039:32 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:32 
msgid "さわってもいい！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:33 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:33 
msgid "えっ？　あ、い、いいですけど、\nでも、今は髪の毛が幽霊になってて弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:34 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:34 
msgid "あはははは！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:35 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:35 
msgid "触れてる―――！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:36 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:36 
msgid "（ど、どういうことだ？\n　僕は触れなかったのに……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:37 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:37 
msgid "ちょ、ちょっとスイマセン。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:38 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:38 
msgid "……触れた。あっ……\nでもまた触れなくなった。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:39 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:39 
msgid "（どういうことだ……？\n　何か条件があるのか……？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:40 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:40 
msgid "ハッ！？　そ、そうか！　科学の力で\n幽霊になった髪に触ったんですね！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:41 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:41 
msgid "なにそれ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:42 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:42 
msgid "違った―――！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:43 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:43 
msgid "（うう……触れたり触れなかったりなんて\n　ハッキリしない幽霊だ……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:44 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:44 
msgid "くっ……だけど博士、博士ならきっと\n科学の力でこの髪の毛を治せますよね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:45 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:45 
msgid "できるよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:46 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:46 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:47 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:47 
msgid "ちょっと待ってて！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:48 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:48 
msgid "は、はいっ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:49 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:49 
msgid "（博士はすでに治し方がわかっているのか！？\n　やっぱりこの子は天才だ！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:50 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:50 
msgid "はい！　この薬を頭につけて。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:51 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:51 
msgid "うん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:52 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:52 
msgid "（これで治るんだ……\n　科学が勝利する瞬間だ……！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:53 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:53 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:54 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:54 
msgid "あはははははははは！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:55 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:55 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:56 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:56 
msgid "あははははは！\nモヒカン３つ！　あははは！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:57 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:57 
msgid "…………！！\n"
msgstr "...... !!\n"

#  speaker 
#  warnings: 0 
#: sc/0039:58 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:58 
msgid "ププ――！　カットパインが３つになった！\nプププププ！　美味しそう！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:59 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:59 
msgid "…………！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:60 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:60 
msgid "は……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:61 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:61 
msgid "……んん？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:62 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:62 
msgid "生えた――――！！！\n横からも髪が生えた――！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:63 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:63 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:64 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:64 
msgid "あははは！\n生えた生えた！　ニョキニョキ生えた！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:65 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:65 
msgid "うおおおおおお！\n僕の毛根は死んでなかったんだ――！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:66 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:66 
msgid "あはははははは！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:67 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:67 
msgid "うおおおおおおおお！！\n"
msgstr "Uwohhhhhhhhhhh!!\n"

#  speaker 
#  warnings: 0 
#: sc/0039:68 
msgid "阪本さん"
msgstr "Sakamoto-san"

#  text 
#  warnings: 0 
#: sc/0039:68 
msgid "…………。\n"
msgstr "………….\n"

#  speaker 
#  warnings: 0 
#: sc/0039:69 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:69 
msgid "……これ、止めた方がいいのか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:70 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:70 
msgid "（か、髪が、生えた……\n　もう生えないと思われた場所に……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:71 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:71 
msgid "（僕の死んだ毛根にさえも効くなんて\n　博士の発明した育毛剤はなんてすごいんだ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:72 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:72 
msgid "（だが……だが、しかし……！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:73 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:73 
msgid "（新しい２つも触れない……！！\n　傷口が拡がっただけだ……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:74 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:74 
msgid "あははは！　変な髪型！\nカットパインが３つも刺さってる！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:75 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:75 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:76 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:76 
msgid "あはははははは！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:77 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:77 
msgid "…………！！\n"
msgstr "………!!\n"

#  speaker 
#  warnings: 0 
#: sc/0039:78 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:78 
msgid "（なんてことだ……調査対象に目もくれず、\n　モヒカンで遊ぶだなんて……！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:79 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:79 
msgid "（だが、仕方ない。この子は確かに天才だけど\n　まだまだ精神的に未熟。責められない）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:80 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:80 
msgid "（責めるべくは自分の身に起こったことを\n　自分で対処できない僕だ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:81 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:81 
msgid "（こうなったら、自分一人で何とかしなくては！\n　さようなら、はかせ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:82 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:82 
msgid "あははははは！　はあ、はぁ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:83 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:83 
msgid "はぁ……面白かった！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0039:84 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0039:84 
msgid "でもどっか行っちゃった。\nせっかく治してあげようと思ったのに。\n"
msgstr ""
