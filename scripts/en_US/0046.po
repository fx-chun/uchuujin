#  speaker 
#  warnings: 0 
#: sc/0046:0 
msgid "高崎先生"
msgstr "Takasaki-sensei"

#  text 
#  warnings: 0 
#: sc/0046:0 
msgid "ふう……昼食がすっかり遅くなってしまった。\nさて、もうひと頑張りするとしよう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:1 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:1 
msgid "しかし相生はまた宿題をやってこなかったが、\nどうすれば反省してくれるんだろうか……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:2 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:2 
msgid "やはり一度じっくりと説教を……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:3 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:3 
msgid "あっ、高崎先生！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:4 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:4 
msgid "安中！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:5 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:5 
msgid "大変なんですけど！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:6 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:6 
msgid "ということは、また中之条か！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:7 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:7 
msgid "それが今回は桜井先生が行方不明なんですけど！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:8 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:8 
msgid "ええ―――っ！！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:9 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:9 
msgid "すぐ来て欲しいんですけど！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:10 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:10 
msgid "わかった、すぐに行くぞ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:11 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:11 
msgid "あっ、先生そっちじゃないんですけど！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:12 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:12 
msgid "っていうかそっちには猫がいるんですけど！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:13 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:13 
msgid "うおぉぉっ！！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0046:14 
msgid "なあ、いいニュ―スと悪いニュ―スがある。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0046:15 
msgid "……いいニュ―スから頼む。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0046:16 
msgid "猫はバリアのおかげで無事だ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0046:17 
msgid "……悪いニュ―スは？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0046:18 
msgid "アンテナを守るのを忘れてた。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0046:19 
msgid "……なるほど。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:20 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:20 
msgid "わかった！　桜井先生の危機と聞いたら\nこうしてはいられん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:21 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:21 
msgid "行くぞ安中！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:22 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:22 
msgid "あっ、先生！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:23 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:23 
msgid "何だ安中！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:24 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:24 
msgid "桜井先生の居場所に心当たりないんですけど！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:25 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:25 
msgid "！！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:26 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:26 
msgid "高崎先生、知ってるんですか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:27 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:27 
msgid "…………。\n"
msgstr "………….\n"

#  speaker 
#  warnings: 0 
#: sc/0046:28 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:28 
msgid "いいか、よく聞け。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:29 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:29 
msgid "はい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:30 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:30 
msgid "桜井先生の居場所は……知らん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:31 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:31 
msgid "ええ―――っ！！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:32 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:32 
msgid "そうだ！！　ダウジングしろ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:33 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:33 
msgid "やってみます。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:34 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:34 
msgid "ええ―――っ！！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:35 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:35 
msgid "ま、待て！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:36 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:36 
msgid "ええ―――っ！！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:37 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:37 
msgid "手がかりが無いからって何だ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:38 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:38 
msgid "教育者として人が行方不明なら何が何でも探す！\nそれが……その、教育者だ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:39 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:39 
msgid "でも、何か今桜井先生が歩いてたんですけど。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:40 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:40 
msgid "ええ――――っ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:41 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:41 
msgid "私の勘違い……だったみたいなんですけど。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:42 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:42 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:43 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:43 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:44 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:44 
msgid "安中、職員室まで来なさい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:45 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:45 
msgid "ハイ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:46 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:46 
msgid "ヒント無しか……。\nこれは地道に聞き込むしか……そうだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:47 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:47 
msgid "安中、中之条を呼んできてくれ！\n失价といえばアイツだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:48 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:48 
msgid "いってきます！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:49 
msgid "中之条"
msgstr "Nakanojo"

#  text 
#  warnings: 0 
#: sc/0046:49 
msgid "何ですか先生。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:50 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:50 
msgid "中之条、桜井先生が失价した。\nお前、居場所を知ってるんじゃないか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:51 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:51 
msgid "あ、ハイ。知ってますけど。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:52 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:52 
msgid "（ええ―――っ！！？\n　勢いで言ったのに知ってるのかよ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:53 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:53 
msgid "今こっちに来る時、すれ違いました。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:54 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:54 
msgid "！？\n"
msgstr "!?\n"

#  speaker 
#  warnings: 0 
#: sc/0046:55 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:55 
msgid "……あ、そういえば見たような気が。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:56 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:56 
msgid "ご、ごめんなさいっ！\n私の勘違いだったみたいです！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:57 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:57 
msgid "……気にするな安中。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:58 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:58 
msgid "（そうさ、安中も悪気があったわけじゃない。\n　むしろ桜井先生を心配した結果だ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:59 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:59 
msgid "（それを怒れるはずがない。\n　それが教師ってもんだろ？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:60 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:60 
msgid "……あの―、呼ばれたのってそれだけ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:61 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:61 
msgid "……知らん！！\nだが、１つだけわかることがある。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:62 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:62 
msgid "な、何ですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:63 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:63 
msgid "桜井先生の居場所を見つける手がかりだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:64 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:64 
msgid "今日の昼休み、\nオレは桜井先生と話をしていたんだが……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:65 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:65 
msgid "たしか、そう！\n桜井先生は早上がりだと言っていた！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:66 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:66 
msgid "た、高崎先生っ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:67 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:67 
msgid "それって、桜井先生は家に帰っちゃってるって\n意味に聞こえるんですけど。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:68 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:68 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:69 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:69 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0046:70 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0046:70 
msgid "あっ、高崎せんせ――――っ！？\n"
msgstr ""
