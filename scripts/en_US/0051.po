#  speaker 
#  warnings: 0 
#: sc/0051:0 
msgid "はかせ"
msgstr "Hakase"

#  text 
#  warnings: 0 
#: sc/0051:0 
msgid "ｙｙＹ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:1 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:1 
msgid "ん…………ふぁあ〜……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:2 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:2 
msgid "ん〜……\nよく寝た〜……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:3 
msgid "なの"
msgstr "Nano"

#  text 
#  warnings: 0 
#: sc/0051:3 
msgid "はかせ―、ただいま―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:4 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:4 
msgid "あれ？　なの帰ってきた。\nもうそんな時間だったんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:5 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:5 
msgid "ただいまはかせ。\nあっ、お昼寝してたんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:6 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:6 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0051:7 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:7 
msgid "……ん？　どうかしましたか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:8 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:8 
msgid "ううん、ネジがあったら\n可愛いと思っただけ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:9 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:9 
msgid "いや、それは無いですから。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:10 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:10 
msgid "ぷぷぷ。阪本と同じこと言ってる。\nでもちゃんとデ―タ取ったもんね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:11 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:11 
msgid "デ―タ？　何のことですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:12 
msgid "バディ"
msgstr "Buddy"

#  text 
#  warnings: 0 
#: sc/0051:12 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:13 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:13 
msgid "あっ！　はかせ、犬拾ってきたんですか！？\nダメですよ、うちには阪本さんが……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:14 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:14 
msgid "わっ！？　い、犬だ！？\nなの―！　助けて――！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:15 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:15 
msgid "えっ？　え？　あ、はい。\n……違ったんですか。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0051:16 
msgid "あ―……抱きかかえられたら、\nアンテナ折れちまった。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0051:17 
msgid "ま、こんなもんさ。\n今回の選びかたは失敗だったんだよ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0051:18 
msgid "……だな。\nやっぱり、ル―レットは難しいな！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0051:19 
msgid "だな！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:20 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:20 
msgid "なのはネジを可愛くないって\n思ってるんだよね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:21 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:21 
msgid "当たり前です。こんなのがついていて\n可愛いと思う人ははかせくらいです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:22 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:22 
msgid "残念でした―！　はかせ以外にも\n可愛いって言った人いたもんね―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:23 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:23 
msgid "えっ！？　いたんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:24 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:24 
msgid "いたんです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:25 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:25 
msgid "と言うか、わざわざ聞きに行ったんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:26 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:26 
msgid "行ったんです。\nそしてはかせが正しいことが証明されました！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:27 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:27 
msgid "う、う―ん……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:28 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:28 
msgid "だからネジは可愛いから\n自信持って、なの！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:29 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:29 
msgid "！？\n"
msgstr "!?\n"

#  speaker 
#  warnings: 0 
#: sc/0051:30 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:30 
msgid "あははは！　いっぱい出てきた！\nあはははは！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:31 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:31 
msgid "ちょっ！？\n何してくれてるんですかはかせ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:32 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:32 
msgid "あははは！　可愛いでしょ！\nあはははは！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:33 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:33 
msgid "こんなの可愛いわけないじゃないですか！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:34 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:34 
msgid "ぷぷぷ！　だからそれはなののセンスが\nはかせたちより悪いからだよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:35 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:35 
msgid "ネジ１つならそうかもしれませんけど！\nこれを可愛いと思うのは異常です！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:36 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:36 
msgid "異常じゃないよ！\n可愛いじゃん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:37 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:37 
msgid "可愛い可愛くないの次元を\n超えちゃってるんですよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:38 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:38 
msgid "こんな姿で外出たら通報されますし、\nそもそも……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:39 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:39 
msgid "そもそも、こんなにネジがあったら\n気持ち悪いです！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:40 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:40 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:41 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:41 
msgid "そんなこと言わないで。はかせ、\nなののためにいっぱい用意したんだから。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:42 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:42 
msgid "その気持ちは嬉しいですけど\nなんでネジを用意するんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:43 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:43 
msgid "可愛いからだよ？\nそれなのにダメなの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:44 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:44 
msgid "日頃からネジは嫌だと\n言っているじゃないですか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:45 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:45 
msgid "なのにこの仕打ちは嫌がらせ以外の\nなにものでもありませんよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:46 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:46 
msgid "…………。\n"
msgstr "………….\n"

#  speaker 
#  warnings: 0 
#: sc/0051:47 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:47 
msgid "あっ……。\n"
msgstr "Ah…….\n"

#  speaker 
#  warnings: 0 
#: sc/0051:48 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:48 
msgid "嫌がらせなんてひどいよ。\nはかせ、なのに喜んでもらいたくて……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:49 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:49 
msgid "はかせ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:50 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:50 
msgid "……想像してください。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:51 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:51 
msgid "………？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:52 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:52 
msgid "ある日突然、体中からたくさんの\nネジが生えてくる光景を！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:53 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:53 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:54 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:54 
msgid "怖くないですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:55 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:55 
msgid "あははははははは！　面白い！\nはかせの腕からにゅってネジが！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:56 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:56 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:57 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:57 
msgid "……異常だ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:58 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:58 
msgid "だってはかせ以外の人も\nネジが可愛いって言ったよ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:59 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:59 
msgid "そうかもしれませんけど、\nそれを聞いたのは今日なんですよね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:60 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:60 
msgid "うん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:61 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:61 
msgid "でも私はそれより前に\n改造されちゃってるじゃないですか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:62 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:62 
msgid "うん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:63 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:63 
msgid "何の確信も無く\n改造してるじゃないですか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:64 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:64 
msgid "そうです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:65 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:65 
msgid "なんでしれっとしてるんですか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:66 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:66 
msgid "だってネジ可愛いもん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0051:67 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0051:67 
msgid "結局はかせ次第じゃないですか！\n"
msgstr ""
