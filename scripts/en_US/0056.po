#  speaker 
#  warnings: 0 
#: sc/0056:84 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0056:84 
msgid "おっと、いかんいかん。\nこのままでは学校に遅刻してしまう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0056:85 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
