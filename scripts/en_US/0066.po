#  speaker 
#  warnings: 0 
#: sc/0066:0 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:0 
msgid "よ―し、あっがり―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:1 
msgid "桜井誠"
msgstr "Makoto"

#  text 
#  warnings: 0 
#: sc/0066:1 
msgid "あっちゃ―、やっぱり左のカ―ドですか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:2 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:2 
msgid "一瞬ヒヤヒヤしたよ。\nなんとかごまかせて助かった。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:3 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:3 
msgid "しょうがない、\n負けちゃったしジュ―ス買ってきます。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:4 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:4 
msgid "桜井くんゴチ！　オレコ―ラね！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:5 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:5 
msgid "わかりました。\n寄り道するんでちょっと遅くなります。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:6 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:6 
msgid "お―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:7 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:7 
msgid "さてと……先生はどこかな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:8 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:8 
msgid "（部長が興味持ってるってのは聞いたし、\n　そうなると問題は、やっぱり先生だよな）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:9 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:9 
msgid "（前にお願いしたときは、\n　結局うやむやになったけど、今度こそ……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:10 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:10 
msgid "弌弌あ、居た。\n先生―！　高崎先生―！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:11 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:11 
msgid "うわっ！　お、おっとぉっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:12 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:12 
msgid "あいたたた……。\n何でこんなトコにボ―ルが転がってるんだよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:13 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:13 
msgid "あれ！？　高崎先生いなくなってる！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:14 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:14 
msgid "……あ―、もうっ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:15 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0066:15 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:16 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:16 
msgid "あ……やっちゃった。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0066:17 
msgid "なあ、こういうの……\nこの星の言葉でなんていうんだっけ？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0066:18 
msgid "とばっちり、だよな？\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0066:19 
msgid "＆"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0066:19 
msgid "うわあああああああああ――っ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:20 
msgid "高崎先生"
msgstr "Takasaki-sensei"

#  text 
#  warnings: 0 
#: sc/0066:20 
msgid "お、おおっ！？　さ、桜井か……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:21 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:21 
msgid "先生、顧問の件、考えて貰えましたか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:22 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:22 
msgid "いや、まあ、その。\nアレについてはまだ検討中というか……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:23 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:23 
msgid "正直、めんど……ああいや、\n教育者としてこういった事は慎重に……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:24 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:24 
msgid "（あまり乗り気じゃなさそうだな……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:25 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:25 
msgid "（でも、囲碁サッカ―部が強くなるには\n　元高校ＬＵＯの高崎先生しか居ないんだ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:26 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:26 
msgid "（何とかして先生を説得しないと……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:27 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:27 
msgid "先生！！\nオレ、真剣なんです！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:28 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:28 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:29 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:29 
msgid "ずっと本気で、この想いを抱いてきました！\n後悔だけはしたくないんです！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:30 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:30 
msgid "先生なら同じ道を歩む者として、オレの\n気持ち、分かってくれると思ってました！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:31 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:31 
msgid "違いますか、先生！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:32 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:32 
msgid "……桜井。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:33 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:33 
msgid "先生が悪かったよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:34 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:34 
msgid "……先生、それじゃあ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:35 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:35 
msgid "ああ、お前の熱い想いは受け取った！\nオレも一人の男として応えよう！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:36 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:36 
msgid "高崎先生！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:37 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:37 
msgid "桜井！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:38 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:38 
msgid "さ、桜井くん……それと高崎先生。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:39 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:39 
msgid "あっ、部長！　丁度いい所に！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:40 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:40 
msgid "ははっ、恥ずかしいところ見られたな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:41 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:41 
msgid "いや……なんていうか、スイマセン。\nまさか二人がそうだったなんて……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:42 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:42 
msgid "え？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:43 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:43 
msgid "いやっ、別にオレはそういうのに\n偏見なんか無いから！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:44 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:44 
msgid "だからその、お、お幸せに！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:45 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:45 
msgid "一体何を……あぁっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:46 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:46 
msgid "弌弌ま、まさかっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:47 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:47 
msgid "待ってください部長！　違うんです！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:48 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:48 
msgid "そうだ大工！　今のは誤解なんだ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:49 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:49 
msgid "部長――――っ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:50 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:50 
msgid "……！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:51 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:51 
msgid "お願いしますよ先生。\n少しだけでもいいんです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:52 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:52 
msgid "う―ん……桜井、\nお前本気で囲碁サッカ―やる気なのか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:53 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:53 
msgid "当たり前ですよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:54 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:54 
msgid "そうか……よし！\nわかった、顧問を引き受けよう！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:55 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:55 
msgid "本当ですか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:56 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:56 
msgid "ああ、お前の熱意に打たれたよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:57 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:57 
msgid "せ、先生声大きいですよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:58 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:58 
msgid "（あれ？　先生オレじゃなくて\n　そのかなり後ろを見てないか？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:59 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:59 
msgid "（……もしかして）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:60 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:60 
msgid "あっ、桜井そっちを向くんじゃないっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:61 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:61 
msgid "……やっぱり姉ちゃんが居たのか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:62 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:62 
msgid "いやっ、その！　違うぞ桜井！\n桜井先生はたまたまあそこに居ただけで……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:63 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:63 
msgid "別にいいですよ。\n先生が顧問をやってくれるんでしたら。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:64 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:64 
msgid "そうか！　ありがとう桜井！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:65 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:65 
msgid "よ―し！　桜井！　先生がお前を\n全国につれてってやるからな―っ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:66 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:66 
msgid "（……ちょっと不安だけど）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:67 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:67 
msgid "先生、どうしてもお願いできませんか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:68 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:68 
msgid "いや、別に特別嫌ってわけじゃないんだ。\nただちょっと時間が……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:69 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:69 
msgid "へ―休み時間のたび、\n姉ちゃんに話しかける時間はあるのにですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:70 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:70 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0066:71 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0066:71 
msgid "姉ちゃんから聞いてますよ。\n最近高崎先生が色話しかけてくれるって。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:72 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:72 
msgid "……あ、あの、桜井君？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:73 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:73 
msgid "なんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:74 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:74 
msgid "それで……何だ、教育者として聞きたいが\n桜井先生は、それについてなんと？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:75 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:75 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0066:76 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:76 
msgid "わ、わかった！　顧問をやろう！\nやろうじゃないか！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:77 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:77 
msgid "ありがとうございます。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:78 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:78 
msgid "で、桜井先生は……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:79 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:79 
msgid "それは先生が顧問を\nある程度やってからということで。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:80 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:80 
msgid "！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0066:81 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0066:81 
msgid "わ、わかった……。\n"
msgstr ""
