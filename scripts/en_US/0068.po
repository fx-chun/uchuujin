msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:1 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:2 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:2 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:3 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:3 
msgid "うい―っす関口―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:4 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:4 
msgid "！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:5 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:5 
msgid "おはようございます。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:6 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:6 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:7 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:7 
msgid "（何ペ―ジまで読んでたっけ？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:8 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:8 
msgid "よいしょっと。\nそれにしても毎日早いよな―、関口。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:9 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:9 
msgid "居心地がいいんです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:10 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:10 
msgid "ふ―ん。\n確かにいっつも居るもんな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:11 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:11 
msgid "部長もですけどね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:12 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:12 
msgid "（むしろ私は部長が居るから……。\n　でも、これは本当にアレなのかな？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:13 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:13 
msgid "（わからないし自信も無いから、\n　切り出せないけど……でも）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:14 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:14 
msgid "なあ、関口。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:15 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:15 
msgid "ッ！！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:16 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:16 
msgid "な、なんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:17 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:17 
msgid "そこの隅に居る猫、関口がつれて来たの？\nじっとこっちを見てるけど。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:18 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:18 
msgid "えっ、猫？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:19 
msgid "阪本さん"
msgstr "Sakamoto-san"

#  text 
#  warnings: 0 
#: sc/0068:19 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:20 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:20 
msgid "……いえ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:21 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:21 
msgid "じゃあ迷い込んだのかな。\nよしよし、こっちおいで〜。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0068:22 
msgid "……ふぅ、見つかった時はあせったけど、\nこれなら逃げる必要はなさそうだな。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0068:23 
msgid "いや、今からでも遅くない！\n逃げるんだ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0068:24 
msgid "好意的だから大丈夫だって。\nほら見ろよ、頭、なでてくれようとしてるぞ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0068:25 
msgid "それがまずいんだよっ！！\n頭なんて、なでられたら弌弌\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0068:26 
msgid "はっ！　しまった――！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:27 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:27 
msgid "だるまさんがころんだやろうぜ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:28 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:28 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:29 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:29 
msgid "いいですよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:30 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:30 
msgid "（……やっぱり気のせいかも）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:31 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:31 
msgid "だ―る―ま―さ―ん―が―弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:32 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:32 
msgid "（もう部長は目の前、だけど……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:33 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:33 
msgid "ころんだ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:34 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:34 
msgid "だ―る―ま―さ―ん―が―弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:35 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:35 
msgid "（なんだか、部長に触るの恥ずかしいな）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:36 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:36 
msgid "（やっぱり、これって……？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:37 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:37 
msgid "ころんだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:38 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:38 
msgid "（あ……でも、むしろ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:39 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:39 
msgid "（自然に部長に触る\n　絶好のチャンスじゃないかな？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:40 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:40 
msgid "（うん、間違いない。\n　神様がくれたチャンスだと思おう）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:41 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:41 
msgid "（よし、思い切って……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:42 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:42 
msgid "だ―る―ま―さ―ん―が―弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:43 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:43 
msgid "え……えい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:44 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:44 
msgid "おわあっ！！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:45 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:45 
msgid "！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:46 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:46 
msgid "ぐえぇ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:47 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:47 
msgid "ぶ、部長、ごめんなさい。\n……あの、大丈夫ですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:48 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:48 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:49 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:49 
msgid "白目剥いてる……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:50 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:50 
msgid "思い切りすぎました。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:51 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:51 
msgid "（いけない。\n　そんなことでうろたえてたら……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:52 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:52 
msgid "（……よしっ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:53 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:53 
msgid "はい、触りました。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:54 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:54 
msgid "お―、やられたな―！\nよし、次は関口が鬼だからな！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:55 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:55 
msgid "！？\n"
msgstr "!?\n"

#  speaker 
#  warnings: 0 
#: sc/0068:56 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:56 
msgid "（交代したら、次は私が触られる番じゃ……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:57 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:57 
msgid "どうした関口―、\n早く数えてくれよ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:58 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:58 
msgid "（部長に触られる？\n　普段なら平気だけど……今はちょっと）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:59 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:59 
msgid "ぶ、部長。\n違う遊びもしませんか……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:60 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:60 
msgid "ん―、じゃあ、せっせっせとか？\nちょっと女の子っぽいけど。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:61 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:61 
msgid "！！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:62 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:62 
msgid "（ア、アレは手を何回も重ねあう遊び。\n　何かもっと危険になったかも……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:63 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:63 
msgid "すみません。\nわ、私、実はあの遊びよく知らなくて。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:64 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:64 
msgid "（少し苦しいけど、これでなんとか……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:65 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:65 
msgid "大丈夫、簡単だから教えてやるって！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:66 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:66 
msgid "！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:67 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:67 
msgid "え、いや……その。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:68 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:68 
msgid "（ウソ！？\n　待って、そう言われたら断り文句が弌弌）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:69 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:69 
msgid "ほら、早速やるぞ―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:70 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:70 
msgid "………あ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:71 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:71 
msgid "なんだ、もう時間か―。\nそれじゃ関口、また後でな！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:72 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:72 
msgid "（ほっとしたような、\n　そうじゃないような……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:73 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:73 
msgid "（どうしよう……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:74 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:74 
msgid "ころんだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:75 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:75 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:76 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:76 
msgid "だ―る―ま―さ―ん―が―弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:77 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:77 
msgid "（部長に触らないと終わらない。\n　でも、変に意識しちゃって触りにくい……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:78 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:78 
msgid "ころんだ！\nだ―る―ま―さ―ん―が―弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:79 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:79 
msgid "（でも、チャンスではあるし。\n　それに……でも、やっぱり……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:80 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:80 
msgid "だ―る―ま―さ―ん―が―……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:81 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:81 
msgid "（よし、決めた。勇気を出して触ろう。\n　次の声をきっかけに弌弌）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:82 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:82 
msgid "あのさ、関口。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:83 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:83 
msgid "！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:84 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:84 
msgid "は、はい、なんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:85 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:85 
msgid "やりたくないなら無理して\n付き合わなくていいぞ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:86 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:86 
msgid "え？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:87 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:87 
msgid "ず―っと振り返るたびに難しい顔してるしさ。\nいや、こんな時間から悪かったよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:88 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:88 
msgid "いえ、それは……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:89 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:89 
msgid "でも、また気が向いたら遊ぼうぜ。\nそれじゃ、また後でな！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:90 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:90 
msgid "あ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0068:91 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0068:91 
msgid "……はぁ。\n"
msgstr ""
