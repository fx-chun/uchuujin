#  speaker 
#  warnings: 0 
#: sc/0075:0 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:0 
msgid "あ―あ、学校終わっちゃった。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:1 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:1 
msgid "う―ん、今日はいい事が\nありそうな予感あったのにな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:2 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:2 
msgid "結局、あったことは、また中之条くんが\n大変なことになってた事件だけだったもの。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:3 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:3 
msgid "……でも、あの時はすごく驚いちゃったけど、\nあれはちょっと面白かったかな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:4 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:4 
msgid "だって山みたいになってて、\nそのてっぺんにモヒカンが……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:5 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:5 
msgid "そうそう、こんな感じこんな感じ。\nあの時もここで山になった中之条くんが……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:6 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:6 
msgid "（ええ―――っ！！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:7 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:7 
msgid "あ、あわわっ！　どうしよっ、また！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:8 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:8 
msgid "きゃっ、カバンが！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:9 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:9 
msgid "どうしよう！\nさっきより磁力が凄いんですけど！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:10 
msgid "阪本さん"
msgstr "Sakamoto-san"

#  text 
#  warnings: 0 
#: sc/0075:10 
msgid "ぐおおおお――っ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:11 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:11 
msgid "きゃあ―っ、ね、猫さんが――っ！！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0075:12 
msgid "何ぃっ！？\nアンテナが引っ張られてるぞ！！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0075:13 
msgid "踏ん張らせろ！！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0075:14 
msgid "ダメだ、猫の体重じゃ負ける！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0075:15 
msgid "何だって！？　だから俺は\n大きな生き物にしろって言ったのに！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0075:16 
msgid "こんなもん想定してるか！\nくっ、もうダメだ！\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0075:17 
msgid "＆"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0075:17 
msgid "うわああああああ――――っ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:18 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:18 
msgid "も、もしかして、また中之条くんが……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:19 
msgid "高崎先生"
msgstr "Takasaki-sensei"

#  text 
#  warnings: 0 
#: sc/0075:19 
msgid "お―い！　誰か助けてくれ―！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:20 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:20 
msgid "（ええ―――っ！！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:21 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:21 
msgid "中之条くんじゃなくて高崎先生！？\nと、とにかく助けないとっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:22 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:22 
msgid "そうだよっ！　さっきみたいに\n機械を止める何かがあれば弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:23 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:23 
msgid "……あれ？　何にもないよ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:24 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:24 
msgid "ど、どうすればいいの！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:25 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:25 
msgid "助けを呼べる人もいないし、\n助ける方法もないし……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:26 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:26 
msgid "助けてくれ―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:27 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:27 
msgid "高崎先生！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:28 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:28 
msgid "その声は安中か！\n頼む！　誰か呼んできてくれ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:29 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:29 
msgid "え、え―っと、誰か、誰か……あっ、居た！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:30 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:30 
msgid "桜井せんせ弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:31 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:31 
msgid "ま、待て安中！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:32 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:32 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:33 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:33 
msgid "いいか、その、何だ……。\n桜井先生には、その……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:34 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:34 
msgid "教育者として同僚に、それも女性に\n助けて貰うのはいかがなものかと……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:35 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:35 
msgid "で、でも、先生がピンチですし……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:36 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:36 
msgid "いいや、こんなものは……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:37 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:37 
msgid "うおおおおぉっ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:38 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:38 
msgid "こうだぁ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:39 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:39 
msgid "（ええ―――っ！！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:40 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:40 
msgid "ふう……助かったぞ安中！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:41 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:41 
msgid "え、え？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:42 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:42 
msgid "お前が来てくれなかったらどうなってたか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:43 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:43 
msgid "もっと礼をしたいが、\nそろそろ職員会議が始まる時間だな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:44 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:44 
msgid "ありがとな安中！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:45 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:45 
msgid "……えっ、えっ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:46 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:46 
msgid "（ええ―――っ！！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:47 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:47 
msgid "先生、大丈夫ですか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:48 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:48 
msgid "安中か！　大丈夫なわけないだろう！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:49 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:49 
msgid "ですよね！\n身体のどこかに機械がついてませんか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:50 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:50 
msgid "機械……？　おおっ！\n袖のところについてる！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:51 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:51 
msgid "それを壊すかスイッチを切ってください！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:52 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:52 
msgid "なるほど、こうか！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:53 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:53 
msgid "おおっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:54 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:54 
msgid "やったぁ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:55 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:55 
msgid "助かったぞ安中！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:56 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:56 
msgid "先生が無事でよかったです。\n……ところで先生、どうしてあんな事に？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:57 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:57 
msgid "いや―、実は面白そうな機械が落ちててな。\n思わずスイッチを……ほら、こんな感じで。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:58 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:58 
msgid "ぬおおおお！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:59 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:59 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0075:60 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:60 
msgid "あ、あ、あ、安中っ！　助けてくれ―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:61 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:61 
msgid "……ダメだこりゃ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:62 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:62 
msgid "あっ、えっと、どうしよ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:63 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:63 
msgid "電源も見あたらないし、\n助けを求めにいく先生がそこにいるし……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:64 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:64 
msgid "あっ、アレってもしかして。\nやっぱり、リモコンだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:65 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:65 
msgid "これを使えば……先生、今助けますね！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:66 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:66 
msgid "ぬおおおおおっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:67 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:67 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:68 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:68 
msgid "え、えっとこっちのボタン？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:69 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:69 
msgid "うおおっ！　やめろっ、誰か！　誰か―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:70 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:70 
msgid "あっ、あれ？\nじゃ、じゃあ最後のコレ……！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:71 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:71 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:72 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:72 
msgid "あっ、あぁ……っ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:73 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:73 
msgid "た、助かったから……いい、よね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:74 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:74 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0075:75 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0075:75 
msgid "（逃げようっ！）\n"
msgstr ""
