#  speaker 
#  warnings: 0 
#: sc/0079:0 
msgid "中之条さん"
msgstr "Nakanojo-san"

#  text 
#  warnings: 0 
#: sc/0079:0 
msgid "（単純明快な欠点を指摘されてしまった。\n　そう、大福の唯一の欠点、それは地味さ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:1 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:1 
msgid "（ならばそれを補うのが私の使命！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:2 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:2 
msgid "（そのための大福くんだが……\n　それだけではまだ弱い）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:3 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:3 
msgid "（そこで毛利元就の故事にならおうと考えた）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:4 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:4 
msgid "弌弌そう、一本の矢は脆くとも、\nそれが三本であれば容易に折れなくなる。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:5 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:5 
msgid "すなわち！\n大福くん三人組！　トリオ・ザ・大福！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:6 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:6 
msgid "これなら、いつも以上の集客が望めるはず！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:7 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:7 
msgid "……いや、待てよ。\nむしろ、もっと増やすべきじゃないか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:8 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:8 
msgid "そうだ、後二人増やして\n大福レンジャ―というのはどうだろう！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:9 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:9 
msgid "……それだ！\nこれは早速実行するしかない！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:10 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:10 
msgid "ああ、そこの方、丁度いい所に！\n今の計画、聞いていただろう？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:11 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0079:11 
msgid "は、はい？　何のことですかな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:12 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:12 
msgid "大福文化存亡の危機なんだ、協力してくれ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0079:13 
msgid "な、何だこの地球人は！\nどうして強引にかぶせようとしているんだ！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0079:14 
msgid "マズいぞ、かぶりものなんて……\nそんなものをかぶってしまったら弌弌！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0079:15 
msgid "……くっ、逃げきれん！！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0079:16 
msgid "やっぱり折れた――っ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:17 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:17 
msgid "……な、何故だ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:18 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:18 
msgid "何故客足が落ちる！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:19 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:19 
msgid "（おかしい……おかしいぞ。\n　普段なら、かきいれ時じゃないか）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:20 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:20 
msgid "（なのに今日に限ってどうしてだ？\n　遠巻きに視線を感じるばかりで誰も来ない）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:21 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:21 
msgid "大福くんを三倍に増やしたというのに……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:22 
msgid "玉村"
msgstr "Tamamura"

#  text 
#  warnings: 0 
#: sc/0079:22 
msgid "あ、あの〜。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:23 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:23 
msgid "ん？\n"
msgstr "Hmm?\n"

#  speaker 
#  warnings: 0 
#: sc/0079:24 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:24 
msgid "（この格好はまさか！\n　大工カフェの店員か！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:25 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:25 
msgid "すいません、それなんですけど……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:26 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:26 
msgid "トリオ・ザ・大福かね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:27 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:27 
msgid "な、名前は何でもいいんですけど、\n営業妨害なので今すぐやめてもらえますか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:28 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:28 
msgid "！？\n"
msgstr "!?\n"

#  speaker 
#  warnings: 0 
#: sc/0079:29 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:29 
msgid "営業妨害とは何だ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:30 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:30 
msgid "だってうちのお客さんも減ってますし……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:31 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:31 
msgid "それはこっちの責任じゃない！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:32 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:32 
msgid "どう考えても関係ありますって。\nその三人組明らかに気持ち悪いですし。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:33 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:33 
msgid "な、なんて事を……！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:34 
msgid "大福くん（？？？）"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:34 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0079:35 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:35 
msgid "ほら見ろ！　豫ねてしまったじゃないか！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:36 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:36 
msgid "ええ―――っ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:37 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:37 
msgid "謝りなさい！　大福くんに謝るんだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:38 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:38 
msgid "え……えぇぇ〜……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:39 
msgid "大福くん（？？？）"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:39 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:40 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:40 
msgid "さあ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:41 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:41 
msgid "ゴ、ゴメンナサイ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:42 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:42 
msgid "よし！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:43 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:43 
msgid "営業妨害？　何のことかな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:44 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:44 
msgid "え、いや、その大福三人組が……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:45 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:45 
msgid "バカな、これはうちのマスコットキャラだ。\n大工カフェの邪魔をするはずないじゃないか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:46 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:46 
msgid "え、あ……ええと、それはそうですけど。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:47 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:47 
msgid "そうだ、せっかくだから\nお嬢さんも少し大福に触れていきなさい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:48 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:48 
msgid "ええっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:49 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:49 
msgid "ほら、こんな事もあろうかと\n大福くんの予備を用意してあるんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:50 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:50 
msgid "い、いえ、私はいいです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:51 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:51 
msgid "そう言わず遠慮なくかぶりなさい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:52 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:52 
msgid "え、ええ〜っ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:53 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:53 
msgid "……な、何でこんな事に。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:54 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:54 
msgid "うむ、よく似合っているじゃないか。\nこれからは大福カルテットで行こう！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:55 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:55 
msgid "さあ、今度こそじゃんじゃん売るとしよう！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:56 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:56 
msgid "……さ、更に客が減った……！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:57 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:57 
msgid "……そりゃそうですよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:58 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:58 
msgid "はっはっは、営業妨害なんて\nするはず無いじゃないか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:59 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:59 
msgid "どうやらお嬢さんは大福くんについて\n何か誤解してるようだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:60 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:60 
msgid "どれ、今から大福くんについて\n詳しく教えてさしあげよう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:61 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:61 
msgid "え……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:62 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:62 
msgid "さ、こっちへ来なさい。\n今から大福の歴史についてみっちりと。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:63 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:63 
msgid "ひっ、た、助けて！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:64 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:64 
msgid "助けてとは酷いじゃないか。\n私は大福について教えてあげようと……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:65 
msgid "警察"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:65 
msgid "おい！　何をしているんだ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:66 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:66 
msgid "！！！！\n"
msgstr "!!!!\n"

#  speaker 
#  warnings: 0 
#: sc/0079:67 
msgid "警察"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:67 
msgid "またアンタか！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:68 
msgid "警察"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:68 
msgid "大工カフェの店員さんを捕まえて\n何をしようとしていたんだ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:69 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:69 
msgid "い、いや、私はただ大福について……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:70 
msgid "警察"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:70 
msgid "詳しい話は署で聞こう。\nほら、さっさと来るんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0079:71 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0079:71 
msgid "（な、何故こうなる……）\n"
msgstr ""
