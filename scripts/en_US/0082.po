#  speaker 
#  warnings: 0 
#: sc/0082:0 
msgid "みお姉"
msgstr "Yoshino"

#  text 
#  warnings: 0 
#: sc/0082:0 
msgid "……あ、そう言えばこの辺だったっけ？\nあの子に会ったの。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:1 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:1 
msgid "また会いたいな……おもしろいから。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:2 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:2 
msgid "そろそろ学校も終わってる時間だから、\nちょっと探してみようっと。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:3 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:3 
msgid "う〜ん、いないな―。\nまだ学校にいるのかな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:4 
msgid "はかせ"
msgstr "Hakase"

#  text 
#  warnings: 0 
#: sc/0082:4 
msgid "ん？\n"
msgstr "Hmm?\n"

#  speaker 
#  warnings: 0 
#: sc/0082:5 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:5 
msgid "？？\n"
msgstr "??\n"

#  speaker 
#  warnings: 0 
#: sc/0082:6 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:6 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0082:7 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:7 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:8 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:8 
msgid "こんにちは。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:9 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:9 
msgid "え、あ、はい。こんにちは。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:10 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:10 
msgid "お嬢ちゃん、このへんの子？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:11 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:11 
msgid "はかせはお嬢ちゃんじゃないよ！\nはかせだよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:12 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:12 
msgid "？　あはは、おもしろい子。\nそっか、博士なんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:13 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:13 
msgid "あ、阪本だ―。どこ行ってたの？\nこのネコ、阪本って言うんだよ―。\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0082:14 
msgid "阪本さん"
msgstr "Sakamoto-san"

#  text 
#  warnings: 2 
#: sc/0082:14 
msgid "あ―、いつもいつも！\nいいかげん、さんをつけろっての！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:15 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:15 
msgid "へ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:16 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:16 
msgid "あ。\n"
msgstr "Ah.\n"

#  speaker 
#  warnings: 0 
#: sc/0082:17 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:17 
msgid "なになに―？\nこのネコ、喋れるの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:18 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:18 
msgid "そ―だよ。\nはかせが喋れる首輪を作ったからだよ。\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0082:19 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0082:19 
msgid "へ―そうなんだ―。\nすごいすごい。わ―かわい―\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:20 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:20 
msgid "ちょ、ちょ、ちょ……！\n撫ですぎ！　それ撫ですぎだ―！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0082:21 
msgid "あ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0082:22 
msgid "ん？　どうした？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0082:23 
msgid "撫でられすぎてアンテナとれちまった。\nこりゃ失敗失敗。あははははは。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0082:24 
msgid "ば、馬鹿！\n笑ってる場合じゃね―だろ！！\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0082:25 
msgid "＆"
msgstr "＆"

#  text 
#  warnings: 2 
#: sc/0082:25 
msgid "うわああああああ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:26 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:26 
msgid "……ところで、\nはかせはこのへんに住んでるの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:27 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:27 
msgid "そうだよ。そこの家。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:28 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:28 
msgid "そうなんだ―……あ、そうだ！\nあのね、お姉さん人を探してるの。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:29 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:29 
msgid "背中にネジが付いてて、手が取れる\n女の子を探してるんだけど、知らない？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:30 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:30 
msgid "知ってるよ！　なのでしょ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:31 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:31 
msgid "そう！　なのちゃん！　よく知ってるね―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:32 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:32 
msgid "いろいろ知ってるよ。\nだってなのを作ったのはかせだもん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:33 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:33 
msgid "え、そうなの！？\nちっちゃいのにすごいね〜。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:34 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:34 
msgid "えへへへ〜、はかせスゴイの。\nもっと褒めて褒めて〜。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:35 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:35 
msgid "よしよし、じゃあお姉さん、\nそんなはかせに良いものをあげよう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:36 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:36 
msgid "良いもの！？　なになに―？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:37 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:37 
msgid "じゃ―ん、手作りクッキ―で―す。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:38 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:38 
msgid "やった、お菓子だ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:39 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:39 
msgid "私の手作りなんだよ。\n良く味わって食べてね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:40 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:40 
msgid "ありがと―。\n"
msgstr "Thank you.\n"

#  speaker 
#  warnings: 0 
#: sc/0082:41 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:41 
msgid "それじゃはかせ、バイバイ。\n……って、そうだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:42 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:42 
msgid "なに？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:43 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:43 
msgid "あのさ、なのちゃん、\n今どこにいるのかわかんない？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:44 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:44 
msgid "うん、わかるよ。\nえ―と、レ―ダ―、レ―ダ―っと……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:45 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:45 
msgid "うん。\nまだ学校にいるみたい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:46 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:46 
msgid "そっか。じゃちょっと行ってみよ。\nありがとね―。\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0082:47 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0082:47 
msgid "バイバ―イ。\n……わ―い、クッキ―食べよ―\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:48 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:48 
msgid "もぐもぐもぐ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:49 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:49 
msgid "マズ―――――――！？\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0082:50 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0082:50 
msgid "他にも色作ってるよ。\n良かったら見ていく？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:51 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:51 
msgid "いいの？　見たい見たい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:52 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:52 
msgid "じゃあ、おね―さん、うちに来てくれる！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:53 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:53 
msgid "うん、ありがと―。\nそれじゃ、おじゃまします。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:54 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:54 
msgid "ただいま―！　……あ、そうだ。\n見たいテレビがあるんだった。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:55 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:55 
msgid "そうなんだ。\nじゃあ一緒に見よっか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:56 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:56 
msgid "うん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:57 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:57 
msgid "それでね、こうやって、メ―ン！　って。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:58 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:58 
msgid "わ―、すごいすごい、かっこい―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:59 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:59 
msgid "あ、そろそろ洗濯物しまわなきゃ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:60 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:60 
msgid "よ―し、それなら、\nおね―さんも手伝ってあげるね！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:61 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:61 
msgid "そういえば、ご飯とかどうしてるの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:62 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:62 
msgid "ご飯はいつもなのが作ってくれるよ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:63 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:63 
msgid "そっか……ね、お菓子作ったら食べる？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:64 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:64 
msgid "食べる！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:65 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:65 
msgid "わ―い、ケ―キできた―！\n食べよ、食べよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:66 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:66 
msgid "あ、私、もうそろそろ帰らなくちゃ。\n沢山あるから、なのちゃんと一緒に食べてね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:67 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:67 
msgid "え―、もう帰っちゃうの―？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:68 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:68 
msgid "ごめんね、また来るから。\nじゃ―ね―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:69 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:69 
msgid "うん、バイバ―イ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:70 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:70 
msgid "あれれっ？\nそういえば発明品見せてもらうの忘れてた！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:71 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:71 
msgid "……ま、いっか。\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0082:72 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0082:72 
msgid "なのは、ただのロボじゃないよ。\n色便利な機能が付いてるの！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:73 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:73 
msgid "ト―スタ―にもなるし、\nＴＲＡメモリもついてるし弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:74 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:74 
msgid "右手は豆鉄砲がついてるんだよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:75 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:75 
msgid "わ―、面白〜い。私は手が取れたとこしか\n見たこと無いんだ〜。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:76 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:76 
msgid "ねえねえ、他にはどんなことができるの？\n教えて教えて！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:77 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:77 
msgid "うん！　教えるよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:78 
msgid "ゆっこ"
msgstr "Yuuko"

#  text 
#  warnings: 0 
#: sc/0082:78 
msgid "あれ？　あれってはかせと……\nみおちゃんのお姉さん？　何してるんだろ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:79 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:79 
msgid "じゃあさ、じゃあさ、\n次は空を飛べるようにしてくれない？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:80 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:80 
msgid "あ、もう飛べるよ！\nダジャレとか言ったら、コケて飛んでくの。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:81 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:81 
msgid "わ―。じゃドリルは？\nドリルで地面掘って欲し―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:82 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:82 
msgid "それ、ナイスアイディア！\n今度つけとくね―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:83 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:83 
msgid "あとあと、\n合体とか変形はやっぱり外せないよね―。\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0082:84 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0082:84 
msgid "だね―。\nよ―し、今度、阪本と合体させちゃお\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:85 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:85 
msgid "そんで、巨大サメロボに変形するの！\n３０メ―トルぐら―い！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:86 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:86 
msgid "あははっ！\nそれもう最高―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:87 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:87 
msgid "よ―し、今晩なのが寝てる隙に\nさっそくやっちゃお―っと。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:88 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:88 
msgid "よろしくね―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:89 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:89 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0082:90 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0082:90 
msgid "（哀れ……なのちゃん……）\n"
msgstr ""
