#  speaker 
#  warnings: 0 
#: sc/0086:0 
msgid "なの"
msgstr "Nano"

#  text 
#  warnings: 0 
#: sc/0086:0 
msgid "あれ？　要……冷蔵？\nはかせ、ここにあるダンボ―ルは一体？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:1 
msgid "はかせ"
msgstr "Professor"

#  text 
#  warnings: 0 
#: sc/0086:1 
msgid "お届け物だって―。\nあっ、そこに置いたままだった。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:2 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:2 
msgid "そうだったんですか。\nいつ頃届いたんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:3 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:3 
msgid "え―っと、\nなのが学校行った後くらい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:4 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:4 
msgid "えぇっ！？　ダメじゃないですかはかせ！\n生ものなんだから冷蔵庫に入れましょうよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:5 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:5 
msgid "あっ！　お肉だって！\nなの―、はかせ、すきやき食べたい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:6 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:6 
msgid "そうですね―。\n腐ってなかったらすきやきもいいですね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:7 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:7 
msgid "……腐ってるの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:8 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:8 
msgid "と、とりあえず開けてみましょう。\nえっと……あれ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:9 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:9 
msgid "あっ！？\nなの、犬だ！　犬がいる！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:10 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:10 
msgid "わわわ、お肉の匂いに誘われて\nここまで来たんですよきっと！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:11 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:11 
msgid "シッシッ！　あっち行け〜！！\nお肉食べちゃダメ〜！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0086:12 
msgid "アンテナの対象者が\n激しい衝撃を受けています！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0086:13 
msgid "このままではアンテナが破損！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0086:14 
msgid "……あ、壊れた。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0086:15 
msgid "激しい衝撃を受けました――っ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:16 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:16 
msgid "はかせ〜、ハサミかカッタ―知りませんか？\n箱のガムテ―プを切りたいんですけど。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:17 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:17 
msgid "えっとね―、それは確かこの辺に……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:18 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:18 
msgid "あっ、あった。このボタンを弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:19 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:19 
msgid "ちょっ！？\nなんてモノを仕込んでるんですか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:20 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:20 
msgid "高枝切りバサミ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:21 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:21 
msgid "それは見ればわかります！　どうしてコレを\n私につけたのかを聞いているんです！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:22 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:22 
msgid "カッコイイから！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:23 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:23 
msgid "どこがですか！\n全然かっこよくありませんよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:24 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:24 
msgid "大体、どうして高枝を切る方なんですか！？\nこれじゃ枝は切れても他の物は切れませんよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:25 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:25 
msgid "じゃあ枝を切ればいいじゃない！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:26 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:26 
msgid "（えええ――！！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:27 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:27 
msgid "私は枝じゃなくて\nこの箱のガムテ―プを切りたいんです！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:28 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:28 
msgid "なのに両手が高枝切りバサミで……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:29 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:29 
msgid "あはははは！\nなの、変な動きしてる！　あははは！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:30 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:30 
msgid "！？\n"
msgstr "!?\n"

#  speaker 
#  warnings: 0 
#: sc/0086:31 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:31 
msgid "もう！\nお肉が腐ってたら、はかせのせいですからね！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:32 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:32 
msgid "……………。\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0086:33 
msgid "なの"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0086:33 
msgid "！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:34 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:34 
msgid "……う、うそですよ。お肉は腐ってません。\n大丈夫ですから。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:35 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:35 
msgid "……………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:36 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:36 
msgid "はかせっ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:37 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:37 
msgid "なの――！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:38 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:38 
msgid "（あ、はかせ、ちょっと怒ってる……\n　機嫌を損ねたら直してもらえないし……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:39 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:39 
msgid "そ、そう言えば庭の木のお手入れをしたいと\n思ってたんですよね〜。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:40 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:40 
msgid "そうなると、手が高枝切りバサミなのも\n悪くは無いかもしれませんね〜。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:41 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:41 
msgid "……本当？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:42 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:42 
msgid "は、はい。本当ですよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:43 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:43 
msgid "じゃあ、枝切って。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:44 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:44 
msgid "えっ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:45 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:45 
msgid "お庭の木の枝を切って。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:46 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:46 
msgid "（ま、まずい……ここで枝を切ったら\n　今度は更に武装強化されそうな気が……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:47 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:47 
msgid "で、でもハシゴありませんし、\n私の身長じゃ高い枝までは届きません……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:48 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:48 
msgid "大丈夫！\nはかせに任せて！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:49 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:49 
msgid "まさか！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:50 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:50 
msgid "（もしかして腕が伸びるような改造を\n　すでにされているんじゃあ……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:51 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:51 
msgid "ほら！　腕が飛んでいくから\n高いところの枝も簡単に切れるよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:52 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:52 
msgid "……………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:53 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:53 
msgid "……………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:54 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:54 
msgid "全てが……遅すぎた……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:55 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:55 
msgid "直してくださいよはかせ〜。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:56 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:56 
msgid "やだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:57 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:57 
msgid "……わかりました。枝を切ってきます。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:58 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:58 
msgid "でも手が高枝切りバサミになったから\nご飯はもう作れません。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:59 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:59 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:60 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:60 
msgid "……じゃあ、直す。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:61 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:61 
msgid "全く……後でちゃんと直してくださいよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:62 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:62 
msgid "とりあえず、時間も時間ですし\nご飯の準備しますから。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:63 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:63 
msgid "わ―い！　すきやき―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:64 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:64 
msgid "仕方ないからガムテ―プは手ではがして……。\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0086:65 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0086:65 
msgid "お肉―\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:66 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:66 
msgid "……………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:67 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:67 
msgid "……腐ってます。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0086:68 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0086:68 
msgid "……………。\n"
msgstr ""
