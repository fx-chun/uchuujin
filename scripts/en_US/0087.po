#  speaker 
#  warnings: 0 
#: sc/0087:0 
msgid "ゆっこ"
msgstr "Yuuko"

#  text 
#  warnings: 0 
#: sc/0087:0 
msgid "う―ん……。\n"
msgstr " "

#  speaker 
#  warnings: 0 
#: sc/0087:1 
msgid "みお"
msgstr "Mio"

#  text 
#  warnings: 0 
#: sc/0087:1 
msgid "どしたのゆっこ？\n"
msgstr "What is it Yuuko?\n"

#  speaker 
#  warnings: 0 
#: sc/0087:2 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:2 
msgid "みおちゃん、コレ、何？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:3 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:3 
msgid "コレ？　……ああ、桜井先生の絵ね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:4 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:4 
msgid "この絵に振り回されてテストの点が\nいつもより低くてさ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:5 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:5 
msgid "よくいるよね、コレ。\n豆テストのマスコットキャラだね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:6 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:6 
msgid "マスコットキャラなんて\n可愛い物じゃないでしょ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:7 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:7 
msgid "そう？　謎の生物だけど\n可愛いと思うけどな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:8 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:8 
msgid "可愛く見えても実は亠猛なモンスタ―、\nもしくは宇宙人のどっちかかもよ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:9 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:9 
msgid "宇宙人って。それはないでしょ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:10 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:10 
msgid "わからないよ。宇宙人の改造技術があれば\n外見なんて自由自在だって。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:11 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:11 
msgid "ま、確かに。本当に宇宙人がいるのなら\nそれくらいの技術は持ってそうだね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:12 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:12 
msgid "実はこのキャラは宇宙人にさらわれた\n地球人の成れの果てなんだって。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:13 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:13 
msgid "そんで改造された人は頭の中に何かを\n埋め込まれたり、頭に何か刺さってたり。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:14 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:14 
msgid "あはは、そんなバカな弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:15 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:15 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0087:16 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:16 
msgid "ん？　誰かいる？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:17 
msgid "阪本さん"
msgstr "Sakamoto-san"

#  text 
#  warnings: 0 
#: sc/0087:17 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0087:18 
msgid "ゆっこ＆みお"
msgstr "Yukko & Mio"

#  text 
#  warnings: 1 
#: sc/0087:18 
msgid "！！！\n"
msgstr "!!!\n"

#  speaker 
#  warnings: 0 
#: sc/0087:19 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:19 
msgid "宇宙人だ――――！！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0087:20 
msgid "あっ、捕まった。\nもう逃げられないな、これ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0087:21 
msgid "だな。ところで俺、未だにあのアンテナの\n刺さり方に疑問があるんだ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0087:22 
msgid "だな。ブッスリ刺さってるし。\n痛くないのかな？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0087:23 
msgid "アンテナが壊れた時とかも\n体に影響がなければいいけど……。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0087:24 
msgid "それは大丈夫じゃないか？\n今まで何の影響もなさそうだったし。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0087:25 
msgid "それもそうだな。\nでも、もし影響あったら寝覚め悪いな―。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0087:26 
msgid "全くだ。きっと体に影響を与えないため、\nこっちにダメ―ジが全部来るんだよ。ほら。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0087:27 
msgid "なるほど、そう言うことか！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:28 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:28 
msgid "また悪いことするね―、宇宙人も。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:29 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:29 
msgid "きっと見た目も悪いよね。\n絶対に人型じゃなさそう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:30 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:30 
msgid "きっとこんな、ぐわ―って感じので、\n鳴き声はキシャ―って感じ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:31 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:31 
msgid "あはは、どんなのだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:32 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:32 
msgid "あっ、じゃあ絵描くよ。\nえっとね―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:33 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:33 
msgid "絵っ？\nあ……うん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:34 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:34 
msgid "（また変な絵が出てくるんじゃ……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:35 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:35 
msgid "いくよ、みおちゃん。\n私が考えた宇宙人は〜……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:36 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:36 
msgid "こんな感じ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:37 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:37 
msgid "死んでるじゃん！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:38 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:38 
msgid "この輪っかは死んでるんじゃないよ。\n天使的なアレだよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:39 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:39 
msgid "じゃあ天使じゃん！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:40 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:40 
msgid "違うって。宇宙人だって。\n天使って言ったら、こういうのでしょ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:41 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:41 
msgid "（羽が生えた――！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:42 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:42 
msgid "お―……これは結構\n良くできてるんじゃない？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:43 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:43 
msgid "ま、まあ、ゆっこにしては\n上出来だと思うけど……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:44 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:44 
msgid "だよねだよね？　よ―っし！\nじゃあ次の豆テストにこの絵を出してもらお！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:45 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:45 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:46 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:46 
msgid "（この絵に翻弄されて\n　また点数が悪くなるに１ペソ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:47 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:47 
msgid "じゃあさじゃあさ、\nみおちゃんの想像する宇宙人はどんな感じ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:48 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:48 
msgid "ん？　そうだな―。\n宇宙人だから普通じゃないだろうし……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:49 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:49 
msgid "こんな感じ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:50 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:50 
msgid "っぽい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:51 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:51 
msgid "でしょ？　宇宙人だから\nこんな姿をしててもおかしくないよね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:52 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:52 
msgid "うんうん。この怪しさなら\n人間をさらって改造してそう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:53 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:53 
msgid "でもちょっと可愛いく描きすぎかな―。\n宇宙人だからもっと怖さを出さないと。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:54 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:54 
msgid "怖さ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:55 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:55 
msgid "うん。本物はもっと凶暴で、人間をさらったら\n改造する前に食べちゃうような弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:56 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:56 
msgid "うわっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:57 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:57 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:58 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:58 
msgid "な、何、今の光……\n雷にしては何の音もなかったし。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:59 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:59 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:60 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:60 
msgid "ゆっこ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:61 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:61 
msgid "う……うちゅ、宇宙人は……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:62 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:62 
msgid "宇宙人？\nあはは、今のが宇宙人の仕業って？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:63 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:63 
msgid "ウチュウジン、ソンナコトシナイ。\nウチュウジン、トテモヤサシイ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:64 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:64 
msgid "！？\n"
msgstr "!?\n"

#  speaker 
#  warnings: 0 
#: sc/0087:65 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:65 
msgid "ウチュウジン、ヒトタベナイ。\nサイショクシュギ。ベジタリアン。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:66 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:66 
msgid "ゆ、ゆっこ……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:67 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:67 
msgid "ウチュウジン、カイゾウシナイ。\nウチュウジン、トテモジンドウテキ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:68 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:68 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:69 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:69 
msgid "改造されてる……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:70 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:70 
msgid "う―ん、ちょっと言葉では\n表しにくいと言うか……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:71 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:71 
msgid "そうだ、絵を描けばわかってくれるよね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:72 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:72 
msgid "まあ、そうだけど……描ける？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:73 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:73 
msgid "任せなさいって。みお先生ほどじゃないけど\n私だって絵の１つや２つくらい描けるよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:74 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:74 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:75 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:75 
msgid "こうやってパパ―っと……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:76 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:76 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:77 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:77 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:78 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:78 
msgid "え、えっと、こう？\nいや、もっとシャ―プな感じで……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:79 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:79 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:80 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:80 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:81 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:81 
msgid "…………。\n"
msgstr "………….\n"

#  speaker 
#  warnings: 0 
#: sc/0087:82 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:82 
msgid "くそぅ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:83 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0087:83 
msgid "……がんばった。\nがんばったよゆっこは。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0087:84 
msgid "ゆっこ"
msgstr "Yukko"

#  text 
#  warnings: 0 
#: sc/0087:84 
msgid "描けない……。\n"
msgstr ""
