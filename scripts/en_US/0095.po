#  speaker 
#  warnings: 1 
#: sc/0095:0 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0095:0 
msgid "〜\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:1 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:1 
msgid "きゃっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:2 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:2 
msgid "いったた……\nちょっと、どこ見て歩いてるの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:3 
msgid "中之条"
msgstr "Nakanojo"

#  text 
#  warnings: 0 
#: sc/0095:3 
msgid "あ、す、すいません！\n大丈夫ですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:4 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:4 
msgid "（あ……このモヒカン……\n　確か、ウェボシ―の好きな……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:5 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:5 
msgid "ホントすいません。\n立てますか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:6 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:6 
msgid "あ、ありが……と……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:7 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:7 
msgid "怪我は……ないみたいですね。良かった。\nあ〜教科書が散らばって……すぐ拾います！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:8 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:8 
msgid "う、うん……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:9 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:9 
msgid "（な、なによこいつ！？\n　モヒカンな不良のくせに優しいじゃない）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:10 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:10 
msgid "ふう、これでよし、と。\nはい、どうぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:11 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:11 
msgid "あ、うん、ありがと……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:12 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:12 
msgid "（あ、でも前にフェっちゃんが\n　花壇に水やってたって言ってたっけ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:13 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:13 
msgid "？　なんですか？\n僕の顔に何かついてますか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:14 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:14 
msgid "（こんな髪型してるけど、もしかしてこの人、\n　普通に良いヤツだったり……？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:15 
msgid "ウェボシ―"
msgstr "Weboshi"

#  text 
#  warnings: 0 
#: sc/0095:15 
msgid "みさと？　何してんの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:16 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:16 
msgid "！？　ウェボシ―！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:17 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:17 
msgid "あれ、そこにいるのは……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:18 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:18 
msgid "あっ、違うの！\nこれは違うのよ―っ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:19 
msgid "バディ"
msgstr "Buddy."

#  text 
#  warnings: 0 
#: sc/0095:19 
msgid "ッ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:20 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:20 
msgid "あっ！？\nご、ごめんねワンちゃん！　じゃあね―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:21 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:21 
msgid "……なんなのよ、みさとったら。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0095:22 
msgid "おっと！　犬は大丈夫か？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0095:23 
msgid "ああ、犬なら、ほんのちょっとかすっただけさ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0095:24 
msgid "……なあ？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0095:25 
msgid "ん？　なんだ？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0095:26 
msgid "ちょっと思ったんだけどよ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0095:27 
msgid "だから、何をだよ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0095:28 
msgid "アンテナ、弱すぎね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:29 
msgid "笹原"
msgstr "Sasahara"

#  text 
#  warnings: 0 
#: sc/0095:29 
msgid "ん？　立花みさとではないか。\nこんなところで何をしている？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:30 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:30 
msgid "さ、さ、さ、さ、笹原！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:31 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:31 
msgid "む？　そこの者は立花みさとの知り合いか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:32 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:32 
msgid "あ、僕は弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:33 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:33 
msgid "！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:34 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:34 
msgid "！？\n"
msgstr "!?\n"

#  speaker 
#  warnings: 0 
#: sc/0095:35 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:35 
msgid "ち、違うの笹原！！\n何でもないのよ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:36 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:36 
msgid "……立花みさとの知己ならば、私にとっても\n竹馬の友だ。ぜひ紹介してほしかったのだが。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:37 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:37 
msgid "だから、ホントに何でもないんだってば！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:38 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:38 
msgid "そうか……なら良いのだが、\n先程から何を恥ずかしがっている？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:39 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:39 
msgid "べ、別に……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:40 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:40 
msgid "……む、いや、これは失敬。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:41 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:41 
msgid "えっ？\n"
msgstr "Huh?\n"

#  speaker 
#  warnings: 0 
#: sc/0095:42 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:42 
msgid "そうか、私に紹介するのが恥ずかしいほど\n抜き差しならない間柄なのだな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:43 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:43 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0095:44 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0095:44 
msgid "気が利かずすまなかったな。\nでは、邪魔者は早に立ち去るとしよう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:45 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:45 
msgid "な、な、な……！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:46 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:46 
msgid "……立花みさとよ、私が一体何をした。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:47 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:47 
msgid "なんですぐに諦めようとするのよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:48 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:48 
msgid "…………。\n"
msgstr "………….\n"

#  speaker 
#  warnings: 1 
#: sc/0095:49 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0095:49 
msgid "怒るとか、慌てるとか、嫉妬するとか、\n色やることはあるでしょ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:50 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:50 
msgid "なによ！　いつもは\n人に迷惑ばかりかけてるくせに！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:51 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:51 
msgid "なんでこんな時に限って気ぃ利かせたり\nすんのよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:52 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:52 
msgid "……良くわからんが、\n気を利かせたのがまずかったのか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:53 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:53 
msgid "では気の利かないことを……\nといっても難しいな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:54 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:54 
msgid "ふむ、これはなかなかに深い問題だ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:55 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:55 
msgid "あえて空気を読まぬ態度というのは\nどのようなものだろう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:56 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:56 
msgid "だから、怒ったり、慌てたり、嫉妬弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:57 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:57 
msgid "〜〜〜〜〜〜〜〜〜〜〜！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:58 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:58 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:59 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:59 
msgid "なによ！　私は別に、あんたがどう思ってるか\nなんて気にしないんだからね！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:60 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:60 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:61 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:61 
msgid "……待つのだ立花みさ弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:62 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:62 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:63 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:63 
msgid "……はて、\nところで件の１年は何処へ行った？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:64 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:64 
msgid "え？　あ、あれ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:65 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:65 
msgid "高崎先生―！\n中之条くんが２階から飛び降りました―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:66 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:66 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:67 
msgid "高崎先生"
msgstr "Takasaki-sensei"

#  text 
#  warnings: 0 
#: sc/0095:67 
msgid "中之条は飛び降りてなどいない！\nそうだ、スカイダイビングかもしれん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:68 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:68 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:69 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:69 
msgid "逃げなきゃ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:70 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:70 
msgid "いやいや立花みさとよ。\nそれは良くないであろう。\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0095:71 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0095:71 
msgid "きちんと現場へはせ参じ、\n堂と釈明するべきだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:72 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:72 
msgid "私にも責任の一端はあるからな。\n一緒について行ってやる。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:73 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:73 
msgid "笹原……うぅ……\nあ、ありがとうなんて言わないんだからね！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:74 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:74 
msgid "はっはっは。さて行こうか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:75 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:75 
msgid "この者は自分で飛び降りた。\n私と立花みさとが証人だ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:76 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:76 
msgid "（ええええ〜〜〜〜！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:77 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:77 
msgid "……なにか用事でも？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:78 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:78 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:79 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:79 
msgid "！？\nそれはどういう意味なのだ、立花みさ弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:80 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:80 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:81 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:81 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:82 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:82 
msgid "ふ、さすがは立花みさと。\nクマの被り物で攻撃とは、なかなか弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:83 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:83 
msgid "なか……なか……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:84 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:84 
msgid "………………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0095:85 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:85 
msgid "まあ良い。\nそれでは私は、これにて失敬弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:86 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:86 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:87 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:87 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:88 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:88 
msgid "とりあえず、立花みさとよ。\nその頭のクマは外してもらえないだろうか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:89 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:89 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0095:90 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0095:90 
msgid "……世の中、ままならぬ。\n"
msgstr ""
