#: sc/0103:65 
msgid "…………。\n"
msgstr "………….\n"

#  speaker 
#  warnings: 0 
#: sc/0103:66 
msgid "中之条"
msgstr "Nakanojou"

#  text 
#  warnings: 0 
#: sc/0103:66 
msgid "（さ、さっぱりだ……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0103:67 
msgid "桜井先生"
msgstr "Sakurai-sensei"

#  text 
#  warnings: 0 
#: sc/0103:67 
msgid "…………？？\n"
msgstr "…………??\n"

#  speaker 
#  warnings: 0 
#: sc/0103:68 
msgid "高崎先生"
msgstr "Takasaki-sensei"

#  text 
#  warnings: 0 
#: sc/0103:68 
msgid "…………！！\n"
msgstr "………！！\n"

#  speaker 
#  warnings: 0 
#: sc/0103:69 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0103:69 
msgid "酢豚のパイナップル……\nそ、それはつまり……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0103:70 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0103:70 
msgid "嫌いな奴は圧倒的に多いが\n私は好きだと言う告白か！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0103:71 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0103:71 
msgid "桜井先生はモヒカンが好み……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0103:72 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0103:72 
msgid "はぁ、はぁ……俺もモヒカンに……！？\n"
msgstr ""
