#  speaker 
#  warnings: 0 
#: sc/0105:0 
msgid "高崎先生"
msgstr "Takasaki-sensei"

#  text 
#  warnings: 0 
#: sc/0105:0 
msgid "よ―し、授業はここまで。\n残り時間でこの間の小テストを返却するぞ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:1 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:1 
msgid "まずは相生―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:2 
msgid "ゆっこ"
msgstr "Yuuko"

#  text 
#  warnings: 0 
#: sc/0105:2 
msgid "は―い。\n"
msgstr "Okay.\n"

#  speaker 
#  warnings: 0 
#: sc/0105:3 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:3 
msgid "（……相変わらず相生のテストはひどいな）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:4 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:4 
msgid "（真面目にやろうとした形跡はあるんだが、\n　……いや、そうでもないか）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:5 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:5 
msgid "（特に漢字の読み問題。\n　何で途中から詩にすりかわっているんだ？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:6 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:6 
msgid "（文章問題はオヤジギャグになってるし……。\n　本当にひどいありさまだ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:7 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:7 
msgid "……むっ！　猫？\n"
msgstr "……Hmm! A cat?\n"

#  speaker 
#  warnings: 0 
#: sc/0105:8 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:8 
msgid "迷い込んだのか？\n何にせよ、授業の邪魔だな。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0105:9 
msgid "あっ、おい、見つかったぞ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0105:10 
msgid "安心しろって、こんな時のために\n姿を隠す装置を搭載してる。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0105:11 
msgid "おおっ、で、どのボタンだ？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0105:12 
msgid "そこの赤くてデカいボタン弌弌\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0105:13 
msgid "これか。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0105:14 
msgid "弌弌の隣にある小さいボタン。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0105:15 
msgid "……紛らわしいよ！\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0105:16 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0105:16 
msgid "（しかし……少し視点を変えると、\n　中面白いかもしれない）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:17 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:17 
msgid "（真面目にやってこの回答が出せるというのは\n　思考の幅が広い証拠じゃないだろうか？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:18 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:18 
msgid "（頭ごなしに何でも叱るのが教育じゃない。\n　時には褒めてやるのも大切なはず……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:19 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:19 
msgid "（しかし、テストの点を思えば……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:20 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:20 
msgid "（いや、ここはきちんと叱るべきだ！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:21 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:21 
msgid "せんせ―、取りに来ましたっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:22 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:22 
msgid "相生っ！\nこの回答は何だ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:23 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:23 
msgid "ええっ、私なんか変な……あ……。\nいや、そのポエムは……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:24 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:24 
msgid "特にこの部分！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:25 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:25 
msgid "衝撃\n一目見て受けたインパクト！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:26 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:26 
msgid "ノォォォ―ウ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:27 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:27 
msgid "他にもだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:28 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:28 
msgid "永久\n私の想いはエタ―ナル！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:29 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:29 
msgid "せ、せせせせせんせぇ―っ！？\nよ、よ、読み上げるのはちょっと……！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:30 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:30 
msgid "そんな事を言ってる場合か！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:31 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:31 
msgid "いいか相生ッ！！\n国語のテストでカタカナ語を使うんじゃない！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:32 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:32 
msgid "ツッコむトコそこ―――――！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:33 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:33 
msgid "（そうだ、きちんと褒めてやろう。\n　この詩はいい詩じゃないか）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:34 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:34 
msgid "せんせ―、取りに来ましたっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:35 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:35 
msgid "あのな、相生……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:36 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:36 
msgid "何ですか？\nあ、もしかしていつもより点数良かったとか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:37 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:37 
msgid "ナイスポエム！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:38 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:38 
msgid "！？\n"
msgstr "!?\n"

#  speaker 
#  warnings: 0 
#: sc/0105:39 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:39 
msgid "出題した漢字を全て使った上で\nしっかりとしたスト―リ―がある！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:40 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:40 
msgid "え、ええええっ！？\n"
msgstr "Eh, ehhhhhhh!?\n"

#  speaker 
#  warnings: 0 
#: sc/0105:41 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:41 
msgid "これだけのポエムが作れるなら、\n詩人としてやっていけるかもしれん！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:42 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:42 
msgid "マ、マジですか！\nそんなに良かったですか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:43 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:43 
msgid "ああ、すばらしいポエムだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:44 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:44 
msgid "高崎先生……。\n"
msgstr "Takasaki-sensei…….\n"

#  speaker 
#  warnings: 0 
#: sc/0105:45 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:45 
msgid "たとえテストは再試験でもな！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:46 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:46 
msgid "！！！！！！！\n"
msgstr "！！！！！！！！\n"

#  speaker 
#  warnings: 0 
#: sc/0105:47 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:47 
msgid "（いや、今回は何も言わないでやろう。\n　結果が結果だ、本人も見て反省するさ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:48 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:48 
msgid "せんせ―、取りに来ましたよ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:49 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:49 
msgid "いや―、今回は結構自信あるんですよ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:50 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:50 
msgid "じ、自信が……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:51 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:51 
msgid "特に漢字の問題なんて\nいいカンジにできてますし！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:52 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:52 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0105:53 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:53 
msgid "あ、でも動詞の活用の所はダメかも。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:54 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:54 
msgid "なんせ考えてる間ず―っと\nドウシよう、なんて思ってましたから！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:55 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:55 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:56 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:56 
msgid "！？\n"
msgstr "!?\n"

#  speaker 
#  warnings: 0 
#: sc/0105:57 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:57 
msgid "え―っと、え―っと……そうだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:58 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:58 
msgid "文章問題もちょっと不安なんですよね。\nでもそっちはあきらめてるんですよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:59 
msgid "ゆっこ"
msgstr "Yukko"

#  text 
#  warnings: 0 
#: sc/0105:59 
msgid "文章だけに、たぶん、しょ―がない！\nな―んて……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0105:60 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0105:60 
msgid "…………。\n"
msgstr "………….\n"

#  speaker 
#  warnings: 0 
#: sc/0105:61 
msgid "高崎先生"
msgstr "Mr. Takasaki"

#  text 
#  warnings: 0 
#: sc/0105:61 
msgid "相生―、１８点―。\n"
msgstr ""
