#  speaker 
#  warnings: 0 
#: sc/0109:0 
msgid "はかせ"
msgstr "Hakase"

#  text 
#  warnings: 0 
#: sc/0109:0 
msgid "う―、見つからないな―……\nこっちに来たと思ったんだけど……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:1 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:1 
msgid "気のせいだったのかなぁ。\n鹿がいるわけないもんね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:2 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:2 
msgid "帰ってマンガの続き読もっと。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:3 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:3 
msgid "あっ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:4 
msgid "ゆっこ"
msgstr "Yuuko"

#  text 
#  warnings: 0 
#: sc/0109:4 
msgid "ん？　おお、なのちゃんちのちっこいの。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:5 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:5 
msgid "はかせだよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:6 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:6 
msgid "そうそう、はかせだ。\n私は覚えてる？　ゆっこだよ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:7 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:7 
msgid "うん、ゆっこ！\nゆっこのうちもこの辺なの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:8 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:8 
msgid "ううん。暇だったから\nなんとなくこっちに来てみただけ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:9 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:9 
msgid "じゃあうちで遊んでいかない？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:10 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:10 
msgid "お、いいね。\nじゃあ遊びに行こうかな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:11 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:11 
msgid "やった―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:12 
msgid "バディ"
msgstr "Buddy"

#  text 
#  warnings: 0 
#: sc/0109:12 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0109:13 
msgid "はかせ＆ゆっこ"
msgstr "Professor & Yuuko"

#  text 
#  warnings: 0 
#: sc/0109:13 
msgid "！？\n"
msgstr "!?\n"

#  speaker 
#  warnings: 0 
#: sc/0109:14 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:14 
msgid "ゆ、ゆっこ……犬……退治できる？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:15 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:15 
msgid "う、うう……厳しい……\nでも、二人一緒なら……！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:16 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:16 
msgid "わ、わかった、二人一緒に！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:17 
msgid "はかせ＆ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:17 
msgid "うわああああ！！！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0109:18 
msgid "…………。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0109:19 
msgid "……地球の女の子は犬を見たら\n戦う習性でもあるのか？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0109:20 
msgid "……あるんだろう。きっと。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:21 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:21 
msgid "そうだはかせ。\nなのちゃんはもう家に帰ってるの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:22 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:22 
msgid "ううん、なのはまだ弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:23 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:23 
msgid "（っとと。ゆっこはなのと仲良いんだった）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:24 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:24 
msgid "（なのになのがいないなんて知ったら\n　遊びに来てくれないかもしれない）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:25 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:25 
msgid "（はかせはまたゆっこと遊びたいし、\n　本当のことは言っちゃダメかも）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:26 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:26 
msgid "あれ？　なのちゃんはまだ帰ってないの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:27 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:27 
msgid "い、いるよ！\nちゃんと、うちにいるよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:28 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:28 
msgid "そうなんだ。じゃあ、はかせとなのちゃんに\n私がマジックを見せてあげる。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:29 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:29 
msgid "マジック！？　できるの！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:30 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:30 
msgid "ま、かじった程度だけどね。\n最近やるようになってさ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:31 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:31 
msgid "すごいすごい！\n見せて見せて！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:32 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:32 
msgid "それは後のお楽しみってね。\nなのちゃん待たせても悪いし、早く行こう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:33 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:33 
msgid "あっ、で、でも、なのはいいの！\nなのは見られないの！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:34 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:34 
msgid "えっ？\n"
msgstr "Eh…?\n"

#  speaker 
#  warnings: 0 
#: sc/0109:35 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:35 
msgid "なの、今メンテナンスしてるから！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:36 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:36 
msgid "メンテナンス！？　マジ！？\nやっぱりするんだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:37 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:37 
msgid "そう！　だからなのはちょっと\n起きられないから、見られないの！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:38 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:38 
msgid "そっか―。\nすぐには終わりそうにないんだ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:39 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:39 
msgid "うん！　メンテナンスは時間かかるの！\n明日までかかっちゃうの！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:40 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:40 
msgid "じゃあ邪魔しても悪いし、\n遊びに行くのはまた今度にするよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:41 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:41 
msgid "（あれ―――！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:42 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:42 
msgid "じゃあね―はかせ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:43 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:43 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:44 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:44 
msgid "あのね、なのはね、帰ってきてたんだよ？\nでもね、今は夕飯の準備してるからいないの。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:45 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:45 
msgid "へ―。なのちゃんがやってるんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:46 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:46 
msgid "そ、そう。でもちょっと時間がかかるから\n帰ってくるのは遅くなるかも。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:47 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:47 
msgid "それなら買い物手伝おうか？\n二人なら早く終わるでしょ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:48 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:48 
msgid "う、うん……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:49 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:49 
msgid "（ど、どうしよう。本当は買い物なんて\n　行かせてないし……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:50 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:50 
msgid "なのちゃん、今どこかな？\n連絡取れたりする？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:51 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:51 
msgid "と、取れるけど、電波届かないかも！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:52 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:52 
msgid "えっ！？\nなのちゃんどこに行ってるの！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:53 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:53 
msgid "……あっ、地下だからか。\nふ―、密林や樹海に行ってるかと思った。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:54 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:54 
msgid "行ってるよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:55 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:55 
msgid "えええっ！？\n"
msgstr "Ehh!?\n"

#  speaker 
#  warnings: 0 
#: sc/0109:56 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:56 
msgid "だ、だからなのには連絡取れないし、\n帰ってくるのも遅くなるの。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:57 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:57 
msgid "夕飯のためにそんなところ行くなんて……\nはかせの家、大変なんだね……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:58 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:58 
msgid "う、うん、大変なの。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:59 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:59 
msgid "なのちゃん帰ってくるまで\nずっと遊んであげるから。大人しく待とうね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:60 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:60 
msgid "うん？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:61 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:61 
msgid "世知辛い世の中になったんだなぁ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:62 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:62 
msgid "えっと、えっと……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:63 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:63 
msgid "どうしたの？\n家にはいないの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:64 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:64 
msgid "……うん。今は外に出てるの。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:65 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:65 
msgid "そっか。じゃあ家で待ってようか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:66 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:66 
msgid "えっ！？\nうちに来てくれるの！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:67 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:67 
msgid "うん？　だってさっきそう言ったじゃん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:68 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:68 
msgid "でもでも、なのがいないから……\nゆっこが来てくれないと思って……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:69 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:69 
msgid "あ―、だから何か言い訳を考えてたんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:70 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:70 
msgid "うん……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:71 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:71 
msgid "……あっ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:72 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:72 
msgid "はかせはかせ、\n言い訳してもいいわけよ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:73 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:73 
msgid "うん……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:74 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:74 
msgid "…………。\n"
msgstr "………….\n"

#  speaker 
#  warnings: 0 
#: sc/0109:75 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:75 
msgid "ん？　どうしたの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:76 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:76 
msgid "い、いや、言い訳してもいいわけよ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:77 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:77 
msgid "うん？　それはさっき聞いたよ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:78 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:78 
msgid "あ、あの、わからない？　言い訳と、\nいいんだよをかけていいわけ―って……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:79 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:79 
msgid "それで？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:80 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:80 
msgid "は、はは、ははは……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:81 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:81 
msgid "？？\n"
msgstr "??\n"

#  speaker 
#  warnings: 0 
#: sc/0109:82 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:82 
msgid "あの……お腹痛いから帰っていい？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0109:83 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0109:83 
msgid "！？\n"
msgstr ""
