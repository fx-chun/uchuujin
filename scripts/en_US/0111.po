msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:69 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:69 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:70 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:70 
msgid "……あ、警察ですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:71 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:71 
msgid "商店街に、変質者が……\n女性を尾行してます。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:72 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:72 
