msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:1 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:1 
msgid "……あれ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:2 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:2 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0111:3 
msgid "高崎先生"
msgstr "Takasaki-sensei"

#  text 
#  warnings: 0 
#: sc/0111:3 
msgid "！？\n"
msgstr "!?\n"

#  speaker 
#  warnings: 0 
#: sc/0111:4 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:4 
msgid "……壊れた？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:5 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:5 
msgid "あっ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:6 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:6 
msgid "！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:7 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:7 
msgid "桜井先生の後を追ってる？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:8 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:8 
msgid "……なんだ、日常の風景だ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:9 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:9 
msgid "あっ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:10 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0111:10 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:11 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:11 
msgid "……私を追いかけていた？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:12 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:12 
msgid "これは非日常の風景……。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0111:13 
msgid "げっ、バレちまった！\nに、逃げないと！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0111:14 
msgid "待て！　逃げたら自分からつけ回してたと\n言っているようなもんだぞ！\n"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0111:15 
msgid "あっ、そうか！\nこういう時は逆に堂としておくんだな！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0111:16 
msgid "そうだ、そして話しかけられても、\n偶然、目が合っただけだと言うんだ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0111:17 
msgid "その判断力、さすがだな！\nまるでスト―キングの経験があるみたいだ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0111:18 
msgid "！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0111:19 
msgid "……えっ？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0111:20 
msgid "な、なななな、なんのことだ？　あれは偶然！\n本当にいつも偶然だったんだよ！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0111:21 
msgid "お、お前……。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0111:22 
msgid "偶然なんだ――！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:23 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:23 
msgid "だけど……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:24 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:24 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:25 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:25 
msgid "……あれでは変質者。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:26 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:26 
msgid "桜井先生もかわいそうに……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:27 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:27 
msgid "……先生。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:28 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:28 
msgid "うわあああ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:29 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:29 
msgid "何してるんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:30 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:30 
msgid "み、水上！？　こ、これは、あの、\nそう、今日の夕飯の材料を買いにだな！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:31 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:31 
msgid "あ、あんなところに桜井先生が。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:32 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:32 
msgid "ドキッ！？　あ、ああ、本当だ、\n桜井先生がいるな。気付かなかった！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:33 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:33 
msgid "学校を出たら赤の他人……\nだから気付かなかったんですね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:34 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:34 
msgid "ち、ちち違う！　例え学校を出ても同僚だ！\n教職者として強い仆で結ばれていてだな！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:35 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:35 
msgid "変質者？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:36 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:36 
msgid "なっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:37 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:37 
msgid "最近、スト―カ―が出るらしいですよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:38 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:38 
msgid "なっ！？　だだだ、誰がだ！\n俺は偶然ここにいただけで弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:39 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:39 
msgid "先生。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:40 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:40 
msgid "な、なんだ……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:41 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:41 
msgid "ス―パ―、通り過ぎましたよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:42 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:42 
msgid "う……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:43 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:43 
msgid "で、では俺はここで！\nスト―カ―に気をつけるんだぞ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:44 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:44 
msgid "はい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:45 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:45 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:46 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:46 
msgid "いつ捕まるかな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:47 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:47 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:48 
msgid "桜井先生"
msgstr "Sakurai-sensei"

#  text 
#  warnings: 0 
#: sc/0111:48 
msgid "あら、水上さん。\n水上さんもお買い物？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:49 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:49 
msgid "ボディガ―ド？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:50 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:50 
msgid "ボディガ―ド……ですか？\nへ―、誰のです？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:51 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:51 
msgid "それは内緒。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:52 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:52 
msgid "あっ、そうですね。\nそれは言えませんよね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:53 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:53 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:54 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:54 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:55 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:55 
msgid "そうだ、水上さん。今日の夕飯なんですけど、\n何にするか迷ってるんです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:56 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:56 
msgid "カレ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:57 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:57 
msgid "カレ―、ですか？　いいですね―。\n私も弟も好きなんですよ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:58 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:58 
msgid "今日はお肉が安い日ですから、\nカレ―にしちゃいますね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:59 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:59 
msgid "ついでに防犯ブザ―も。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:60 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:60 
msgid "ぼ、防犯ブザ―？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:61 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:61 
msgid "この辺りはスト―カ―が出るから。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:62 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:62 
msgid "……そ、そうなんですか！？\nじゃあ防犯ブザ―も買います！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:63 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:63 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:64 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:64 
msgid "！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:65 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:65 
msgid "フフフフ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:66 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:66 
msgid "……？　水上さん？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:67 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:67 
msgid "ククク……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0111:68 
msgid "麻衣"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0111:68 
