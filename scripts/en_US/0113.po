#  speaker 
#  warnings: 0 
#: sc/0113:0 
msgid "笹原"
msgstr "Sasahara"

#  text 
#  warnings: 0 
#: sc/0113:0 
msgid "ふう……喉が渇いたな。\n飲料でも飲みにいくとしよう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:1 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:1 
msgid "おっと……うむ、財布はあるな。\n以前のような過ちはせんさ、はっはっは。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:2 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:2 
msgid "後は自販機が従順である事を祈るばかり。\nだが、かぶいたものも捨てがたいか……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:3 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:3 
msgid "さて、本日の自販機はどちらであろう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:4 
msgid "なの"
msgstr "Nano"

#  text 
#  warnings: 0 
#: sc/0113:4 
msgid "わっ！？　あ、あぶな―いっ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:5 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:5 
msgid "むうっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:6 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:6 
msgid "きゃっ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:7 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:7 
msgid "あわわわっ！　手、手が！！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0113:8 
msgid "ぜ、前方より飛行物体あり！\nはやく回避指示をっ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0113:9 
msgid "正体は！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0113:10 
msgid "ええと……手です！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0113:11 
msgid "……手？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0113:12 
msgid "手！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0113:13 
msgid "バカヤロウ！\n勤務中に飲んでるのか！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0113:14 
msgid "本当に手なんです！\nほら、こっちに向かって飛んで弌弌\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0113:15 
msgid "あぶねぇ―っ！\n頭の上、ギリギリセ―フだったぞ！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0113:16 
msgid "って、アウトだった―！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0113:17 
msgid "だから、手が飛んでくるって\n言ったじゃないですか――っ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:18 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:18 
msgid "む、大丈夫か？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:19 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:19 
msgid "あ、はい……すみません、\nあっ、あなたは……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:20 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:20 
msgid "貴様はいつぞやの。\nひとまず怪我は無いか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:21 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:21 
msgid "あ、はい、ありがとうございま弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:22 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:22 
msgid "ぬおおっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:23 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:23 
msgid "あわわわっ！！　だ、大丈夫ですか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:24 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:24 
msgid "ぐぅ……大事無い。\nしかし、手が飛ぶとは……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:25 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:25 
msgid "あっ、いえっ、違うんです！\nこれはその……違うんです！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:26 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:26 
msgid "そうか、いつぞやの右手は貴様のだったか。\n礼を言わねばならんな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:27 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:27 
msgid "え……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:28 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:28 
msgid "あの時、飛んでいた右手のおかげで\nいい句が浮かんだのでな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:29 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:29 
msgid "え、えっと……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:30 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:30 
msgid "感謝するぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:31 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:31 
msgid "あ、ええと、どういたしまして。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:32 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:32 
msgid "！？\n"
msgstr "!?\n"

#  speaker 
#  warnings: 0 
#: sc/0113:33 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:33 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0113:34 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:34 
msgid "あうあうあう……ち、違うんですよ？\nこれはその、ト―ストですけど、違って……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:35 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:35 
msgid "おお、また一句浮かんだぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:36 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:36 
msgid "いや、一句ではない！\nもう１つ……いや、まだまだ浮かぶぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:37 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:37 
msgid "貴様を見ていると句がまるで泉のように\n浮かんでくるではないか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:38 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:38 
msgid "え……は、はい？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:39 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:39 
msgid "気に入った！\n貴様のことをもっと教えて貰おう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:40 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:40 
msgid "えっ、わわっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:41 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:41 
msgid "さあ、他にどんな機能がついている？\n私に見せてみるといい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:42 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:42 
msgid "き、機能なんてついてません！\n気のせいです！　ついてないんです！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:43 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:43 
msgid "とぼけなくてもいい。\nさあ、早く見せるんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:44 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:44 
msgid "し、知らないです！！\n私ロボットなんかじゃありませ――ん！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:45 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:45 
msgid "む……しくじったか。\n私としたことがつい熱くなりすぎてしまった。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:46 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:46 
msgid "あの娘には後日謝罪するとしよう。\nしかし良い機会だと思ったのだが、残念だ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:47 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:47 
msgid "さて……改めて飲料を……。\nむ弌弌この気配は！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:48 
msgid "声（みさと）"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:48 
msgid "ささはらぁぁぁぁぁっ！！！\n"
msgstr "Sasaharaaaaaaaaaaa！！！\n"

#  speaker 
#  warnings: 0 
#: sc/0113:49 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:49 
msgid "狐色　産むは小麦の　旨みかな\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:50 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:50 
msgid "ううむ、これではただのパンの句だな。\nだが季語もある、悪くは無かろう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:51 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:51 
msgid "よし、この句は貴様に進呈しよう。\nさあ、受け取るが良い！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:52 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:52 
msgid "あ、えっと……ありがとうございます。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:53 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:53 
msgid "礼はいい。\nいや、そうだな……少し頼みがある。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:54 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:54 
msgid "はい、なんでしょう？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:55 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:55 
msgid "その匂いをかいでいたら小腹が空いてきた。\n良ければト―ストを馳走してくれんか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:56 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:56 
msgid "へ……？　あ、はい、どうぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:57 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:57 
msgid "うむ、ありがたい。\nそれでは頂くとしよう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:58 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:58 
msgid "っと、これはしたり。\nこの場では、また桜井教諭に叱られてしまう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:59 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:59 
msgid "自販機で飲料を買い、そちらで頂くとするか。\nでは、さらばだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:60 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:60 
msgid "…………。\n"
msgstr "………….\n"

#  speaker 
#  warnings: 0 
#: sc/0113:61 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:61 
msgid "……あ、ありがとうございました。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:62 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:62 
msgid "雲間より　光差し込む　雨上がり\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0113:63 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0113:63 
msgid "少ひねりが足りんな。\n雨上がりを言い換えてみるか……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:64 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:64 
msgid "ともあれ句が浮かんだので一安心だ。\n今回も感謝するぞ、娘よ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:65 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:65 
msgid "あ、あの……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:66 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:66 
msgid "どうした？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:67 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:67 
msgid "い、今の出来事、\n関係ないような気がするんですが。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:68 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:68 
msgid "…………ふむ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:69 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:69 
msgid "……えっと？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:70 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:70 
msgid "良い着眼点だ。\nだが浮かんだものは仕方ないのだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:71 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:71 
msgid "そして浮かんだのだから感謝する。\nこれが道理というものであろう？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:72 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:72 
msgid "え……う―んと、えっと、は、はい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:73 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:73 
msgid "よろしい。ではさらばだ。\n授業が開始する前に飲料を買いたいのでな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:74 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:74 
msgid "あ、はい。\nえっと……ありがとうございました。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:75 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:75 
msgid "うむ、また縁があれば。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0113:76 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0113:76 
msgid "……で、でも関係ないよね？\n"
msgstr ""
