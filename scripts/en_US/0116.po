#  speaker 
#  warnings: 0 
#: sc/0116:0 
msgid "桜井誠"
msgstr "Makoto"

#  text 
#  warnings: 0 
#: sc/0116:0 
msgid "ふあ……あ―、眠い……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:1 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:1 
msgid "あんな深夜に囲碁サッカ―の試合\nやってるんだもんな、そりゃ見ちゃうよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:2 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:2 
msgid "とりあえず学校行って、\n授業始まるまで寝よっと。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:3 
msgid "桜井先生"
msgstr "Sakurai Sensei"

#  text 
#  warnings: 0 
#: sc/0116:3 
msgid "マコちゃ―――ん！\n待ってよ、マコちゃんってば―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:4 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:4 
msgid "げ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:5 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:5 
msgid "はぁはぁ……も―、マコちゃんってば、\n一緒に学校行こうって言ったでしょ―？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:6 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:6 
msgid "行きたくないって言っただろ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:7 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:7 
msgid "いいでしょ、どうせ同じ道なんだから。\nほら、行こ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:8 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:8 
msgid "はぁ……。\n"
msgstr "Ha...\n"

#  speaker 
#  warnings: 0 
#: sc/0116:9 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:9 
msgid "（も―、姉ちゃんって何でこうなんだろ。\n　もうちょっと弟のこと気遣ったりとか……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:10 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:10 
msgid "あっ、マコちゃん前っ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:11 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:11 
msgid "え？　うわっ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:12 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:12 
msgid "こ、校長先生！　大丈夫ですか？\nほらっ、マコちゃんも早く謝って！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:13 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:13 
msgid "す、すいません。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0116:14 
msgid "マコちゃんだってさ！\n高校生で、ちゃんはきついよな―。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0116:15 
msgid "…………。\n"
msgstr ".........\n"

#  text 
#  warnings: 1 
#: sc/0116:16 
msgid "なあ、お前もそう思うだろ？\nちゃん、マコちゃんだぜ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0116:17 
msgid "…………。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0116:18 
msgid "おい、聞いてる？\n今、地球人がマコちゃんって呼ばれて弌弌\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0116:19 
msgid "う、う、うるさ―い！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0116:20 
msgid "うわっ、おいやめろ！\nいきなり何を弌弌おい、そのスイッチは！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0116:21 
msgid "うるせえ！　未だに母ちゃんから\nミっちゃんって呼ばれてるオレに謝れ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0116:22 
msgid "！！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0116:23 
msgid "うっ、うう……\n俺だって……俺だって……。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0116:24 
msgid "なんていうか……その、ごめんな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:25 
msgid "女子生徒（女子生徒１）"
msgstr "Female student #1"

#  text 
#  warnings: 0 
#: sc/0116:25 
msgid "おはようございま―す。\n先生、弟さんと一緒なんですね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:26 
msgid "男子生徒（男子生徒２）"
msgstr "Male student #2"

#  text 
#  warnings: 0 
#: sc/0116:26 
msgid "桜井おはよ！\n今日は姉ちゃんと一緒なんだな！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:27 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:27 
msgid "ふふっ、みんな声かけてくれるね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:28 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:28 
msgid "あ―も―……だから嫌なんだよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:29 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:29 
msgid "え、マコちゃん今何か言った？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:30 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:30 
msgid "先に学校行ってろって言った。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:31 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:31 
msgid "何でよ、間に合う時間なんだから\n一緒に行けばいいでしょ―？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:32 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:32 
msgid "はぁ……。\n"
msgstr "sigh...\n"

#  speaker 
#  warnings: 0 
#: sc/0116:33 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:33 
msgid "あ―もう！\nだからうっとうしいんだって！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:34 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:34 
msgid "学校じゃからむなって言っただろ！？\n恥ずかしいんだよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:35 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:35 
msgid "え、でも、だって。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:36 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:36 
msgid "家でもそんな仲いいわけでもないくせに！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:37 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:37 
msgid "そんなことないでしょ！？\nお姉ちゃんはマコちゃんのこと好きだもん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:38 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:38 
msgid "！？\n"
msgstr "!?\n"

#  speaker 
#  warnings: 0 
#: sc/0116:39 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:39 
msgid "だ、だからそういう問題じゃないって！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:40 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:40 
msgid "じゃあどういう問題なの！？\n私たち仲良しでしょ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:41 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:41 
msgid "それともマコちゃんは\n私のこと嫌いなの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:42 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:42 
msgid "う……いや、それは。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:43 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:43 
msgid "どうなの？\nもしかしてお姉ちゃんのこと嫌い？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:44 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:44 
msgid "いや、それは……そうじゃないけどさ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:45 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:45 
msgid "弌弌良かった！\nやっぱりマコちゃんはいい子だね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:46 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:46 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:47 
msgid "女子生徒（女子生徒２）"
msgstr "Female student #2"

#  text 
#  warnings: 0 
#: sc/0116:47 
msgid "おはよ―ございます。\n朝から姉弟ケンカなんて仲良しなんですね〜。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:48 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:48 
msgid "あ、おはようございます。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:49 
msgid "女子生徒（女子生徒２）"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:49 
msgid "桜井くん、お姉ちゃんいじめちゃダメだよ？\nそれじゃ、後でね！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:50 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:50 
msgid "マコちゃんのクラスの人だったね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:51 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:51 
msgid "……み、見られた……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:52 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:52 
msgid "だったらオレが先行ってるよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:53 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:53 
msgid "あっ、待ってよマコちゃん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:54 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:54 
msgid "え……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:55 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:55 
msgid "あいたたた……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:56 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:56 
msgid "まったく、なんで転ぶんだよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:57 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:57 
msgid "マコちゃんを\n追いかけようと思ったら弌弌痛っ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:58 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:58 
msgid "足、ひねっちゃったかも……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:59 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:59 
msgid "なら無理に立つなって！\nほら、見せてみろよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:60 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:60 
msgid "いたたっ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:61 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:61 
msgid "……はあ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:62 
msgid "女子生徒（女子生徒２）"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:62 
msgid "うわっ、先生おんぶですか？\nすっごい仲良しじゃないですか〜。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:63 
msgid "男子生徒（男子生徒１）"
msgstr "Male student #1"

#  text 
#  warnings: 0 
#: sc/0116:63 
msgid "桜井〜、お前すっげえ目立ってるぞ。\nいいね―、姉弟仲よさそうでさ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:64 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:64 
msgid "ご、ごめんねマコちゃん……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:65 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:65 
msgid "（……余計恥ずかしくなった。\n　誰か助け……っていうか、むしろ死にたい）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:66 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:66 
msgid "（……しょうがない、リフティングでもして、\n　姉ちゃんのことは見ないでおこう）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:67 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:67 
msgid "マコちゃんまたボ―ル蹴りながら歩いてる。\nも―、危ないからダメって言ったでしょ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:68 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:68 
msgid "よっ……と。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:69 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:69 
msgid "もうっ、ほら、貸して。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:70 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:70 
msgid "うわっ、何すんだよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:71 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:71 
msgid "よそ見しながら歩いてたら危ないよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:72 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:72 
msgid "ボ―ル横取りしようとする\n姉ちゃんの方が危ないって！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:73 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:73 
msgid "ほら、そんな事したら……あぁっ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:74 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:74 
msgid "あわわ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:75 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:75 
msgid "うわ……\nこれ、歴史の富岡先生じゃ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:76 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:76 
msgid "富岡先生、大丈夫ですか？\n富岡先生っ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:77 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:77 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:78 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:78 
msgid "（逃げろっ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0116:79 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0116:79 
msgid "ええっ！？\nま、待ってよマコちゃ―ん！！\n"
msgstr ""
