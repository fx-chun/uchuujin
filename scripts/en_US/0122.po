#  speaker 
#  warnings: 0 
#: sc/0122:0 
msgid "教頭先生"
msgstr "Vice Principal"

#  text 
#  warnings: 0 
#: sc/0122:0 
msgid "……おかしい……\nやはりどう考えても、おかしい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:1 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:1 
msgid "（なぜアイツが校長なんだ？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:2 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:2 
msgid "（私のほうが人望もあるし、\n　能力だって……たぶん上だ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:3 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:3 
msgid "（なのに、なぜ教頭のままなんだ！？\n　おかしい！　納得がいかない！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:4 
msgid "高崎先生"
msgstr "Takasaki-sensei"

#  text 
#  warnings: 0 
#: sc/0122:4 
msgid "すみません、教頭先生。\nこの資料に目を通してもらえますか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:5 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:5 
msgid "え？　あ、ああ。え―と……\nふむ……よくできてるよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:6 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:6 
msgid "ほ、本当ですか？\n"
msgstr "R-really?\n"

#  speaker 
#  warnings: 0 
#: sc/0122:7 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:7 
msgid "うむ。だが、こことここは……\nこんな感じに修正してもらえるかな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:8 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:8 
msgid "あ……そうですね、その方がいいですね。\nわかりました、すぐにやります！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:9 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:9 
msgid "いや、急がなくてもいい。\nコツコツ丁寧に、正確にやりなさい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:10 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:10 
msgid "つまりは、そう……コツコツ勝つコツ、だ。\nわかったね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:11 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:11 
msgid "はい！　いや―、それにしても\n教頭先生の指導はわかり易くていいですね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:12 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:12 
msgid "そ、そうかな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:13 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:13 
msgid "ええ、すごく。\nそれじゃすぐに修正してきますから。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:14 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:14 
msgid "あ、ああ、頼んだよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:15 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:15 
msgid "（これだ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:16 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:16 
msgid "（なぜ今まで気付かなかった？　これだよ！\n　私が校長より有能な姿を見せるんだ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:17 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:17 
msgid "（そうすれば先生や生徒たちがいつか\n　私の方が校長に相応しいと言ってくれる！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:18 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:18 
msgid "（その為には……地味だが指導していくのが\n　一番のようだな。では早速……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:19 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:19 
msgid "弌弌ああ、桜井先生。\n"
msgstr "——Ah, Sakurai-sensei.\n"

#  speaker 
#  warnings: 0 
#: sc/0122:20 
msgid "桜井先生"
msgstr "Sakurai Sensei"

#  text 
#  warnings: 0 
#: sc/0122:20 
msgid "あ、はい、何でしょうか？\n"
msgstr "Ah, yes. What's the matter?\n"

#  speaker 
#  warnings: 0 
#: sc/0122:21 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:21 
msgid "生活指導のほうなんですが、どうです？\nうまくいってますか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:22 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:22 
msgid "あ……はい。……スミマセン……\nあまり……その……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:23 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:23 
msgid "いやいや、気にしないでください。\nすぐには慣れませんよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:24 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:24 
msgid "だからあまり気負わないで。\n何かあったら相談してください……ね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:25 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:25 
msgid "は、はい……あっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:26 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:26 
msgid "！？　何かありましたか？\n何でも相談してください！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:27 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:27 
msgid "えっ？\nいえ、あの……そこにワンちゃんが……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:28 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:28 
msgid "ワンちゃん？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:29 
msgid "バディ"
msgstr "Buddy"

#  text 
#  warnings: 0 
#: sc/0122:29 
msgid "…………。\n"
msgstr "..........\n"

#  speaker 
#  warnings: 0 
#: sc/0122:30 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:30 
msgid "……ふふ、任せなさい。\n私が責任をもって外に連れて行くよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:31 
msgid "桜井先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:31 
msgid "えっ？\n……わ、わかりました、お願いします。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:32 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:32 
msgid "（ふふふ、地道なことだが、\n　こうやって信頼を積み重ねればきっと……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:33 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:33 
msgid "ん？\n今、何か扉に挟まったような。ま、いいか。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0122:34 
msgid "なあ？　あのかわいい女の先生、\n自分で連れて行きたかったんじゃないのか？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0122:35 
msgid "だよなぁ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0122:36 
msgid "あ―あ、あの教頭、\nこれじゃ校長への道は厳しそうだな―。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0122:37 
msgid "はは、まったくだぜ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0122:38 
msgid "お前ら、何のんきな話してるんだよ！\nさっきの扉に挟まったのは弌弌！\n"
msgstr ""

#  speaker 
#  warnings: 3 
#: sc/0122:39 
#, fuzzy
msgid "＆＆"
msgstr "&&"

#  text 
#  warnings: 3 
#: sc/0122:39 
msgid "うあああああああああ！？\n"
msgstr "Uwaaaaaaaaaaaaaah!?\n"

#  speaker 
#  warnings: 0 
#: sc/0122:40 
msgid "桜井先生"
msgstr "Ms. Sakurai"

#  text 
#  warnings: 0 
#: sc/0122:40 
msgid "は、はい！　ありがとうございます！\n"
msgstr "Y-yes! Thank you very much!\n"

#  speaker 
#  warnings: 0 
#: sc/0122:41 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:41 
msgid "いやいや。\nそれでは弌弌\n"
msgstr ""
"No, no.\n"
"Well then, c'ya——\n"

#  speaker 
#  warnings: 0 
#: sc/0122:42 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:42 
msgid "（え―と……次は高崎先生だな。\n　さっきの資料の様子を見がてら指導するか）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:43 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:43 
msgid "（……って、あれ？　高崎先生がいない……\n　どこに行ったんだ？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:44 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:44 
msgid "弌弌あれ？　教頭先生、どうしたんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:45 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:45 
msgid "い、いや、ちょっと休憩ですよ。\nそれより……どうですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:46 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:46 
msgid "あ……資料ですか？\nちょっと詰まってます……すみません。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:47 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:47 
msgid "ゆっくりでいいですよ。\n焦らず、正確にが肝心ですから。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:48 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:48 
msgid "それはわかってるんですが……\nがんばります。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:49 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:49 
msgid "何かわからないことはありますか？\n相談したいことがあれば何でも結構ですよ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:50 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:50 
msgid "え？　本当ですか？\nいや―、助かります！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:51 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:51 
msgid "（お、好感触だな。だったらここは弌弌）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:52 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:52 
msgid "いやぁ、今日の夜あたりどうです？\n二人……いや桜井先生でも誘って？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:53 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:53 
msgid "はいきた―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:54 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:54 
msgid "は、はあ？　何がきたんです？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:55 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:55 
msgid "何でもありません！\nそれで桜井先生はなんと？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:56 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:56 
msgid "は？\nいや……まだ誘ってませんが。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:57 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:57 
msgid "なんで！？\n"
msgstr "Why!?\n"

#  speaker 
#  warnings: 0 
#: sc/0122:58 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:58 
msgid "あ……す、すいません……\nちょっと忙しくて……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:59 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:59 
msgid "そんな大事なことを！？　なぜ！？\n俺をからかったんですか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:60 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:60 
msgid "そ、そ、そんなつもりはないんです！\nわかりました！　すぐに誘いますから！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:61 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:61 
msgid "お願いします！　すぐに！　大至急！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:62 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:62 
msgid "は、はいはいはいはいはいはい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:63 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:63 
msgid "あの……それで……何ですが。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:64 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:64 
msgid "何が？\n"
msgstr "What?\n"

#  speaker 
#  warnings: 0 
#: sc/0122:65 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:65 
msgid "私、ちゃんと指導できてるでしょうか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:66 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:66 
msgid "コツコツ勝つコツ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:67 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:67 
msgid "……ありがとうございました。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:68 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:68 
msgid "そんなこと言わないでください。\n同じ職場の仲間じゃないですか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:69 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:69 
msgid "ぜひ、どんなことでも頼ってくださいよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:70 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:70 
msgid "（ふふふ……これが職員室に席のある\n　教頭ならではのメリットだ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:71 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:71 
msgid "教頭先生、ありがとうございます！\nでしたら……資料の作成、お願いします！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:72 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:72 
msgid "え？\nそ、それとこれとは別では……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:73 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:73 
msgid "でも、俺が修正するより、教頭先生が\nやったほうが早くないでしょうか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:74 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:74 
msgid "そ、そ、それは……まあ……。\nけど、やはりそれはマズいような……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:75 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:75 
msgid "実はこの手の資料は初めてなんです。\nだからぜひ教頭先生にお願いしたいんです！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:76 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:76 
msgid "は、はあ……。\n"
msgstr "H-ha…….\n"

#  speaker 
#  warnings: 0 
#: sc/0122:77 
msgid "高崎先生"
msgstr "Mr. Takasaki"

#  text 
#  warnings: 0 
#: sc/0122:77 
msgid "と言うわけで、よろしくお願いします！\nできればすぐに！　お願いします！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:78 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:78 
msgid "……………………。\n"
msgstr "…………………….\n"

#  speaker 
#  warnings: 0 
#: sc/0122:79 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:79 
msgid "……はい。\n"
msgstr "……Yes.\n"

#  speaker 
#  warnings: 0 
#: sc/0122:80 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:80 
msgid "人の上に立つ者として当然だよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:81 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:81 
msgid "ふふふ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:82 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:82 
msgid "この調子なら、おそらく先生方だけでなく\n生徒たちも私を支持するようになるはず……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:83 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:83 
msgid "あと少し……いや、もうすぐだ！\n私が校長になる日まで、もう……！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:84 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0122:84 
msgid "うわっ！？\n"
msgstr "Uwah!?\n"

#  speaker 
#  warnings: 0 
#: sc/0122:85 
msgid "みお"
msgstr "Mio"

#  text 
#  warnings: 0 
#: sc/0122:85 
msgid "ご、ごめんなさい！　怪我していませんか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0122:86 
msgid "教頭先生"
msgstr ""

