#  speaker 
#  warnings: 1 
#: sc/0123:0 
msgid "中之条"
msgstr "Nakanojo"

#  text 
#  warnings: 1 
#: sc/0123:0 
msgid "ふ〜ん、ふふん、ふふふ〜ん\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:1 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:1 
msgid "よ―し、出来上がり。\nそれじゃ、行ってきま―す！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:2 
msgid "中之条さん"
msgstr "Nakanojo-san"

#  text 
#  warnings: 0 
#: sc/0123:2 
msgid "ああ、行ってらっしゃい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:3 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:3 
msgid "……懐かしいな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:4 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:4 
msgid "私も昔はあんな風に髪のセットから\n１日が始まっていたもんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:5 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:5 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 1 
#: sc/0123:6 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0123:6 
msgid "久に少しやってみるか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:7 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:7 
msgid "そうと決まればクシを弌弌っと！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:8 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:8 
msgid "しまった、どこに落としたかな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:9 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:9 
msgid "……ああ、あったあった。\nあんな隙間に落としてしまったのか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:10 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:10 
msgid "しかし、クシにしては形が妙な気が……\nとりあえず引っ張ってみよう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:11 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:11 
msgid "よっ……むっ、なかなか手が……よし届いた！\nこのまま……よいしょ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:12 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:12 
msgid "……クシじゃないな。\n何だろうこれは。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0123:13 
msgid "アンテナがもげた―！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0123:14 
msgid "誰だ！　手でもげるようにしたやつは！\n呼んでこい！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0123:15 
msgid "無理です。\n……もう、あいつはいません。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0123:16 
msgid "……え？\n"
msgstr "……Eh?\n"

#  text 
#  warnings: 1 
#: sc/0123:17 
msgid "塩と砂糖を間違えて……姫に……。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0123:18 
msgid "そうか、だったら弌弌\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0123:19 
msgid "あっちで文句言ってやろうぜ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0123:20 
msgid "あきらめないで――っ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:21 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:21 
msgid "……こうして髪を整えていると\n若い頃を思い出すなあ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:22 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:22 
msgid "あの頃は髪型のことでずいぶん荒れたもんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:23 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:23 
msgid "どうせならモヒカンらしく\n不良になってやろうと暴れたりもしたっけ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:24 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:24 
msgid "そう……そんな自分を救ってくれたのは\n大福だった。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:25 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:25 
msgid "こうして今、まっとうな生活が出来ているのも\n大福のおかげか……。\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0123:26 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0123:26 
msgid "よし、完成。\nうむ、中悪くない……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:27 
msgid "中之条母"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:27 
msgid "ギャ――――ッ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:28 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:28 
msgid "！？\n"
msgstr "!?\n"

#  speaker 
#  warnings: 0 
#: sc/0123:29 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:29 
msgid "な、何もそこまで驚く事は無いだろう！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:30 
msgid "中之条母"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:30 
msgid "あなた……もうモヒカンは止めるって\nあの子が生まれる前に誓ったじゃないですか！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:31 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:31 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:32 
msgid "中之条母"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:32 
msgid "あの子はあんなに気にしているのに……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:33 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:33 
msgid "そ、そうか……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:34 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:34 
msgid "……すまなかった。\nもう二度とモヒカンにはしない！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:35 
msgid "中之条母"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:35 
msgid "あなた……わかってくれたんですね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:36 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:36 
msgid "ああ、すまなかった。\n私は何をバカなことをしていたんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:37 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:37 
msgid "忘れ物忘れ物―っと。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:38 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:38 
msgid "モヒカンなんて格好悪いのにな！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:39 
msgid "中之条母"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:39 
msgid "あ、剛っ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:40 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:40 
msgid "な……っ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:41 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:41 
msgid "と、父さん……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:42 
msgid "中之条"
msgstr "Nakanojou"

#  text 
#  warnings: 0 
#: sc/0123:42 
msgid "くっ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:43 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:43 
msgid "つよし！　違うんだ、今のは違う！！\n違うんだ、つよし――――――！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:44 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:44 
msgid "おいおい、そんなに驚かないでくれ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:45 
msgid "中之条母"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:45 
msgid "あ、あら……あなた。\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0123:46 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0123:46 
msgid "若い頃を思い出してな。\n久にやってみたんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:47 
msgid "中之条母"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:47 
msgid "まあ、そうでしたの。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:48 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:48 
msgid "こうしていると\n母さんと出会った頃のことも思い出すな……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:49 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:49 
msgid "そうだ、若い頃のように、\n今からデ―トでもしてみないか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:50 
msgid "中之条母"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:50 
msgid "あらあら、嬉しいお誘いだこと。\nそれじゃ、お化粧してきますね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:51 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:51 
msgid "ははは、ゆっくり準備してきなさい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:52 
msgid "中之条母"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:52 
msgid "ええ、めいっぱいおめかししてきますよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:53 
msgid "中之条母"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:53 
msgid "あ、ですけどその髪型はやめてくださいね。\nいい歳して、似合ってませんから。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:54 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:54 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:55 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:55 
msgid "待て、母さん！　私だ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:56 
msgid "中之条母"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:56 
msgid "……あ、あら、あなたでしたか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:57 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:57 
msgid "ああ、驚かせたかな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:58 
msgid "中之条母"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:58 
msgid "そりゃあ驚きますよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:59 
msgid "中之条母"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:59 
msgid "まさかあの頃みたいな\nダサい格好をしてるなんて思いませんし。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:60 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:60 
msgid "え……？\n"
msgstr "Eh……?\n"

#  speaker 
#  warnings: 0 
#: sc/0123:61 
msgid "中之条母"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:61 
msgid "嫌ですよみっともない。\nすぐに直してくださいね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:62 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:62 
msgid "いや……そこまで悪くは無いと思うんだが。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:63 
msgid "中之条母"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:63 
msgid "どこがですか？　いい年して格好悪い。\n若い頃でも酷かったのに。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:64 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:64 
msgid "若い頃でも……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:65 
msgid "中之条母"
msgstr "Ms. Nakanojo"

#  text 
#  warnings: 0 
#: sc/0123:65 
msgid "ともかく、直さないと夕食はなしですよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0123:66 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0123:66 
msgid "…………。\n"
msgstr "………….\n"

#  speaker 
#  warnings: 0 
#: sc/0123:67 
msgid "中之条さん"
msgstr "Mr. Nakanojo"

#  text 
#  warnings: 0 
#: sc/0123:67 
msgid "……わかった。\n"
msgstr ""
