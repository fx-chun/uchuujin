msgid "きよし"
msgstr "Kiyoshi"

#  text 
#  warnings: 0 
#: sc/0124:0 
msgid "ふぁあ〜……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0124:1 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0124:1 
msgid "いつものことだけど、朝の散歩は\n早起きしないといけないのが辛いなぁ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0124:2 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0124:2 
msgid "おはようバディ、コバディ。\nそれじゃ、散歩に行くぞ〜。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0124:3 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0124:3 
msgid "……おや？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0124:4 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0124:4 
msgid "あっ、おじいちゃん。おはよ〜。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0124:5 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0124:5 
msgid "おはよう。ご飯は……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0124:6 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0124:6 
msgid "ご飯は後でね。\n先にバディたちと散歩に行ってくるよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0124:7 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0124:7 
msgid "ふむ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0124:8 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0124:8 
msgid "あれ？　今、何か音が弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0124:9 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0124:9 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0124:10 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0124:10 
msgid "わっ！？　おじいちゃん！\n変な人がうちの中に！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0124:11 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0124:11 
msgid "…………！\n"
msgstr "…………!\n"

#  speaker 
#  warnings: 0 
#: sc/0124:12 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0124:12 
msgid "やっちゃえ、じいちゃ―ん！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0124:13 
msgid "げっ、見つかった！\n上手く隠れてたと思ったのに！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0124:14 
msgid "…………。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0124:15 
msgid "おい、早く逃げるように操作しろって！\nあのオッサンやられるぞ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0124:16 
msgid "ああ……。\n"
msgstr "Ah…\n"

#  text 
#  warnings: 1 
#: sc/0124:17 
msgid "……どうした？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0124:18 
msgid "いや、あの子供を見ていたら、\n昔飼ってたペットを思い出して……うう。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0124:19 
msgid "お、おい、泣くなよ……いい大人だろ？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0124:20 
msgid "そうだけど……まだ心は子供のつもりだよ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0124:21 
msgid "（うわぁ……こいつ、本気で言ってるよ。\n　いい歳こいて、子供のつもりだなんて弌弌）\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0124:22 
msgid "……ええっ？　どうして俺まで、\n子供の頃の思い出が、走馬灯のように弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0124:23 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0124:23 
msgid "そうだ、おじいちゃんも\n一緒に散歩に行こうよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0124:24 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0124:24 
msgid "ふむ……。そうじゃな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0124:25 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0124:25 
msgid "やった！　じゃあ早く行こう！\nバディ、コバディ、行くぞ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0124:26 
msgid "鹿"
msgstr "Deer"

#  text 
#  warnings: 0 
#: sc/0124:26 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0124:27 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0124:27 
msgid "…………。\n"
msgstr "………….\n"

#  speaker 
#  warnings: 0 
#: sc/0124:28 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0124:28 
msgid "（鹿っ！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0124:29 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0124:29 
msgid "お、おじいちゃん！？\n玄関の前に、何かいたよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0124:30 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0124:30 
msgid "これは羊と言って弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0124:31 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0124:31 
msgid "鹿だよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0124:32 
msgid "鹿"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0124:32 
msgid "…………！！\n"
msgstr "………!!\n"

#  speaker 
#  warnings: 0 
#: sc/0124:33 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0124:33 
msgid "わっ！？　怒った！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0124:34 
msgid "鹿"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0124:34 
msgid "！！\n"
msgstr "!!\n"

#  speaker 
#  warnings: 0 
#: sc/0124:35 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0124:35 
msgid "！？\n"
msgstr "!?\n"

#  speaker 
#  warnings: 0 
#: sc/0124:36 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0124:36 
msgid "おじいちゃん！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0124:37 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0124:37 
msgid "う……ううっ……。\n"
msgstr ""

