#  speaker 
#  warnings: 0 
#: sc/0132:0 
msgid "ウェボシ―"
msgstr "Weboshi"

#  text 
#  warnings: 0 
#: sc/0132:0 
msgid "あっ、そこの子、ちょっといい？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:1 
msgid "なの"
msgstr "Nano"

#  text 
#  warnings: 0 
#: sc/0132:1 
msgid "はい？\n"
msgstr "Yes?\n"

#  speaker 
#  warnings: 0 
#: sc/0132:2 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:2 
msgid "キミ１年だよね？\n東雲なのちゃんって知らない？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:3 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:3 
msgid "え？　なのは私ですけど……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:4 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:4 
msgid "あなたがなのちゃんか〜。\nえっとね、今度体育祭があるでしょ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:5 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:5 
msgid "あっ、はい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:6 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:6 
msgid "おめでとうっ。３学年混合の組み体操で、\nピラミッド役に選ばれました。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:7 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:7 
msgid "えっ？　えっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:8 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:8 
msgid "先生がくじで勝手に決めちゃったんだって。\n学年ちがうけど、私たちも仲間だよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:9 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:9 
msgid "と言うわけでピラミッドのどの位置がいいか\nなのちゃんの意見を聞きにきたんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:10 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:10 
msgid "どの位置がいいかですか。\nう―ん……私は別にどこでも弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:11 
msgid "バディ"
msgstr "Buddy"

#  text 
#  warnings: 0 
#: sc/0132:11 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0132:12 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:12 
msgid "あれ？　この犬、また……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:13 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:13 
msgid "おっ！　犬だ！\nよしよし、こっちおいで。\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0132:14 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0132:14 
msgid "あははは、かわい―\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0132:15 
msgid "おい、タ―ゲットが見えないぞ？\nちゃんと映せないのか？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0132:16 
msgid "ううむ、がっちり捕まってるから\n身動きが取れないな。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0132:17 
msgid "全く……うらやましいやつめ。\nとりあえず暴れてみろ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0132:18 
msgid "オッケ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:19 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:19 
msgid "あっ、こいつ！\n逃がさないぞ――！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0132:20 
msgid "……俺も女の子にハグされて、\nこんなこと言われてみたいよ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0132:21 
msgid "はは、まったくだ。\nま、今は気分だけでも味わおうぜ。\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0132:22 
msgid "＆"
msgstr "＆"

#  text 
#  warnings: 2 
#: sc/0132:22 
msgid "！？\n"
msgstr "!?\n"

#  text 
#  warnings: 1 
#: sc/0132:23 
msgid "ぎゃあああ！？　折れた――！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0132:24 
msgid "余計なこと、\nするんじゃなかった――！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:25 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:25 
msgid "（あっ！？　背中！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:26 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:26 
msgid "（組み体操でピラミッドになったら\n　ネジが邪魔になる！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:27 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:27 
msgid "（こんなところでもネジの弊害が……\n　う―ん、一番上なら邪魔にならないかな？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:28 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:28 
msgid "それじゃあ私は一番上でお願いします。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:29 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:29 
msgid "一番上か―。\nなのちゃんって体重軽い方？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:30 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:30 
msgid "えっ？\n"
msgstr "Huh?\n"

#  speaker 
#  warnings: 0 
#: sc/0132:31 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:31 
msgid "ピラミッドって下の人に負担かかるじゃん？\nだからできるだけ軽い人を上にしたいんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:32 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:32 
msgid "あっ、そうですね。\nえ―、私の体重は……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:33 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:33 
msgid "（あっ！？　体重！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:34 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:34 
msgid "（私はロボットだから絶対に軽くない。\n　なのに私が一番上になったら……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:35 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:35 
msgid "（ピラミッドは崩壊を免れない！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:36 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:36 
msgid "（これじゃ私は一番下になるしか……\n　でもネジのせいで下にもなれない……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:37 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:37 
msgid "……あれ？　もしかしてなのちゃんって\n言えないくらい重いの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:38 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:38 
msgid "そ、そんなことないです！\n他の人よりちょっと重いだけです！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:39 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:39 
msgid "ん―、それなら一番上は無理かな。\n悪いけど下の方に弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:40 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:40 
msgid "いえ！　大丈夫です！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:41 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:41 
msgid "えっ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:42 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:42 
msgid "ダイエットしますから！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:43 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:43 
msgid "で、でも、今からダイエットしても\n一番軽くなるのは難しいし……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:44 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:44 
msgid "いえ！　大丈夫です！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:45 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:45 
msgid "えっ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:46 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:46 
msgid "軽量化なら一晩ですから！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:47 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:47 
msgid "えっ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:48 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:48 
msgid "あっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:49 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:49 
msgid "軽量化？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:50 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:50 
msgid "軽量化という名のダイエットです！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:51 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:51 
msgid "じゃあやっぱり一晩じゃ\n間に合わないんじゃ弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:52 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:52 
msgid "いえ！　大丈夫です！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:53 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:53 
msgid "ふぇっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:54 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:54 
msgid "腕の立つ人を知っていますから！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:55 
msgid "ウェボシ―＆フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:55 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:56 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:56 
msgid "そ、そっか―。じゃあ、ダイエット、\n頑張ってきてよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:57 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:57 
msgid "はい！　頑張ります！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:58 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:58 
msgid "（重い！\n　きっと普通の女の子の何倍も重い！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:59 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:59 
msgid "（それを認めると強制的に下の方に……\n　でもネジ……上はみんなが怪我……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:60 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:60 
msgid "あれ？　お―いなのちゃ―ん？\n……聞こえてないみたい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:61 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:61 
msgid "重いって言われてショックだったんだよ。\nオブラ―トにつつみなって。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:62 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:62 
msgid "あっ、そっか……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:63 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:63 
msgid "あっ、すみません。\n何か言いましたか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:64 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:64 
msgid "い、いや、何も。それより、\nごめんねなのちゃん。私たちそろそろ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:65 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:65 
msgid "ほ、他の人にも聞かないといけないし。\nじゃあね！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:66 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:66 
msgid "あっ……\n行っちゃった……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:67 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:67 
msgid "……結局、私はどのポジションになれば\nいいんだろう……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:68 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:68 
msgid "ど、どうでしょう。\n最近体重を量ってませんし。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:69 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:69 
msgid "前に量った時は\n何キロだったか覚えてない？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:70 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:70 
msgid "う―ん……ちょっと覚えてないですね。\nでも軽い方だったはずです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:71 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:71 
msgid "へ〜。なら一番上になれるかも。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:72 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:72 
msgid "あ、そうだ。\n今から保健室に行って量ってみようよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:73 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:73 
msgid "あ―……すみません、今日は急いでいて\nすぐに帰らなくちゃいけなくて……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:74 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:74 
msgid "あ、そうなんだ。\nごめんね呼び止めて。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:75 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:75 
msgid "いえ。\nまた体重量ってきますから。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:76 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:76 
msgid "うん。じゃあね―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:77 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:77 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:78 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:78 
msgid "嘘をつき通しちゃった……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0132:79 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0132:79 
msgid "ピラミッドの崩壊は免れない……。\n"
msgstr ""
