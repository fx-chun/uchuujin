msgid "高崎先生"
msgstr "Takasaki-sensei"

#  text 
#  warnings: 0 
#: sc/0142:0 
msgid "……であるからして、\nここで作者の言いたかったことは弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:1 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:1 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0142:2 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:2 
msgid "（あ……消しゴム、落としちゃった。\n　拾わなきゃ弌弌）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:3 
msgid "笹原"
msgstr "Sasahara"

#  text 
#  warnings: 0 
#: sc/0142:3 
msgid "ふ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:4 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:4 
msgid "（え……笹原……？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:5 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:5 
msgid "（消しゴム、拾ってくれるなんて……\n　なによ、良いトコあるじゃないの）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:6 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:6 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:7 
msgid "ウェボシ―"
msgstr "Weboshi"

#  text 
#  warnings: 0 
#: sc/0142:7 
msgid "にやにやにやにや\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:8 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:8 
msgid "にこにこにこにこ\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:9 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:9 
msgid "（はっ！？　み、見られてたっ！！\n　〜〜〜〜〜〜〜〜〜〜〜〜〜〜〜〜〜！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:10 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:10 
msgid "！？\n"
msgstr "!?\n"

#  speaker 
#  warnings: 0 
#: sc/0142:11 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:11 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:12 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:12 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:13 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:13 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:14 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:14 
msgid "何をしてるんだ立花！\n授業中だぞ。これは没収だ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:15 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:15 
msgid "あ……。\n"
msgstr "Ah...\n"

#  speaker 
#  warnings: 0 
#: sc/0142:16 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:16 
msgid "さて、授業を続けるぞ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:17 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0142:17 
msgid "…………。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0142:18 
msgid "なあ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0142:19 
msgid "ん？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0142:20 
msgid "何がどうなってんだか、\nオレにはさっぱりわからんのだが。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0142:21 
msgid "安心しろ。オレにもわからん。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0142:22 
msgid "ただ、ひとつ確かなことは弌弌\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0142:23 
msgid "＆"
msgstr "&"

#  text 
#  warnings: 2 
#: sc/0142:23 
msgid "アンテナが壊れたって事だな！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:24 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:24 
msgid "……こ、こら立花、今は授業中だぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:25 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:25 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:26 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:26 
msgid "その変なかぶり物を早く脱げ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:27 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:27 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:28 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:28 
msgid "どうした？　聞こえないのか。\nそれを脱げと言ってるんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:29 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:29 
msgid "み、みさと……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:30 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:30 
msgid "わ―、どうするんだろ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:31 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:31 
msgid "脱げといってるだろ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:32 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:32 
msgid "…………！\n"
msgstr ".........!\n"

#  speaker 
#  warnings: 0 
#: sc/0142:33 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:33 
msgid "ぐっ……！　教師の言うことが……\n聞けんの……か……っ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:34 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:34 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:35 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:35 
msgid "ありゃ〜。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:36 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:36 
msgid "…………！！\n"
msgstr "...... !!\n"

#  speaker 
#  warnings: 0 
#: sc/0142:37 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:37 
msgid "暴発した―！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:38 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:38 
msgid "こ、こら立花！　マジ危ないから、\nそんなものは弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:39 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:39 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:40 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:40 
msgid "い、いや、これは……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:41 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:41 
msgid "狙い撃ちだ―！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:42 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:42 
msgid "こ、こら立花、は、早く脱いで弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:43 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:43 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:44 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:44 
msgid "その弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:45 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:45 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:46 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:46 
msgid "頼むから弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:47 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:47 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:48 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:48 
msgid "……わかった。もういい。\nもう何も言うまい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:49 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:49 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:50 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:50 
msgid "え―、では授業を再開する。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:51 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:51 
msgid "……授業してていいの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:52 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:52 
msgid "……すみません。今、脱ぎます。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:53 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:53 
msgid "うむ、わかればよろしい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:54 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:54 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:55 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:55 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:56 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:56 
msgid "そ、それも脱ぎなさい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:57 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:57 
msgid "……はい。\n"
msgstr "...yes.\n"

#  speaker 
#  warnings: 0 
#: sc/0142:58 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:58 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:59 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:59 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:60 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:60 
msgid "立花……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:61 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:61 
msgid "すみませんでした。\n罰として、廊下に立ってます。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:62 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:62 
msgid "いや、反省したなら別に……それより、\nかぶりものを取って顔を弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:63 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:63 
msgid "た、立ってます！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:64 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:64 
msgid "……みさと、赤面しているの見られるの弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:65 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:65 
msgid "弌弌あの、かぶりもの姿より恥ずかしいんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:66 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:66 
msgid "わ、私は笹原のことなんか、\n何とも思ってないんだからね〜〜〜っ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:67 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:67 
msgid "（反応がない……\n　立花はどうしたと言うんだ……？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:68 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:68 
msgid "（そもそも、一体なんなんだこれは）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:69 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:69 
msgid "（砲台？　戦車？　それともアニメの\n　ロボットか何かか？　わからん……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:70 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:70 
msgid "（わからんのは見た目だけじゃない。\n　この砲身……果たして本物なのか？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:71 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:71 
msgid "（本物だとしたら大問題だ。とても危険だ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:72 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:72 
msgid "（いやしかし、そんなことがありえるのか？\n　普通まずあり得ない！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:73 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:73 
msgid "（しかも、周りの生徒に驚いた様子がない。\n　これは……これはまさか……！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:74 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:74 
msgid "（この妙なかぶり物が見えているのは、\n　まさかオレ一人なんじゃ……！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:75 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:75 
msgid "（つまり、オレだけ幻覚を〜〜〜！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:76 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:76 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:77 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:77 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:78 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:78 
msgid "……では、次のペ―ジ。\n立花、読みなさい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:79 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:79 
msgid "……はい。\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0142:80 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0142:80 
msgid "（良かったね、みさと。\n　恥ずかしいの、ごまかせて〜）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:81 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:81 
msgid "（な、なんでおとがめなしなんだろう……\n　高崎先生が良いなら、別に良いけど……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:82 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:82 
msgid "（まあ、砲台を被って授業を受けてはいかん\n　という校則はないのだ。やむを得まい）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:83 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:83 
msgid "（誰も何も言わない……\n　やはりオレは幻覚を見てるのか……？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0142:84 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0142:84 
msgid "春は、あけぼの。\nやうやう白くなりゆく山ぎは弌弌\n"
msgstr ""
