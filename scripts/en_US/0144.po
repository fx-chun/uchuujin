#  speaker 
#  warnings: 0 
#: sc/0144:0 
msgid "中村先生"
msgstr "Nakamura-sensei"

#  text 
#  warnings: 0 
#: sc/0144:0 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0144:1 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:1 
msgid "……やはり材料が足りないな。\n明日の授業、実験にするつもりだったんだが。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:2 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:2 
msgid "仕方ない、教科書を進めるとするか。\nテスト範囲を超えなければ問題ないだろう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:3 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:3 
msgid "テスト……そう言えば、あったな。\nそろそろ問題を考えないといけないな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:4 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:4 
msgid "面倒だ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:5 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:5 
msgid "あっ、すべった！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:6 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:6 
msgid "あ―……割ってしまったか。\nやれやれ、面倒だが片付けるとする弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:7 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0144:7 
msgid "……うぅ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:8 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:8 
msgid "弌弌なんで机の下に校長が！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0144:9 
msgid "アンテナ破損！\n落ちてきた実験器具で一発だ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0144:10 
msgid "くそ、まだまだだったか！？\n俺の技で完全に気配を消していたはずなのに！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0144:11 
msgid "……いや、見事だったよ。\n出来ればその技、俺にも教えて欲しいんだが。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0144:12 
msgid "フ、いいだろう。\nただし飯三回おごりが条件だぜ？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0144:13 
msgid "はは。\nそのくらいなら、すぐにでも。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0144:14 
msgid "決まりだな。\nそれじゃ……明日たのむぜ？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0144:15 
msgid "ああ……明日だな。\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0144:16 
msgid "＆"
msgstr "&"

#  text 
#  warnings: 2 
#: sc/0144:16 
msgid "（明日……か）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:17 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:17 
msgid "はい？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:18 
msgid "教頭先生"
msgstr "Vice Principal"

#  text 
#  warnings: 0 
#: sc/0144:18 
msgid "……中村先生、少しよろしいですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:19 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:19 
msgid "はい。何でしょうか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:20 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:20 
msgid "（怪しいな……何コソコソしてるんだ？\n　関わりたくないなぁ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:21 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:21 
msgid "実は、作ってもらいたいものが\nあるのですが……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:22 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:22 
msgid "はあ。どんなものでしょうか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:23 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:23 
msgid "……絶対に演説を長引かせる機械です。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:24 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:24 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:25 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:25 
msgid "……演説を長引かせる機械？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:26 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:26 
msgid "絶対に、です。ここが重要なんです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:27 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:27 
msgid "……よくわかりませんが、わかりました。\nそれは何に使うんでしょうか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:28 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:28 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:29 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:29 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:30 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:30 
msgid "また校長先生ですか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:31 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:31 
msgid "！？\n"
msgstr "!?\n"

#  speaker 
#  warnings: 0 
#: sc/0144:32 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:32 
msgid "教頭先生も良く続きますね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:33 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:33 
msgid "い、いや、そうでは……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:34 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:34 
msgid "（いつもうざい教頭だが……継続は力なり、だ。\n　そこは見習う部分だな）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:35 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:35 
msgid "校長先生の話がもっと長くなれば、生徒たちの\n貧血率は上昇し人気は低下……良い発想です。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:36 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:36 
msgid "だから……\nいや……その……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:37 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:37 
msgid "（一見、目的から離れているようだが、\n　目指す成果に直結している……なるほど）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:38 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:38 
msgid "さすがは元校長。\n私には、とてもできないやり方ですね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:39 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:39 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:40 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:40 
msgid "（私には出せない素晴らしいアイディアだな。\n　教頭のくせに……いや待て？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:41 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:41 
msgid "（うまく教頭の力を借りることができれば、\n　私が東雲を手に入れる作戦も弌弌よし）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:42 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:42 
msgid "ところで教頭先生弌弌\nひとつ交換条件があるのですが……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:43 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:43 
msgid "ヒッ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:44 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:44 
msgid "……教頭？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:45 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:45 
msgid "い、いや、この話はなかったことに……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:46 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:46 
msgid "え、演説を長引かせる機械とやらは？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:47 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:47 
msgid "……も、もういいんです！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:48 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:48 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:49 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:49 
msgid "……なぜだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:50 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:50 
msgid "（まあ、どうせろくでもないことだろう。\n　考えないでおこう）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:51 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:51 
msgid "それで、その機械をいつまでに\n作ればいいんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:52 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:52 
msgid "来週の全校集会までにあれば\n嬉しいのですが……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:53 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:53 
msgid "（ってことは休日返上か。めんどくさ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:54 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:54 
msgid "やってみますけど、教頭……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:55 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:55 
msgid "はい？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:56 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:56 
msgid "休日を使って作るわけですから、\nもちろん代休は頂けますよね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:57 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:57 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:58 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:58 
msgid "あと材料費も請求しますね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:59 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:59 
msgid "…………！！\n"
msgstr "...... !!\n"

#  speaker 
#  warnings: 0 
#: sc/0144:60 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:60 
msgid "……わかりました。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:61 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:61 
msgid "では週明けまでに用意しますね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:62 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:62 
msgid "はい……お願いします……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:63 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:63 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:64 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:64 
msgid "適当な装置１つで休日と臨時収入……\n良いカモだな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:65 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:65 
msgid "（確実に話が長くなる機械、か……）\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0144:66 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0144:66 
msgid "（元話の長い校長だからな。話の途中で\n　適当に邪魔するだけの装置でも十分だろう）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:67 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:67 
msgid "（だが、教頭の相手は面倒だな……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:68 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:68 
msgid "それで中村先生、機械は作れそうですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:69 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:69 
msgid "無理ですね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:70 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:70 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:71 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:71 
msgid "使うたびに何度でも、\n確実に演説を長引かせる機械。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:72 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:72 
msgid "そんな物が作れるのなら、私は\n教師ではなく科学者になっています。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:73 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:73 
msgid "は、はは……\nそ、それもそうですね……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:74 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:74 
msgid "すみません。何も力になれなくて。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:75 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:75 
msgid "いえ、無理を言ったのは私のようですし。\nこちらこそすいません。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:76 
msgid "教頭先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:76 
msgid "仕事の邪魔をしてすいません。\nそれでは。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:77 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:77 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0144:78 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0144:78 
msgid "あれは諦めてないな。\n次の全校集会が楽しみだ。\n"
msgstr ""
