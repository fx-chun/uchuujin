#: sc/0146:0 
msgid "玉村"
msgstr "Tamamura"

#  text 
#  warnings: 0 
#: sc/0146:0 
msgid "いらっしゃいませ―。\n"
msgstr "Welcome!\n"

#  speaker 
#  warnings: 0 
#: sc/0146:1 
msgid "中村先生"
msgstr "Nakamura-sensei"

#  text 
#  warnings: 0 
#: sc/0146:1 
msgid "え―、今日は何にするかな……\nコ―ヒ―フラペチ―ノでも……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0146:2 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0146:2 
msgid "いや、たまにはコ―ヒ―から\n離れてみるのもいいか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0146:3 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0146:3 
msgid "ん―……。\n"
msgstr "Hmn.......\n"

#  speaker 
#  warnings: 0 
#: sc/0146:4 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0146:4 
msgid "お、あのケ―キ、うまそう……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0146:5 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0146:5 
msgid "あれに合いそうなのは……\nハ―ブティかな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0146:6 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0146:6 
msgid "さて、頼むか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0146:7 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0146:7 
msgid "あの、お客様……。\n"
msgstr "Ah, costumer may I....\n"

#  speaker 
#  warnings: 0 
#: sc/0146:8 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0146:8 
msgid "ん？　ああ、注文いいですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0146:9 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0146:9 
msgid "あの、当店はペットを連れての入店は\nお断りさせて頂いていまして……。\n"
msgstr ""
"Sorry for the inconvinience but we do not allow pets to be in this café..\n"

#  speaker 
#  warnings: 0 
#: sc/0146:10 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0146:10 
msgid "ペット？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0146:11 
msgid "バディ"
msgstr "Buddy"

#  text 
#  warnings: 0 
#: sc/0146:11 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0146:12 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0146:12 
msgid "……この犬は私が飼っているわけでは\nないんだが。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0146:13 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0146:13 
msgid "で、ですが、ピッタリと離れませんし……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0146:14 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0146:14 
msgid "……はぁ。\n外に出してくる。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0146:15 
msgid "うわっ！　店の外に放り出される！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0146:16 
msgid "危ない！　アンテナが折れないように、\n手でガ―ドするんだ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0146:17 
msgid "おう！\n"
msgstr "Roger!\n"

#  text 
#  warnings: 1 
#: sc/0146:18 
msgid "おい！\n今、手でガ―ドしろって言ったろ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0146:19 
msgid "やろうとしたさ！\nでも……。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0146:20 
msgid "でも？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0146:21 
msgid "犬だから、手は無理だった……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0146:22 
msgid "笹原"
msgstr "Sasahara"

#  text 
#  warnings: 0 
#: sc/0146:22 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0146:23 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0146:23 
msgid "！？\n"
msgstr "!?\n"

#  speaker 
#  warnings: 0 
#: sc/0146:24 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0146:24 
msgid "（こ、こいつは……うちの学校の生徒……\n　どうしてカウンタ―にこいつが？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0146:25 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0146:25 
msgid "どうした？　注文は決まっておらんのか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0146:26 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0146:26 
msgid "あ、いや。イチゴのシフォンケ―キと、\nハ―ブティを。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0146:27 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0146:27 
msgid "ふむ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0146:28 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0146:28 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0146:29 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0146:29 
msgid "ふふふ。シフォンケ―キとハ―ブティか。\nなかなかやりおるな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0146:30 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0146:30 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0146:31 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0146:31 
msgid "笹原。今のお前は店員だ。\n客に向かってその口調で話すのはやめておけ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0146:32 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0146:32 
msgid "何故だ？　笹原家の長男である以上、\nいつどんな時も偽らぬ姿を見せるのが弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0146:33 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0146:33 
msgid "今は店員だ。偽れ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0146:34 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0146:34 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0146:35 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0146:35 
msgid "プッ……ワハハハハハ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0146:36 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0146:36 
msgid "深いな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0146:37 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0146:37 
msgid "どんな身分の者であろうと時と場合によって\n恥を捨てろというか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0146:38 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0146:38 
msgid "流石は教師。言葉の重みが違う。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0146:39 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0146:39 
msgid "だが私は私のままで行こう。\nそれが笹原家の長男としての誇りだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0146:40 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0146:40 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0146:41 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0146:41 
