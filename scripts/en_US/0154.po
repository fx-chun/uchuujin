#  speaker 
#  warnings: 0 
#: sc/0154:0 
msgid "高崎先生"
msgstr "Takasaki-sensei"

#  text 
#  warnings: 0 
#: sc/0154:0 
msgid "さてと、今日の夕飯は何にしようかな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:1 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:1 
msgid "毎日毎日コンビニ弁当も飽きたし、\nたまには少しくらい奮発して……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:2 
msgid "声（中之条さん）"
msgstr "Voice (Nakanojo-san)"

#  text 
#  warnings: 0 
#: sc/0154:2 
msgid "ぐふぅっ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:3 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:3 
msgid "！？\n"
msgstr "!?\n"

#  speaker 
#  warnings: 0 
#: sc/0154:4 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:4 
msgid "（何だ今の苦しそうな声は！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:5 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:5 
msgid "（なぜか誰も気にしてないが……。\n　何だ？　どういう事だ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:6 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:6 
msgid "（いや、そんなことはいい！　行くぞ高崎学！\n　それが教育者ってもんだ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:7 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:7 
msgid "なにか飛び出してきた！！！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:8 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:8 
msgid "って、校長先生っ！？\nよ、避けてくださ――いっ！！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0154:9 
msgid "回避行動、急げ！！\nぶつかるぞ―！！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0154:10 
msgid "み、右っ！\nうわ違った、左……あれ？　右！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0154:11 
msgid "違う、そのまま右に弌弌あああっ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0154:12 
msgid "俺、昔からラジコン苦手なんだ――――っ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:13 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:13 
msgid "（誰か倒れてるぞ！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:14 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:14 
msgid "な、なんだこのきぐるみは……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:15 
msgid "中之条さん"
msgstr "Nakanojo-san"

#  text 
#  warnings: 0 
#: sc/0154:15 
msgid "うぐぐ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:16 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:16 
msgid "（しゃべった―！！！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:17 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:17 
msgid "フェアは……大福フェアはこれからなのに。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:18 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:18 
msgid "大福くんが居なければ\n大福フェアは成り立たないというのに……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:19 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:19 
msgid "（だ、大福？\n　その珍妙なかぶりものは大福だったのか！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:20 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:20 
msgid "（……何だか嫌な予感がしてきた。\n　ここは救急車を呼ぶだけに弌弌）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:21 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:21 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0154:22 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:22 
msgid "（目があった―――！！！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:23 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:23 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:24 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:24 
msgid "（わ、渡された―っ！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:25 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:25 
msgid "やるしか……ないのか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:26 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:26 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:27 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:27 
msgid "ええい、ままよ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:28 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:28 
msgid "（と、かぶってみたが……\n　何をやればいいのかさっぱりわからん）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:29 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:29 
msgid "（近くにあった大福の屋台を\n　宣伝すればいいんだろうか……？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:30 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:30 
msgid "（そうだ、そうに違いない。\n　弌弌よし、行くぞ！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:31 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:31 
msgid "みなさ―ん！　大福フェア開催中ですよ―！\nどうぞ見ていってくださ―い！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:32 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:32 
msgid "どうぞ―、寄ってらっしゃい見てらっしゃい！\n大福フェア開催中ですよ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:33 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:33 
msgid "おい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:34 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:34 
msgid "あ、はい。なんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:35 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:35 
msgid "大福がしゃべるな！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:36 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:36 
msgid "！！！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:37 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:37 
msgid "見ちゃおれん！\nホレッ、早く脱げ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:38 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:38 
msgid "（ええ―――っ！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:39 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:39 
msgid "（動けなかったはずじゃないのか！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:40 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:40 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:41 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:41 
msgid "……おい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:42 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:42 
msgid "ぼ―っとしてないで店番でもやったらどうだ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:43 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:43 
msgid "（ええええ――――っ！！！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:44 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:44 
msgid "（……仕方ない。\n　困ってる人を助けるのが教師ってもんだ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:45 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:45 
msgid "わかりました。\nそれで、これをかぶって何をすれば？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:46 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:46 
msgid "……踊ってくれ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:47 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:47 
msgid "（ええ―っ！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:48 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:48 
msgid "……わ、わかりました。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:49 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:49 
msgid "（踊るって……こんな感じだろうか）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:50 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:50 
msgid "（よっ！　はっ！\n　……何だろう、この微妙な感覚は）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:51 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:51 
msgid "（そうか！\n　外がまったく見えないのが原因か！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:52 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:52 
msgid "（今、誰かが見てるのか？　それとも\n　オレは一人で寂しく踊ってるのか？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:53 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:53 
msgid "（だ、誰か反応してくれ……！\n　頼む、せめて笑ってくれっ！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:54 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:54 
msgid "（……だ、誰も居ない？\n　いや、少なくともあの男性は居るはず）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:55 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:55 
msgid "（ちょ、ちょっとぐらい外してもいいよな？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:56 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:56 
msgid "……あの人はどこだ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:57 
msgid "救急隊員"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:57 
msgid "多分ぎっくり腰ですね―、\n搬送しますのでじっとしててください。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:58 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:58 
msgid "（救急車―！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:59 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:59 
msgid "だ、大丈夫ですか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:60 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:60 
msgid "……大丈夫だ。\nそれよりも青年！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:61 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:61 
msgid "は、はい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:62 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:62 
msgid "ナイスダンス！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:63 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:63 
msgid "（イェェェェス！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:64 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:64 
msgid "後は頼んだ弌弌ぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:65 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:65 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:66 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:66 
msgid "（イェェェェスじゃね―！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:67 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:67 
msgid "（……仕方ない、かぶるとしよう）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:68 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:68 
msgid "（しかしこれ、\n　かぶったはいいが何をすればいいんだ？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:69 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:69 
msgid "（…………とりあえず手でも振っておくか）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:70 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:70 
msgid "（そもそも、大福フェアってのは何なんだ？\n　いや、言葉の意味はわかるが……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:71 
msgid "声（警察官）"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:71 
msgid "すみません。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:72 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:72 
msgid "あ、はい。なんでしょうか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:73 
msgid "警察"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:73 
msgid "警察の者ですがちょっと\nお話を聞かせてもらっても？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:74 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:74 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:75 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:75 
msgid "いや、私は頼まれて代理をしてただけで……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:76 
msgid "警察"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:76 
msgid "あ―、動かないで。\nとにかくお話をですね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:77 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:77 
msgid "ち、違います！　違うんです！\n普段はこんな格好していなくて！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:78 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:78 
msgid "むしろ教育者として……その、何だ。\nえ―っと、そう！　教育者なんです！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:79 
msgid "警察"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:79 
msgid "あ―、そうですか。\nとりあえず詳しいお話は署でお願いします。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0154:80 
msgid "高崎先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0154:80 
msgid "え、えぇ―……。\n"
msgstr ""
