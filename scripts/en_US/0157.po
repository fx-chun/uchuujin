#  speaker 
#  warnings: 0 
#: sc/0157:0 
msgid "はかせ"
msgstr "Hakase"

#  text 
#  warnings: 0 
#: sc/0157:0 
msgid "ん―、いいお天気。\nこんな日はお散歩もいいね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:1 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:1 
msgid "せっかくだから、\n少し遊んでいこうかな―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:2 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:2 
msgid "……ん？\n"
msgstr "……Hmm?\n"

#  speaker 
#  warnings: 0 
#: sc/0157:3 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:3 
msgid "あの人、何やってるんだろう？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:4 
msgid "桜井誠"
msgstr "Makoto"

#  text 
#  warnings: 0 
#: sc/0157:4 
msgid "よっと。１、２、３、４……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:5 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:5 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0157:6 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:6 
msgid "よし。これから碁石を追加して……\n１、２、３……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:7 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:7 
msgid "…………！\n"
msgstr "………… !\n"

#  speaker 
#  warnings: 0 
#: sc/0157:8 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:8 
msgid "よし、いい感じ。このまま弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:9 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:9 
msgid "…………！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:10 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:10 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:11 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:11 
msgid "え、え―っと……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:12 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:12 
msgid "（何なんだ……？\n　すごい見られてる……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:13 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:13 
msgid "（囲碁サッカ―に興味があるのかな？\n　それともリフティングが続くのが楽しい？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:14 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:14 
msgid "あっ、強く蹴りすぎた！\n……ああっ、あぶない！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:15 
msgid "バディ"
msgstr "Buddy"

#  text 
#  warnings: 0 
#: sc/0157:15 
msgid "キャイン！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0157:16 
msgid "ミスキックのボ―ルが激突！\nアンテナ、破損しました！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0157:17 
msgid "く、なんて不運な！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0157:18 
msgid "ふう―ん。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0157:19 
msgid "え！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:20 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:20 
msgid "あれ？　なんでやめちゃうの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:21 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:21 
msgid "えっ？　見てたかった？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:22 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:22 
msgid "うん。すごく楽しそうだったもん。\nこれサッカ―だよね？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:23 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:23 
msgid "う―ん、ちょっと違うかな。\nサッカ―じゃなくて、囲碁サッカ―だよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:24 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:24 
msgid "試しにやってみる？\n楽しいよ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:25 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:25 
msgid "本当！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:26 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:26 
msgid "うん。頑張ってみて。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:27 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:27 
msgid "よ―し！　せ―の！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:28 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:28 
msgid "あれ？　おにいさんみたいに\n上手くできない。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:29 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:29 
msgid "リフティングは難しかったかな？\nでも練習すればすぐにできるようになるよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:30 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:30 
msgid "む―……\nはかせ今すぐできるようになりたい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:31 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:31 
msgid "そうだ！　囲碁サッカ―のためのロボットを\n作ればいいんだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:32 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:32 
msgid "ロボット？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:33 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:33 
msgid "うん！　ちょっとボ―ル借りるね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:34 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:34 
msgid "え、う、うん……。\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0157:35 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0157:35 
msgid "これをこ―して\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:36 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:36 
msgid "うわっ！？　ちょっと！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:37 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:37 
msgid "じゃじゃ―ん！　完成―！\n自動リフティングボ―ル！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:38 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:38 
msgid "このスイッチを押せば……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:39 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:39 
msgid "ほらほら！　はかせもリフティング\nできるよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:40 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:40 
msgid "…………。\n"
msgstr "………….\n"

#  speaker 
#  warnings: 0 
#: sc/0157:41 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:41 
msgid "ははは、すぐには無理だよ。\n囲碁サッカ―は難しい競技だからね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:42 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:42 
msgid "オレだってここまでくるのに\n何年も練習したんだから。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:43 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:43 
msgid "じゃあはかせもいっぱい練習したら\n囲碁サッカ―を極められる？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:44 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:44 
msgid "うん。オレもまだ極めてないから\nどっちが先に極めるか勝負だね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:45 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:45 
msgid "勝負する！　はかせ負けないからね―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:46 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:46 
msgid "よし。じゃあキミにこのボ―ルをあげるよ。\nこれでいっぱい練習してね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:47 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:47 
msgid "うん！　ありがとう！\n"
msgstr "Yeah! Thank you!\n"

#  speaker 
#  warnings: 0 
#: sc/0157:48 
msgid "ナレ―タ―お姉さん"
msgstr "Narrator Lady"

#  text 
#  warnings: 0 
#: sc/0157:48 
msgid "数日後、このボ―ルは物置に収められ、\n永久に忘れられることになったといいます。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:49 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:49 
msgid "今すぐか―。囲碁サッカ―は奥が深いから\n難しいと思うよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:50 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:50 
msgid "囲碁サッカ―ってどんなことするの？\nさっきみたいなこと？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:51 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:51 
msgid "ん―、そうだね。\n一連の流れを見せてあげるよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:52 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:52 
msgid "よっ。これが普通のリフティング。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:53 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:53 
msgid "わくわく。わくわく。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:54 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:54 
msgid "それで、ここから碁石を追加。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:55 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:55 
msgid "おお―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:56 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:56 
msgid "ははは。そしてここからが\n囲碁サッカ―、最大の見せ場！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:57 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:57 
msgid "一人ジャ―マン！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:58 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:58 
msgid "！？\n"
msgstr "!?\n"

#  speaker 
#  warnings: 0 
#: sc/0157:59 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:59 
msgid "これぞ定石・三連星！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:60 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:60 
msgid "！？\n"
msgstr "!?\n"

#  speaker 
#  warnings: 0 
#: sc/0157:61 
msgid "桜井誠"
msgstr "Makoto Sakurai"

#  text 
#  warnings: 0 
#: sc/0157:61 
msgid "…………！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0157:62 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:62 
msgid "…………！\n"
msgstr "…………！\n"

#  speaker 
#  warnings: 0 
#: sc/0157:63 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0157:63 
msgid "……囲碁サッカ―するのやめる。\n"
msgstr ""
