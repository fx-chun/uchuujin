#  speaker 
#  warnings: 0 
#: sc/0160:0 
msgid "笹原"
msgstr "Sasahara"

#  text 
#  warnings: 0 
#: sc/0160:0 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0160:1 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:1 
msgid "こうして一句捻ろうと思って歩くと、\nどうにも上手く浮かばないものだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:2 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:2 
msgid "世の中おしなべてそういうものか。\n悩むだけで簡単に浮かぶなら苦労はせん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:3 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:3 
msgid "気長に行くとしよう、悩みながら歩くうちに\n何かいいきっかけでもあるだろうからな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:4 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:4 
msgid "とはいえ、こうして代わり映えしない\n風景を見続けるのもいささか飽きてきた所だ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:5 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:5 
msgid "何か愉快な現場でもあれば……む、あれは。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:6 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:6 
msgid "あの先を歩くは校長先生ではなかろうか？\n後ろ姿ではあるが……間違いないようだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:7 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:7 
msgid "……ふむ、それにしても、\nかぶいた帽子をかぶっておられる。\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0160:8 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0160:8 
msgid "うっすらと現れたり消えたり……実に面白い。\n少話を聞かせて頂こう！\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0160:9 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0160:9 
msgid "校長！　少よろしいか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:10 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:10 
msgid "その愉快な帽子なのだが、\n見せて貰ってもいいだろうか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:11 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:11 
msgid "いやなに、手間はとらせん。\nこうして手にとって見るだけで……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:12 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:12 
msgid "む！？\n"
msgstr "Hmm!?\n"

#  speaker 
#  warnings: 0 
#: sc/0160:13 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:13 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:14 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:14 
msgid "……ふむ。\n"
msgstr "……Hmm.\n"

#  speaker 
#  warnings: 0 
#: sc/0160:15 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:15 
msgid "なんと、飾り部分は使い捨てであったか。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0160:16 
msgid "ええ―っ！？\n"
msgstr "EHH—!?\n"

#  text 
#  warnings: 1 
#: sc/0160:17 
msgid "ええ――――っ！？\n"
msgstr "Ehh————!?\n"

#  speaker 
#  warnings: 0 
#: sc/0160:18 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:18 
msgid "（子供か。\n　丁度あそこの家から出てきたようだな）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:19 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:19 
msgid "ふむ……しかし、あれだな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:20 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:20 
msgid "（引きずるような白衣姿とは、\n　これはなんとも子供らしからぬ格好）\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0160:21 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0160:21 
msgid "（なにやら面白げな予感がするな。\n　少見物させてもらうとしよう）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:22 
msgid "はかせ"
msgstr "Hakase"

#  text 
#  warnings: 0 
#: sc/0160:22 
msgid "ふっふっふ、こっそりおかしをかくしても\nはかせにはお見通しだもんね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:23 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:23 
msgid "なのに見つかるとおこられちゃうから、\nここでこっそり食べちゃおっと。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:24 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:24 
msgid "……おいしい！\n外で食べるとおかしがさらにおいしい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:25 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:25 
msgid "これは大発見かも！\n後でなのにもおしえてあげなくっちゃ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:26 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:26 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:27 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:27 
msgid "（何とものどかな光景だ。\n　これは一句浮かびそうだぞ……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:28 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:28 
msgid "……あれ？\n"
msgstr "……Wait, what?\n"

#  speaker 
#  warnings: 0 
#: sc/0160:29 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:29 
msgid "む。\n"
msgstr "Hmm.\n"

#  speaker 
#  warnings: 0 
#: sc/0160:30 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:30 
msgid "こんにちは。\n"
msgstr "Hello.\n"

#  speaker 
#  warnings: 0 
#: sc/0160:31 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:31 
msgid "うむ、こんにちは。\n"
msgstr "Hmm, hello.\n"

#  speaker 
#  warnings: 0 
#: sc/0160:32 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:32 
msgid "ん―と、え―っと……。\n"
msgstr "Um, eh…….\n"

#  speaker 
#  warnings: 0 
#: sc/0160:33 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:33 
msgid "もしかして、不審者？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:34 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:34 
msgid "！？\n"
msgstr "!?\n"

#  speaker 
#  warnings: 0 
#: sc/0160:35 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:35 
msgid "いや、ただの学生である。\n丁度一句捻っていたところでな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:36 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:36 
msgid "いっく？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:37 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:37 
msgid "俳句のことだ。\n御子のおかげで浮かんだ所だ、感謝する。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:38 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:38 
msgid "俳句……！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:39 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:39 
msgid "すごいすごい！\n俳句よむひとだ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:40 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:40 
msgid "おや、御子は俳句に興味が？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:41 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:41 
msgid "うん！\nだって芥川賞とれるでしょ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:42 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:42 
msgid "……芥川賞、か。\n切れ味の良い返しだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:43 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:43 
msgid "よし、浮かんだ句は御子に進呈しよう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:44 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:44 
msgid "えっ、くれるの！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:45 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:45 
msgid "うむ、これだ。\nこっそりと　おかしを食べる　子供かな\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:46 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:46 
msgid "弌弌実にいまひとつだ。\n良いのが浮かんだと思ったのだが。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:47 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:47 
msgid "さて、これを進呈する。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:48 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:48 
msgid "えっ？　え……？\nでも、いまひとつじゃないの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:49 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:49 
msgid "だが、進呈するといったからには\n渡さぬわけにはいかん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:50 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:50 
msgid "それが筋というものだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:51 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:51 
msgid "うん、ありがと……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:52 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:52 
msgid "うむ、では達者でな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:53 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:53 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:54 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:54 
msgid "……すごいひとだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:55 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:55 
msgid "人をいきなり不審者呼ばわりとは……\n無礼であろうが！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:56 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:56 
msgid "ひっ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:57 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:57 
msgid "おっと、すまぬ。\nついつい熱くなってしまったわ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:58 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:58 
msgid "だが御子、見知らぬ人間を唐突に\n悪人扱いするのは良くない。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:59 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:59 
msgid "……ゴメンナサイ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:60 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:60 
msgid "よしよし、良い子だ。\n褒美に菓子でもやるとしよう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:61 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:61 
msgid "む……しまった、飴の補充を忘れていたか。\nいつも懐に入れているというのに。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:62 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:62 
msgid "……ええっ、アメ無いの！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:63 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:63 
msgid "この私としたことが失態だ。\n……そうだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:64 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:64 
msgid "これも何かの縁だ、\n共に菓子を買いに行こうではないか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:65 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:65 
msgid "ええっ、いいの！？\n"
msgstr "Huh, really!?\n"

#  speaker 
#  warnings: 0 
#: sc/0160:66 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:66 
msgid "うむ、その程度お安い御用だ。\nすぐそこの店で構わんな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:67 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:67 
msgid "うんっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:68 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:68 
msgid "では行くとしよう。\n……しかし何だな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:69 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:69 
msgid "こうやって菓子でつって、一緒に他所へ\n行くあたりは……まるで弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:70 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:70 
msgid "まるで？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:71 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:71 
msgid "まるで誘拐しているようだな。\nわははははっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:72 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:72 
msgid "！？\n"
msgstr "!  ?\n"

#  speaker 
#  warnings: 0 
#: sc/0160:73 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:73 
msgid "ぬかしおる。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:74 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:74 
msgid "え？　……え？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:75 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:75 
msgid "おお、いい句がひらめいたぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:76 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:76 
msgid "夕暮れに　遊ぶ子供の　不審顔\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:77 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:77 
msgid "うむ、上出来じゃないか！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:78 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:78 
msgid "…………。\n"
msgstr "..........\n"

#  speaker 
#  warnings: 0 
#: sc/0160:79 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:79 
msgid "うむ、そこの御子。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:80 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:80 
msgid "は、はい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:81 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:81 
msgid "貴様の言葉が句を生む手助けとなった。\n感謝するぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:82 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:82 
msgid "ええっ？　ど、どういたしまして。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:83 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:83 
msgid "うむ。\nでは、私はここで失礼するとしよう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:84 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:84 
msgid "……はい。\n"
msgstr "……Yes.\n"

#  speaker 
#  warnings: 0 
#: sc/0160:85 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:85 
msgid "達者でな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:86 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:86 
msgid "……あっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0160:87 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0160:87 
msgid "なの―！　なのなのっ！\n何かいますごい人がっ！！\n"
msgstr ""
