#  speaker 
#  warnings: 0 
#: sc/0165:0 
msgid "桜井誠"
msgstr "Makoto"

#  text 
#  warnings: 0 
#: sc/0165:0 
msgid "よっ、は……っ、ああっ！？\n"
msgstr "sak\n"

#  speaker 
#  warnings: 0 
#: sc/0165:1 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:1 
msgid "……う―ん。\n何か今日は調子が悪いなぁ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:2 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:2 
msgid "さっきからこれで五回目の失敗かぁ。\nいつもと同じはずなんだけど……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:3 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:3 
msgid "ひょっとしてスランプ？\nなんて、そんなわけないか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:4 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:4 
msgid "悩んでないで練習練習。今、戦力になるのは\n部でオレだけっぽいし、頑張らないと。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:5 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:5 
msgid "よっ、っと……おっとと。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:6 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:6 
msgid "よし……今度はいい感じに弌弌あっ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0165:7 
msgid "ボ―ルが飛んでくるぞ、回避急げ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0165:8 
msgid "まあ慌てなさんな。こんなこともあろうかと、\n覇道砲を準備しておいたのさ！\n"
msgstr ""

#  text 
#  warnings: 3 
#: sc/0165:9 
msgid "覇道砲、発射準備！エネルギ―充填１００！\n……ぽちっとな\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0165:10 
msgid "おい、早くしろって！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0165:11 
msgid "……あれ？　あれれっ！？　……あ、そうだ！\n"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0165:12 
msgid "覇道砲と言えば１２０ですよね―！\n……次は気をつけよっと。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0165:13 
msgid "次って、もうないわ――！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:14 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:14 
msgid "うわぁっ！？\n人に当たっちゃった……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:15 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:15 
msgid "スッスススイマセン！\n大丈夫ですかっ！！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:16 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:16 
msgid "あっ、あの人ってもしかして……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:17 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:17 
msgid "やっぱり、モヒカンの人だ！！\nどうしよう……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:18 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:18 
msgid "あの人って色んな噂あるんだよなぁ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:19 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:19 
msgid "あんな髪型なのに\n先生にぜんぜん呼び出されないらしいし。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:20 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:20 
msgid "あと、横断歩道でゆっくり歩いてたお年寄りを\n無理やり早く渡らせたとか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:21 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:21 
msgid "他にも花壇でジョウロ持って\n何かしてたとか……うわっ、こっち来てる！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:22 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:22 
msgid "ボ―ル持ってるってことは、\nオレのだってわかってるって事だよな……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:23 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:23 
msgid "ひょ、ひょっとして殴られるかも……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:24 
msgid "中之条"
msgstr "Nakanojo"

#  text 
#  warnings: 0 
#: sc/0165:24 
msgid "お―い、これ君のだよな―？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:25 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:25 
msgid "…………。\n"
msgstr "………….\n"

#  speaker 
#  warnings: 0 
#: sc/0165:26 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:26 
msgid "（う、うわ、どうしよう……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:27 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:27 
msgid "（わ、悪いのはオレなんだけど……\n　でも、やっぱり怖いし……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:28 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:28 
msgid "お―い！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:29 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:29 
msgid "（弌弌よしっ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:30 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:30 
msgid "（逃げろっ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:31 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:31 
msgid "あっ、おい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:32 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:32 
msgid "なっ！！？　追いかけてきた！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:33 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:33 
msgid "うおおおおおお！！\n"
msgstr "AHHHHHH!!\n"

#  speaker 
#  warnings: 0 
#: sc/0165:34 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:34 
msgid "うわっ、足速っ！！\nこ、これは捕まったらヤバい……！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:35 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:35 
msgid "待てええぇぇぇ！！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:36 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:36 
msgid "はぁっ、はぁっ、は弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:37 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:37 
msgid "ぜぇ……ぜぇ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:38 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:38 
msgid "……ははっ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:39 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:39 
msgid "弌弌ふっ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:40 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:40 
msgid "ナイスラン！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:41 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:41 
msgid "ナイスラン！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:42 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:42 
msgid "（いや、素直に謝るべきだよ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:43 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:43 
msgid "（そりゃあ悪い噂はあるけど、\n　そんなとんでもない人じゃないはず！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:44 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:44 
msgid "お―い、これ君の？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:45 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:45 
msgid "はい。すいません、大丈夫でしたか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:46 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:46 
msgid "平気だよ。\nはい、これ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:47 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:47 
msgid "ありがとうございます！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:48 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:48 
msgid "（ほら、いい人じゃないか。\n　素直に謝って正解だったよ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:49 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:49 
msgid "ところで君、サッカ―部？\nこんな所で練習なんて珍しいね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:50 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:50 
msgid "いえ、オレは囲碁サッカ―部なんです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:51 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:51 
msgid "い、囲碁サッカ―！？\nそ、そっか……うん、練習頑張って。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:52 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:52 
msgid "ありがとうございます！\nあの、本当にすいませんでした。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:53 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:53 
msgid "いいっていいって。そ、それじゃあ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:54 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:54 
msgid "良かった……変な噂の多い人だし、\nどうなるかと思ったよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:55 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:55 
msgid "でも、やっぱり噂は噂。\n噂とか見かけで判断しちゃダメだよな！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:56 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:56 
msgid "ふぅ、まさか囲碁サッカ―部だったとは。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:57 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:57 
msgid "頭痛いけど、変な噂ばっかり聞く部だし、\nこれ以上関わらない方がいいもんな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:58 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:58 
msgid "……そうだ、知らないフリしよう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:59 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:59 
msgid "お―い、これ君のボ―ル？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:60 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:60 
msgid "え……いえ、チガイマス。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:61 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:61 
msgid "え、違うの？\nこっちから飛んできたんだけど。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:62 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:62 
msgid "（おかしいな。すごくあせっていたし、\n　方向も間違いないと思うんだけど……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:63 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:63 
msgid "そ、そういえば！\nあっちに誰か走っていきましたよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:64 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:64 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:65 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:65 
msgid "（まさか僕を怖い不良だと思っている！？\n　やはり、この髪型のせいだろうか……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:66 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:66 
msgid "（それはまずい、誤解はちゃんと解かないと。\n　決して僕は不良なんかじゃないんだって！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:67 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:67 
msgid "……よし。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:68 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:68 
msgid "はい？\n"
msgstr "Yes?\n"

#  speaker 
#  warnings: 0 
#: sc/0165:69 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:69 
msgid "このボ―ル、ずいぶん汚れてるな―。\nきれいに磨いてから返してあげようかな―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:70 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:70 
msgid "え！？　あ、あのっ。\nそこまでしなくてもいいんじゃないですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:71 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:71 
msgid "（……ちょっとわざとらしかっただろうか）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:72 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:72 
msgid "（でも、不良と思われてるなら、\n　これぐらいしないと取り戻せないはずだ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:73 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:73 
msgid "いや―、何だか驚かせたみたいだし、\nそれに汚れも気になったからさ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:74 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:74 
msgid "（よし、いけるぞ。\n　このまま不良のイメ―ジを打ち消すんだ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:75 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:75 
msgid "この辺りに置いとけば、\nボ―ルの持ち主も気がついてくれるかな―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:76 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:76 
msgid "た、多分、気付くと思います。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:77 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:77 
msgid "（……あれ？\n　この人、実は不良じゃないのか？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:78 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:78 
msgid "（だとしたら、オレ、\n　ものすごく悪いことしてるんじゃ……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:79 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:79 
msgid "…………あ、あの。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:80 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:80 
msgid "ん？\n"
msgstr "Hmm?\n"

#  speaker 
#  warnings: 0 
#: sc/0165:81 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:81 
msgid "す、すいません。\n弌弌実はオレなんです、ボ―ルぶつけたの。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:82 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:82 
msgid "（多分、正直に謝ればこの人も許して弌弌）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:83 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:83 
msgid "あ、うん、それは知っていたんだ。\nでも気にしてないから大丈夫。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:84 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:84 
msgid "え！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:85 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:85 
msgid "（よし！　これで\n　僕が不良じゃないと証明されたはずだ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:86 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:86 
msgid "……し、知っていた？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:87 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:87 
msgid "（ま、まさか、\n　オレが逃げられないよう自白させたっ！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:88 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0165:88 
msgid "（マズイぞ。オレ、この人のこと誤解していた。\n　不良どころじゃない……プロだっ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0165:89 
msgid "中之条"
msgstr ""

#  text 
