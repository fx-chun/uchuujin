#  speaker 
#  warnings: 0 
#: sc/0176:0 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:0 
msgid "ふむ……。\n"
msgstr "Hmph…….\n"

#  speaker 
#  warnings: 0 
#: sc/0176:1 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:1 
msgid "………………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0176:2 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:2 
msgid "屋上から眺める時定市、絶景ですな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:3 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:3 
msgid "………………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:4 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:4 
msgid "……しかし、なぜわしはここに？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:5 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:5 
msgid "この学校に迷い込んで、はや数時間。\n一体、いつになったら出られるのやら……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:6 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:6 
msgid "……だよ。やっぱりアレだよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:7 
msgid "ウェボシ―"
msgstr "Weboshi"

#  text 
#  warnings: 0 
#: sc/0176:7 
msgid "うん。紳士だ。絶対に紳士だ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:8 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:8 
msgid "……ん、この声は。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:9 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:9 
msgid "……おや、お嬢さんが二人。\nごきげんよう。良いお日柄ですな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:10 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:10 
msgid "すご―い！　ごきげんようだって！\nお日柄だって！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:11 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:11 
msgid "さすが紳士！　やるわね。\nけど、こっちだって……！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:12 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:12 
msgid "ボ……ボンボヤ―ジ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:13 
msgid "ジェントルマン＆フェっちゃん"
msgstr "Gentleman & Fe-chan"

#  text 
#  warnings: 0 
#: sc/0176:13 
msgid "………………。\n"
msgstr "……………….\n"

#  speaker 
#  warnings: 0 
#: sc/0176:14 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:14 
msgid "ウェボシ―、何それ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:15 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:15 
msgid "挨拶に決まってるじゃない。\nどう？　ジェントルでしょ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:16 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:16 
msgid "ん―、違うんじゃない？\nていうかボンボヤ―ジって何？\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0176:17 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0176:17 
msgid "だから挨拶だって。\nイタリア語でごきんげようだっけ？\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0176:18 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0176:18 
msgid "ボンボヤ―ジは良い旅を！という\nフランス語ですぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:19 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:19 
msgid "だってさ。\nウェボシ―かっこ悪〜い。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:20 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0176:20 
msgid "…………。\n"
msgstr "………….\n"

#  speaker 
#  warnings: 0 
#: sc/0176:21 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:21 
msgid "あはは……ん？　あれ？\nフェっちゃん、あそこに校長先生が……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:22 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:22 
msgid "ホントだ。\n何やってんだろ。あんな隅っこで。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:23 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:23 
msgid "校長―！　ボンボヤ―ジ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:24 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:24 
msgid "だから、それは良い旅をだから。\nこんにちはじゃないって。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:25 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:25 
msgid "ボンボヤ―ジ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:26 
msgid "ウェボシ―＆フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:26 
msgid "！？\n"
msgstr "!?\n"

#  speaker 
#  warnings: 0 
#: sc/0176:27 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:27 
msgid "い、いかん……\nあの者は飛び降りる気じゃ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:28 
msgid "ウェボシ―＆フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:28 
msgid "！？　こ、校長先生―！\n待って、早まらないで―！！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0176:29 
msgid "おおおお！？\nなんだ、急に女の子たちがこっちに……！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0176:30 
msgid "抱きつかれた！？　ま、まさか、これが噂の\n桃源郷！？　ハ―レムってやつじゃ？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0176:31 
msgid "そうか！　さっきのボンボヤ―ジって言葉には\nそういう意味があったんだな！　地球すげえ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0176:32 
msgid "まさか、ボンボヤ―ジが、\nそんな夢の暗号だったとは弌弌\n"
msgstr ""

#  speaker 
#  warnings: 3 
#: sc/0176:33 
msgid "＆＆"
msgstr ""

#  text 
#  warnings: 3 
#: sc/0176:33 
msgid "……………………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 3 
#: sc/0176:34 
msgid "＆＆"
msgstr ""

#  text 
#  warnings: 3 
#: sc/0176:34 
msgid "そうか……\n桃源郷って……こうやって行くんだ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:35 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:35 
msgid "あはは……流石は紳士。詳しいね―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:36 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:36 
msgid "ところで紳士は\nこんなところで何してたの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:37 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:37 
msgid "それが、いつの間にか迷って弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:38 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:38 
msgid "あっ、わかった！\n子供の様子を見に来たんでしょ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:39 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:39 
msgid "紳士だったら、\n子供って言うよりは孫じゃないの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:40 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:40 
msgid "まあそうかな。\nねえねえ、紳士の孫の名前は何？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:41 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:41 
msgid "孫の名前？　ただしじゃ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:42 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:42 
msgid "ただし？　うちのクラスにいたかな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:43 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:43 
msgid "たかしは小学生じゃから\nいないと思うがのぉ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:44 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:44 
msgid "たかし！？　増えた！　更に孫がいた！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:45 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:45 
msgid "いやいや、孫はさとし一人じゃぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:46 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:46 
msgid "また増えた―！\nしかも一人しか孫と認めてな―い！！\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0176:47 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0176:47 
msgid "し、紳士って実は見えないところで\n色大変なんだね……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:48 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:48 
msgid "ふむ……？\nそういえばつよしは今何をしとるのかの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:49 
msgid "ウェボシ―＆フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:49 
msgid "また増えた―――！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:50 
msgid "中之条"
msgstr "Nakanojo"

#  text 
#  warnings: 0 
#: sc/0176:50 
msgid "……えっ？　今、僕のこと呼びました？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:51 
msgid "ウェボシ―＆フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:51 
msgid "居た―――――！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:52 
msgid "ウェボシ―＆フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:52 
msgid "（ってお前か―――――っ！！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:53 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:53 
msgid "……誰かの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:54 
msgid "ウェボシ―＆フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:54 
msgid "認められてな――――い！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:55 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:55 
msgid "……誰ですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:56 
msgid "ウェボシ―＆フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:56 
msgid "こっちも認めてな――い！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:57 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:57 
msgid "すまんがこの学校の外に\n連れて行ってくれんかの？　迷ってしまって。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:58 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:58 
msgid "あ、はい。\n良いですよ、おじいさん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:59 
msgid "ウェボシ―＆フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:59 
msgid "他人行儀だ―――！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:60 
msgid "ウェボシ―＆フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:60 
msgid "……………………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:61 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:61 
msgid "……あの二人、赤の他人じゃない？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:62 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:62 
msgid "だよね！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:63 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:63 
msgid "いや、実は迷ってしまいましてな。\n気付いたらこんなところにいたんじゃよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:64 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:64 
msgid "え―？　紳士、迷ってたの？\n紳士でも迷うことがあるんだ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:65 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:65 
msgid "じゃあ本当はどこに行きたかったの？\n職員室かな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:66 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:66 
msgid "いや……\nわしはバディを捜していただけなんじゃ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:67 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:67 
msgid "バディ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:68 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:68 
msgid "あ、知ってる、確かバディって\n友達とか仲間って意味だよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:69 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:69 
msgid "おお！　紳士、仲間探ししてんの！？\nこれから旅にでも出るの！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:70 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:70 
msgid "？？　旅に出るわけじゃありませんぞ。\nただ、バディを捜しているだけです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:71 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:71 
msgid "く〜〜〜！　ただ仲間を求めるなんて、\n紳士カッコイイ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:72 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:72 
msgid "じゃあさ、じゃあさ、\nどんなバディを捜してるの？　希望の職業は？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:73 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:73 
msgid "おっ、重要だよ、そこ。条件が合えば\nあたしも仲間にしてもらおっかな―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:74 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:74 
msgid "条件？　職業？　う―む……\nわしの探しているバディは犬なんじゃが。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:75 
msgid "ウェボシ―＆フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:75 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:76 
msgid "ウェボシ―＆フェっちゃん"
msgstr "Weboshi & Fe-chan"

#  text 
#  warnings: 0 
#: sc/0176:76 
msgid "（……犬！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:77 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:77 
msgid "（もしかして……バディって飼い犬？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:78 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:78 
msgid "（犬扱い……そこまで自分を捨てさせないと\n　仲間にしないとは……ヘヴィな紳士め……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:79 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:79 
msgid "（バディは一体どこにいるのかのう……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:80 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:80 
msgid "子供って……紳士の年齢を考えたら\n私たちと同じくらいなわけないじゃん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:81 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:81 
msgid "いや、生徒じゃなくて\n先生って可能性もアリでしょ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:82 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:82 
msgid "あっ！　そっちがあった！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:83 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:83 
msgid "いや……ただ、迷っただけで弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:84 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:84 
msgid "誰かな誰かな？\n高崎先生？　それとも桜井先生！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:85 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0176:85 
msgid "高崎先生は無いよね。この紳士っぽさを\n全く受け継いでないし。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0176:86 
msgid "ジェントルマン"
msgstr ""

#  text 
