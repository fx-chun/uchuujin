#  speaker 
#  warnings: 0 
#: sc/0188:0 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0188:0 
msgid "見ろよ関口、コレ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0188:1 
msgid "関口"
msgstr "Sekiguchi"

#  text 
#  warnings: 0 
#: sc/0188:1 
msgid "囲碁サッカ―だより？\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0188:2 
msgid "大工"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0188:2 
msgid "うちの部のこと書いてあるんだよ。\n新鋭、時定高校囲碁サッカ―部だってさ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0188:3 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0188:3 
msgid "たしかに書いてますね。\nでも、練習試合に勝っただけなのに？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0188:4 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0188:4 
msgid "いや―、それがさ。\n試合した高校って全国常連だったらしくて。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0188:5 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0188:5 
msgid "なるほど。\n"
msgstr "I see.\n"

#  speaker 
#  warnings: 0 
#: sc/0188:6 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0188:6 
msgid "そんなトコに勝てたのも\n桜井くんと高崎先生のおかげだよな！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0188:7 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0188:7 
msgid "私たちは試合に出てませんしね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0188:8 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0188:8 
msgid "この調子でどんどん勝って、\nうちの部がもっと盛り上がればいいよな―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0188:9 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0188:9 
msgid "部員も増えたりしてさ、\n団体戦に出られるようになって……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0188:10 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0188:10 
msgid "部長。\n"
msgstr "Chief.\n"

#  speaker 
#  warnings: 0 
#: sc/0188:11 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0188:11 
msgid "囲碁サッカ―、\n本気で専念するつもりなんですね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0188:12 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0188:12 
msgid "え？\n"
msgstr "Huh?\n"

#  speaker 
#  warnings: 0 
#: sc/0188:13 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0188:13 
msgid "いや、別に専念する気は無いけど？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0188:14 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0188:14 
msgid "ええ……っ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0188:15 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0188:15 
msgid "さっきも言っただろ。\n盛り上がれば、部員が増えるかもって。\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0188:16 
msgid "大工"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0188:16 
msgid "そうすれば、\nもっと色なことで遊べるんだぞ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0188:17 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0188:17 
msgid "部員が増えて、団体戦にも\n出られるような人数になったら……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0188:18 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0188:18 
msgid "人数が足りなくて遊べない、なんて悩みは\nなくなるだろ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0188:19 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0188:19 
msgid "…………。\n"
msgstr "………….\n"

#  speaker 
#  warnings: 0 
#: sc/0188:20 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0188:20 
msgid "で、今、部員が増えたときのこと、\n考えているんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0188:21 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0188:21 
msgid "いや―、大人数でって考えたら、\nこれはこれで何で遊べばいいか悩んでさ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0188:22 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0188:22 
msgid "関口も何かいいの浮かんだら\n教えてくれよな！！\n"
msgstr ""
