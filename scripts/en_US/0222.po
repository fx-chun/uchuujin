#: sc/0222:183 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:183 
msgid "うわ―、すごい弾力！\nこれどうなってるの！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:184 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:184 
msgid "あ、ちょ、ちょっと……\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:185 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:185 
msgid "これはこれで、\nおもしろいよね―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:186 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:186 
msgid "えっ！？\n"
msgstr "Ehh!?\n"

#  speaker 
#  warnings: 0 
#: sc/0222:187 
msgid "みお姉"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:187 
msgid "取れちゃった。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:188 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:188 
msgid "ええええ――――っ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:189 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:189 
msgid "（お―ウェボシ―が勝ったよ）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:190 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:190 
msgid "（ってことは、ついに！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:191 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:191 
msgid "あの、それで僕に\n何か用だったんでしょうか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:192 
msgid "ウェボシ―"
msgstr "Weboshi"

#  text 
#  warnings: 0 
#: sc/0222:192 
msgid "え、えっと……実は、勝てたら\n言おうと思っていたことがあって……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:193 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:193 
msgid "あ、はい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:194 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:194 
msgid "あ、あのさ……もし……\nモシ……モシ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:195 
msgid "みさと＆フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:195 
msgid "（おおおお！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:196 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:196 
msgid "モシカン似合ってるよ！\nじゃあね！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:197 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:197 
msgid "強引に変えた―っ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:198 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:198 
msgid "ちょっと噛んでたけど……\nなんで急に褒められたんだろう？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:199 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:199 
msgid "それじゃ帰ろっか―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:200 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:200 
msgid "あ、ところでフェっちゃん、\nなんで私を探してたの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:201 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:201 
msgid "ん。\n一緒に帰ろうかな―って。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:202 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:202 
msgid "あ、そうなんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:203 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:203 
msgid "うん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:204 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:204 
msgid "……それだけ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:205 
msgid "フェっちゃん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:205 
msgid "うん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:206 
msgid "ウェボシ―"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:206 
msgid "……ま、いっか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:207 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:207 
msgid "おまちなさい、お嬢さん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:208 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:208 
msgid "あ、はい、私ですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:209 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:209 
msgid "……実は先ほど、落とし物を拾いましてな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:210 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:210 
msgid "え、落とし物！？\nごめんなさい、気づいてなかったです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:211 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:211 
msgid "いえいえ、気づけないからこそ、落とし物に\nなるという物でしょう。さあどうぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:212 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:212 
msgid "ありがとうございました！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:213 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:213 
msgid "では、わたしはこれで。\nごきげんよう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:214 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:214 
msgid "わざわざ紙袋に入れてくれたんだ―。\nでも、何を落としたんだっけ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:215 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:215 
msgid "………………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:216 
msgid "安中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:216 
msgid "違う！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:217 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:217 
msgid "貴女、時定高校の先生ですな？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:218 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:218 
msgid "……はあ、なんでしょうか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:219 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:219 
msgid "やれやれ。\nようやく話を聞いてもらえたわ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:220 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:220 
msgid "宗教の勧誘ならお断りしますが……。\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0222:221 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0222:221 
msgid "いえ、そちらの学校にいる少変わった\n生徒に会わせて頂ければと思いまして。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:222 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:222 
msgid "（科学者ではなさそうだが、関係者か。\n　……ま、これは良い口実になるな）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:223 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:223 
msgid "お待たせしました―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:224 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:224 
msgid "ネ、ネ、ネジじゃと！？\nなんじゃ、この変わったロボット娘は！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:225 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:225 
msgid "かかか、変わってなんていません！\nっていうか、ロボットじゃありませんから！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:226 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:226 
msgid "………………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:227 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:227 
msgid "……アレ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:228 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:228 
msgid "おまたせいたしました―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:229 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:229 
msgid "バナナのマフィンがお１つ、\nクレ―ムデニッシュがお１つ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:230 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:230 
msgid "クロワッサンと、シナモンロ―ルと\nシュガ―ド―ナツがお１つずつ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:231 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:231 
msgid "サラダラップとミックスサンドイッチが\nお１つ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:232 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:232 
msgid "エスプレッソマキア―トの\nベンティがお１つ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:233 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:233 
msgid "以上でおそろいでしょうか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:234 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:234 
msgid "は、はい！　もちろんです！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:235 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:235 
msgid "……やった、完亢に全部言えた！\nこれも勝負に勝った勢いってやつ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:236 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:236 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:237 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:237 
msgid "……注文しすぎた。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:238 
msgid "兵隊"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:238 
msgid "……なんてこった！\nここまできて、負けてしまった！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:239 
msgid "鹿"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:239 
msgid "……？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:240 
msgid "兵隊"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:240 
msgid "これでは、ギャラクシ―テレビの\nデジタル化イメ―ジキャラクタ―弌弌\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0222:241 
msgid "兵隊"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0222:241 
msgid "略してギャデジカ！　その役を\nお願いすることができないじゃないか！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:242 
msgid "兵隊"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:242 
msgid "まずいぞ。\n絶対、責任取らされる……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:243 
msgid "兵隊"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:243 
msgid "でも、俺だってクジ引きで決まったんだぞ！\n負けてしまって何が悪い！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:244 
msgid "鹿"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:244 
msgid "………………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:245 
msgid "兵隊"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:245 
msgid "う、うう、ダメだ……開き直っても仕方ない。\n俺はどうすればいいんだ弌弌え、なに？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:246 
msgid "鹿"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:246 
msgid "……コクリ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:247 
msgid "兵隊"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:247 
msgid "もしかして、いいの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:249 
msgid "兵隊"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:249 
msgid "姫様、観念して船にお戻りください！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:250 
msgid "スタ―ラ姫"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:250 
msgid "戻らん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:251 
msgid "兵隊"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:251 
msgid "やっぱり！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:252 
msgid "兵隊"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:252 
msgid "（予想していた通りの返事だが……\n　このままだと、任務は失敗。俺の命は……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:253 
msgid "兵隊"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:253 
msgid "（い、いやだ、死にたくないっ！\n　俺、死にたくないよ！　俺だってまだ弌弌）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:254 
msgid "兵隊"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:254 
msgid "（……ん？　いや、待てよ？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:255 
msgid "兵隊"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:255 
msgid "（姫が戻らないなら、俺も戻らなければ……\n　それで俺も自由になるじゃないか！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:256 
msgid "兵隊"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:256 
msgid "（そう、姫様は余程のことがない限り、\n　考えを曲げられない御方だ。だったら弌弌）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:257 
msgid "兵隊"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:257 
msgid "（だったら、兵隊の任務を放棄するしかない！\n　そうさ、俺はまだ生きたいんだ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:258 
msgid "兵隊"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:258 
msgid "……姫様の意志の固さはわかりました。\nこうなったら、俺もお供させてもらいます！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:259 
msgid "スタ―ラ姫"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:259 
msgid "うわ……だったら戻る。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:260 
msgid "兵隊"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:260 
msgid "死にたい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:261 
msgid "コジロウ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:261 
msgid "メェ―\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:262 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:262 
msgid "ちょ、ちょっと！\n勝負は終わったでしょ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:263 
msgid "コジロウ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:263 
msgid "……メェ\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:264 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:264 
msgid "……もしかして、お腹すいてる？\nでも、今日はあげらそうな紙もないし……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:265 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:265 
msgid "あ、でも待って。あんた野菜も\n食べられるわよね？　だったら弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:266 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:266 
msgid "いらっしゃいま……っ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:267 
msgid "コジロウ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:267 
msgid "メェ―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:268 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:268 
msgid "サラダラップ１つ。\n野菜のみで。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:269 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:269 
msgid "野菜のみ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:270 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:270 
msgid "………………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0222:271 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0222:271 
msgid "できる！\n"
msgstr ""
